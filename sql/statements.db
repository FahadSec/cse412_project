INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EDA', 501, 'Foundations of Educational Leadership', 3, '{}', 'Nature of educational administration and the concept of competency as it applies to educational administration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EDA', 505, 'American Education System', 3, '{}', 'Political, social, historical, and philosophical analyses of American education at all levels. Examines primary sources, legal findings, and case studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EDA', 511, 'School Law', 3, '{}', 'Interpreting state and federal law and policy in school settings. Includes statutory authority for administrators, liability, constitutional rights, law, and ethics.');
