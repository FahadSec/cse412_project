INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 212, 'Applied Engineering Mechanics: Dynamics', 3, '{}', 'Masses; motion kinematics; dynamics of machinery.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 213, 'Applied Mechanics of Materials', 3, '{}', 'Stress, strain, stress-strain relations. Axial, shear, bending, torsional and combined loads and deflections.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 214, 'Applied Mechanics of Materials Laboratory', 1, '{}', 'Measurements of loads and deformations relating stress and strain in axial, shear, bending, torsional, and combined loading configurations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 230, 'Introduction to Engineering Materials', 2, '{}', 'Introduces materials and their properties, emphasizing basic concepts and structures and how these properties relate to manufacturing and design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 300, 'Applied Material Science', 3, '{}', 'Principles of materials science emphasizing concepts relevant to design, manufacturing, and use. Covers metals, polymers, ceramics, and composites.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 309, 'Nondestructive Testing and Quality Assurance', 1, '{}', 'Part and material inspection using metrology and nondestructive inspection tools and techniques. Theory and application with use of pertinent standards.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 321, 'Introduction to Automotive Engineering', 3, '{}', 'Engineering principles and the design of ground vehicles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 331, 'Machine Design I', 3, '{}', 'Applies mechanics to design of machine elements and structures. Stress analysis, failure modes, tolerances, cylindrical fits, and shaft design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 344, 'Casting and Forming Processes', 3, '{}', 'Analyzes various forming processes to determine load requirements necessary for a particular metal-forming operation. Information used to select equipment and design tooling. Metal casting processes and design of castings. Introduces powder metallurgy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 345, 'Advanced Manufacturing Processes', 3, '{}', 'Material removal processes emphasizing advanced turning, milling, and machinability studies using cutting tools. CNC programming for machining and turning centers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 351, 'Introduction to Automation', 3, '{}', 'Introduces automation. Topics include assembly techniques, fixed and flexible automation systems, robots, material-handling systems, sensors, and controls.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 396, 'Professional Orientation', 1, '{}', 'Topics related to a successful career in engineering technology industry. Includes safety, life-long learning, ethics, and career planning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 401, 'Quality Assurance', 3, '{}', 'Statistical quality control methods, design of experiments, sampling, gauge requirements and specifications, use of quality assurance tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 409, 'Applied Engineering Economics', 3, '{}', 'Fundamentals of engineering economics in a practical, industry-based approach. Includes effects of depreciation, taxes, inflation, and replacement analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 410, 'Manufacturing Resource Management', 3, '{}', 'Measures like cycle time, throughput, capacity, work-in-process, inventory, variability, and how they drive operating relationships in a factory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 415, 'Manufacturing Simulation', 3, '{}', 'Computer simulation of manufacturing operations. Discrete event simulation models range from individual processes to whole factories.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 416, 'Applied Computer-Integrated Manufacturing', 3, '{'CS'}', 'Techniques and practices of computer-integrated manufacturing as applied in a broad range of industry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 418, 'Composites Materials Manufacturing', 3, '{}', 'Introduces composite materials and associated manufacturing issues, including tooling, processes, and quality control. Related issues, including testing and joining.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 421, 'Vehicle Powertrains', 3, '{}', 'Internal combustion engine design, performance and operation, and power transmission systems within vehicles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 423, 'Vehicle Chassis and Running Gear Design', 3, '{}', 'Design and manufacture of chassis, structures, steering and braking systems and components, wheels and tires, suspension systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 424, 'Vehicle Electrical Systems', 3, '{}', 'Vehicle electrical power and signal systems, computer control and analog control systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 426, 'Vehicle Thermal Design', 3, '{}', 'Heat management in vehicle systems, including thermal design of engine and transmission cooling systems, cabin heating, and air conditioning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 427, 'Vehicle System Integration and Testing', 3, '{}', 'Systems engineering principles. Design of the interfaces between vehicle subsystems. Testing of integrated systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 432, 'Engineering Thermo-Fluids II', 3, '{}', 'Thermodynamic analysis of mixtures and power cycles. Design and analysis of fluid systems, conduction heat transfer, and introduction to convection and radiation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 434, 'Engineering Thermo-Fluids III', 3, '{}', 'Transient conduction, convection and radiation heat transfer, heat exchangers, thermo-fluid systems engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 438, 'Machine Design II', 3, '{}', 'Applies mechanics to the design of machine elements and structures. Emphasizes basics of gears, springs, brakes, clutches, and bearings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 440, 'Finite Element Analysis', 3, '{}', 'Analyzes machine elements, frames, and aerospace structures employing finite element analysis and software. Includes sensitivity analysis and estimating accuracy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 442, 'Specialized Production Processes', 3, '{}', 'Nontraditional manufacturing processes, emphasizing EDM, ECM, ECG, CM, PM, HERF, EBW, and LBW.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 443, 'CNC Computer Programming', 3, '{}', 'Theory and application of N/C languages using CAM software and CNC machine tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 444, 'Production Tooling', 3, '{}', 'Design and fabrication of jigs, fixtures, and special industrial tooling related to manufacturing methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 455, 'Automation Systems Integration', 3, '{}', 'Applies sensors and devices and their integration with PLCs and computers into automated devices and systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 460, 'Capstone Project I', 3, '{}', 'Group project designing, evaluating, and analyzing components, assemblies, and systems. Develop products/manufacturing techniques demonstrating state-of-the-art technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MET', 461, 'Capstone Project II', 3, '{}', 'Small-group projects applying manufacturing techniques, with emphasis on demonstrating state-of-the-art technology.');
