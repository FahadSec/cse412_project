INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 496, 'Undergraduate Thesis', 3, '{}', 'Guided research culminating in the preparation of an undergraduate thesis based on supervised research done in this and previous semesters.');
