INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EDT', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 120, 'Digital Design Fundamentals', 3, '{}', 'Number systems, conversion methods, binary and complement arithmetic, Boolean algebra, circuit minimization, ROMs, PLAs, flipflops, synchronous sequential circuits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 202, 'Circuits I', 4, '{}', 'Principles for analyzing linear and nonlinear circuits.  Uses SPICE and MATLAB. Design and measurement of linear analog electrical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 203, 'Signals and Systems I', 3, '{}', 'Introduces continuous and discrete time signal and system analysis, linear systems, Fourier, and z-transforms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 230, 'Computer Organization and Assembly Language Programming', 3, '{}', 'Register-level computer organization. Instruction set architecture. Assembly language. Processor organization and design. Memory organization. IO programming. Exception/interrupt handling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 241, 'Fundamentals of Electromagnetics', 3, '{}', 'Vector analysis, differential operators, fourier analysis, scalar, vector fields, electro/magneto statics, time-varying fields, boundary value problems, dielectric, magnetic materials, Maxwell's equations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 304, 'Signals and Systems II', 4, '{}', 'Communication, signal processing, control systems, continuous, discrete transforms, sampling theorem, analog, digital modulation, filter design, signal processing applications, state space.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 307, 'Signal Processing for Digital Culture', 3, '{}', 'Signal processing for non-majors. Digital filters, Fourier and z transform, DFT and FFT, digital signal processing computing, interdisciplinary DSP applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 333, 'Hardware Design Languages and Programmable Logic', 4, '{}', 'Develops digital logic with modern practices of hardware description languages. Emphasizes usage, synthesis of digital systems for programmable logic, VLSI.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 334, 'Circuits II', 4, '{}', 'Design of analog and digital circuits. Diodes/BJTs/Mosfets. Digital and analog circuit building blocks. Fundamentals of mixed signal circuits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 335, 'Analog and Digital Circuits', 4, '{}', 'Analog, digital microelectronic circuits and systems. Amplifiers, frequency response, gate sizing, timing analysis, sequential digital circuits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 341, 'Engineering Electromagnetics', 4, '{}', 'Time-varying electromagnetic fields, waves in homogeneous and stratified media, transmission lines, waveguides and cavity resonators, radiation and antennas.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 350, 'Random Signal Analysis', 3, '{}', 'Probabilistic and statistical analysis as applied to electrical signals and systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 352, 'Properties of Electronic Materials', 4, '{}', 'Schrodinger's wave equation, potential barrier problems, bonds of crystals, the band theory of solids, semiconductors, superconductor dielectric, and magnetic properties.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 360, 'Energy Systems and Power Electronics', 4, '{}', 'Conventional and alternate energy sources for power systems, three-phase analysis, AC generators, transformers, induction, DC motors, power electronic speed control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 404, 'Real-Time DSP Systems', 4, '{}', 'Digital signal processors, translating signals and systems concepts into real-time multimedia and communications applications, real-time algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 407, 'Digital Signal Processing', 4, '{}', 'Time and frequency domain analysis, difference equations, z-transform, FIR and IIR digital filter design, discrete Fourier transform, FFT, and random sequences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 419, 'Python for Rapid Engineering Solutions', 3, '{}', 'Rapid engineering solutions using Python libraries and functions readily available on the internet.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 425, 'Digital Systems and Circuits', 4, '{}', 'Digital logic gate analysis and design. Propagation delay times, fan out, power dissipation, noise margins. Design of MOS and bipolar logic families, including NMOS, CMOS, standard and advanced TTL, ECL, and BiCMOS. Inverter, combinational and sequential logic circuit design, MOS memories, VLSI circuits. Computer simulations using PSPICE.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 433, 'Analog Integrated Circuits', 4, '{}', 'Analysis, design, and applications of modern analog circuits using integrated bipolar and field-effect transistor technologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 434, 'Quantum Mechanics for Engineers', 3, '{}', 'Angular momentum, wave packets, Schroedinger wave equation, probability, problems in one dimension, principles of wave mechanics, scattering, tunneling, central forces, angular momentum, hydrogen atom, perturbation theory, variational techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 435, 'Fundamentals of CMOS and MEMS', 3, '{}', 'Introduces basic CMOS processing and fabrication tools. Covers the fundamentals of thermal oxidation, CVD, implantation, diffusion, and process integration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 436, 'Fundamentals of Solid-State Devices', 3, '{}', 'Semiconductor fundamentals, pn junctions, metal-semiconductor contacts, metal-oxide-semiconductor capacitors and field-effect transistors, bipolar junction transistors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 437, 'Optoelectronics', 3, '{}', 'Basic operating principles of various types of optoelectronic devices that play important roles in commercial and communication electronics; light-emitting diodes, injection lasers, and photodetectors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 439, 'Semiconductor Facilities and Cleanroom Practices', 3, '{}', 'Microcontamination, controlled environments, cleanroom layout and systems, modeling, codes and legislation, ultrapure water, production materials, personnel and operations, hazard management, advanced concepts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 443, 'Antennas for Wireless Communications', 3, '{}', 'Fundamental parameters; radiation integrals; wireless systems; wire, loop, and microstrip antennas; antenna arrays; smart antennas; ground effects; multipath.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 445, 'Microwaves', 4, '{}', 'Waveguides; circuit theory for waveguiding systems; microwave devices, systems, and energy sources; striplines and microstrips; impedance matching transformers; measurements.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 448, 'Fiber Optics', 3, '{}', 'Principles of fiber-optic communications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 455, 'Communication Systems', 4, '{}', 'Signal analysis techniques applied to the operation of electrical communication systems. Introduction to and overview of modern digital and analog communications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 459, 'Communication Networks', 3, '{}', 'Fundamentals of communication networks. Study of Seven-Layer OSI model. Focus on functionality and performance of protocols used in communication networks.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 460, 'Nuclear Power Engineering', 3, '{}', 'Radioactivity and decay. Radiation interactions and dose. Nuclear reaction, fission and fusion theory. Fission reactors, four factor formula, moderation. Nuclear power, TMI, Chernobyl. Nuclear fuel cycle.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 463, 'Electrical Power Plants', 3, '{}', 'Generation of electric power using fossil, nuclear, and renewable, including solar, geothermal, wind, hydroelectric, biomass and ocean, energy sources. Power plant thermal cycle analysis. Cogeneration and combined cycles. Economics, operations, and design of electric power stations. Energy storage.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 465, 'Photovoltaic Energy Conversion', 3, '{}', 'Photovoltaics for production of electricity from solar energy. The grand energy challenge. Solar radiation and utilization. Fundamentals of semiconductors for photovoltaics: crystal structures, band theory, doping, carrier statistics, optical absorption, and p-n junctions. Principles of solar cells including loss mechanisms and design principles. Current solar cell technologies. Manufacturing of silicon solar cells. Solar modules, arrays and systems. Economic analysis of solar photovoltaic systems. Terawatt-scale deployment of solar cells.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 470, 'Electric Power Devices', 3, '{}', 'Analyzes devices used for short circuit protection, including circuit breakers, relays, and current and voltage transducers. Protection against switching and lightning over voltages. Insulation coordination.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 471, 'Power System Analysis', 3, '{}', 'Review of transmission line parameter calculation. Zero sequence impedance, symmetrical components for fault analysis, short circuit calculation, review of power flow analysis, power system stability, and power system control concepts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 472, 'Power Electronics and Power Management', 3, '{}', 'Principles of switch mode power conversion, analysis, design and control of dc-dc converters, PWM rectifiers and inverters, power management, power electronics applications in information technology, renewable energy systems, motion control and lighting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 473, 'Electrical Machinery', 3, '{}', 'Operating principles, modeling and speed control of DC, brushless DC, and AC machines; power converters for DC and AC motors drives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 480, 'Feedback Systems', 4, '{}', 'Analysis and design of linear feedback systems. Frequency response and root locus techniques, series compensation, and state variable feedback.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 481, 'Computer-Controlled Systems', 4, '{}', 'Implements computer-based, embedded, control systems using MATLAB xPC Target toolbox. Small-scale, representative projects demonstrate theoretical issues and provide hands-on expertise.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 488, 'Senior Design Laboratory I', 3, '{'L'}', 'Design process: research, concept, feasibility, simulation, specifications, benchmarking, and proposal generation. Technical communications and team skills enrichment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 489, 'Senior Design Laboratory II', 3, '{'L'}', 'Implement, evaluate, and document EEE 488 design. Social, economic, and safety considerations. Technical communications and team skills enrichment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 498, 'Pro-Seminar', 7, '{}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 505, 'Time-Frequency Signal Processing', 3, '{}', 'Joint time-frequency analysis of time-varying signals and systems; linear and quadratic time-frequency representations; applications in current areas of signal processing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 506, 'Digital Spectral Analysis', 3, '{}', 'Principles and applications of digital spectral analysis, least squares, random sequences, parametric, and nonparametric methods for spectral estimation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 507, 'Multidimensional Signal Processing', 3, '{}', 'Processing and representation of multidimensional signals. Design of systems for processing multidimensional data. Introduces image and array processing issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 508, 'Digital Image and Video Processing and Compression', 3, '{}', 'Fundamentals of digital image perception, representation, processing, and compression. Emphasizes image coding techniques. Signals include still pictures and motion video.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 509, 'DSP Algorithms and Software', 3, '{}', 'Linear systems review, digital filter design, software aspects, DFT, FFT, random signals, programming aspects, applications projects, MATLAB and Java simulations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 510, 'Multimedia Signal Processing', 3, '{}', 'Speech/audio coding algorithms. LPC, CELP, MPEG, Cell phone, DTV, cinema, and surround sound standards. MPEG/JPEG introduction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 511, 'Artificial Neural Computation', 3, '{}', 'Networks for computation, learning function representations from data, learning algorithms and analysis, function approximation and information representation by networks, applications in control systems and signal analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 512, 'System-Level Design for Heterogeneous Multiprocessor Architectures', 3, '{}', 'In-depth introduction to multicore computing architectures ranging from low-power multiprocessor systems-on-chip (MpSoC) to high-performance chip-multiprocessors (CMP). Fundamental topics on modeling, analysis, and optimization of multicore systems; particular attention on low-power and communication-centric design of multicore systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 515, 'Machine Vision and Pattern Recognition', 3, '{}', 'Covers the field of computer vision, whose broad goal is to create algorithms and systems for processing of visual signals (e.g., images, videos) for low-level, mid-level, and high-level perceptual tasks. Presents the broad principles and techniques for devising computer vision algorithms starting from understanding the imaging process for a pin-hole camera; understanding lenses, image-statistics such as gradients and edges, 3D structure estimation, motion estimation, illumination modeling to perceptual tasks such as shape recognition, texture modeling, face recognition, activity recognition, and scene recognition. The class is a mixture of in-class lectures and discussions, and individual and group projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 516, 'Physics-Based Computer Vision', 3, '{}', 'Covers topics in physics-based computer vision and graphics. Includes work on visual computing systems including computational cameras, illumination systems, 3D scanners, rendering/animation and displays. Starting with the basics of traditional photography and the imaging pipeline, proceeds to explore new ways to capture visual information by co-designing camera hardware and software algorithms. Topics covered include plenoptic or multi-modal imaging, coded and compressive sensing, light transport and rendering, programmable illumination, and computational displays. Focuses on understanding and evaluating new imaging technology for specific applications including autonomous vehicles, entertainment and graphics, computer vision and visual recognition, and robotics. Course content delivered via lectures along with written and programming assignments as well as a final project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 517, 'Sensors and Machine Learning', 1, '{}', 'Integrated sensor devices and algorithms; microphone arrays, chemical sensing, mobile sensing; accelerometers and magnetometers, nanopore sensors, and flexible and patch sensors. Signal pre-processing for sensors; feature extraction; image sensing; voice processing; filters; frequency analysis; autocorrelation; principal components; k means algorithm; sensor fusion; neural nets; applications to health, security and mobile systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 520, 'VLSI Design for Reliability', 3, '{}', 'Modeling and design solutions for very large-scale integration (VLSI) reliability. Statistical design under static and dynamic variability. Aging effects and resilient design techniques. Radiation effects in VLSI.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 521, 'Low-Power Bioelectronics', 3, '{}', 'Begins with fundamental theory and techniques for low-power analog circuit design especially subthreshold CMOS and BJT circuits (e.g., translinear circuits), then moves to biomedical applications and bio-inspired systems focused upon neuromorphic circuits. Also touches on concepts such as wireless challenges for implants, energy harvesting and electrochemistry. Students have the opportunity to have their final projects fabricated in a commercial CMOS process.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 522, 'Radio Frequency Test', 4, '{}', 'Covers current production test schemes for mixed-signal and RF circuits, the economics of production test, and recent research techniques in reducing the production test cost. Prior knowledge of Matlab is necessary to be successful in this class.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 523, 'Advanced Analog Integrated Circuits', 4, '{}', 'Analysis and design of analog integrated circuits: analog circuit blocks, reference circuits, operational-amplifier circuits, feedback, and nonlinear circuits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 524, 'Communication Transceiver Circuits Design', 4, '{}', 'Communication transceivers and radio frequency system design; fundamentals of transceivers circuits; RF, IF, mixers, filters, frequency synthesizers, receivers, CAD tools, and lab work on IC design stations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 525, 'VLSI Design', 4, '{}', 'Analysis and design of Very Large Scale Integrated (VLSI) circuits. Physics of small devices, fabrication, regular structures, and system timing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 526, 'VLSI Architectures', 3, '{}', 'High throughput and low-power VLSI architectures for signal processing. Array processor systems; data path design and optimization; memory design; high-level synthesis; low-power design at system level, algorithm level, and architecture level.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 527, 'Analog to Digital Converters', 4, '{}', 'Detailed introduction to the design of Nyquist rate, CMOS analog to digital converters. Requires knowledge of analog integrated circuits (analog circuit blocks, reference circuits, operational-amplifier circuits, feedback, and nonlinear circuits) to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 528, 'Sensors for the Internet of Things and Wearable Devices', 3, '{}', 'Fundamentals, concepts of system analysis and design, and principles that apply to phase-locked loops (PPLs) used in frequency synthesis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 529, 'Semiconductor Memory Technologies and Systems', 4, '{}', 'Design of semiconductor memory technologies and systems, from the device cell structures to the array and architecture design, with emphasis on the industry trends and cutting-edge technologies including SRAM, DRAM and FLASH technologies and emerging memory technologies such as STT-MRAM, PCRAM and RRAM.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 530, 'Advanced Silicon Processing', 3, '{}', 'Thin films, CVD, oxidation, diffusion, ion-implantation for VLSI, metallization, silicides, advanced lithography, dry etching, rapid thermal processing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 531, 'Semiconductor Device Theory I', 3, '{}', 'Transport and recombination theory, pn and Schottky barrier diodes, bipolar and junction field-effect transistors, and MOS capacitors and transistors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 532, 'Semiconductor Device Theory II', 3, '{}', 'Advanced MOSFETs, charge-coupled devices, solar cells, photodetectors, light-emitting diodes, microwave devices, and modulation-doped structures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 533, 'Semiconductor Process/Device Simulation', 4, '{}', 'Device simulation concepts: conventional and advanced MOS devices, bipolar transistors, heterostructures including HEMTs and solar cells. Process simulation concepts: oxidation, ion implantation, diffusion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 534, 'Semiconductor Transport', 3, '{}', 'Carrier transport in semiconductors. Hall effect, high electric field, Boltzmann equation, correlation functions, and carrier-carrier interactions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 535, 'Electron Transport in Nanostructures', 3, '{}', 'Nanostructure physics and applications. 2-D electron systems, quantum wires and dots, ballistic transport, quantum interference, and single-electron tunneling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 536, 'Semiconductor Characterization', 3, '{}', 'Measurement techniques for semiconductor materials and devices. Electrical, optical, physical, and chemical characterization methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 537, 'Semiconductor Optoelectronics', 3, '{}', 'Electronic states in semiconductors, quantum theory of radiation, absorption processes, radiative processes, nonradiative processes, photoluminescence, and photonic devices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 538, 'Optoelectronic Devices', 3, '{}', 'Provides graduate students with detailed theory and practical knowledge of semiconductors materials and optoelectronic devices such as light-emitting diodes, lasers, photodetectors and solar cells. Also discusses the applications of these devices. Offers not only classroom lectures but also demonstration of real-world experiments in research laboratories at ASU.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 539, 'Intro Solid-State Electronics', 3, '{}', 'Crystal lattices, reciprocal lattices, quantum statistics, lattice dynamics, equilibrium, and nonequilibrium processes in semiconductors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 540, 'Fast Computational Electromagnetics', 3, '{}', 'Method of moments, finite difference time-domain, finite element methods implemented using fast algorithms (wavelets, FMM, Nystrom) to gain high efficiency.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 541, 'Electromagnetic Fields and Guided Waves', 3, '{}', 'Polarization and magnetization; dielectric, conducting, anisotropic, and semiconducting media; duality, uniqueness, and image theory; plane wave functions, waveguides, resonators, and surface guided waves.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 543, 'Antenna Analysis and Design', 3, '{}', 'Impedances, broadband antennas, frequency independent antennas, miniaturization, aperture antennas, horns, reflectors, lens antennas, and continuous sources design techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 544, 'High-Resolution Radar', 3, '{}', 'Fundamentals; wideband coherent design, waveforms, and processing; stepped frequency; synthetic aperture radar (SAR); inverse synthetic aperture radar (ISAR); imaging.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 545, 'Microwave Circuit Design', 3, '{}', 'Analysis and design of microwave attenuators, in-phase and quadrature-phase power dividers, magic tees, directional couplers, phase shifters, DC blocks, and equalizers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 546, 'Advanced Fiber Optics', 3, '{}', 'Theory of propagation in fibers, couplers and connectors, distribution networks, modulation, noise and detection, system design, and fiber sensors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 547, 'Microwave Solid-State Circuit Design I', 3, '{}', 'Applies semiconductor characteristics to practical design of microwave mixers, detectors, limiters, switches, attenuators, multipliers, phase shifters, and amplifiers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 548, 'Coherent Optics', 3, '{}', 'Diffraction, lenses, optical processing, holography, electro-optics, and lasers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 550, 'Transform Theory and Applications', 3, '{}', 'Introduces abstract integration, function spaces, and complex analysis in the context of integral transform theory. Applications to signal analysis, communication theory, and system theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 551, 'Information Theory', 3, '{}', 'Entropy and mutual information, source and channel coding theorems, applications for communication and signal processing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 552, 'Digital and Wireless Communications', 3, '{}', 'Complex signal theory, digital modulation, optimal coherent and incoherent receivers, channel codes, coded modulation, Viterbi algorithm.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 553, 'Coding and Applications', 3, '{}', 'Introduces algebra, block and convolutional codes, decoding algorithms, turbo codes, coded modulation, private and public key cryptography.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 554, 'Probability and Random Processes', 3, '{}', 'Applies statistical techniques to the representation and analysis of electrical signals and to communications systems analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 556, 'Detection and Estimation Theory', 3, '{}', 'Combines the classical techniques of statistical inference and the random process characterization of communication, radar, and other modern data processing systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 557, 'Broadband Networks', 3, '{}', 'Physics of wireless and optical communications. Broadband multiplexing and switching methods. Blocking and queuing analysis. Network optimization, routing, and economics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 558, 'Recent Advances in Communications', 3, '{}', 'Cellular systems, path loss, multipath fading channels, modulation and signaling for wireless, diversity, equalization coding, spread spectrum, TDMA/FDMA/CDMA.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 559, 'Wireless Networks', 3, '{}', 'Design principles of cellular networks. Multiple access control protocols for wireless systems. Wireless routing and TCP/IP. Mobile management. Call admission control and resource allocation (e.g., power control and rate control). Wireless security. Future-generation wireless networks. A previous course in random signal theory is required to be successful in this class.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 560, 'Mathematical Foundations of Machine Learning', 3, '{}', 'Serves as a primer in statistical learning theory and as a platform for exploring emerging algorithms and theory in large-scale data analytics and learning. This study is at the intersection of information processing, statistical theory and computational sciences. Contains a healthy mix of topics from all of these disciplines.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 562, 'Nuclear Reactor Theory and Design', 3, '{}', 'Principles of neutron chain reacting systems. Neutron diffusion and moderation. One-, two-, and multigroup diffusion equation solution methods. Heterogeneous reactors. Nuclear fuel steady-state performance. Core thermal-hydraulics. Core thermal design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 563, 'Nuclear Reactor System Dynamics and Diagnostics', 3, '{}', 'Time-dependent solution to neutron diffusion equation. Reactor kinetics and reactivity changes. Dynamics, stability, and control of reactor systems. Modeling neutronic and thermal processes. System characterization in time and frequency domains. Reactor surveillance and diagnostics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 564, 'Interdisciplinary Nuclear Power Operations', 3, '{}', 'Nuclear power plant systems. Studies interrelationship and propagation of effects that systems and design changes have on one another, especially in relation to nuclear power plant safety and operations. Case studies and design projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 565, 'Solar Cells', 3, '{}', 'Introduction to the generation and utilization of electricity from solar energy. Exploration of the science and engineering of direct conversion (photovoltaics), including the design, fabrication, and operation of solar cells, and the construction and performance of solar cell modules. Prior knowledge of properties of electronic materials is required to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 566, 'Advanced Device Modeling and Simulation', 3, '{}', 'Understanding semi-classical and quantum transport theory in conjunction with device simulations at the nanoscale. Prior knowledge of semiconductor device theory, quantum mechanics and classical semiconductor device simulation is necessary to be successful in this class.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 571, 'Power System Transients', 3, '{}', 'Simple switching transients. Transient analysis by deduction. Damping of transients. Capacitor and reactor switching. Transient recovery voltage. Travelling waves on transmission lines. Lightning. Protection of equipment against transient overvoltages. Introduces computer analysis of transients.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 572, 'Advanced Power Electronics', 3, '{}', 'Analyzes device operation, including thyristors, gate-turn-off thyristors, and transistors. Design of rectifier and inverter circuits. Applications such as variable speed drives, HVDC, motor control, and uninterruptable power supplies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 573, 'Electric Power Quality', 3, '{}', 'Sinusoidal waveshape maintenance; study of momentary events, power system harmonics, instrumentation, filters, power conditioners, and other power quality enhancement methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 574, 'Computer Solution of Power Systems', 3, '{}', 'Algorithms for digital computation for the Newton and fast-decoupled power flow problem, and fault analysis. Sparse matrix and vector programming methods, creation of elimination trees, network equivalencing, solution of the least squares problem, introduction to state estimation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 575, 'Power System Stability', 3, '{}', 'Dynamic performance of power systems with emphasis on stability. Modeling of system components and control equipment. Analysis of the dynamic behavior of the system in response to small and large disturbances. Knowledge of EEE470, 471 and 473 (or equivalents) is required to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 576, 'Power System Dynamics', 3, '{}', 'Dynamic performance of power systems with emphasis on control. Modeling of control equipment, FACTS devices, wind generators, and nonlinear loads. Design of power system stabilizers. Prior knowledge of electric power devices, power system analysis, and electrical machinery is required to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 577, 'Power Engineering Operations and Planning', 3, '{}', 'Economic dispatch, unit commitment, dynamic programming, power system planning and operation, control, generation modeling, AGC, and power production.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 579, 'Power Transmission and Distribution', 3, '{}', 'High-voltage transmission line electric design; conductors, corona, RI and TV noise, insulators, clearances. DC characteristic, feeders voltage drop, and capacitors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 581, 'Filtering of Stochastic Processes', 3, '{}', 'Modeling, estimation, and filtering of stochastic processes, with emphasis on the Kalman filter and its applications in signal processing and control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 582, 'Linear System Theory', 3, '{}', 'Controllability, observability, and realization theory for multivariable continuous time systems. Stabilization and asymptotic state estimation. Disturbance decoupling, noninteracting control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 585, 'Security and Privacy in Networked Systems', 3, '{}', 'Comprehensive understanding of critical cyber security and privacy threats as well as corresponding solutions in emerging wireless networks, mobile systems, social networks, Internet-of-Things, critical infrastructures, cloud computing, big data analytics, wearable and edge computing, and mobile health. Requires prior knowledge of communications systems or computer networking to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 586, 'Nonlinear Control Systems', 3, '{}', 'Stability theory, including phase-plane, describing function, Liapunov's method, and frequency domain criteria for continuous and discrete, nonlinear, and time-varying systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 587, 'Optimal Control', 3, '{}', 'Optimal control of systems. Calculus of variations, dynamic programming, linear quadratic regulator, numerical methods, and Pontryagin's principle.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 588, 'Design of Multivariable Control Systems', 3, '{}', 'Practical tools for designing robust MIMO controllers. State feedback and estimation, model-based compensators, MIMO design methodologies, CAD, real-world applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 589, 'Convex Optimization', 3, '{}', 'Linear algebra and convex optimization. Vector spaces, matrix algebra, linear programming, Lagrange multipliers, Karush-Kuhn-Tucker (KKT) conditions, duality theory and algorithms for convex optimization. Newton's method, gradient and steepest descent methods. Algorithms for unconstrained, equality constrained and inequality constrained problems, which include interior point methods. Applications to approximation and data fitting and some geometric problems. Applications to signal processing, communications and control systems. Background in linear algebra necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 606, 'Adaptive Signal Processing', 3, '{}', 'Principles and applications of adaptive signal processing, adaptive linear combiner, Wiener least-squares solution, gradient search, performance surfaces, LMS/RLS algorithms, block time/frequency domain LMS.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 607, 'Speech Compression and Recognition', 3, '{}', 'Speech and audio coding algorithms for applications in wireless communications and multimedia computing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 625, 'Advanced VLSI Design', 4, '{}', 'Practical industrial techniques, circuits, and architectures appropriate to high-performance and low-power digital VLSI designs such as microprocessors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 627, 'Oversampling Sigma-Delta Data Converters', 4, '{}', 'Introduces design and analysis of sigma delta oversampled data converters from an IC design perspective.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 641, 'Advanced Electromagnetic Field Theory', 3, '{}', 'Cylindrical wave functions, waveguides, and resonators; spherical wave functions and resonators; scattering from planar, cylindrical, and spherical surfaces; Green's functions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 643, 'Advanced Topics in Electromagnetic Radiation', 3, '{}', 'High-frequency asymptotic techniques, geometrical and physical theories of diffraction (GTD and PTD), moment method (MM), radar cross section (RCS) prediction, Fourier transforms in radiation, and synthesis methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 680, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 684, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 686, 'Adaptive Control', 3, '{}', 'Main topics covered: adaptive identification, convergence, parametric models, performance and robustness properties of adaptive controllers, persistence of excitation, and stability.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 690, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 731, 'Advanced MOS Devices', 3, '{}', 'Threshold voltage, subthreshold current, scaling, small geometry effects, hot electrons, and alternative structures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EEE', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 210, 'Circuit Analysis I', 3, '{}', 'Parallel/Series circuits, ohm's law, network theorems, capacitors/inductors and transient analysis. Overview of electronic devices, BJT's, FETs, op-amps.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 310, 'Circuit Analysis II', 3, '{}', 'Electronic device analysis and introduction to design. Power supplies, diode circuits, FETs, BJTs, op-amps. Emphasizes frequency response.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 315, 'Digital Systems II', 3, '{}', 'Digital electronics systems, HDL circuits and systems design techniques, and device applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 350, 'Microcontroller Applications', 3, '{}', 'Interfacing of microcontrollers to sensors, switches, indicators, and other human interfaces using the C programming language. Applications involve the integration of both hardware and software. Also addresses applications dealing with open- and closed-loop applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 370, 'Sensors', 3, '{}', 'Overview of sensor technology, including mechanical, magnetic, radiation, thermal, chemical, and biological applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 380, 'Electronic Devices', 3, '{}', 'Covers materials, structures, and technology for the design, production, and application of the major categories of electronic devices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 410, 'Electronic System Design', 3, '{}', 'Analysis and design of Op-amps, power amplifiers, and digital logic families, Feedback design using frequency response. Computer analysis and design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 465, 'Statistical Process Control', 3, '{}', 'Introduces statistics, seven QC tools, and control charts for data analysis and interpretation; process capability, 6-sigma, lean manufacturing, and quality system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 470, 'Capstone Design I', 3, '{}', 'Small-group projects designing, building, evaluating, and analyzing components, assemblies, and systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 471, 'Capstone Design II', 3, '{}', 'Continuation of a small-group project encompassing designing, building, evaluating, and analyzing components, assemblies, and systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EST', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EET', 406, 'Control System Technology', 4, '{}', 'Control system components, analysis of feedback control systems, stability, performance, and application.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EET', 408, 'Digital Real-Time Control', 3, '{}', 'Analysis, simulation, and design of discrete systems. Approach taken in this class begins with the analysis, simulation and design of a continuous system, which yields an s-based, transfer function compensator. Then, this compensator is transformed into a z-based transfer function, which can be realized with a difference equation in a microcontroller.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EET', 430, 'Instrumentation and Measurement Systems', 3, '{}', 'Instrumentation and measurement techniques, sensors operating principles. Signal and error analysis. Signal conditioning and data acquisition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EET', 506, 'System Dynamics and Control', 3, '{}', 'Time, frequency, and transform domain analysis of physical systems. Transfer function analysis of feedback control systems performance and stability. Compensation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EET', 508, 'Digital Real-Time Control', 3, '{}', 'Sample data control techniques and applications to process control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EET', 530, 'Electronic Test Systems and Applications', 3, '{}', 'Analysis, design, and application of electronic test equipment, test systems, specifications, and documentation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 284, 'Introduction to Integrated STEAM Learning Experiences', 3, '{}', 'Examines theories and frameworks for guiding STEAM (Science, Technology, Engineering, Arts/Aesthetics, Mathematics) instruction in K-8 classrooms. Exposes prospective educators to strategies and tools for creating equitable and inclusive learning experiences. Addresses planning, enacting, and assessing effective disciplinary integrated approaches');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 324, 'Social Studies in Elementary Schools', 3, '{}', 'Provides strategies for integrating social studies content knowledge with other curricular areas. Addresses a variety of issues related to social studies instruction in the elementary school, with an emphasis on valuing diverse cultures and experiences within historical and geographical perspectives. Emphasizes meeting the academic and linguistic needs of all learners including English language learners. Explores instructional materials, technology tools and social studies programs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 396, 'Clinical Experience I', 2, '{}', 'Develops knowledge and skills for effective classroom management, establishing a positive classroom environment, and effective lesson delivery. Introduces use of the TAP instructional rubric, the college professionalism rubric, and strategies for co-teaching. Provides opportunities for students to engage as a teacher and learner in a K-8 classroom setting. Emphasizes focused observation; co-planning with a placement teacher; and co-teaching lessons to a whole group, small group and individual students. Successful completion of EED 396 fulfills two (2) of eight (8) required semester hours of practicum in grades K-8, as required by SBR R7-2-608 for an Elementary Education Teaching Certificate.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 397, 'Clinical Experience II', 2, '{}', 'Develops knowledge and skills for effective classroom management, establishing a positive classroom environment, and effective lesson delivery. Continues use of the TAP instructional rubric, the college professionalism rubric, and strategies for co-teaching. Provides opportunities for students to engage as a teacher and learner in a classroom setting. Emphasizes focused observation; co-planning with a placement teacher; and co-teaching lessons to a whole group, small group and individual students. Successful completion of this course fulfills two (2) of eight (8) required semester hours of practicum as required by SBR R7-2-608 for an Elementary Education Teaching Certificate or two (2) of eight (8) required semester hours of practicum as required by SBR R7-2-602 for a Special Education Teaching Certificate.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 411, 'Science in Elementary Schools', 3, '{}', 'Science education standards, curriculum, instructional and assessment strategies for all children.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 412, 'Mathematics in Elementary Schools', 3, '{}', 'Methods of teaching mathematics to all students at the K-8 level, instructional planning and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 433, 'Writing in the 21st Century', 3, '{}', 'Theory on the social nature of oral and written language and congruent teaching, management and assessment practices. Emphasizes skills, strategies and dispositions for teaching 21st-century multimodal literacies to diverse learners including ELL with a focus on learners as creators of content, communicators, collaborators and global citizens.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 478, 'Student Teaching: Elementary School (Grades 1-8)', 15, '{}', 'Student teaching in elementary education classrooms grades 1-8: synthesized experience in curriculum, instruction, assessment and classroom management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 511, 'Curriculum Theory and Instructional Planning', 3, '{}', 'Contemporary curriculum theories. Curriculum as an interrelated entity. Process for instructional planning for student understanding and achievement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 521, 'Instructional Planning for the Diverse Classroom', 3, '{}', 'Planning and delivering instruction; making adaptations for English language learners and students with special needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 524, 'Social Studies Methods and Assessment', 3, '{}', 'Provides strategies for integrating social studies content knowledge with other curricular areas. Addresses a variety of issues related to social studies instruction in the elementary school, with an emphasis on valuing diverse cultures and experiences within historical and geographical perspectives. Emphasizes meeting the academic and linguistic needs of all learners including English language learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 529, 'Science Methods and Assessment', 2, '{}', 'Focuses on inquiry-based processes, lesson design, teaching methods, and content overview to support science instruction in elementary grades.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 531, 'Teaching with Educational Technology', 3, '{}', 'Focuses on using technology in K-12 classrooms. Addresses the integration of technology in all curricular areas for all students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 532, 'Literacy Methods of Instruction and Assessment for Diverse Learners', 3, '{}', 'Focuses on applying scientifically based teaching methods for delivering differentiated literacy instruction, assessment and intervention.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 534, 'Social Studies Methods and The Arts', 2, '{}', 'Provides strategies for integrating social studies content knowledge with the arts. Addresses a variety of issues related to social studies instruction in elementary school, with an emphasis on valuing diverse cultures and experiences within historical and geographical perspectives. Emphasizes meeting the academic and linguistic needs of all learners including English language learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 536, 'Integrated Humanities (SS/ARTS/ELA) Methods and Assessment', 3, '{}', 'Focuses on content, assessments and methods of integrated humanities (social studies, arts and English language arts) for the elementary teacher. Apprentice teachers critically analyze and discuss course materials, generate meaningful questions and seek answers concerning the purpose, content, methods and assessments of integrated humanities instruction, and actively participate in course activities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 537, 'K-8 Mathematics Methods and Assessment', 3, '{}', 'Mathematical knowledge for learning, teaching and assessment in grades K through 8. Focuses on developing conceptual understanding of mathematics through inductive, inquiry-based, problem-centered learning and teaching. Emphasizes representing and explaining mathematics using concrete, digital and real-world representations. Focuses on theories of mathematics learning, standards learning trajectories, and learning designs sequenced concrete-to-pictorial-to-abstract. Introduction to cognitively guided instruction and brain science messages of productive struggle and growth mindset for equitable mathematics learning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 550, 'Elementary Methods in Content Areas', 3, '{}', 'Advanced level of development of knowledge and skills of instructional planning and methods of teaching and evaluating in elementary schools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 557, 'Internship I', 8, '{}', 'Develops knowledge and skills for effective classroom management, establishing a positive classroom environment, and examining K-12 learner behavior. Provides opportunities for students to engage as a teacher and learner in a classroom setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 567, 'Internship II', 8, '{}', 'Develops knowledge and skills for effective classroom management, establishing a positive classroom environment and effective lesson delivery. Reinforces the application and evaluation of the NIET Aspiring Teacher Rubric. Provides opportunities for students to engage as a teacher and learner in a classroom setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 571, 'Apprentice Teaching I', 15, '{}', 'Semester one of apprentice teaching in an elementary (EED)/secondary (SED)/special education (SPE) setting for students who are currently working as the teacher of record. Introduces classroom management, curriculum, instruction and assessment supervised and reinforced by a clinical instructor.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 574, 'Apprentice Teaching II', 15, '{}', 'Semester two of apprentice teaching in an elementary (EED)/secondary (SED)/special education (SPE) setting for students who are currently working as the teacher of record. Reinforced application of developing proficiency in classroom management, curriculum, instruction and assessment occurs through supervision and individualized support by a clinical instructor.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 576, 'Internship III', 8, '{}', 'Develops knowledge and skills for creating learning experiences for a range of learners, executing instructional cycles, using assessment methods, and reflecting on best teaching practices. Provides opportunities for pre-service educators to engage as a teacher and learner in a classroom.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 578, 'Residency/Student Teaching in Elementary Education', 15, '{}', 'Student teaching in elementary (EED) school classroom. Synthesized experience in curriculum, instruction and classroom management. Builds upon previous professional experiences to extend learning in a classroom setting and to obtain practice and feedback to continuously improve educator knowledge and skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 579, 'Apprentice Teaching III', 15, '{}', 'Semester three of apprentice teaching in an elementary (EED)/secondary (SED)/special education (SPE) setting for students who are currently working as the teacher of record and enrolled in the masters plus certification program. Continued development toward proficiency in classroom management, curriculum, instruction and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 582, 'Residency/Student Teaching in Elementary Education for Dual Certification', 15, '{}', 'This 8-week capstone residency experience meets the EED requirement for dual certification in SPE and EED. Synthesized experience in curriculum, instruction and classroom management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 596, 'Residency/Student Teaching', 8, '{}', 'Student teaching in elementary (EED), secondary (SED), or special education school classrooms. Synthesized experience in curriculum, instruction and classroom management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EED', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 101, 'Foundations of Engineering Design Project I', 3, '{}', 'Introduces engineering as a profession, critical thinking in engineering design and modeling, team dynamics, engineering communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 102, 'Foundations of Engineering Design Project II', 3, '{}', 'Development and experimental verification of engineering models, engineering design, data acquisition, critical analysis of data, introduction to CADD, engineering communications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 104, 'Critical Inquiry in Engineering', 3, '{'L'}', 'Critical thinking. Systematic evaluation of information as input to well-informed decision making. Close reading and substantive writing in a technical setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 201, 'Use-Inspired Design Project I', 3, '{}', 'Design, professionalism, and engineering practice developed in a multidisciplinary project setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 202, 'Use-Inspired Design Project II', 3, '{}', 'Design, problem solving, and engineering practice developed in a multidisciplinary project setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 216, 'Engineering Electrical Fundamentals', 3, '{}', 'Introduces modeling of electrical systems using mathematical and scientific principles. Covers electrical circuit analysis techniques including Kirchhoff's and Ohm's laws, series and parallel circuit elements, equivalent elements and circuits, and nodal analysis. These techniques are developed for DC, AC, and transient conditions. Also introduces the basic structure of a modern electronic measurement system and relates this structure to measurement accuracy, precision and resolution.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 217, 'Engineering Mechanics Fundamentals', 3, '{}', 'Provides a foundation in the theory and principles of statics and dynamics. Examines the effect of forces acting on particles and rigid bodies. Extensively uses vector mechanics. Statics addresses the topics of equilibrium in two and three dimensions, to include distributed loads, trusses, frames, friction and the moment of inertia. Dynamics addresses kinematics and kinetics of particles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 218, 'Materials and Manufacturing Processes', 3, '{}', 'Applies material properties and manufacturing processes to the design and fabrication of engineered artifacts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 219, 'Computational Modeling of Engineering Systems', 3, '{}', 'Introduces computing that develops software to find solutions to engineering problems. Students write and document code to solve problems in data acquisition, modeling of physical systems, and optimization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 240, 'CADD and Solid Modeling', 3, '{}', 'Uses 3-D solid modeling software to model mechanical parts and produce valid engineering drawings, including use of geometric dimensioning and tolerancing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 280, 'Engineering Statistics', 3, '{'CS'}', 'Applications-oriented introduction to statistics with computer-based experience using statistical software for formulating and solving engineering problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 303, 'Mechanical Systems Project I', 3, '{}', 'Analyze, design, implement and characterize a mechanical system through an in-depth mechanical system design project. Project includes modeling, analysis and measurement of the dynamic behavior of the mechanical system. Develops professional and engineering skills in this project setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 304, 'Embedded Systems Design Project I', 3, '{}', 'Design, implement and debug an embedded electromechanical system through an in-depth design project. Develops professional and engineering skills in this project setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 305, 'Robotics Systems Project I', 3, '{}', 'Analyze, design, implement and debug a robotic electromechanical system through an in-depth robotic system design project. Develops professional and engineering skills in this project setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 306, 'Automotive Systems Project I', 3, '{}', 'Analyze, design, implement and verify an automotive system through an in-depth design project. Develops professional and engineering skills in this project setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 307, 'Humanitarian Engineering Project I', 3, '{}', 'Provides students with the opportunity to work on a real-world project that is or will be taking place 'in the field,' in a part of the world with fewer material resources than most places in the United States. Builds the skill sets of students who are interested in pursuing development-related work after graduation. Course is team-based, project-focused and extremely hands-on. Emphasizes skills development on equal par as working with peers, mentors and in-field partners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 313, 'Mechanical Systems Project II', 3, '{}', 'Applies design principles to conceptualize, implement and characterize a fluid and/or thermodynamic system in a project setting. Project emphasizes communication with project stakeholders; applying an appropriate design approach in the context of a fluid and/or thermodynamic system; critical thinking in developing system specifications and evaluating a prototype relative to these specifications; and increasing technical competence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 314, 'Embedded Systems Design Project II', 3, '{}', 'Applies design principles to conceptualize, implement and characterize an embedded electromechanical system in a project setting. Project emphasizes communication with project stakeholders; applying a human-centered design approach in the context of an embedded system; critical thinking in developing system specifications and evaluating a prototype relative to these specifications; and increasing technical competence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 315, 'Robotic Systems Project II', 3, '{}', 'Applies design principles to conceptualize, implement and characterize an electromechanical robotic system in a project setting. Project emphasizes communication with project stakeholders; applying a human-centered design approach in the context of a robotic system; critical thinking in developing system specifications and evaluating a prototype relative to these specifications; and increasing technical competence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 316, 'Automotive Systems Project II', 3, '{}', 'Applies design principles to conceptualize, implement and characterize an automotive system in a project setting. Project emphasizes communication with project stakeholders; applying a human-centered design approach in the context of an automotive system; critical thinking in developing system specifications and evaluating a prototype relative to these specifications; and increasing technical competence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 317, 'Humanitarian Engineering Project II', 3, '{}', 'Applies design principles to conceptualize, implement and characterize an engineered system that addresses a humanitarian objective in a project setting. Project emphasizes communication with project stakeholders, applying an appropriate design approach, thinking critically in developing system specifications and evaluating a prototype relative to these specifications, and increasing technical competence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 321, 'Water Resource Systems', 3, '{}', 'Fundamentals of water supply and surface water hydrology in the context of the engineering, design and analysis of water resources systems. Places particular emphasis on those aspects of water systems that apply in the context of the developing world as well as in the context of environmentally sustainable systems. Course delivery is designed for engineering students with a broad understanding of engineering fundamentals but without specialist civil engineering knowledge.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 325, 'Water and Wastewater Treatment', 3, '{}', 'Essential elements of aquatic chemistry, water pollution and water and wastewater treatment technologies. Provides a fundamental basis for understanding sustainable water and wastewater treatment processes and their design considerations. Includes critical evaluation of contemporary issues concerning our water environment and the associated regulatory framework to strengthen and emphasize importance of adequate practices used in operation and design of engineered water and wastewater treatment systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 330, 'Design of Electrical Systems', 3, '{}', 'Overview of electrical system architectures and components used in design of smart electrical and electromechanical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 334, 'Analog-Digital Interface', 3, '{}', 'Signal processing and operational tools that lie between sensors and digital data processors developed from a system perspective. Includes selection, deployment and analysis of sensors, analog signal processors and converters as well as their appropriate power supplies and the impact of all these components on system performance and cost. Uses experimental and simulation tools to demonstrate typical-use cases for medical, military, industrial and consumer applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 338, 'Microcontrollers in Smart Systems', 3, '{}', 'Microcontroller implementation of smart systems. Microcontrollers applied to control dynamic systems, including the interface with sensors and actuators as well as developing simple open- and closed-loop control laws.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 340, 'Engineering Thermo-Fluids I', 3, '{}', 'First and second laws of thermodynamics, systems, processes, equations of state, property tables. Fluid statics, conservation of momentum, Bernoulli's equation, applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 343, 'Mechanics of Solid Materials', 3, '{}', 'Studies strength and physical performance, such as deformations and load-carrying capacity, of natural and artificial solid structures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 360, 'Concepts in Automotive Engineering', 3, '{}', 'Fundamental engineering principles as they apply to the design of ground vehicles used for the transportation of individuals or materials.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 363, 'Automotive Powertrains and Thermal Systems', 3, '{}', 'Internal combustion engine (ICE) design in the context of automotive systems and heat management in vehicle systems, including thermal design of engine and transmission cooling systems. Operating characteristics and efficiencies of the ICE, terminology and abbreviations used in the automotive industry, contemporary fuels and the latest hybrid technology, CAFE standards and EPA vehicle emissions standards, lubrication systems and fluids appropriate for a given design, and current and future standards requirements.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 370, 'Welding Survey', 3, '{}', 'Theory and practical application of the most widely used industrial welding processes; introductory welding, fundamental welding metallurgy, weldment design and weld cost estimating; lab experiences using SMAW, GTAW, GMAW, FCAW, RSW, SW, OFC, OAW, PAC, and brazing. Aligns with the American Welding Society (AWS) SENSE standards.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 371, 'Best Practices for Humanitarian Engineering', 3, '{}', 'Technological and process-based best practices for engineering students to gain applied competence in low-cost and low-tech appropriate methods and technologies for small-scale technical problems in local and international humanitarian development contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 380, 'Advanced Computer Aided Design and Drafting (CADD) and Solid Modeling', 3, '{}', 'Advanced techniques and practices of solid modeling and drafting, including motion, Finite Element Analysis (FEA), and sheet metal.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 401, 'Professional Design Project I', 3, '{'L'}', 'First half of a comprehensive project experience based on cumulative knowledge and skills gained in earlier course work.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 402, 'Professional Design Project II', 3, '{}', 'Second half of a comprehensive project experience based on cumulative knowledge and skills gained in earlier course work.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 425, 'Unit Treatment Technologies and Systems', 3, '{}', 'Addresses fundamental concepts of unit treatment technologies and the scientific and engineering principles that govern the proper design, operation and management of these systems. Course is divided into two overlapping major categories of unit treatment technologies: air-pollution controls and soils/groundwater remediation. Enhances understanding of the unit treatment technologies by reviews of dimensional analysis, mass transport mechanisms, mass balances, modeling, kinetics and reactors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 426, 'Integrated Solid Waste Systems', 3, '{}', 'Addresses the fundamental concepts of solid waste, pathways and transformation processes following the from cradle-to-grave approach. Follows the life of the solid waste from point of generation, reclamation and treatment to point of disposal. Emphasizes describing the present solid waste systems as an integrative set of functional units. Considers physical, chemical and biological transformations of the solid waste, together with the existing technologies and practices for management, treatment and disposal of the solid waste. Includes a specific overview of sustainable solid waste practices such as waste minimization, pollution prevention, reuse and reclamation, together with current domestic and foreign regulations. Addresses planning and design of solid waste operations and facilities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 427, 'Principles of Hazardous Waste and Materials', 3, '{}', 'Trains future environmental engineers and professionals in fundamental hazardous waste and materials concepts. Addresses regulatory requirements; materials hazard communication; material safety concepts; types of hazardous waste/materials (chemical, radioactive, and infectious); risk assessment and routes of exposure; personal protective equipment; prudent practices; treatment; storage and disposal of hazardous waste/materials through the lens of the cradle-to-grave concept. Gains knowledge about the hazardous waste enforcement processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 430, 'Design, Fabrication, and Architecture of Electronic Devices', 3, '{}', 'Comprehensive introduction to the design, operation, production and application of the most common semiconductor devices and categories of integrated circuit. Demonstrates the close interaction among materials, semiconductor physics and circuit performance, especially in terms of the applications and economics of digital systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 431, 'Power Management', 3, '{}', 'Devices and techniques to supply and manage power in electrical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 432, 'Engineering Thermo-Fluids II', 3, '{}', 'Thermodynamic analysis of mixtures and power cycles. Design and analysis of fluid systems, conduction heat transfer, and introduction to convection and radiation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 433, 'Transforms and Systems Modeling', 3, '{}', 'State-space and transform-domain modeling and analysis of electrical and electromechanical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 438, 'RF Systems', 3, '{}', 'Applies fundamental principles of electromagnetics to design and analyze engineered RF systems and applications. Introduces and describes transmission lines, Smith Charts, s-parameters, waves in free space, reflection and refraction and their use in engineering systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 444, 'Engineering Design', 3, '{}', 'Applies fundamentals of mechanics in analysis and synthesis of machine components and systems, with emphasis on stress and failure.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 445, 'Mechanical Engineering Systems', 3, '{}', 'Design and modeling of multicomponent electromechanical systems. Applications of computers, controllers, electronics using system hardware and software development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 455, 'Robotic Systems I', 3, '{}', 'Analysis and design of robotic systems focusing on kinematics, dynamics, coordinate transformations and modeling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 456, 'Robotic Systems II', 3, '{}', 'Design of robotic systems focusing on dynamics, modeling and controlling a robot.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 463, 'Vehicle Electrical Systems and Hybrid Systems', 3, '{}', 'Basic theory and operation of hybrid electric drive systems of motor vehicles. Covers fundamentals of next generation electrification including: batteries technology, charging, power electronic operation. Emphasizes the principals of HEV energy management strategies and HEV modeling fundamentals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 465, 'Ground Vehicle Dynamics', 3, '{}', 'Basic understanding of vehicle dynamics principles and a sufficient knowledge to analyze vehicle dynamic performance and associated chassis system design concepts. Understanding of various dynamical equations of motion governing vehicle behavior as well as computer simulation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 475, 'Alternative Energy', 3, '{}', 'Covers basic principles, thermodynamics and economics of alternative energy conversion technologies including solar photovoltaics, concentrating solar, wind, biomass, nuclear, fuel cells and energy storage. Briefly discusses conventional heat and power systems as they pertain to alternative energy integration. Introduces operating characteristics of renewable energy technologies through simulation and then reinforces through hands-on demonstrations and experiments with renewable energy systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 476, 'Microgrid Design and Operation', 3, '{}', 'Design, modeling, integration, operation and maintenance of microgrids. Simulation-based design is coupled with hands-on integration to provide a comprehensive approach to microgrid systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 500, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 520, 'Engineering Analysis I', 3, '{}', 'Analytical methods for evaluation of engineering systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 521, 'Engineering Analysis II', 3, '{}', 'Mathematical techniques for developing solutions of engineering problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 522, 'Statistics for Quality Control in Manufacturing', 3, '{}', 'Seven QC tools and control charts for data analysis and interpretation; Process capability, 6-Sigma, Lean Manufacturing, Quality system, Case study.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 523, 'Finite Element Modeling and Analysis', 3, '{}', 'Analyzes machine elements, frames and aerospace structures employing finite element analysis and software. Includes sensitivity analysis and estimating accuracy. Introduces the fundamentals of finite element analysis as well as providing hands-on experience using finite element software to model, analyze and design structures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 525, 'Lean Systems', 3, '{}', 'Creates a framework and model of lean engineering and a methodology for applying lean practices to systems engineering as a basis for lean product development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 530, 'Principles of Systems Engineering', 3, '{}', 'Engineering is increasingly about complex systems. Addresses scale, scope and complexity increase that engineers need to consider to bring together technical and social issues in a highly integrated way as they design flexible, adaptable, robust systems that can be easily modified and reconfigured to satisfy changing requirements and new technological opportunities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 535, 'Innovation and Design of Engineering Academic Settings (IDEAS)', 3, '{}', 'Innovation plays an important role in the evolution of the field of engineering education. Explores innovation processes in a variety of contexts, including (but not limited to) engineering education research and methods, theory, dissemination, teaching and training within formal and informal academic settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 545, 'Robotic Systems I', 3, '{}', 'Analysis and design of robotic systems focusing on kinematics, dynamics, coordinate transformations and modeling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 546, 'Robotic Systems II', 3, '{}', 'Design of robotic systems focusing on dynamics, modeling and controlling a robot.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 550, 'Mechatronic Systems', 3, '{}', 'Presents a clear and comprehensive approach to the area of mechatronic systems, including the sensors, actuators, microcontrollers and applications which are found at the heart of everyday products and devices. Provides a systems-level approach to integrating these elements into a functioning device.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 555, 'Mechatronics Device Innovation', 3, '{}', 'Project-based course on the design of mechatronic devices to address needs identified by hospital-based clinicians. Students work in teams with physicians to develop a novel mechatronic device while in parallel an entrepreneurial mindset is fostered. The design process includes: needs finding; problem identification; prior art searches; strategy and concept generation; estimation; sketching; sketch modeling; machine elements, ergonomics and prototyping. Emphasizes the development of creative designs that is motivated by analytical models. Also includes lectures on specific aspects pertinent to novel devices including intellectual property, marketing, licensing and startups and lean launchpad for startups, provided by Skysong Innovations. Significant communication component as students present ideas in class and at an industry forum. Students also write a publication quality final paper, which they are encouraged to submit to a conference or journal.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 556, 'System Control and Optimization', 3, '{}', 'Topics include state-space models, Lyapunov stability, controllability and observability, optimization and optimal control, and nonlinear systems and control. Includes both theories and real-world engineering examples/demos, and students apply the advanced control system approaches to self-selected course projects. Prior knowledge in classical linear control theory is necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 557, 'Foldable Robotics', 3, '{}', 'Organized around new types of robots being developed in research labs and industry across the country, foldable robots are designed and built using layered, flat sheets of a wide variety of materials and folded up to create both form and motion. Design of these devices begins with initial prototypes and proceeds through implementation and optimization, with a focus on application-specific projects that seek to solve problems of cost, parallelism, complexity and time using a relatively fast and easy prototyping method. Students delve deeper into the analytical problems associated with foldable mechanisms in topics such as design, kinematics, manufacturing, dynamics, simulation, optimization and stiffness analysis. Familiarity with programming fundamentals, scripted languages like Python or Matlab, linear algebra, differential equations, calculus, trigonometry and vector math is strongly suggested.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 560, 'Vehicle Dynamics and Control', 3, '{}', 'Covers modeling, estimation, control and optimization of a typical vehicle (sub)system dynamics. Recent advances in automotive technology, such as vehicle electrification and automated/connected driving, further complicate vehicle (sub)systems as nonlinear multi- input-multi-output systems with unmodeled dynamics, structure uncertainties and external disturbances. Thus, effective vehicle control design requires to sophisticatedly combine advanced control theory with knowledge of vehicle characteristics. To achieve this goal, the course introduces a variety of modeling, advanced control, estimation and optimization techniques for typical vehicle (sub)systems. Discusses applications of control theories to various vehicle (sub)system dynamics through examples inspired by pertinent research projects. Extensively utilizes co-simulation between Matlab/Simulink and CarSim for vehicle dynamics analysis, vehicle control design and verification.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 565, 'Qualitative Methods for Engineering Education Research', 3, '{}', 'Provides a deep, empirical exposure to interpretive research methods in engineering education research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 572, 'Quantitative Methods for Engineering Education Research', 3, '{}', 'Specific quantitative analysis techniques used in the field of engineering education, with special focus on instrument design, ANOVA and multiple regression. Prior coursework or experience with basic statistical techniques is necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 574, 'Engineering Education Systems in Context', 3, '{}', 'Provides a systems understanding of current trends in engineering education research to engage engineering education research graduate students with the latest developments in the field in which they will situate their research projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 576, 'From Then Until Now: Examining Inequities in STEM', 3, '{}', 'Provides a novel opportunity for students to learn about inequities that have persisted in STEM and to challenge the way inclusion is cultivated. Challenges students to become aware of the past, give context to the present and think creatively about new ways forward. Additionally, takes an entrepreneurial approach toward being inclusive. As both teachers and students, in developing critical consciousness, it is imperative that we refute the banking approach to education and constantly work to critically consider our realities. Adopts an inclusive, student-centered pedagogy. Facilitates learning through activities that provide exposure to and experience with critical content.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 581, 'Simulating Manufacturing Systems', 3, '{}', 'Specific modeling and simulation tools used to understand and optimize manufacturing systems operations, with an emphasis on both model building and statistical interpretation of model output.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 594, 'Conference and Workshop', 12, '{}', 'Topical instruction, usually in compressed format, leading to academic credit. Often offered off campus to groups of professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 602, 'Principles of Independent Research', 3, '{}', 'Practice of key components of independent research that include project planning, literature survey, interview, data collection and analysis, proposal development, technical writing and presentation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 608, 'Advanced Simulation', 3, '{}', 'Simulation has a central role in design, operation, optimization and evolution of complex systems. There are many tools but they have a common need for rigorous science/math, parameterized models and realistic validation. Uses and compares five common platforms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 611, 'Complex Engineering Systems', 3, '{}', 'Advanced concepts in design and evaluation of engineering systems: subsystems, systems architectures and seamless integration of subsystems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 671, 'Applications of Qualitative Methods for Engineering Education Research', 3, '{}', 'Provides a deep, situated exposure to making and handling data in qualitative engineering education research projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 673, 'Applications of Quantitative Methods for Engineering Education Research', 3, '{}', 'Applies quantitative analysis techniques to an engineering education research project. Specific focus on data collection, instrument development, and ANOVA/regression analysis techniques. Prior coursework or experience with basic statistical techniques is necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 784, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EGR', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 101, 'First-Year Composition', 3, '{}', 'Discovers, organizes and develops ideas in relation to the writer's purpose, subject and audience. Emphasizes modes of written discourse and effective use of rhetorical principles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 102, 'First-Year Composition', 3, '{}', 'Critical reading and writing; emphasizes strategies of academic discourse. Research paper required.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 105, 'Advanced First-Year Composition', 3, '{}', 'Concentrated composition course for students with superior writing skills; intensive reading; research papers; logical and rhetorical effectiveness.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 107, 'First-Year Composition', 3, '{}', 'For students for whom English is not the native language. Combines classroom and supplemental instruction to teach academic genres of writing, including definition, summary and analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 108, 'First-Year Composition', 3, '{}', 'For students for whom English is not the native language. Critical reading and writing; emphasizes strategies of academic discourse. Research writing required. Satisfies graduation requirement of ENG 102.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 110, 'Approaching Big Problems', 3, '{}', 'Examines how language helps to shape important issues and big questions in the world today. By studying the process of meaning-making, students work toward understanding these issues better and answering these questions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 130, 'Prototyping Dreams', 3, '{'L'}', 'How do you build your dreams? Explores speculative design and inspirational prototyping as a way to imagine and organize worlds. These prototypes do not have to be functional or material, but they must be functional stories, effectively communicating the creator's vision with others. Surveys a number of creative paradigms for imagining alternative worlds that are more ethical and sustainable, and experiments with techniques for physical, digital, literary and artistic prototyping.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 131, 'Poetry in America', 3, '{}', 'Through the lens of American poetry, provides highly engaging learning experiences anchored in four central learning outcomes, or 'pillars,' which form a foundational approach to close reading poems and other texts: making observations, understanding structure and form, situating texts in history and enjoying language. Additionally, students practice critical and creative analysis and reflect on their goals and progress as readers and writers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 191, 'First-Year Seminar', 3, '{}', 'Small course emphasizing student-faculty discussion and interaction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 200, 'Critical Reading and Writing About Literature', 3, '{'L','HU'}', 'Introduces the terminology, methods, and objectives of the study of literature, with practice in interpretation and evaluation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 201, 'World Literature', 3, '{'HU','H','G'}', 'Classical and medieval periods. Selections from the great literature of the world in translation and lectures on the cultural background.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 202, 'World Literature', 3, '{'HU','H'}', 'Renaissance and modern periods. Selections from the great literature of the world in translation and lectures on the cultural background.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 203, 'Introduction to Health Humanities', 3, '{'HU'}', 'Introduces the field of health humanities. Through readings in cultural studies of literature and other media, history of medicine, bioethics, disability studies, cross-cultural and global health, and related scholarly areas, introduces some of the fundamental challenges and debates in this growing interdisciplinary field. Also addresses questions of medical professional ethics and teaches structural awareness of the sociocultural aspects of health and health care.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 204, 'Topics in Contemporary Literature', 3, '{'HU'}', 'Topics in contemporary literature, which may include narrative and medicine.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 205, 'Introduction to Writing, Rhetorics and Literacies', 3, '{'L'}', 'How do we understand the ways in which people in systems--both small and large--social, political, and economic, throughout the world use language and other symbolic resources to carry out work? Three fields of study take this kind of questioning especially seriously: studies in writing, studies in rhetoric, and studies in literacy. Inquiry--that persistent, deliberate commitment to question and to build methods sufficient to that questioning--is a generative force in each of these fields of study. Familiarizes students with such questioning traditions and to help them judge for themselves what each is good for. The goal is to strengthen your own repertoire and decision-making power for producing and circulating work (widely defined across material, print, and digital media) that matters in our risk-ridden world. Quite simply, then, this is a course in asking good questions and in evaluating responses to situations that spur questioning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 206, 'Introduction to Literary Studies', 3, '{}', 'Introduces the terminology, methods and objectives for interpreting and writing about literary and cultural texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 207, 'Elements of English Language and Cultures I', 3, '{}', 'English communication and cultural awareness for international users of English. Relies on the many contexts of English use around the world to teach aspects of grammar, reading, writing, listening and speaking.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 208, 'Elements of English Language and Cultures II', 3, '{}', 'English communication and cultural awareness for international users of English. Relies on the many contexts of English use around the world to teach aspects of grammar, reading, writing, listening and speaking.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 210, 'Introduction to Creative Writing', 3, '{}', 'Beginning creative writing in genres other than poetry and fiction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 211, 'Introduction to English Studies', 3, '{'HU'}', 'Introduces key terms and conventions for analyzing and interpreting diverse literary and cultural texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 213, 'Introduction to the Study of Language', 3, '{}', 'Language as code; phonetics, phonology, morphology, and syntax; the lexicon; language acquisition; sociolinguistics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 214, 'English Grammar and Usage', 3, '{}', 'Fundamentals of English grammar (word and phrase structure), English usage (punctuation, grammatical correctness), and grammatical and usage theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 215, 'Strategies of Academic Writing', 3, '{'L'}', 'Advanced course in techniques of analyzing and writing academic expository prose. Writing is research based.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 216, 'Persuasive Writing on Public Issues', 3, '{'L'}', 'Advanced course in techniques of analyzing and writing persuasive arguments addressing topics of current public interest. Papers are research based.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 217, 'Writing Reflective Essays', 3, '{'L'}', 'Critical examination of the influences discourse has on formation of identity; narrative analyses of self and culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 218, 'Writing About Literature', 3, '{'L','HU'}', 'Advanced writing course requiring analytical and expository essays about fiction, poetry, and drama.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 220, 'Gender, Media and Culture', 3, '{'HU','C'}', 'Introduction to gender and the media including magazines, films, advertising, new media, and other popular culture forms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 221, 'Survey of English Literature', 3, '{'HU'}', 'Medieval, Renaissance, and 18th-century literature. Emphasizes major writers and their works in their literary and historical contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 222, 'Survey of English Literature', 3, '{'HU','H'}', 'Romantic, Victorian, and 20th-century literature. Emphasizes major writers and their works in their literary and historical contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 228, 'Introduction to Shakespeare', 3, '{}', '');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 229, 'Love and Lust in Medieval Literature', 3, '{}', 'The medieval period, roughly 600-1500, cannot reasonably be called the 'dark ages.' Indeed, the Middle Ages provides us with a corpus of literature replete with complex characters struggling to live and love, dealing with sin and lust, unity and diversity, faith and reason within a tumultuous cultural milieu and religious rigor, all of which are feeling the winds of changes. Explores medieval literature, noting the astonishing differences from our own time, as well as surprising similarities. Western civilization, our own 21st century, for better or for worse, is indebted to the love and lust of the medievals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 230, 'Introduction to Film Studies', 3, '{'L','HU'}', 'Introduces the style and cultural significance of feature films.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 235, 'Introduction to Contemporary Theory', 3, '{}', 'Introduces key ideas and concepts in contemporary critical and cultural theory. In addition to the 'posts'--postmodernism, poststructuralism, post-Marxism, postcolonial studies, post-feminism and posthumanism--explores new theoretical formations that have emerged in response to our contemporary social condition, such as queer theory, trauma theory, disability studies, cognitive studies, the new 'object-oriented' philosophies and materialist theories. And then there's the 'trans' domains--transnationalism, transgender studies and transhumanism--which, like so many contemporary theoretical formations opens up new perspectives on where and how we live, who we are and even what we are. The contemporary theories studied in this course provide the tools for understanding 21st-century life and the limits and enjoyments of our humanity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 241, 'Literatures of the United States to 1860', 3, '{'(L','HU)','C'}', 'Surveys literary movements and genres from colonization to the Civil War.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 242, 'Literatures of the United States, 1860 to Present', 3, '{'HU'}', 'Surveys literary movements and genres from the Civil War to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 243, 'Introduction to Writing Family History', 3, '{'L'}', 'Interdisciplinary writing course on theories, methodologies and issues in writing family history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 244, 'Introduction to Researching Family History', 3, '{}', 'Introduces research methods in family history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 245, 'Popular Culture Issues', 3, '{}', 'Selected topics in various forms of popular culture related to written texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 287, 'Beginning Creative Writing Workshop in Poetry', 3, '{}', 'Beginning writing of poetry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 288, 'Beginning Creative Writing Workshop in Fiction', 3, '{}', 'Beginning writing of fiction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 300, 'Your Degree in the World', 1, '{}', 'Inquiry-driven course where students examine the place of an English degree within a larger humanities context, and explore the kinds of work they can do in the world with their education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 301, 'Writing for the Professions', 3, '{'L'}', 'Advanced practice in writing and editing expository prose. Primarily for preprofessional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 302, 'Business Writing', 3, '{'L'}', 'Advanced interdisciplinary writing course designed to improve the workplace writing competence of W. P. Carey School of Business professional and preprofessional students. Lecture, discussion, case studies, cooperative learning, interactive, Internet, student presentations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 303, 'Classical Backgrounds of English Literature', 3, '{'HU'}', 'Readings of Greek and Latin literature in translation as they relate to literature in English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 304, 'Critical Theories and Methods', 3, '{}', 'Covers ideas and concepts in contemporary critical and cultural theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 305, 'Interpretive Theory', 3, '{'HU'}', 'Develops interpretive practices necessary for English studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 307, 'Writing Science Fiction', 3, '{}', 'Writing science fiction, primarily the short story.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 308, 'Form and History/Culture', 3, '{'HU'}', 'Examines how literary and rhetorical forms are situated both historically and culturally.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 309, 'Rhetoric for Writers', 3, '{'L'}', 'Students identify, analyze and employ key concepts and theories of rhetoric to understand and compose written texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 310, 'Intermediate Creative Writing', 3, '{}', 'Intermediate creative writing in genres other than poetry, fiction, or science fiction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 311, 'Persuasive Writing', 3, '{'L'}', 'Advanced writing course that focuses on persuasive writing for diverse audiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 312, 'English in its Social Setting', 3, '{'L','HU','SB'}', 'Introduces the sociolinguistic study of the English language.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 313, 'Phonology and Morphology', 3, '{}', 'The study of language sound-structure (phonology) and word-structure (morphology) in the framework of current linguistic theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 314, 'Modern Grammar', 3, '{}', 'Modern descriptive models of English grammar.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 315, 'Medieval Literature in Translation', 3, '{}', 'Medieval literature (insular and continental) in translation, from Beowulf to Malory (excluding Chaucer), emphasizing cultural and intellectual backgrounds.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 316, 'Literary Readings of the Hebrew Bible', 3, '{'L','HU'}', 'Literary analysis of two of the main genres of the Hebrew Bible: narrative art and poetry. Students learn the main literary conventions of biblical prose and poetry, and how these conventions are applied or subverted. Each class comprises the study of a literary aspect, close readings, and thematic discussions. Biblical texts are read only in translation. They are situated in the religious, literary and artistic traditions that have developed on their basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 317, 'Cross-Cultural Writing', 3, '{'(L','HU)','G'}', 'Cultural differences and their impact on writing and/or their role in miscommunication and misinterpretation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 318, 'The Life and Death of Languages', 3, '{}', 'The history of language development, spread and contraction as well as thinking on where language may be going in the future. Discusses how languages differentiate and become new languages, how they mix with each other and how the development of writing can change this process. Maintains a focus on the ways speakers from different language communities have found to communicate through pidgin languages, bilingualism and languages of wider communication. Moreover, discusses the invention of new varieties and languages by people who feel their current choices do not fit their needs. Lastly, in considering what might be needed in the future, addresses issues such as machine translation, brain-to-brain communication, aspects of intergalactic communication posed in science fiction and interspecies communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 319, 'Future of English', 3, '{}', 'This transdisciplinary course investigates what varieties of English are currently used by whom and for what purposes, and attempts to predict the future of English and its significance at the individual, national and global levels.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 320, 'Medievalism in Modern Culture', 3, '{}', 'Studies the representation of Medieval narratives, characters, themes, myths, and images in modern culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 321, 'Shakespeare', 3, '{'L','HU'}', 'Readings in Shakespeare's major comedies, tragedies, histories, romances and/or poems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 322, 'Westerns: Film and Short Stories', 3, '{}', 'A survey of classical short stories and recent films. Focuses on Wister, L'Amour, Grey and Leonard.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 323, 'Rhetoric and Grammar', 3, '{'L'}', 'Practical course taught in the computer lab. Emphasizes rhetorical strategies and grammar for writers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 324, 'Horror: Film and Short Stories', 3, '{}', 'A survey of major writers since 1800, and of classic filmmakers. The historical development of horror in relation to Gothic and grotesque short stories and films.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 325, 'Restoration and the 18th Century', 3, '{}', 'Writers and movements in nondramatic literature of the restoration and early 18th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 326, 'English Drama 1660-1800', 3, '{'HU'}', 'English drama 1660-1800.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 327, 'English Studies Research', 3, '{}', 'How to evaluate and conduct various research methods for different areas of English studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 328, 'The Novel to Jane Austen', 3, '{'HU','H'}', 'From origins of prose fiction through the 18th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 329, '19th-Century British Fiction', 3, '{}', 'Includes such authors as Austen, Dickens, Eliot, and Conrad.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 330, '19th-Century British Poetry', 3, '{}', 'Romantic and Victorian poets studied in context.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 331, 'American Drama', 3, '{'HU'}', 'Major works in the development of American drama from its beginnings to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 332, 'Studies in Race and Ethnicity in Literature and Culture', 3, '{}', 'Investigates racial and ethnic dynamics through literature and culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 333, 'American Ethnic Literature', 3, '{}', 'Examines America's multiethnic identity through works of literature that depict American ethnic, gender, and class sensibilities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 334, 'The American Southwest in Literature and Film', 3, '{'(L','HU)','C'}', 'Evaluates literary texts and films addressing the diverse literatures and cultures of the American Southwest.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 335, 'American Poetry', 3, '{}', 'Themes and developments in American poetry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 336, 'Women, Film and Culture', 3, '{'SB','C'}', 'Critical examination of the way films can reflect, reinforce or challenge mainstream cultural ideas about women.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 337, 'Major American Novels', 3, '{'L','HU'}', 'Major American novels studied in their ethnically diverse literary, historical, and cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 338, 'Protest Literature', 3, '{}', 'Examines traditions of social protest literature across a variety of historical periods and literary and rhetorical genres.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 339, 'Creating Dramatic Works: Page to Stage to Screen', 3, '{}', 'Practical study of writing, directing, acting and performance techniques essential for the creation of theatrical works for stage, television and theatrical film presentation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 342, '20th-Century British and Irish Literature', 3, '{'HU'}', 'Major works in the development of literature since 1900, studied in their historical and cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 343, 'Crime: Film and Short Stories', 3, '{}', 'A survey of classical crime fiction and recent crime films. From Poe, Doyle, Hammett and Chandler through Sayers, Highsmith, Grafton and Paretsky.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 344, 'Intermediate Family History Research', 3, '{}', 'How to locate records, analyze and interpret evidence, and write a family history narrative.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 345, 'Selected Authors or Issues', 4, '{}', 'Different topics may be offered. Film topics with lab may carry 4 credits. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 347, 'Modern Israeli Literature in Translation', 3, '{'HU','G'}', 'Modern Hebrew, or Israeli Hebrew, started at the end of the 19th century in Europe and today it is the language of the State of Israel. During this short period modern Hebrew developed literature with its unique local characteristic, as well as universal ideas. Reads and examines four books against their social and historical background.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 348, 'The Borders of Language', 3, '{'L'}', 'In-depth focus on language use, multilingualism and social life in borderlands contexts, in and out of school. Explores the historical and contemporary dimensions of language use in the borderlands, including Indigenous languages, English and Spanish, and hybrid forms. Also introduces theories and methods in the field of linguistic anthropology. Students conduct research on everyday linguistic practice and connect it to broader sociopolitical realities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 349, 'Global Literary Studies', 3, '{}', 'Studies multiple literatures and cultures across the globe, often deploying transnational and comparative methodologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 350, 'Studies in Literary Histories and Traditions', 3, '{}', 'Studies of one or more literary traditions or historical movements');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 352, 'Short Story', 3, '{'L','HU'}', 'Development of the short story as a literary form; analysis of its technique from the work of representative authors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 353, 'African American Literature: Beginnings through the Harlem Renaissance', 3, '{'(L','HU)','C'}', 'Focuses on the history of African American literature through the Harlem Renaissance, as exemplified in selected slave narratives, poems, and fiction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 354, 'African American Literature: Harlem Renaissance to the Present', 3, '{'(L','HU)','C'}', 'Examines the literary productions of African Americans from the Harlem Renaissance (about 1920) to the present focusing on fiction, poetry, drama, and essays.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 355, 'European Dramatic Traditions', 3, '{'L','HU'}', 'Development of European drama since Aeschylus.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 356, 'The Bible as Literature', 3, '{'HU'}', 'Readings in the Jewish and Christian Scriptures in modern translation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 359, 'Indigenous American Literature', 3, '{'(L','HU)','C'}', 'Selected oral traditions and contemporary works by Indigenous American authors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 360, 'Western American Literature', 3, '{'L','HU'}', 'Critical examination of ideas and traditions of the literature of the western United States, including the novel.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 361, 'Scriptwriting', 3, '{}', 'The art of scriptwriting using 10-minute plays as the platform. Designed to help students learn craft techniques and to expose them to the various forms of the 10-minute script. Includes lectures, writing assignments, discussion, and criticism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 362, 'Film and Media Franchises', 3, '{}', 'Students gain an appreciation for film and media franchises by studying their production, reception and cultural significance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 363, 'Transborder Mexican and Chicano/a Literature of Southwest North America', 3, '{'(L','HU)','C'}', 'Development of Chicana and Chicano literature; study of genres and themes; attention to literary antecedents.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 364, 'Women and Literature', 3, '{'HU'}', 'Approaches to issues of gender and representation in literature by and about women.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 365, 'History of Film', 4, '{'HU'}', 'Development of motion pictures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 366, 'Literature on Film', 3, '{}', 'Studies literary texts and their adaptation to film.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 367, 'Environmental Issues in Literature and Film', 3, '{'L','HU'}', 'Evaluates literary texts and films that address interconnected cultural, historical, and environmental issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 368, 'Art of the Personal Essay', 3, '{'L'}', 'Writing personal essays drawing on both research and personal experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 369, 'Science Fiction Studies', 3, '{}', 'Development of science fiction and fantasy literature. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 370, 'Science, Values, and the Public', 3, '{}', 'Examines science, communication, and a range of public opinion to understand science as a discourse of profound social power.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 371, 'Rhetoric of the Environmental Movement', 3, '{}', 'Examines how language is used to conceptualize, clarify/obfuscate, and resolve problems among those who concern themselves with the disposition of natural resources.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 373, 'Publishing in Literary Magazines', 3, '{}', 'Analyzes the history, publishing preferences, editorial staff, and submission guidelines of literary magazines.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 374, 'Technical Editing', 3, '{}', 'Fundamentals of editing technical and professional materials. Role of editors in analyzing, revising, and polishing manuscripts. Successful writer-editor dialogues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 375, 'Titans of Business', 1, '{}', 'Explores leading CEOs and corporations from a humanities perspective.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 376, 'Writing a Personal History', 3, '{'L'}', 'Interdisciplinary writing course emphasizing theories, methodologies, and issues of composing personal histories.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 377, 'Editing Family History for Public Audiences', 3, '{'L'}', 'Fundamentals of editing family writing. Role of editors in analyzing, revising, and polishing manuscripts for publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 378, 'Environmental Creative Nonfiction', 3, '{'L','HU'}', 'Lectures, discussion, and criticism concerning the literary history of nature writing and techniques of writing environmental creative nonfiction for publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 379, 'Travel Writing', 3, '{'L'}', 'Studies the genesis of travel writing, what theories inform travel writing, and the various genres of travel writing today.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 384, 'Studies in Gender and Sexuality in Literature and Culture', 3, '{}', 'Approaches to issues of gender and sexuality in literary and cultural texts. May focus on historical period, literary movement, genre, single author, literary theory, theme or other field of study.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 385, 'Career Development for English Majors', 3, '{'L'}', 'Theoretical and practical aspects of career planning related to skills and interests developed in English studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 386, 'American Gothic', 3, '{}', 'The development of modern American Gothic as a literary form from Poe to Oates. Emphasizes the short story.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 387, 'Intermediate Creative Writing Workshop in Poetry: Form, Theory, and Practice', 3, '{}', 'Intermediate writing of poetry. Requires term paper or equivalent out-of-class written work.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 388, 'Intermediate Creative Writing Workshop in Fiction: Form, Theory, and Practice', 3, '{}', 'Intermediate writing of fiction.  Requires term paper or equivalent out-of-class written work.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 390, 'Methods of Inquiry', 3, '{'L'}', 'Studies in writing, rhetoric and literacy are multiple and diverse because these areas deal with complicated ways people create meaning, circulate meaning, and understand meaning in a variety of contexts and a variety of modal formats. Examines ways of developing research problems and questions, designing studies and conducting, reading and evaluating research in the areas of writing, rhetoric and literacy. Qualitative methods (e.g., case study, ethnographic methods, interviewing); historical methods (e.g., archival, document analysis and artifact interpretation); rhetorical analysis and discourse analysis; feminist approaches to research and analysis; and recent advances in research in the fields of rhetoric, writing and literacy studies. Investigates the many attitudes and assumptions about creativity and cultural work that make methods of inquiry as controversial as they are significant to contemporary public life.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 391, 'Writing in Context', 3, '{}', 'How do we understand the ways in which people use written language to carry out work in professional, academic, social and/or personal contexts? How does our understanding of the ways writing is produced for an audience shift between and among these different contexts? More importantly, how do writers adapt their writing practices in order to facilitate audience understanding as well as cultivate an audience's capacity for action? Familiarize students with rhetorical practices that position them to read, assess and respond to the rhetorical demands of a wide range of writing contexts. Strengthens students' existing rhetorical skill sets and problem-setting capacities for producing and circulating work (widely defined across material, print and digital media) that responds to a range of problems in an applied manner. Assesses the rhetorical demands of writing contexts and in developing and producing tailored, audience-specific texts that forward work in complex, interdisciplinary environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 392, 'History of Rhetorical Theory', 3, '{'HU'}', 'Begins in ancient Greece with the rise of the art of public oratory (known as rhetoric) and ends in the present, an era dominated by mass media. Engages in a great deal of reading--lots of very old writings as well as some newer ones. Covers a great many topics and ideas; focuses by continually returning to the subject of public discourse. Challenging readings designed to emphasize the connection between systems of philosophy (or 'worldview') and systems of communication. By focusing on big ideas, students become a better thinker and communicator in any chosen career.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 393, 'Theories of Literacy', 3, '{}', 'While theories of literacy have, for the most part, been determined by the primacy of reading and writing script, more recent scholarship in literacy theory and history has served to challenge such limitations. Many contemporary perspectives on literacy view scripted text as one component of complex acts and practices of written communication that occur in social contexts. Studies that examine such communicative acts in their local contexts reveal that literate practices and the texts that are produced are imbued with social and cultural values and traditions and the needs and desires of individuals. As the statements above illustrate, theories of literacy themselves are embedded within the values and assumptions of individuals, institutions, and academic specialization. Examines some of these theories, their influences on academic scholarship and pedagogy, the ideological assumptions that inform them, and how we might view these theories within broader social-cultural landscapes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 395, 'Combat in Film', 3, '{'SB','H'}', 'Interdisciplinary approach to and qualitative method for analyzing war movies and military dramas as both historical and cinematic narratives that represent the experiences of infantry marines and soldiers in 20th- and 21st-century U.S. wars.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 400, 'History of Literary Criticism', 3, '{'L','HU'}', 'Major critics and critical traditions in the Western world.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 401, 'Topics in Critical Theory', 3, '{}', 'Major critical schools of recent decades--postcolonialist, psychoanalytic, deconstructionist, feminist, new historicist.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 403, 'Semantics', 3, '{}', 'Program and methods of contemporary formal semantics. Characterizes knowledge of meaning as knowledge of truth conditions. The goal of semantics is thus to uncover the rules that allow the speakers of a language to know under what conditions any sentence is true; or in other words, to construct a theory of truth for natural language. Simple fragments including: predication, quantification, definite descriptions, simple temporal and modal expressions, and attitude verbs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 404, 'Studies in Second Language Acquisition', 3, '{}', 'Specialized topics in second language teaching and learning, including theory, pedagogy, curriculum, and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 408, 'Gender, Sexualities and Literacies', 3, '{}', 'Presents diverse perspectives on gender, sexualities and literacies from preschool to adulthood. Focuses on race, ethnicities, sexual orientation and gender identities to provide overviews of current research on gender, sexualities and ethnicities and examines how these subjectivities are expressed in multiple modes of communication. Explores representations and performances of gender and sexualities through literacy practices in sociocultural and educational contexts. Explores related issues through textbook readings, videos, websites, activities, online discussions and projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 409, 'Analyzing Rhetoric', 3, '{}', 'Provides students with the tools and methods by which the effectiveness, truthfulness, ethicality, and/or artistry of persuasive speech and language can be evaluated. Students take a systematic approach to analyzing rhetoric on a variety of selected topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 410, 'Writing for Publication', 3, '{}', 'Lectures and conferences concerning techniques of writing for publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 411, 'Advanced Creative Writing', 3, '{}', 'Advanced creative writing in genres other than poetry, fiction, creative nonfiction, or screenwriting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 412, 'Creative Nonfiction', 3, '{}', 'Lectures, discussion, and criticism concerning techniques of writing creative nonfiction for publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 413, 'History of English Language', 3, '{'HU'}', 'Development of English from the earliest times to the modern period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 414, 'Studies in Linguistics', 3, '{}', 'Specialized topics in linguistics including phonology, syntax, semantics, pragmatics, discourse, language variation, and change.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 415, 'Studies in Medieval Literature and Culture', 3, '{'HU'}', 'Medieval English and continental literature in translation, emphasizing cultural and intellectual backgrounds.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 416, 'Chaucer in Middle English', 3, '{'HU'}', 'Yearly alternate between Chaucer's The Canterbury Tales and Troilus and Criseyde.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 417, 'Virtual Worlds for Global Communication', 3, '{}', 'Explores the nature of virtual worlds and the multiple uses of these three-dimensional environments (also called metaverses) for new literate and global communications in the 21st century. Uses a combination of a textbook; Blackboard; popular virtual worlds, including Second Life (SL), and Internet resources (e.g., Web sites, e-discussion lists, YouTube videos) to explore the potential of virtual worlds. Explores the capabilities of several of the more than 70 virtual worlds for education, recreation, advancing social justice, global communication, socializing and networking. Participants critically examine the affordances of virtual worlds through readings, videos, online discussions, self-paced tutorials and brief reaction papers. Students receive demonstrations and guided tours by the professor and participate in individual and group explorations of virtual worlds');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 418, 'Renaissance Literature', 3, '{'L','HU'}', 'Prose, poetry, and drama of the Renaissance Era with an emphasis on cultural history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 419, 'Writing for Podcasts', 3, '{}', 'Focuses on digital communication skills, which are highly valued by companies and nonprofits. Increases awareness and understanding of how podcasts require specific attention to context, audience and meaning making. Emphasizes the theory and practical rhetorical conventions for social and digital media.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 420, 'Multicultural Autobiographies', 3, '{'(L','HU)','C'}', 'Examines the human experience via autobiographies and personal narratives. Multicultural, literary, and psychological perspectives on individual lives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 421, 'Studies in Shakespeare', 3, '{'L','HU'}', 'Close examination in selected dramatic and/or nondramatic works.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 422, 'Special Topics in Shakespeare', 3, '{}', 'Close examination in selected dramatic and/or nondramatic works.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 423, 'Renaissance Drama', 3, '{'L','HU'}', 'Topics, authors, and themes in the drama of the Tudor and early Stuart periods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 424, 'Milton', 3, '{'HU'}', 'Selected prose and poetry, emphasizing Paradise Lost, Paradise Regained, and Samson Agonistes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 425, 'Studies in Romanticism', 3, '{'HU'}', 'Romanticism in continental, British, and American literature and culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 426, 'British Culture and Empire', 3, '{}', 'Examines British imperial culture in art and literature, 1870-1914, and postcolonial theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 427, 'Studies in 18th-Century Literature and Culture', 3, '{'HU'}', 'Literary, social, and cultural issues of the period studied in an interdisciplinary format. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 428, 'The Pre-Raphaelites', 3, '{}', 'Visual and literary works by Victorian Pre-Raphaelites as works in themselves and through relations between images and texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 429, 'Studies in European Literature and Culture', 3, '{}', 'Literary, cultural, and historical issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 430, 'Studies in Victorian Literature and Culture', 3, '{'L','HU'}', 'Literary, social, and cultural issues of the period studied in an interdisciplinary format. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 431, 'Whitman and Dickinson', 3, '{'L','HU'}', 'Evaluates the 19th-century American Literary Renaissance through the specialized examination of its poetry and authors in their historical context.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 432, 'The American Renaissance', 3, '{'HU'}', 'Novels, poetry, short fiction, and criticism of the major literary figures of the early to mid-nineteenth century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 433, 'Literary Magazine Publishing: Canyon Voices', 3, '{}', 'Practical study of literary magazine production. Students learn every facet of curating and producing an issue of Canyon Voices literary magazine, from reading and reviewing submissions, to editing and designing the magazine, to soliciting submissions. In addition, students learn to organize a public literary reading that showcases the artists and writers featured in the magazine and fosters a literary presence in the community.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 434, 'Studies in the Literature and Culture of the Americas', 3, '{}', 'Literature and culture of North America, South America, and the Caribbean. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 435, 'Literatures and Cultures of Immigration and Diaspora', 3, '{}', 'Specialized topics in literary and cultural representations of immigration and diaspora studied in an interdisciplinary context.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 436, 'Studies in Anglophone Literature and Culture', 3, '{}', 'Literary, social, and cultural issues of English-speaking former colonial territories. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 437, 'The American Novel, 1900 - 1960', 3, '{'L','HU'}', 'Developments in theory and practice of major novelists.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 438, 'American Novel Since 1960', 3, '{'L','HU'}', 'Major novelists of the period. Developments in theory and practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 439, 'Technoliteracies', 3, '{}', 'Critically explores the nature of digital communications and expanded definitions of what counts as text and as literate expression in the 21st century. Emphasizes the 21st-century skills and abilities required for and fostered by digital communications (e.g., collaboration, multitasking, intertextual analyses) and examines the newest technologies that enable digital communications (e.g., digital storytelling, virtual worlds, social media, digital crafting, video gaming/modding). In doing so, focuses on the intersections of technologies and the arts as literate communications with implications for teaching and learning. Participants explore these topics through readings, videos and Web sites and by reflecting on their own experiences. Students respond to questions/prompts through online discussions, reflective thought papers, and media or virtual presentations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 440, 'Studies in American Literature and Culture', 3, '{}', 'Various genres in their literary, political, theoretical, and historical contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 441, 'Introduction to Participatory Media', 3, '{}', 'The spread of participatory media, ranging from zines, e-zines, podcasting and video production tools to video games and virtual worlds has led to significant changes in how we communicate, learn, collaborate, network and create. Introduces theory and research on participatory media and the changing nature of 'literacy' in a media age to 'literacies.' Addresses key concepts and issues, such as language, identity, access and community. Explores these topics through engaging in readings, videos and Web sites; by reflecting on their own experiences; and by responding to questions/prompts through online discussions, thought papers, social media or virtual presentations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 442, 'Studies in 20th-Century British and Irish Literature and Culture', 3, '{}', 'Major literary genres (novel, poetry, and drama) in their cultural and historical contexts. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 443, 'Gender, Culture and Literacy', 3, '{}', 'Explores the interactions and influences of gender and culture on literacy development and practice. Literacy is defined broadly as including the new literacies (e.g., blogs, wikis, zines or self publications) and multiliteracies (e.g., communication through oral, lyrical, visual, digital or written texts) within a broad view of what counts as text. Addresses varying feminist theories and perspectives on masculinities and gender in relation to literacy in home, school, community and work place settings. Participants explore their own written, digital and oral language practices through readings, videos, Web sites, Blackboard discussions, self-reflections, thought papers and action research essays.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 444, 'Topics for Interdisciplinary Study of Literature and Culture', 3, '{}', 'Uses methodologies from literary studies and other disciplines to understand a topic or problem. Interdisciplinary areas may include fields such as environmental humanities or American studies, as well as other areas of interdisciplinary study.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 445, 'Studies in American Realism', 3, '{'L','HU'}', 'Writers and influences that shaped the development of literary realism. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 446, 'Visual Narratives', 3, '{'L','HU'}', 'Students analyze and compose visual narratives (e.g., picture books, comics, graphic novels, film, animation, video games).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 447, 'Experimental Narrative', 3, '{'HU'}', 'Examines texts that experiment with form, content and narrative perspective within print, film and digital media.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 448, 'Studies in Irish Literature and Culture', 3, '{}', 'Themes and problems pertaining to Irish literature, film, and social and cultural history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 449, 'Masculinities, Femininities and Literacies', 3, '{}', 'Explores perspectives on and representations of masculinities and femininities in literacy development and practice. Addresses, beginning with adolescence, a time of identity exploration, feminist and gender perspectives in relation to oral language, reading, writing and electronic communications in home, school, community, education and work settings. Examines gender performance as a local/global issue. Explores the influence of gender on written, digital and oral language practices through textbook readings, videos, websites, self-reflections, thought papers and/or alternative format responses to weekly questions on the course materials, and Blackboard discussions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 450, 'The British Novel', 3, '{'HU'}', 'Focuses on the British novel from the 18th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 452, 'Studies in the Novel', 3, '{'HU'}', 'May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 453, 'Studies in the American Novel', 3, '{'HU'}', 'Poetics and politics of the novel, 18th through 21st centuries. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 454, 'Gender and Literature', 3, '{'L','HU'}', 'Focuses on the representation of gender in literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 455, 'Contemporary Women Writers', 3, '{}', 'Critical examination of literature by contemporary women writers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 456, 'Studies in Forensic Linguistics', 3, '{}', 'Focuses on the three main aspects of that branch of applied linguistics, namely language and the law, court cases where language is the central dispute, and linguistic evidence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 457, 'Studies in American Poetry', 3, '{'HU'}', 'May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 458, 'Latina/o Literature', 3, '{'(L','HU)','C'}', 'Development of Latina/o literature. Study of genres and themes; attention to literary antecedents of the new U.S.-based Latina literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 459, 'Studies in African American/Caribbean Literatures', 3, '{'(L','HU)','G'}', 'Studies in African American or Caribbean literatures according to genre, period, theory, or selected authors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 460, 'Ethnic Women Writers', 3, '{}', 'Concentrates on selected women writers of the U.S. who are Native American, African American, Hispanic, and Asian American.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 461, 'Studies in Women and Literature', 3, '{}', 'Advanced topics in literature by or about women.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 462, 'Africana Literature', 3, '{'HU','G'}', 'Focuses on the literature of the African Diaspora, including texts from the Caribbean, the Americas, and Africa. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 463, 'African American Literature', 3, '{}', 'Thematic and cultural study of African American literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 464, 'Great Directors', 3, '{'L','HU'}', 'Studies the work of one or more influential directors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 465, 'Studies in Film', 4, '{}', 'Advanced topics in cinema.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 466, 'Studies in International Film', 3, '{'(L','HU)','G'}', 'Studies non-American narrative film.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 467, 'American Film Musicals', 3, '{'L','HU'}', 'A cultural history of American film musicals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 468, 'Environmental Literary Criticism', 3, '{'L','HU'}', 'Evaluates the disparate critical practices that constitute what is the emerging literary project known as 'ecocriticism' or 'environmental literary criticism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 469, 'Science and Literature', 3, '{}', 'Historical and theoretical links between science and literature, from Francis Bacon to the present, examined in cultural context. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 470, 'Symbols and Archetypes in Children's Literature', 3, '{'L','HU'}', 'Various critical approaches and recurring themes studied in relation to classical and contemporary children's literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 471, 'Literature for Young Adults', 3, '{'HU'}', 'Prose and poetry that meet the interests and capabilities of junior high and high school students. Stresses recent literature.  A passing grade of at least 'C' (2.00) required before students are permitted to student teach in English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 472, 'Rhetorical Studies', 3, '{'L'}', 'Developments in theory and practice of major rhetorical inquiries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 473, 'Critical Approaches to Children's Literature', 3, '{'L','HU'}', 'Reads picture books, chapter books and fairy tales and analyzes these texts through multiple critical lenses (e.g., feminist, Marxist, historical, structuralism, formalist).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 474, 'Review Writing', 3, '{}', 'Advanced writing course exploring rhetorical approaches to writing reviews.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 475, 'Popular Periodical Writing', 3, '{'L'}', 'Advanced writing course analyzing and imitating genres in popular periodical writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 476, 'Studies in Folklore', 3, '{}', 'Surveys the history, genres, and dynamics of folklore, with emphasis on oral traditions. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 477, 'World Literature', 3, '{'(L','HU)','G'}', 'Readings in world literature, mostly outside the United States and England.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 478, 'Studies in Modernism', 3, '{}', 'Cultural, historical, and literary issues in American and European modernism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 479, 'Studies in Postmodernism', 3, '{'HU','G'}', 'Literary, social, and cultural topics in postmodernism. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 480, 'Methods of Teaching English: Composition', 3, '{'L'}', 'Methods of instruction, organization, and presentation of appropriate content in the teaching of composition and other writing skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 482, 'Methods of Teaching English: Language', 3, '{'L'}', 'Methods of instruction, organization, and presentation of appropriate content in language and usage for junior and senior high schools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 483, 'Methods of Teaching Secondary Literature and Language', 3, '{}', 'Emphasizes educational methods of planning, teaching, and assessing appropriate content in the instruction of secondary literature, literacy, and language usage.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 485, 'Digital Tools', 3, '{}', 'Digital (online, electronic) tools as best utilized in language arts curriculum and instruction for junior and senior high schools. Addresses many practical classroom matters; broadens knowledge of Web 2.0 opportunities for teaching and learning. Creates a detailed inventory of the digital tools familiar to secondary students, and compares these to what a clever English teacher can access in a public school classroom. Imaginations, studies, and designs digital tools to enhance student achievement. Uses digital tools to increase student access to history and culture and opportunities for individuals to find and articulate their voices, as well as opens avenues for their creativity. Empowers the English language arts classroom with critical understandings of media, culture and public relations (digital information manipulation) and develops secondary students from passive consumers to literate participants in democracy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 486, 'Teaching Text', 3, '{}', 'Approaches to texts (poetry, nonfiction and fiction) as best utilized in language arts curriculum and instruction for junior and senior high schools. Language arts teachers want all of their students to love literature and become immersed in the texts they assign (poems, short stories, plays, nonfiction and informative texts, and novels). Explores activities and philosophies for building a strong reading workshop in the secondary language arts classroom. Through identifying proficient readers, conducting thoughtful conversations about students and reading, and using a variety of ways to invite readers into books, designs and experiences reading workshops based on innovative and effective teaching strategies. Offers hands-on strategies for supporting struggling readers and for teaching reading from a critical lens.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 487, 'Advanced Creative Writing Workshop in Poetry', 3, '{}', 'Advanced writing of poetry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 488, 'Advanced Creative Writing Workshop in Fiction', 3, '{}', 'Advanced writing of fiction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 489, 'Scholarly Products', 3, '{}', 'Students develop culminating projects that demonstrate comprehensive knowledge and command of skills within the field of English studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 490, 'Literary Forms:  Theory and Practice in Poetry', 3, '{}', 'Types, history, analysis of traditional forms, and contemporary adaptations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 491, 'Topics for Advanced Research', 3, '{}', 'Students define a conceptual question related to a specific topic in literary scholarship; conduct appropriate research; and craft a response or argument that assesses the question, the evidence and possible responses to it.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 497, 'Honors Colloquium', 6, '{}', 'A class emphasizing discussion, written and/or oral presentation by students, and active participation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 498, 'Pro-Seminar', 7, '{'L'}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 500, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 501, 'Approaches to Research', 3, '{}', 'Theories, methods and/or methodologies as well as qualitative and/or quantitative approaches to the specific discipline. Familiarizes students with the areas of research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 502, 'Contemporary Critical Theories', 3, '{}', 'Studies the principles and techniques of contemporary theory and criticism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 503, 'History of Narrative', 3, '{}', 'Historical overview of narrative as a mode of discourse and communication. Begins with the study of influential traditional narrative forms from Biblical and classical sources and moves to study innovation in narrative, including flashbacks, interior monologue, nonlinear narrative and frustration of narrative expectations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 504, 'Cross-Cultural Studies', 3, '{}', 'Theoretical and analytical issues for comparative research across distinct cultural regions and traditions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 505, 'Narrative Research Methods', 3, '{}', 'Addresses narrative and arts-based forms of qualitative research. Focuses on reporting study findings in narrative form.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 506, 'Methods and Issues in Teaching Language', 3, '{}', 'Developing the language skills of native and non-native speakers through a process approach and advanced writing workshops in junior and senior high schools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 507, 'Methods and Issues in Teaching Composition', 3, '{}', 'Up-to-date theory, practice, and implementation of secondary writing instruction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 509, 'Studies in Narrative Writing', 3, '{}', 'Writing course focusing on specific kinds of narrative writing (e.g., blogging, travel writing). Regardless of particular topic focus, this course covers basic structural issues in narrative writing and gives students practical and applied experience crafting their own narratives for multiple audiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 513, 'Classic Texts in Leadership and Statesmanship', 3, '{}', 'Encounters classic texts featuring analyses and discussions of important historical leaders and their actions. These texts both situate examples of leadership and statesmanship in their historical context, and highlight timeless principles of wise and effective leadership. Texts include Thucydides' History of the Peloponnesian War, Plutarch's Lives, Cicero's On Duties, Shakespeare's histories, and the writings and speeches of Abraham Lincoln.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 514, 'Studies in Experimental Narrative', 3, '{}', 'Critical analysis of experimental or unconventional narratives, including nonlinear narrative, surrealist narrative and metanarrative. Course materials include both text and visual narratives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 516, 'Literary Publishing', 3, '{}', 'Explores various aspects of literary publishing, including market profile of journals, techniques for preparing submissions, role of literary awards and contests.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 517, 'History of the English Language', 3, '{}', 'Surveys the development of the English language, with an emphasis on major linguistic transformations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 518, 'Scientific Narratives', 3, '{}', 'Examines how fictional narratives have shaped popular constructions of science and how scientific works can be read as narrative. Coursework emphasizes critical thinking, discussion and argumentative writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 519, 'Superstition Review', 3, '{}', 'Students collaborate to complete tasks that lead to the publication of one issue of the national online literary magazine Superstition Review.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 520, 'Visual Narratives', 3, '{}', 'Students analyze and compose visual narratives (e.g., picture books, comics, graphic novels, film, animation, video games) and explore relevant theory and research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 521, 'Writing the Southwest', 3, '{}', 'Cultural representation in twentieth-century narratives of the Southwest United States. Focuses on canonical and noncanonical texts and addresses multiple representations of race, time periods and genres in the Southwest literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 522, 'Narratives of Conquest', 3, '{}', 'Critical study of narratives relating/responding to the conquest of the Americas from the fifteenth century to the present. Addresses conquest narratives that reflect the Spanish colonization period (1510-1810) and the United States colonization period (1848-1965) in the Southwest. Chosen texts illuminate themes related to the nature of colonial relationships, notions of barbarie/civilization and empire building');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 525, 'Teaching Second Language Writing', 3, '{}', 'Provides a critical introduction to the field of second language writing as well as issues and strategies in the teaching of second language writing in a wide variety of contexts. After exploring various instructional contexts as well as the characteristics of different types of students and their texts, considers various instructional practices and strategies, focusing on course and assignment designs, reading-writing connection, teacher and peer feedback, grammar instruction, classroom assessment, plagiarism and text borrowing strategies, and negotiating language differences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 530, 'Old English', 3, '{}', 'Study of Old English grammar, syntax, and phonology, with selected readings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 531, 'Old English Literature', 3, '{}', 'Intensive literary, linguistic, and cultural study of Old English literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 532, 'Middle English Dialects', 3, '{}', 'Study of the principal dialects of Middle English, with selected readings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 533, 'Studies in Medieval Literature', 3, '{}', 'Selected topics in English literature from the 11th through the 15th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 534, 'Studies in Renaissance Literature', 3, '{}', 'Selected topics and literary works studied in the contexts of English Renaissance culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 535, 'Studies in 18th- and 19th-Century British Literature', 3, '{}', 'Selected topics, issues, figures, and genres in British literature and culture of the 18th and 19th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 536, 'Studies in American Literature before 1900', 3, '{}', 'Selected topics, issues, figures, and genres in 17th-, 18th-, and 19th-century American literature, including the literature of conquest and contact.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 537, 'Studies in Modern and Contemporary British Literature', 3, '{}', 'Selected topics, issues, figures, and genres in British literature and culture after 1900.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 538, 'Studies in Modern and Contemporary American Literature', 3, '{}', 'Selected topics, issues, figures, and genres in American literature and culture after 1900.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 539, 'Studies in Modernist and Postmodernist Literature and Theory', 3, '{}', 'Selected topics in Modernist and Postmodernist studies. May include literary and theoretical texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 540, 'Teaching Young Adult Literature', 3, '{}', 'Issues and new approaches in teaching contemporary literature in high school.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 541, 'Teaching Texts in Secondary Classrooms', 3, '{}', 'English language arts teachers want all of their students to love literature and become immersed in the texts they assign. Explores activities and philosophies for building a strong reading curriculum in the secondary language arts classroom. Through identifying proficient readers, conducting thoughtful conversations about students and reading, and using a variety of ways to invite readers into texts, designs and experiences reading workshops based on innovative and effective teaching strategies. Students learn to create and implement reading units, literature circles, books of choice, literary tea parties, reading portfolios and final projects. Students also learn hands-on strategies for supporting ethnically and linguistically diverse readers and for teaching reading from a critical lens.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 542, 'Studies in North American Ethnic Literatures', 3, '{}', 'Selected works studied in their cultural contexts from authors representing ethnic experiences in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 544, 'Studies in Colonial and Postcolonial Literature', 3, '{}', 'Selected topics, periods, theories, and figures in works by authors representing colonial and postcolonial regions and/or experiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 550, 'Translation', 3, '{}', 'Surveys theories and practices of translation into English. Considers target, audience and market.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 551, 'Rhetorical Traditions', 3, '{}', 'Examines rhetorical traditions spanning ancient to contemporary rhetorics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 552, 'Composition Studies', 3, '{}', 'Selected topics in the history and theories of composition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 553, 'Technologies of Writing', 3, '{}', 'Critical study and cultural analysis of information technologies and their effects on various writing practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 554, 'Rhetorics of Race, Class, and Gender', 3, '{}', 'Study of interdependent relationships of race, class, and gender in rhetorical constructions of self and community.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 556, 'Theories of Literacy', 3, '{}', 'Examines various theories of literacy, their embedded values and assumptions, and their influences on academic scholarship and pedagogy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 557, 'Computer-Assisted Language Learning', 3, '{}', 'Explores the field of computer-assisted language learning. Traces the development of computer-assisted language learning (CALL) over the last three decades. Focuses on research that explores how computer technology can help facilitate second language development; relies primarily on empirical data to do so. Examine established as well as new research methods in CALL. Participants participate in one research study and apply theoretical and practical aspects of CALL in these projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 560, 'Genre Studies', 3, '{}', 'Critical analysis and study of works from a single genre or comparative analysis and study of multiple genres.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 561, 'Film Studies', 3, '{}', 'Analysis and study of film genres, cinematic techniques, and problems of interpretation and representation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 562, 'Forms of Poetry', 3, '{}', 'Types, history, criticism, and schools of theory of metrical form. Analyzes lyric, narrative, and dramatic poetry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 563, 'Forms of Fiction', 3, '{}', 'Types, history, criticism, and schools of theory in the forms of fiction. Analyzes narrative and dramatic structure.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 567, 'World War II in Film', 3, '{}', 'Offers a comprehensive survey of how film has represented the Second World War. Using a transdisciplinary approach, studies how films from the time of the Second World War to the present have portrayed that conflict. Compares different perspectives on the causes and results of the war, as well as engage in analysis of how important issues such as international political and economic relationships, religion, gender, ethnicity, class, race, technology and the psychological effects of violence get shown in movies about World War II.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 568, 'World War II in Literature', 3, '{}', 'Explores the topic of World War II across fiction, non-fiction, film and other film genres. Focus areas and topics of literature covered in the course may vary from semester to semester, depending upon instructor preference.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 571, 'Foundations for Teaching English Learners', 3, '{}', 'Provides teachers with foundational knowledge of English language learner education including: identification and knowledge of English language learners in school contexts, the role of culture in learning, principles of second language acquisition and teaching, historical and current perspectives and policies related to English learner education, and the role of parents and communities in the education of English learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 572, 'Strategies for Teaching English Learners', 3, '{}', 'Supports teachers through a review of core knowledge of strategies and correlated assessments for the instruction of English learners. Teachers learn to assess EL language needs and apply appropriate teaching strategies for supporting learning English and content. Primary emphasis is in the integration of EL language and learning needs and implementation of lesson planning, strategies and appropriately leveled instruction. Teachers complete a semester-long inquiry on integrating course content into their teaching practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 581, 'Methods of Teaching Secondary Writing', 3, '{}', 'Emphasizes methods of planning, teaching, and assessing appropriate content in the instruction of composition and related skills in secondary classrooms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 582, 'Pedagogy', 3, '{}', 'Overview of diverse theories of English instruction, including composition/rhetoric, linguistics and literary, with emphasis upon development and implementation for high school- and community college-level instructors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 583, 'Methods of Teaching Secondary Literature and Language', 3, '{}', 'Emphasizes educational methods of planning, teaching, and assessing appropriate content in the instruction of secondary literature, literacy, and language usage.');
