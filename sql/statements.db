INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ADE', 511, 'Core Architectural Studio I', 6, '{}', 'Applies design fundamentals in architectural problems, including construction, technology, programmatic and environmental determinants.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ADE', 512, 'Core Architectural Studio II', 6, '{}', 'Applies architectural design fundamentals to increasingly complex problems, including specific sites and activities.');
