INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MKT', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MKT', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 101, 'Everyday Grammar', 1, '{}', 'Helps students master the elements of English grammar.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 102, 'Fundamentals of Coding', 1, '{}', 'Provides a basic understanding of how the Internet works, knowledge of how to create and upload Web pages, and a basic introduction to programming with JavaScript. Assumes no previous knowledge of any of the topics, although requires basic knowledge of how to use a Web browser and navigate the Web.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 120, 'Media and Society', 3, '{'SB'}', 'Covers the role of newspapers, magazines, radio, television and motion pictures in American society.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 240, 'Media Issues in American Pop Culture', 3, '{}', 'Examines the production and consumption of popular culture as disseminated by the mass media with emphasis on the societal implications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 302, 'Media Research Methods', 3, '{}', 'Surveys research methods used in the social sciences, with a focus on mass communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 307, 'The Digital Audience', 3, '{}', 'Introduces students to the foundations of digital strategy, including audience research, social media campaigns, SEO, analytics, audience targeting, goal setting and more. Through hands-on exercises with real brands, students learn how audiences differ across industries and platforms, approaches for engaging users, and how organizations use audience data to create growth strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 335, 'Social Media', 3, '{}', 'Surveys the social media landscape: what are social media platforms, who uses them, and how are they transforming the way we inhabit the world? Explores and critically analyzes the social, cultural, legal, ethical, economic and technological dimensions of social media environments from Facebook to Foursquare, Twitter to Flickr and beyond.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 366, 'Media Ethics and Diversity', 3, '{}', 'Explores contemporary news media ethical problems in a seminar setting. Uses journalism and mass communication ethical perspectives to obtain keys to unlock contemporary dilemmas surrounding the news media.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 403, 'Media Law', 3, '{}', 'Provides a grounding in the laws and regulations that govern various aspects of the media with a focus on First Amendment guarantees and privileges and the extent to which those have been limited by courts and lawmakers. Topics include freedom of expression, libel, privacy and obscenity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 418, 'History of Mass Communication', 3, '{'SB','H'}', 'American journalism from its English and colonial origins to the present day. Development and influence of newspapers, magazines, radio, television, and news gathering agencies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 420, 'Strategic Sports Communication', 3, '{}', 'Covers the wide-ranging field of strategic sports communication. Prepares students for an entry-level position in the strategic sports communication field.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 425, 'Digital Media Literacy I', 3, '{}', 'Helps students begin to navigate the 21st-century media, starting by becoming active users of media, not just passive consumers. The Internet-fueled democratization of media creation has enabled anyone to be a publisher, and given audiences' almost infinite choices, with both great and questionable effects. In a world of information abundance, it can be difficult to discern fact from opinion and truth from fiction. We need to know how to wade through this barrage of information with a critical eye, to be better informed, and ultimately become creators of media ourselves.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 426, 'Digital Media Literacy II', 3, '{}', 'Discusses digital media consumption to some degree, but focuses more on media creation and the many issues we need to understand for best practices. Students learn the importance of understanding the tools and techniques of media creation, and how media are used to persuade, the ethical and legal issues in creating media, policy issues that can (or will) affect media creation, and an assortment of media-creation techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 427, 'Misinformation and Society', 3, '{}', 'Develops strategies and tactics to recognize misinformation, and inoculates students--and helps them join the battle--against it. Misinformation in our context is false information purposefully created to deceive and cause harm. It runs rampant in today's digital information ecosystem, and is becoming more and more sophisticated and difficult to spot. Bad actors are using powerful tools and techniques to infect our information supply, sowing confusion and discord. While there are laudable efforts to staunch the flow of misinformation, media users need help in deciding what information to trust. Examines the evolution of misinformation and the range of content types that fall under its umbrella. Explores tools, techniques and tactics that individuals can employ to identify misinformation, and gives them a head start on making informed decisions on what, and whom, they can trust.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 428, 'Digital Media and Freedom of Expression in the 21st Century', 3, '{}', 'Covers the extraordinary nature of media in this century. Examines how the media and communications tools of the past 100+ years--telephones, print publications and broadcasting outlets--gave way to the Internet, a radically centralized network of networks that is rapidly re-centralizing. Also explores how the ability to exercise freedom of expression became radically easier in the early days of the Internet, but how that freedom, among others, may be in jeopardy--and, if so, what can be done about that, as individuals and members of communities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 430, 'International Mass Communication', 3, '{'G'}', 'Comparative study of communication and media systems. Information gathering and dissemination under different political and cultural systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 431, 'Media Entrepreneurship', 3, '{}', 'Familiarizes students with media entrepreneurship and innovation in a quickly changing digital media environment. Students study the principles of entrepreneurship and innovation, assess their own tolerance for risk, evaluate opportunity and market conditions, and learn about various revenue models for entrepreneurial enterprises.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 433, 'Social Media Campaigns, Engagement and Research', 3, '{}', 'Learn how to attract, engage and grow audiences on social media platforms through both organic and paid social media campaigns, and research social media behavior and performance. Includes evaluating organizations' existing social media audiences, identifying opportunities for audience growth, creating content and campaigns for specific social platforms, measuring the impact of social media content, and developing an organizational strategy for managing social media across an enterprise.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 434, 'Search Engine Research and Strategy', 3, '{}', 'Learn how to research, attract and grow audiences through the strategic use of search engine optimization and search engine marketing. Includes search trend research, keyword research, user behavior, SEO (search engine optimization), tagging and coding for on-page SEO, SEM (search engine marketing), competitive analysis, domain authority and pay-per-click advertising.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 436, 'Audience Research and Behavior', 3, '{}', 'Prepares students to undertake or manage research on customers, employees, readers, viewers, influencers and others with whom an organization seeks to communicate or build relationships. Digital data have profoundly reshaped how organizations mine and interpret audience data, so the course introduces new techniques and tools for developing audience insights, as well as traditional qualitative and quantitative research methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 437, 'Audience Acquisition and Engagement', 3, '{}', 'Students learn how to attract and grow audiences on established and emerging digital platforms. Students discover tactics and strategies for targeting, obtaining and engaging audiences by leveraging social media, search marketing and SEO, digital content, ecommerce, digital advertising and others.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 438, 'Digital Audience Analysis', 3, '{}', 'Students discover how to access, manipulate and translate audience data. Students learn how to mine data from Web sites, social networks, apps, email campaigns and video platforms, and how to use analysis tools and techniques to translate metrics into valuable, actionable insights that drive engagement strategies and business decisions. By working with real audience data, students receive hands-on analysis and reporting experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 439, 'Digital Audience Growth', 3, '{}', 'Capstone challenges students to synthesize their digital audience knowledge and skills acquired in the previous courses through an applied project. Working with a real client and live, ever-changing data, students work to increase and engage the client's digital audience, executing a growth campaign from audience identification, targeting and strategy development to implementation, engagement and measurement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 450, 'Visual Communication', 3, '{'HU'}', 'Theory and tradition of communication through the visual media with emphasis on the continuity of traditions common to modern visual media.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 456, 'Political Communication', 3, '{'SB'}', 'Explores the intersection of media and politics and examines how the political system works and how journalists cover politics, political campaigns and elections. Students gain knowledge of opinion polling, debates, campaign finance, the party system, political communication, issues coverage, fact checking, crowd sourcing and more.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 460, 'Race, Gender, and Media', 3, '{'C'}', 'Readings seminar designed to give students a probing examination of the interface between AHANA Americans and the mass media in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 465, 'Sports and Media', 3, '{}', 'Designed for aspiring journalists and media users, explores why great sports journalism is essential to our society. Establishes the goals, dreams, and expectations for sports journalism and introduces ethical philosophy and popular ethical models and decision-making tools. Students then apply these tools to sports journalism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 470, 'Exploring the Business of Journalism', 3, '{}', 'Exposes students to broad media management problems. Focuses on how essential the bottom-line and quality journalism are to the entire journalistic and mass communication enterprise; helps students think about how to balance those two concerns.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 473, 'Sex, Love, and Romance in the Mass Media', 3, '{'SB'}', 'The role of the mass media in constructing and/or reinforcing unrealistic mythic and stereotypic images of sex, love, and romance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 498, 'Pro-Seminar', 7, '{}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 502, 'Journalism Skills', 8, '{}', 'Basic journalism skills for print, digital and broadcast. Significant hands-on practice writing and reporting stories on deadline as well as an immersion in the principles and practices of good journalism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 503, 'Media Law', 3, '{}', 'Examines the philosophical and legal aspects of press freedom with an emphasis on the First Amendment from 1791 to present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 504, 'Cronkite Master's Seminar', 1, '{}', 'Provides an introduction to the Cronkite School as well as challenges and opportunities in media leadership today. Students develop strategies and tactics for establishing themselves as leaders now and into the future, with a focus on driving change and innovation in media.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 505, 'Depth Reporting', 3, '{}', 'Covers in-depth and investigative reporting and writing techniques. Students produce long-form, deeply sourced articles and produce multimedia with the goal of publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 510, 'Data Journalism', 3, '{}', 'Students use data to tell stories, employing computer spreadsheets, database software, mapping programs and statistical techniques and learn how these tools can be used across a wide variety of beats and stories. Students negotiate with government officials for public records and use the internet and other online sources to find information and sources for stories.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 512, 'Humphrey Seminar', 3, '{}', 'Identifies, examines and exports role models of leadership in the field of global communications. Explores leadership issues in journalism, strategic communications and management under different cultural and political systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 515, 'Advanced Online Media', 3, '{}', 'Builds on and expands basic multimedia knowledge. Students leave with heightened Web-language literacy and visual sophistication; fluency with multiple digital tools that create, publish and distribute content; and the requisite storytelling skills to produce works of a professional level on diverse online and mobile platforms. Draws on Adobe Creative Cloud software and other current tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 517, 'Public Relations Campaigns', 3, '{}', 'Enables students to synthesize and apply the theories, principles and techniques of public relations. This body of knowledge includes research, planning techniques, types of budgets, organizational systems, management styles, campaign components, types of campaigns, technology, ethics, diverse channels of communication and target audiences. The student develops an in-depth understanding of a client's needs; conducts primary and secondary research; works with a client in a team-based approach to prepare a comprehensive campaign proposal to address the client's problem or issue; designs and develops media products for client; formally presents the proposal to the client and the class and analyzes, discusses then responds to public relations case studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 519, 'Strategic Communications Leadership and Ethics', 3, '{}', 'Communications and the ability to tell an organization's story to a variety of audiences and stakeholders is a key part of corporate strategy. Provides a broad understanding of ethical communications practices in agency and corporate settings, and includes instruction in reputation management, corporate communications, brand management, and stakeholder engagement in a digital context. Especially useful for future communications professionals and those who will work with them such as journalists and business leaders.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 521, 'News Producing', 3, '{}', 'Principles of broadcast news producing, delivered via a combined lecture/lab experience. Students complete assignments based on editorial judgment, technical skills and industry awareness and develop strategies for total newsroom management. In the process, students build confidence in producing news content and newscasts under deadline pressure and are prepared for a variety of producer-related roles in professional newsroom environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 525, '21st-Century Media Organization and Entrepreneurship', 3, '{}', 'Focuses on the future of journalism, the future of journalism business organizations and entrepreneurship, new media experiences, online solutions to popular media problems, financial realities of the media, and media innovation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 526, 'Advanced Issues in 21st-Century Sports and Media', 3, '{}', 'Through critical analyses of major contemporary issues in sports and media, provides tools and information needed to more fully engage in and become fluent with the main conversations about the business, technology and the future of sports journalism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 529, 'Ethics and Diversity in Sports Journalism', 3, '{}', 'Explores the connection among sports media, journalism and society in an ethical and issue-oriented context. Students gain an understanding of the issues and ethics of the sports journalism profession while becoming better and more informed consumers of sports media. At the end of the course students have a better understanding of the importance of sports in our society and how that is reflected in coverage by local and national media outlets.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 530, 'History, Philosophy, and Ethics of Journalism', 3, '{}', 'Explores contemporary news media issues in a seminar setting. Critically analyzes historical and contemporary dilemmas in journalism ethics using a case study approach.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 531, 'Advanced Broadcast Reporting', 3, '{}', 'Teaches news and information practices of networks and local television stations. Students report, write and edit broadcast packages with an emphasis on video.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 533, 'Advanced Spanish-Language Broadcast Reporting', 3, '{}', 'Students produce bilingual television packages on a wide range of issues of interest to Latino audiences. Students also report and produce an occasional Spanish-language newscast. Opportunity to develop and master advanced reporting, interviewing, visual storytelling and writing skills in English and Spanish needed to obtain an entry-level position in a small/medium market local television newsroom.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 534, 'Advanced Spanish-Language Reporting', 3, '{}', 'Students get experience reporting and writing stories on topics related to Latinos in the U.S. Requires ability to write and report in Spanish, although students do not have to be proficient.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 535, 'Latino and Transnational Issues', 3, '{}', 'Focuses on some of the most pressing and controversial issues of the day that are taking place in the Southwest borderlands. Successful students move on to the Borderlands Initiative depth reporting project in the spring semester that includes an international reporting experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 536, 'Public Relations Research', 3, '{}', 'Teaches the importance and practice of research in a public relations context. Students conduct research using qualitative and quantitative methods and analyze data to understand implications within a specific communications context. They also critically evaluate published academic and professional research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 540, 'Sports Reporting', 3, '{}', 'Hands-on experience reporting, writing, script writing and video producing sports stories. Students report, write and produce professional-level, clear, well-reported, accurate stories and video packages about athletes and sports stories newsworthy in the Valley and beyond.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 541, 'Narrative Writing and Reporting', 3, '{}', 'Students learn the elements of narrative journalism--structure and narrative arc, the use of character, description, dialogue and quotes, context and background, and pacing--and apply them to a long-form story they report and write during the semester.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 542, 'Sports and Culture Commentary', 3, '{}', 'Students learn how to craft informed opinion in both news and sports across a variety of platforms, including written columns and video and audio commentaries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 544, 'Accountability in Journalism', 1, '{}', 'Seminar about accountability journalism in the digital age: its history, mission, values, techniques, ethics and future. Students research and analyze notable recent examples of accountability journalism and produce accountability story ideas about water quality and supply in Arizona.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 545, 'Techniques of Investigative Reporting', 3, '{}', 'Student teams work under the direction of a veteran investigative journalist, learning how to do research and reporting on projects to expose wrongdoing, focusing on public records, databases, in-depth interviews and how to conceptualize, organize and tell these stories on multiple platforms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 546, 'News 21 Seminar', 3, '{}', 'Prepares students for the Carnegie-Knight News21 investigative journalism summer fellowship. Immerses students in the selected topic for the annual investigation, conducting deep research, generating data and public records, interviewing experts, developing sources and producing initial reports and multimedia elements. Considers students who perform well in the seminar for paid summer fellowships during which they travel the country to complete their reporting and produce the annual project on an issue of national import.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 548, 'Non-Narrative Video Storytelling', 3, '{}', 'Teach students how to create video pieces that effectively communicate a story, information or concept to your audience in non-narrative form that is engaging, interesting and intelligent. Focuses on the various forms of non-narrative storytelling to gain experience creating both longer and shorter pieces with DSLR cameras, advanced editing and graphics that are appropriate to the intended platform. Students learn how to shoot video and stills with a DSLR camera and when to use each in video creation, while advancing skills as a video editor and graphic designer. Students broaden their creativity and range as a video producer in the areas of journalism and mass communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 550, 'Issues in Coverage of Business and the Economy', 3, '{}', 'Teaches the basics of business and economics reporting, one of the fastest-growing areas of journalism. Students learn what skills are needed to cover business and economic stories as well as how private companies operate and the factors that drive the economy. They also learn about opportunities in the field and hear from leaders in business journalism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 551, 'Better Business Reporting', 3, '{}', 'Provides training and practice in reporting and writing stories related to business and the economy. Students learn tools and techniques to enhance their coverage of various kinds of business stories, from profiles to financial reports. They learn best practices for pitching stories, cultivating sources, conducting effective interviews, structuring stories, using appropriate narrative elements and translating complex information for a general audience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 552, 'Data in Business Journalism', 3, '{}', 'Students learn how to find powerful business and economic data from U.S. sources such as the Census Bureau, the Bureau of Labor Statistics, the Securities and Exchange Commission and the Federal Reserve. Examines international data sources of the United Nations, the World Bank, the European Union and the statistics offices of various countries. Covers basic mathematical and statistical formulas for working with business data, how to find stories in data using tools such as Microsoft Excel and Google Fusion Tables and how to create basic data visualizations that are useful for understanding business and economic trends.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 553, 'Reporting on Business and the Economy', 3, '{}', 'Trains students in the basics of reporting about business and the economy. They learn to tell the stories behind companies, people and money, while improving their competency in using numbers in stories. Students have the opportunity to have stories appear in print, online or broadcast.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 555, 'Digital Media Entrepreneurship', 3, '{}', 'Students identify a marketable idea and transform it into a product and/or company. In the process, they learn about key concepts of entrepreneurship and practice skills that they can employ as entrepreneurs themselves. Students are exposed to a range of national and regional entrepreneurs and emerge with a deeper understanding of entrepreneurship's importance within the media and communications landscape.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 556, 'Media Entrepreneurship', 3, '{}', 'Familiarizes students with media entrepreneurship and innovation in a quickly changing digital media environment. Students study the principles of entrepreneurship and innovation, assess their own tolerance for risk, evaluate opportunity and market conditions, and learn about various revenue models for entrepreneurial enterprises.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 557, 'Advanced Radio Reporting', 3, '{}', 'Students report and produce long-form radio stories in commercial and public radio formats. They develop planning, writing, voicing and detailed editing skills in a variety of radio broadcast formats, including general news and business as well as hard news and feature reporting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 560, 'Arizona Media Law', 3, '{}', 'Case study approach to first amendment issues, media access, libel, confidentiality, and invasion of privacy as applied to media organizations in Arizona.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 561, 'Defining the Digital Audience', 3, '{}', 'Who are the people who make up digital audiences? How do digital audiences behave? And why is it important to research, identify and monitor digital audiences and their engagement with your organization? Covers the very essence of the digital audience, exploring how community, consumer, social psychology and theory converge on digital platforms and social networks. Through case-study analyses, teaches how audiences differ across industries and platforms, the elements that engage or dissuade users, and how organizations use audience data to manipulate growth strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 562, 'Search Engine Strategy for Digital Audience Acquisition', 3, '{}', 'How to attract and grow audiences through strategic use of search engine marketing. Includes user search behavior, SEO (search engine optimization), tagging and coding for on-page SEO, SEM (search engine marketing), keyword research, competitive analysis, domain authority and pay-per-click advertising.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 563, 'Social Media Campaigns and Engagement', 3, '{}', 'How to attract, engage and grow audiences on social media platforms through both organic and paid social media campaigns. Includes evaluating an organization's existing social media audience, identifying opportunities for audience growth across social media platforms, designing and generating content for specific platforms, measuring the impact of social media content, and developing an organizational strategy for managing social media across an enterprise.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 564, 'Digital Audience Research and Behavior', 3, '{}', 'Provides skills to undertake research on digital audiences, customers, viewers, influencers and others with whom an organization seeks to communicate or build relationships. Digital data have profoundly reshaped how organizations mine and interpret audience data. Digital audience research as a subset of social research. Overviews both traditional and newer research methods for developing audience insights.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 565, 'Digital Audience Analytics', 3, '{}', 'Provides skills to access, manipulate and translate audience data. How to mine data from websites, social networks, apps, email campaigns and video platforms, and how to use analysis tools and techniques to translate metrics into valuable, actionable insights that drive engagement strategies and business decisions. Works with real audience data to receive hands-on analysis and reporting experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 566, 'Digital Audience Management', 3, '{}', 'Challenges students to synthesize their digital audience knowledge and skills acquired in previous courses through a culminating experience. Working with a real client and live, ever-changing data, students work to increase and engage the client's digital audience, executing a growth campaign from audience identification, targeting and strategy development to implementation, engagement and measurement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 570, 'Master of Mass Communication Capstone', 9, '{}', 'Students synthesize and apply their knowledge and skills in these full-day immersive programs that are powered by students and led by faculty. Students may choose from among several programs, including newsroom environments, sports bureaus, strategic communications agencies, special projects labs and more. Students emerge with real professional experience, portfolio work and preparedness for careers across the media industry. Programs are available in Phoenix, Los Angeles and Washington, D.C.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 710, 'Research Methodology in Mass Communication', 3, '{}', 'Identifies research problems in mass communication and provides an overview of questionnaire construction as well as attention to survey, historical, content analysis, experimental and legal research methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 711, 'Critical, Historical, Legal Research Methods in Mass Communication', 3, '{}', 'Critical, legal and historical methods necessary to conduct qualitative mass communication research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 712, 'Freedom of Expression Theory', 3, '{}', 'Examines philosophical and legal aspects of press freedom; emphasizes First Amendment theory evolution from 1791 to present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 720, 'Mass Communication Theory', 3, '{}', 'Analyzes various theoretic models of mass communication with emphasis on the applications of these theories to professional communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 722, 'Philosophy of Mass Communication', 3, '{}', 'Mass media as social institutions, particularly interaction with government and public; emphasizes criticism and normative statements. Seminar. Prerequisite: Journalism and Mass Communication PhD student.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 755, 'Research Apprenticeship', 1, '{}', 'Students and faculty work on a research project with the expected outcome of a scholarly publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MCO', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 501, 'Writing About Social Issues', 3, '{}', 'Reading, discussion and creation of nonfiction writing on diverse societal issues. Investigates diverse forms and genres of writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 502, 'Religion, Culture, and Health: Where Cultures Intersect', 3, '{}', 'Focuses on how cultural and religious notions may come into conflict around issues of health. Contrasts two societies, not necessarily contemporary, from different continents.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 503, 'Ethics, Science, and Culture', 3, '{}', 'Introduces ethics related to the practice of art, science, and medical practice in modern and premodern society.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 504, 'Film Analysis', 3, '{}', 'Introduce formal and cultural issues important to understanding movies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 505, 'The Research Process', 3, '{}', 'Contemporary scholarly writing and research processes that adult learners, new to graduate study, can use in all MLS classes and for years ahead.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 520, 'Ecologies of Possibility: Creative and Critical Approaches', 3, '{}', 'Provides a foundation of experiential, creative and critical exploration into ways of knowing that are reflexive and justice oriented.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 525, 'Creative and Critical Research Design', 3, '{}', 'Emphasizes that research, as an open-ended process, is a creative act that can help solve problems, generate possibilities, destabilize oppressive systems and promote justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 582, 'Applied Project Plan', 3, '{}', 'Students employ select research methodologies, creative/critical thinking, reflexive practices and peer feedback to devise an applied project plan and an annotated literature review.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MLS', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 208, 'Patterns in Nature', 4, '{'SQ'}', 'Examines topics from optics to analytical techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 211, 'Introduction to Mechanics of Materials', 3, '{}', 'Topics include stress, strain, elasticity, axial loading, torsion, bending, yield criteria, fracture, general energy methods and Castigliano's Theorem.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 212, 'Microstructure and Properties Lab', 1, '{}', 'Lab experiments introducing optical and electron microscopy techniques, metallography and microstructure of metals, effects of composition and processing, and compositional identification.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 215, 'Materials Synthesis', 3, '{}', 'Introduces contemporary techniques for synthesizing both traditional and novel materials and the corresponding correlation to synthesized materials properties.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 250, 'Structure and Properties of Materials', 3, '{}', 'Basic concepts of material structure and its relation to properties. Application to engineering problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 301, 'Materials and Civilization', 3, '{}', 'Focuses on exploring how materials technology can change people's lives. Traces how societies evolved based on the access to materials and the ability to exploit them and trade them among nations. Studies the role of materials in a variety of contexts such as food, art, warfare, transportation, medicine, information and energy from the Stone Age through the industrial revolution to the modern world. Analyzes the connection between the dramatic developments in civilization and advances in materials through the lens of structure, properties and processing of materials.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 308, 'Sensing the World', 4, '{'SQ'}', 'Project-oriented science course develops critical thinking and technical skill using Internet modules on the five senses.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 330, 'Thermodynamics of Materials', 3, '{}', 'Principles of statistical mechanics, statistical thermodynamics of single crystals, solutions, phase equilibrium, free energy of reactions, free electron theory, and thermodynamics of defects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 335, 'Materials Kinetics', 3, '{}', 'Introduces kinetics in processing of materials as illustrated through real-world materials processing examples.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 355, 'Structure and Defects', 3, '{}', 'Intermediate-level grounding in crystallography, crystal defects, surfaces and interfaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 356, 'Thin Film and Microelectronic Devices Lab', 1, '{}', 'Lab experiments correlating atomic structure, defects and microstructure of processed metals, ceramics polymers and composites to their mechanical and thermal properties.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 415, 'Mathematical and Computer Methods in Materials', 3, '{'CS'}', 'Mathematical, computational, and statistical methods and computer programming used to model materials science phenomena and materials engineering applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 420, 'Advanced Metallurgical Alloys and Processes', 3, '{}', 'Phase diagrams, structural alloys, additive manufacturing and metallurgy processing, materials selection and high entropy alloys.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 421, 'Physical Metallurgy Laboratory', 1, '{}', 'Focuses on analysis of microstructure of metals and alloys and includes correlation with mechanical properties to some extent.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 435, 'Computational Materials Science and Engineering', 3, '{}', 'Computer simulation has become a tool for the advancement of our understanding of fundamental material physics and practical material applications. Provides an introductory understanding of concepts and techniques for the modeling and simulations of materials on a wide spectrum of length scales, ranging from atomic to macroscopic. Accordingly, discusses a variety of computational modeling: particle-based simulations (Monte-Carlo and molecular dynamics); mesoscale methods (cellular automaton and phase-field modeling); and continuum models (finite-element analysis, stochastic microstructure reconstruction). Recommend a background in or working knowledge of a computer programming language (C/ C++ /FORTRAN /MATLAB) linear algebra, differential equations, thermodynamics and statistical mechanics. Reviews the essence of the C/C++ programming language and explains example codes to make the course accessible to students with minimal programming experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 440, 'Mechanical Behavior of Materials', 3, '{}', 'Effects of environmental and microstructural variables of mechanical properties, including plastic deformation, fatigue, creep and brittle fracture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 442, 'Fatigue, Fracture, and Creep of Materials', 3, '{}', 'Relationship between microstructure and fracture; fatigue and creep properties of materials. Environmental effects and recent developments. Current theories and experimental results.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 450, 'Introduction to Materials Characterization', 3, '{}', 'Introduces materials characterization techniques for analysis of thin films and bulk materials by TEM, SEM, XRD, XPS, and AES.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 451, 'Nanomaterials and Electronics Characterization Lab', 1, '{}', 'Lab for materials characterization techniques for analysis of thin films and bulk materials by TEM, SEM, XRD, XPS, and AES.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 457, 'Quantum Mechanics for Understanding Properties of Atoms and Solids', 3, '{}', 'The effect on quantum mechanics on the electronic structure and properties of atoms and solid materials.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 458, 'Electronic, Magnetic, and Optical Properties', 3, '{}', 'Electrical, optical, and magnetic properties of solids and microstructure effects as examined through materials-based examples.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 460, 'Nanomaterials in Energy Production and Storage', 3, '{}', 'Emphasizes the relationship among fundamental materials properties, chemical and energy transformation, as well as energy storage. Applications include the role of materials for photovoltaics, nanocatalysts, batteries, and fuel cells. Stresses fundamental principles and charge transfer processes in nanostructured materials in addressing energy needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 466, 'Electrochemical Energy Storage and Conversion', 3, '{}', 'Electrochemical concepts relevant to real-world devices. Discusses basic electrochemical principles with an emphasis on solid-state electrochemistry and the nature of the solid-electrolyte interface in order to give students the background knowledge needed for understanding and analyzing real device performance of, e.g., batteries, solar hydrogen generation, photoelectrochemical cells, electrocatalysts, electrochemical capacitors, fuel cells. Critically evaluates media reports and journal publications describing electrochemical energy conversion and storage devices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 470, 'Polymers and Composites', 3, '{}', 'Relationship among chemistry, structure, and properties of engineering polymers. Design, properties, and behavior of fiber composite systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 471, 'Introduction to Ceramics', 3, '{}', 'Principles of structure and property relations in ceramic materials. Processing techniques. Applications in mechanical, electronic, and superconducting systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 476, 'Growth and Processing of Semiconductors', 3, '{}', 'Briefly introduces semiconducting materials and emphasizes the characteristics of defects in semiconductors and their influence on electronic properties. Covers principles of growth of bulk crystals, epitaxial growth, oxidation, diffusion, ion implantation, and metallization. Discusses the introduction of defects during growth and processing of semiconductors and their influence on device behavior.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 482, 'Materials Engineering Design', 3, '{'L'}', 'Principles of the design process. Feasibility and optimization. Manufacturing processes, materials selection, failure analysis, and economics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 489, 'Capstone Design Project I', 1, '{'L'}', 'Covers how to plan an open-ended design project, including experience with technical writing and technical presentations. Both MSE 489 & 490 must be taken to secure L General Studies designation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 490, 'Capstone Design Project II', 2, '{'L'}', 'Students learn how to conduct an open-ended design project, including experience with technical writing and technical presentations. Both MSE 489 and 490 must be taken to secure L General Studies designation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 498, 'Pro-Seminar', 7, '{}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 501, 'Linear Algebra in Engineering', 3, '{}', 'Development and solution of systems of linear algebraic equations. Applications from mechanical, structural, and electrical fields of engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 502, 'Partial Differential Equations', 3, '{}', 'Development and solution of partial differential equations in engineering. Applications in solid mechanics, vibrations, and heat transfer.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 503, 'Concepts in Materials Science', 3, '{}', 'Familiarizes students with fundamental concepts and terms used in MSE. Introduces the relationship between structure-processing-property-performance-characterization. From this point, explores crystal structure, symmetry and defects. After which, introduces fundamental theory of diffusion, kinetic and phase transformation and gives examples of their impact on the material. Next, presents the correlation between structure-processing-property on performance. Throughout the course, exposes concepts of materials characterization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 510, 'Introduction to Materials Characterization', 3, '{}', 'Introduces materials characterization techniques for analysis of thin films and bulk materials by TEM, SEM, XRD, XPS, and AES.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 511, 'Mathematical and Computer Methods in Materials', 3, '{}', 'Mathematical, computational and statistical methods and computer programming used to model materials science phenomena and materials engineering applications. Prior completion of undergraduate-level calculus, differential equations, and linear algebra needed for success in the course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 513, 'Polymers and Composites', 3, '{}', 'Relationship among chemistry, structure, and properties of engineering polymers. Design, properties, and behavior of fiber composite systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 514, 'Advanced Metallurgical Alloys and Processes', 3, '{}', 'Phase diagrams, structural alloys, additive manufacturing and metallurgy processing, materials selection and high entropy alloys.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 515, 'Introduction to Electronic, Magnetic, and Optical Properties', 3, '{}', 'Electrical, optical and magnetic properties of solids and microstructure effects as examined through materials-based examples.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 516, 'Mechanical Behavior of Materials', 3, '{}', 'Effects of environmental and microstructural variables of mechanical properties, including plastic deformation, fatigue, creep and brittle fracture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 517, 'Introduction to Ceramics', 3, '{}', 'Principles of structure, property relations in ceramic materials. Processing techniques. Applications in mechanical, electronic, and superconducting systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 519, 'Growth and Processing of Semiconductors', 3, '{}', 'Briefly introduces semiconducting materials and emphasizes the characteristics of defects in semiconductors and their influence on electronic properties. Covers principles of growth of bulk crystals, epitaxial growth, oxidation, diffusion, ion implantation, and metallization. Discusses the introduction of defects during growth and processing of semiconductors and their influence on device behavior.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 523, 'Structural and Mechanical Properties of Materials', 3, '{}', 'Designed for first-year PhD students in the School of Materials. Topics include crystallography theory, tensor properties of materials, elasticity, crystal defects, dislocation theory, plasticity, fracture, fatigue, and creep.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 524, 'Advanced Thermodynamics', 3, '{}', 'Equilibrium thermodynamics. Chemical reactions, phase equilibria, and stability in multicomponent systems. Aqueous solutions and electrochemistry. Introduces statistical thermodynamics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 525, 'Fundamentals of Electrical, Optical, and Magnetic Materials and Device Applications', 3, '{}', 'Behavior of materials; electronic band structure, electronic and ionic conduction, electronic processes in semiconductors, dielectric, optical and magnetic properties. Emphasizes the fundamental understanding of material properties while establishing structure-property relations. Aims to link these properties to applications in optoelectronics, photonics and optics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 526, 'Materials Physics I', 3, '{}', 'Fundamentals of materials physics: crystal structure, diffraction, elasticity, point defects, dislocations, lattice vibrations, thermal properties, periodic potential, band structure.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 527, 'Materials Physics II', 3, '{}', 'Electronic behavior of materials: energy bands, electronic properties, metals, semiconductors, insulators, optical properties, magnetic properties, superconductivity, biophysics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 535, 'Computational Materials Science and Engineering', 3, '{}', 'Computer simulation has become a tool for the advancement of our understanding of fundamental material physics and practical material applications. Provides an understanding of concepts and techniques for the modeling and simulations of materials on a wide spectrum of length scales, ranging from atomic to macroscopic. Accordingly, discusses a variety of computational modeling: particle-based simulations (Monte-Carlo and molecular dynamics), mesoscale methods (cellular automaton and phase-field modeling), and continuum models (finite-element analysis, stochastic microstructure reconstruction). Recommend a background in/working knowledge of a computer programming language (C/C++/ FORTRAN/ MATLAB), linear algebra, differential equations, thermodynamics and statistical mechanics. Reviews the essence of the C/C++ programming language and explains example codes to make the course accessible to students with minimal programming experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 540, 'Fracture, Fatigue, and Creep', 3, '{}', 'Relationship between microstructure and fracture; fatigue and creep properties of materials. Environmental effects and recent developments. Current theories and experimental results.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 546, 'Surfaces and Thin Films', 3, '{}', 'Thermodynamics and kinetics of surface structures and thin film processes. Ultra-high vacuum techniques for examining surfaces. Case studies of physics, chemistry and nanoscience applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 548, 'Fundamentals of Microelectronics Packaging', 3, '{}', 'Multidisciplinary course in addressing electrical, thermal, materials, chemical, manufacturing, and reliability problems in packaging.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 550, 'Advanced Materials Characterization', 3, '{}', 'Analytical instrumentation for characterization of materials; SEM, SIMS, Auger, analytical TEM, and other advanced research techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 551, 'Applied Machine Learning for Mechanical Engineers', 3, '{}', 'Machine learning impacts every aspect of our daily lives, from recognizing faces in a crowd to designing drugs to treat epidemic diseases. There is no doubt that, as a result of the impact, the job market for future engineers will be drastically different. This course equips engineering students with a working understanding of machine learning and better prepares them for the future job market.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 552, 'Electron Microscopy I', 3, '{}', 'Kinematical and dynamical electron diffraction and microscopy. Defect structure and composition using STEM imaging, x-ray and electron-energy-loss spectroscopy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 553, 'Electron Microscopy Laboratory I', 3, '{}', 'Lab support for MSE 552.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 554, 'Electron Microscopy II', 3, '{}', 'Determination of structure and composition of materials using high-resolution imaging, convergent-beam diffraction, and electron holography. Novel developments and applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 555, 'Electron Microscopy Laboratory II', 3, '{}', 'Lab support for MSE 554.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 560, 'Nanomaterials in Energy Production and Storage', 3, '{}', 'Emphasizes the relationship among fundamental materials properties, chemical and energy transformation, as well as energy storage. Applications include the role of materials for photovoltaics, nanocatalysts, batteries, and fuel cells. Stresses fundamental principles and charge transfer processes in nanostructured materials in addressing energy needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 561, 'Phase Transformations, Kinetics, and Diffusion in Solids', 3, '{}', 'Hetero/homogeneous diffusion-controlled transformations, order-disorder transformations, and shear martensitic) athermal transformations in solids. Courses in advanced thermodynamics and structure and properties of materials are strongly recommended in preparation for this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 566, 'Electrochemical Energy Storage and Conversion', 3, '{}', 'Electrochemical concepts relevant to real-world devices. Discusses basic electrochemical principles with an emphasis on solid-state electrochemistry and the nature of the solid-electrolyte interface in order to give students the background knowledge needed for understanding and analyzing real device performance of, e.g., batteries, solar hydrogen generation, photoelectrochemical cells, electrocatalysts, electrochemical capacitors, fuel cells. Critically evaluates media reports and journal publications describing electrochemical energy conversion and storage devices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 571, 'Quantum Physics', 3, '{}', 'Review modern physics, chemistry, math. Differential equation, operator, matrix formulations. Free particle, bound state problems. Examples from physics, chemistry and nanoscience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSE', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 110, 'Enhanced Freshman Mathematics', 3, '{}', 'Individually paced, computer-mediated course designed to prepare students for college-level mathematics. Students work on various modules in an online adaptive learning technology platform created to help students progress through the course material based on individual needs in preparation for enrollment in the next level math course required by the student's major (pre-MAT 117 'Track 1' or pre-MAT 142 'Track 2'). Upon completion of this course, students should see improvement of basic math skills, an increase in the confidence in mathematical abilities, and advancement in ability to relate math to real-life applications.  Credit for this course does not count towards university graduation requirements');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 117, 'College Algebra', 3, '{'MA'}', 'Linear functions, quadratic functions, general polynomial functions, rational functions, systems of linear equations, logarithmic and exponential functions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 117S, 'College Algebra Stretch', 0, '{}', 'For students who have an in-progress (Z) grade for MAT 117. The grade earned in MAT 117S replaces the Z grade for MAT 117. Course content is the same as MAT 117, which is intended to develop skills in linear functions, quadratic functions, general polynomial functions, rational functions, exponential functions and logarithmic functions. Also studies systems of linear equations. Emphasizes problem-solving techniques, specifically by means of discussing concepts in each of these topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 119, 'Finite Mathematics', 3, '{'MA'}', 'Topics from linear algebra, linear programming, combinatorics, probability, and mathematics of finance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 142, 'College Mathematics', 3, '{'MA'}', 'Applies basic college-level mathematics to real-life problems. Topics include numerical reasoning, sets, counting techniques, probability, basic statistics and finance. Appropriate for students whose major does not require MAT 117 or 170.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 170, 'Precalculus', 3, '{'MA'}', 'Intensive preparation for calculus. Topics include functions (including trigonometric), vectors and complex numbers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 171, 'Precalculus: Science. Technology, Engineering and Mathematics (STEM)', 4, '{'MA'}', 'Robust treatment of advanced algebraic skills and trigonometry that are vital in calculus. Algebra topics include functions, compositions, inverses, transformations, complex numbers, roots of polynomial functions, rational functions, exponential and logarithmic functions and their properties. Trigonometry is approached via the unit circle as well as the right triangle. Topics include the trigonometric functions, their graphs and identities, inverse trigonometry, the laws of sine and cosine, vectors and the dot product with an application toward work, and polar coordinates and roots of complex numbers (deMoivre's theorem).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 191, 'First-Year Seminar', 3, '{}', 'Small course emphasizing student-faculty discussion and interaction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 194, 'Special Topics', 5, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 207, 'Algebra and Geometry in the High School', 3, '{}', 'Provides a conceptually rigorous treatment of the central ideas of algebra and geometry in high school. Explores and develops familiar mathematical content from a very different perspective. Focuses squarely on creating meaning for concepts and on building coherence of meaning across concepts and various representations of them. As such, course activities and assessments (class/lab activities, homework assignments, and exams) reflect these expectations, focusing on both doing high school mathematics with meaning and on expressing and communicating that meaning accurately and coherently in verbal and written form. Prior to enrolling in this course, students should have experience with algebra/geometry to at least the high school level.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 208, 'Discrete Mathematics for Secondary Teachers', 3, '{}', 'Potential secondary teachers of mathematics should master the Math Standards proposed by the National Council of Teachers of Mathematics for college-intending students. Focuses on those aspects of these standards that apply to discrete mathematics with particular emphasis on: mathematical problem solving, mathematics as communication, mathematical reasoning, and discrete mathematics. Taking a broader view, introduces mathematics as a discipline that uses known methods to discover and verify new conclusions through the study of discrete mathematics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 210, 'Brief Calculus', 3, '{'MA'}', 'Differential and integral calculus of elementary functions with applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 211, 'Mathematics for Business Analysis', 3, '{}', 'Topics in business analysis, including: Lagrange multipliers, linear programming, linear algebra, intermediate probability, random variables, discrete distributions, and continuous distributions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 242, 'Elementary Linear Algebra', 2, '{}', 'Introduces matrices, systems of linear equations, determinants, vector spaces, linear transformations, and eigenvalues. Emphasizes development of computational skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 243, 'Discrete Mathematical Structures', 3, '{}', 'Logic, sets, functions, elementary number theory and combinatorics, recursive algorithms, and mathematical reasoning, including induction. Emphasizes connections to computer science.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 251, 'Calculus for Life Sciences', 3, '{'MA'}', 'Differential and integral calculus of elementary functions. Introduces differential and difference equations. Emphasizes applications to the life sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 265, 'Calculus for Engineers I', 3, '{'MA'}', 'Limits and continuity, differential calculus of functions of one variable, introduction to integration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 266, 'Calculus for Engineers II', 3, '{'MA'}', 'Methods of integration, applications of calculus, elements of analytic geometry, improper integrals, Taylor series.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 267, 'Calculus for Engineers III', 3, '{'MA'}', 'Vector-valued functions of several variables, partial derivatives, multiple integration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 270, 'Calculus with Analytic Geometry I', 4, '{'MA'}', 'Real numbers, limits and continuity, and differential and integral calculus of functions of 1 variable.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 271, 'Calculus with Analytic Geometry II', 4, '{'MA'}', 'Methods of integration; applies calculus, elements of analytic geometry, improper integrals, sequences and series.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 272, 'Calculus with Analytic Geometry III', 4, '{'MA'}', 'Vector-valued functions of several variables, multiple integration, and introduction to vector analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 274, 'Elementary Differential Equations', 3, '{'MA'}', 'Introduces ordinary differential equations, adapted to the needs of students in engineering and the sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 275, 'Modern Differential Equations', 3, '{'MA'}', 'Introduces differential equations, theoretical and practical solution techniques. Applications. Problem solving using MATLAB.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 298, 'Honors Directed Study', 6, '{}', 'Independent study or practicum under the supervision of a faculty member.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 300, 'Mathematical Structures', 3, '{'L'}', 'Logic and set theory, induction, functions, order and equivalence relations, cardinality. Emphasizes writing proofs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 310, 'Introduction to Geometry', 3, '{}', 'Congruence, area, parallelism, similarity and volume, and Euclidean and non-Euclidean geometry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 342, 'Linear Algebra', 3, '{}', 'Linear equations, matrices, determinants, vector spaces, bases, linear transformations and similarity, inner product spaces, eigenvectors, orthonormal bases, diagonalization, and principal axes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 343, 'Applied Linear Algebra', 3, '{}', 'Solving linear systems, matrices, determinants, vector spaces, bases, linear transformations, eigenvectors, norms, inner products, decompositions, applications. Problem solving using MATLAB.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 350, 'Techniques and Applications of Applied Mathematics', 3, '{}', 'Mathematical techniques and applications representative of the range of applied mathematics used in operations research, biology, economics, finance, and sociology. In depth exploration of these techniques in detail by focusing on selected applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 353, 'Mathematics and Cancer', 3, '{}', 'Addresses some of the mathematical questions regarding the dynamics of cancer growth and treatment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 355, 'Introduction to Computational Molecular Biology', 3, '{'CS'}', 'Introduces the mathematical skills used in molecular biology, genomics, and bioinformatics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 370, 'Intermediate Calculus', 3, '{}', 'Theory behind basic 1-variable calculus: continuity, derivative, Riemann integral, sequences, and series.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 371, 'Advanced Calculus I', 3, '{}', 'Real numbers, completeness, sequences/series, continuity, uniform theorems, derivative, Riemann integral, pointwise/uniform convergence, Taylor's theorem. Credit is allowed for only MAT 371 or 370 toward a mathematics degree.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 372, 'Advanced Calculus II', 3, '{}', 'Open, closed, compact sets in Rn continuity, differentiation, partial differentiation, integration in Rn Inverse/implicit function theorems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 410, 'Introduction to General Topology', 3, '{}', 'Point set topology, topological spaces, metric spaces, continuity, compactness, connectedness and product spaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 411, 'History and Philosophy of Mathematics', 3, '{'HU','H'}', 'Examines nature of mathematics from origins to present, revealed by its history and philosophy. Strong background in mathematics not required.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 412, 'Differential Geometry', 3, '{}', 'Rigorous treatment to the concepts and methods of differential geometry via the classical theory of curves and surfaces in Euclidean space, the Gauss-Bonnet theorem, and additional topics as time and interest permits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 415, 'Introduction to Combinatorics', 3, '{}', 'Enumerating permutations and combinations of sets and multisets, inclusion-exclusion, recurrence relations, generating functions, Plya theory and combinatorial structures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 416, 'Graph Theory', 3, '{}', 'Trees, cycles, matchings, planarity, connectivity, hamiltonicity, chromatic number, Ramsey theory with emphasis on proof techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 419, 'Introduction to Linear Optimization', 3, '{'CS'}', 'Simplex method, duality, and network flows. Applications to game theory, geometry, combinatorics, graph theory, and posets.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 420, 'Scientific Computing', 3, '{}', 'Surveys and applies programming languages, libraries, and scientific visualization tools. Programming assignments emphasize software development skills. Completion of advanced calculus recommended prior to this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 421, 'Applied Computational Methods', 3, '{'CS'}', 'Numerical methods for quadrature, differential equations, roots of nonlinear equations, interpolation, approximation, linear equations, floating-point arithmetic, and roundoff error.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 423, 'Numerical Analysis I', 3, '{'CS'}', 'Solution of systems of linear and nonlinear equations, optimization, eigenvalues, round-off error, and floating point arithmetic.  Fluency in computer programming is required for enrollment in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 425, 'Numerical Analysis II', 3, '{'CS'}', 'Analysis of and algorithms for numerical interpolation, integration, and differentiation. Numerical solution of ordinary and partial differential equations, introductory level. Applications.  Fluency in computer programming is required for success in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 429, 'Optimization', 3, '{}', 'A variety of optimization models and their applications to management, economics, engineering, and science. Topics include linear programming, integer programming, nonlinear programming, and stochastic programming. Stresses formulation and modeling as well as numerous applications and solving algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 440, 'Group Theory', 3, '{}', 'Covers basic group theory. Students learn about the concept of a 'group,' related structures, their underlying theory and examples of how they arise in mathematics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 441, 'Ring Theory', 3, '{}', 'Covers basic ring theory. Students learn about the concept of a 'ring,' related structures, their underlying theory, and examples of how they arise in mathematics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 442, 'Advanced Linear Algebra', 3, '{}', 'Fundamentals of linear algebra, dual spaces, invariant subspaces, canonical forms, bilinear and quadratic forms, and multilinear algebra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 443, 'Introduction to Abstract Algebra', 3, '{}', 'Introduces concepts of abstract algebra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 444, 'Intermediate Abstract Algebra', 3, '{}', 'Basic theory of groups, rings, and fields, including an introduction to Galois theory. Appropriate as preparation for MAT 543.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 445, 'Theory of Numbers', 3, '{}', 'Prime numbers, unique factorization theorem, congruences, Diophantine equations, primitive roots, and quadratic reciprocity theorem.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 447, 'Cryptography I', 3, '{}', 'Block ciphers, stream ciphers, congruence arithmetic, information theory, public key cryptosystems, key exchange, electronic signatures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 448, 'Cryptography II', 3, '{}', 'Mathematical underpinnings of contemporary cryptosystems, electronic signatures, key exchange, primlity testing and factorization algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 450, 'Mathematical Models in Biology', 3, '{}', 'Introduces mathematical models in biological systems. Applications range from the cellular level to systems level and may include applications from areas such as physiology, ecology, epidemiology, population biology, computational and systems biology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 451, 'Mathematical Modeling', 3, '{'CS'}', 'Detailed study of one or more mathematical models that occur in the physical or biological sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 452, 'Introduction to Chaos and Nonlinear Dynamics', 3, '{}', 'Properties of nonlinear dynamical systems; dependence on initial conditions; strange attractors; period doubling; bifurcations; symbolic dynamics; Smale-Birkhoff theorem; and applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 460, 'Vector Calculus', 3, '{}', 'Vectors, curvilinear coordinates, Jacobians, implicit function theorem, line and surface integrals, Green's, Stokes', and divergence theorems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 461, 'Applied Complex Analysis', 3, '{}', 'Analytic functions, complex integration, Taylor and Laurent series, residue theorem, conformal mapping, and harmonic functions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 462, 'Applied Partial Differential Equations', 3, '{}', 'Second-order partial differential equations, emphasizing Laplace, wave, and diffusion equations. Solutions by the methods of characteristics, separation of variables, and integral transforms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 472, 'Intermediate Real Analysis I', 3, '{}', 'Introduces analysis in metric spaces with emphasis on the real line.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 473, 'Intermediate Real Analysis II', 3, '{}', 'Analysis in Rn: implicit function theorem, introduction to manifolds, Lebesque integration, change of variables formula, convergence theorems for integrals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 475, 'Differential Equations', 3, '{}', 'Linear and nonlinear ordinary differential equations, asymptotic behavior of solutions, stability, existence and uniqueness, limit sets, Poincar-Bendixson theorem.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 476, 'Partial Differential Equations', 3, '{}', 'First-order quasilinear, method of characteristics, second-order linear (hyperbolic, parabolic, elliptic), Green's functions, integral representations, eigenfunction expansions, maximum principles, harmonic functions, and additional topics as time permits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 485, 'History of Mathematics', 3, '{}', 'Topics from the history of the origin and development of mathematical ideas.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 486, 'Peer Mentoring in STEM', 1, '{}', 'Advanced students learn and apply mentoring skills in science classes or teaching labs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 495, 'Undergraduate Research', 4, '{}', 'Supervised research in mathematical sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 500, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 501, 'Geometry and Topology of Manifolds I', 3, '{}', 'Brief review of general topology, cell complexes, topological manifolds, fundamental groups, covering spaces, homology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 502, 'Geometry and Topology of Manifolds II', 3, '{}', 'Differentiable manifolds, vector and tensor fields, differential forms, flows, Lie groups, homogeneous spaces, de Rham cohomology, Riemannian metrics and curvature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 512, 'Introduction to Combinatorics', 3, '{}', 'Enumerating permutations and combinations of sets and multisets, inclusion-exclusion, recurrence relations, generating functions, Polya theory and combinatorial structures. Same topics as MAT 415; mastery at much higher level required. Students with MAT 415 (even as undergraduates) may not enroll.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 513, 'Graph Theory', 3, '{}', 'Rigorous introduction to graph theory. High-level mastery in topics such as trees, cycles, matchings, planarity, connectivity, hamiltonicity, chromatic number, Ramsey theory with emphasis on proof techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 514, 'Enumerative Combinatorics I', 3, '{}', 'First semester of a systematic development of enumerative combinatorics, including elementary counting techniques, sieve methods, and partially ordered sets.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 515, 'Enumerative Combinatorics II', 3, '{}', 'Second semester of a systematic development of enumerative combinatorics, including lattices, exponential structures, symmetric functions, and selected special topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 516, 'Graph Theory I', 3, '{}', 'First semester of a systematic development of graph theory, including matchings, connectivity, arboricity, planarity, coloring, network flows.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 517, 'Graph Theory II', 3, '{}', 'Second semester of a systematic development of graph theory, including dense and sparse graphs, Ramsey theory, hamiltonicity, random graphs, minors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 541, ''p'-Adic Numbers', 3, '{}', 'Introduces 'p'-Adic numbers and analysis. Topics include valuations, completions of number fields, extensions of the field of 'p'-adic numbers and 'p'-adic analysis. Students should have previously taken courses in analysis and algebra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 542, 'Elliptic Curves', 3, '{}', 'Geometry and arithmetic of elliptic curves, points of finite order, the group of rational points, curves over finite fields, complex multiplication. Students should have previously taken a senior-level course in abstract algebra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 543, 'Abstract Algebra I', 3, '{}', 'Groups, modules, rings and fields, Galois theory, homological algebra, and the representation theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 544, 'Abstract Algebra II', 3, '{}', 'Continuation of groups, modules, rings and fields, Galois theory, homological algebra, and the representation theory');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 545, 'Abstract Algebra III', 3, '{}', 'Covers modules, Galois theory, and preparation for advanced study in algebraic parts of mathematics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 547, 'Algebraic Number Theory', 3, '{}', 'Algebraic properties of number fields and their completions. Topics include rings of integers, ramification, finiteness of the class group, and Dirichlet's unit theorem.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 570, 'Real Analysis I', 3, '{}', 'Lebesgue integration, selected function spaces, differentiation, abstract measure theory, and elements of functional analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 571, 'Real Analysis II', 3, '{}', 'Continuation of Lebesgue integration, selected function spaces, differentiation, abstract measure theory, and elements of functional analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 572, 'Complex Analysis I', 3, '{}', 'Analytic functions, series and product representations, entire and meromorphic functions, normal families, Riemann mapping theorem, harmonic functions, and Riemann surfaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 573, 'Complex Analysis II', 3, '{}', 'Continuation of analytic functions, series and product representations, entire and meromorphic functions, normal families, Riemann mapping theorem, harmonic functions, and Riemann surfaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 578, 'Functional Analysis I', 3, '{}', 'Locally convex, normed, and Hilbert spaces. Linear operators, spectral theory, and application to classical analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 579, 'Functional Analysis II', 3, '{}', 'Continuation of locally convex, normed, and Hilbert spaces. Linear operators, spectral theory, and application to classical analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 583, 'Fieldwork', 12, '{}', 'Structured, supervised field experience in a field science or other discipline requiring experience in field techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 594, 'Conference and Workshop', 12, '{}', 'Topical instruction, usually in compressed format, leading to academic credit. Often offered off campus to groups of professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 784, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAT', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 210, 'Mentored Tutoring Internship', 1, '{}', 'Introduces math education students to issues of balancing the content of precalculus mathematics with the needs of someone learning it. Addresses core issues, including articulating what is involved in learning the mathematical idea(s); interpreting students' questions; analyzing student thinking and inferring their underlying rationale from a learner-centered perspective; techniques of question-posing, problem-posing, and instructional scaffolding.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 250, 'Technology and Mathematical Visualization', 3, '{}', 'Uses several software packages for exploring mathematics and for preparing mathematical materials, starting from introductory skills and leading to advanced techniques. Places particular emphasis on using computer software with the aim of visualizing mathematical relationships, structures, and processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 280, 'Numbers, Operations and Proportional Reasoning for K-8 Teaching', 3, '{}', 'Investigation of mathematical principles and processes regarding the real number system and its properties develop number sense and qualitative reasoning abilities. Designed to meet the requirements for preservice elementary school teachers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 281, 'Investigating Space: Geometry, Measurement, and Visualization', 3, '{}', 'Explores three aspects of geometry: spatial visualization, measurement, and proof. Uses visualization, spatial reasoning, and geometric modeling to solve problems. Provides opportunities for participants (1) to use physical models, technology, and pattern-finding strategies to identify properties of and develop formulas for measuring two- and three-dimensional shapes; and (2) to develop powerful ways of thinking to justify geometric relationships');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 298, 'Honors Directed Study', 6, '{}', 'Independent study or practicum under the supervision of a faculty member.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 301, 'Investigating Change: Patterns, Functions, and Modeling', 3, '{}', 'Examines different conceptions of algebra as the study of (1) generalized arithmetic, (2) reasoning with symbols, (3) reasoning with patterns and functions, (4) structure in the number system, and (5) modeling. Distinguishes arithmetic from algebraic reasoning, and develops an appreciation for the pervasiveness of the function concept in the K-8 school mathematics curriculum. Features algebraic thinking and representation using algebra tiles and other concrete models, realistic problems, dynamic geometry software, graphing calculators, and a variety of virtual tools and Web sites. Topics include, but are not limited to: patterns, relations, functions and covariational thinking; multiple conceptions of quantity, variables, constants, and unknowns; inductive, deductive, and analogical reasoning; rate of change and proportional thinking; algebra learning trajectories; and comparisons of additive vs. multiplicative, absolute vs. relational, and arithmetic vs. algebraic thinking.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 310, 'Number Sense, Spatial Relationships, and Measurement and Data for Early Childhood Education Teaching', 3, '{}', 'Focuses on key concepts in counting, number operations, spatial relations, measurement and data in early childhood education. Prospective teachers acquire content knowledge by using physical models, technology and pattern-finding strategies to explore the progressions of these concepts from birth to grade 3.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 311, 'Geometry, Algebra, Statistics and Probability for K-8 Teaching', 3, '{}', 'Focuses on key concepts needed to prepare K-8 teacher candidates to teach in algebra, geometry, measurement, data, probability and statistics. Prospective teachers will (1) use physical models, technology, and pattern-finding strategies to explore the trajectories of these concepts; and (2) develop strategies to predict, generalize, and justify decisions based on data, statistics and probability.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 320, 'Conceptual Foundations of 7-12 Mathematics Curricula and Assessment', 3, '{}', 'Focuses on concepts of mathematics curricula and their relationships to perspectives on learning, instruction, and assessment. Using a variety of participation formats (e.g., individual and small group work, and whole group discussions) and directed activities, participants examine curricular principles underlying the historical development of mathematics curricula and assessment, and current curricula and curricular recommendations and controversies surrounding them.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 412, 'Mathematics in Elementary Schools', 3, '{}', 'Methods of teaching mathematics to all students at the K-8 level, instructional planning and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 430, 'The Development of Mathematical Thinking', 3, '{}', 'This capstone course requires students' sustained engagement with, and analysis of, artifacts from research on learning mathematics from instruction. The artifacts include: curricular/instructional task sequences, samples of student work, video-cases showcasing students' involvement in instruction, problem-solving interviews, and teaching experiments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 482, 'Methods of Teaching Mathematics in Secondary School', 3, '{}', 'Examines secondary school curricular material and analyzes instructional devices. Teaching strategies, evaluative techniques, diagnosis, and remediation and problem solving.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 483, 'Mathematics in the Secondary School', 3, '{}', 'Topics in geometry, number theory, algebra, and analysis. Emphasizes unifying principles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 485, 'Methods of Teaching Standards-Based Middle Grades and Early Secondary Mathematics', 3, '{}', 'Experiential introduction to standards-based teaching in middle and early secondary mathematics grades 6-9. Targets five aspects of a mathematics teacher's work, including knowledge of (1) mathematics content; (2) learning; (3) learners; (4) teaching; and (5) curriculum and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 501, 'Research in Undergraduate Mathematics Education I', 3, '{}', 'Research in Undergraduate Mathematics Education I (RUME I) covers academic research in (undergraduate) mathematics education. Develops the ability to read, discuss, criticize and draw practical conclusions from research in (undergraduate) mathematics education, all with the overarching aim of broadening perspective on areas of past and current mathematics education research. Research on the learning and teaching of undergraduate mathematics is one of the foci of the course. Students read research literature drawn from a variety of content areas and grade bands: quantitative reasoning, early learning of algebra, proportional reasoning, algebra and functions, calculus, statistics and probability. Prepares for Part 1 of the qualifying exam in mathematics education');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 502, 'Research in Undergraduate Mathematics Education II', 3, '{}', 'RUME II is the second course in a core four-course sequence in the mathematics education doctoral program. Explores research on problems in the teaching and learning of algebra, geometry, calculus and probability as seen from multiple theoretical perspectives. Develops ability to read, discuss, criticize and draw practical conclusions from research in mathematics education, all with the overarching aim of broadening perspectives on areas of past and current mathematics education research. RUME II builds on its prerequisite in a subtle but important way. Increased emphasis and focus on theory in mathematics education research--a consideration of various types of theories and the roles they may play in the research endeavor. Also prepares for the required qualifying exam');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 503, 'Research in Undergraduate Mathematics Education III', 3, '{}', 'RUME III is the third course in a series of mathematics education courses designed to prepare students to conduct independent mathematics education research. Students learn different methods for implementing data collection and analysis, reporting results; and continue to conduct a pilot study with the goal of developing a dissertation proposal from their research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 504, 'Research in Undergraduate Mathematics Education IV', 3, '{}', 'RUME IV is the fourth and final course in a series of mathematics education courses designed to prepare students to conduct independent mathematics education research. Students continue to conduct a pilot study with the goal of developing a dissertation proposal from their research. Continues developing the student's ability to: synthesize research literature in a specific area of mathematics education, refine research statement and question(s) to advance knowledge in a specific area of mathematics education, refine design and methods for conducting a study, analyze research data using theoretical constructs, describe results of analysis, report findings using theoretical constructs and submit a paper for publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 585, 'Modern Geometry for Teachers', 3, '{}', 'Euclidean, projective, and non-Euclidean geometries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 594, 'Conference and Workshop', 12, '{}', 'Topical instruction, usually in compressed format, leading to academic credit. Often offered off campus to groups of professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTE', 784, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
