INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 496, 'Playworks Portfolio', 3, '{}', 'Students devise a career plan, establish a digital portfolio, prepare artist statement, and become familiar with contracts and legal documents associated with a career as a serious independent game developer or studio artist.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
