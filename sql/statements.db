INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 632, 'Applied Musicology', 3, '{}', 'Explores practice-oriented entrepreneurial opportunities and methods in musicology, acquires knowledge and develops skills in community engagement and outreach, explores pathways of professional publication and network building, and participates in organizing curated concert series and scholarly conferences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 657, 'Topics in Symphonic Literature', 3, '{}', 'Examines the evolution of the symphony and symphonic poem from the early classic era through the 19th century, with emphasis on the analysis of selected works.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 668, 'Introduction to Ethnomusicology', 3, '{}', 'Experiencing theory and methodology by being ethnomusicologists through musical performance, primary writings on fieldwork, career structuring, the history of the discipline, its relationship with historical musicology, and the cultural theories which ethnomusicologists draw on.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 670, 'Area Studies in Ethnomusicology', 3, '{}', 'Study of the music of a particular culture, country, or area (e.g., music of Mexico, Latin America, China, Africa).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 675, 'History of Choral Music and Institutions', 3, '{}', 'Examines the history of choral music through the lens of choral institutions: patronage, education, ritual, performing forces, status (professional vs. amateur), and locale.');
