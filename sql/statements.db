INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 105, 'Computer Applications and Information Technology', 3, '{'CS'}', 'Introduces business information systems from a business intelligence perspective and the uses of application software with emphasis on database and spreadsheet packages for efficient and effective problem solving.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 235, 'Introduction to Information Systems', 3, '{}', 'Survey course introducing the competitive and strategic uses of information systems, how information systems are transforming organizations and their management, and the issues, difficulties, and opportunities facing the technology professional and business manager today.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 236, 'Honors Introduction to Information Systems', 3, '{'CS'}', 'Introduces strategic leveraging of IS solutions and surveys different facets of IS in order to gain an understanding of how business problems are addressed and opportunities realized.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 300, 'Web Design and Development', 3, '{}', 'Focuses on Web site development, including target audience, image presentation, page/content topic organization, site navigational functionality, implementation needs, and future needs. Web site design and development practice using business software products.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 308, 'Advanced Excel in Business', 3, '{}', 'Covers advanced Excel skills using an applied focus on different types of decisions one may analyze using spreadsheet capabilities; graphs and charts to communicate complex analytics; pivot tables to slice and dice data for reporting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 309, 'Business Process Management', 3, '{}', 'Flow charting, swimlane diagramming, process simulation using modern tool suites. Teaches the types of process evaluation decision analytics most commonly used and covers applied process analysis in a variety of contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 310, 'Business Data Visualization', 3, '{}', 'The design of insightful business data visualizations and dashboards to improve business decision making. Applies advanced data visualization techniques to make sense of temporal, geospatial, topical and network business data while also making it easier to digest and utilize for business users.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 311, 'Business Agile Project Management', 3, '{}', 'Overview of project management emphasizing the fundamentals, analytics and value that support project decision making. Covers how and why projects are selected within organizations, the purpose of projects, and the way that they should be properly managed using industry best practices and frameworks. Includes classical (Waterfall), modern (Agile), and hybrid project management techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 312, 'Business Mobile Commerce and Monetization', 3, '{}', 'Teaches students the entire mobile commerce process starting with recognizing what it is, what it does, how to build it and the business value of it. Familiarizes students with software development platforms and ecosystems, strategies for development and deployment, and monetization strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 315, 'Introduction to Business Data Analytics', 3, '{}', 'Overview of analytics in the business context; concepts of strategic and operational analytics; overview of concepts like dimensional modeling, the Model Lifecycle, data mining, big data, KPIs and metrics, ERP and analytics, in-database/memory analytics; real-time analytics and data stream analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 325, 'Fundamental Techniques for Business Data Analytics', 3, '{}', 'Manipulating, processing, cleaning and crunching data with a focus on utilizing scripting and programming tools toward business applications. Also a practical, modern introduction to extraction, transformation and loading of enterprise data for business applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 340, 'Business Information System Development I', 3, '{}', 'Systems Development Life Cycle (SDLC) tools, techniques, and methodologies used to plan and analyze the needs of modern business information system development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 345, 'Business Information System Development II', 3, '{}', 'Provides a clear understanding of the Systems Development Life Cycle (SDLC) tools, techniques, and methodologies used to design, implement, and support modern business information systems development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 355, 'Business Data Warehouses and Dimensional Modeling', 3, '{}', 'SQL, stored procedures and data warehouse architectures; data warehouse requirements definition, design and analysis; overview of the steps in building information-enabled business models; role of data warehousing in data mining and analytics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 360, 'Business Database Concepts', 3, '{}', 'Database theory, design, and application, including the entity-relationship model; the relational, hierarchical, and network database models; and query languages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 365, 'Business Database Systems Development', 3, '{}', 'Focuses on business database and data warehousing systems, development and management. Topics include capturing business requirements using data modeling, SQL, data security, operational and analytical data stores and data integrity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 375, 'Business Data Mining', 3, '{}', 'Building predictive analytics (e.g., SEMMA, KDD); exposure to logistic regression, machine learning and decision tree methods; Understanding lift factors, ROC curves; hands-on use of mining software; business case studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 401, 'Managing Cyber Risks in Enterprise Business Processes', 3, '{}', 'Covers the activities, decisions, risks and controls associated with enterprise business processes. Specific focus on the activities in the sales-to-cash (revenue cycle) and purchase-to-pay (expenditure cycle) processes. Students understand how IT enables performing those processes more efficiently and effectively. In addition to identifying internal controls necessary for accurate, complete performance of processes, also covers the fundamentals of cybersecurity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 402, 'Privacy, Ethics and Compliance Issues', 3, '{}', 'Covers the activities, mechanisms and challenges associated with implementing the principles of privacy into corporate structures. Also provides an overview of the privacy regulatory landscapes as well as emerging challenges with privacy. Specific focus areas include the Generally Accepted Privacy Principles framework and the implementation of privacy controls via the use of process and technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 405, 'Business Intelligence', 3, '{}', 'Business intelligence (BI) is a term describing a broad category of applications, technologies, architectures and processes for gathering, storing, accessing and analyzing data to provide business users with timely information to enable better insights into project and process management and evidence-based decision making. BI is based on techniques for data decision capture, cleansing, validation, warehousing and analytics to help decision makers improve decision outcomes. Provides a capstone understanding of the concepts of managing data resources and the development of business intelligence capabilities using data visualization, data warehousing, data mining, online analytical processing decision support systems and other advanced BI topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 415, 'Big Data Analytics in Business', 3, '{}', 'Applications of machine learning, data and text mining for big data, Hadoop, cloud-based solutions, fundamentals of big data programming, social media and big data, NoSQL, GIS, business case studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 425, 'Web Technologies for the Enterprise', 3, '{}', 'Modern Web application technologies for the enterprise. Covers development of Web applications for handling business transactions and strategies including the use of cloud-based applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 430, 'Mobile Platforms for Business', 3, '{}', 'Mobile application development for the enterprise to address mobile transactions and consumer engagement. Covers business frameworks, app development; and management of devices, applications and services in today's multi-platform environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 440, 'Capstone in Information Systems', 3, '{'L'}', 'Offers a capstone experience integrating concepts from the business as well as the technology curricula. Students analyze, propose and develop a full technology solution for a business enterprise.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 450, 'Enterprise Analytics', 3, '{}', 'Model lifecycle analysis, analytics governance, aligning governance, aligning firm and analytics strategy, big data security issues, enterprise search, applied projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 502, 'Information Systems: Digital Innovations and Transformation', 4, '{}', 'Organizational, strategic, and technical issues of the management of information. Evaluation, design, and use information systems as competitive advantage.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 503, 'Decision Making with Data Analytics', 3, '{}', 'Presents frameworks and approaches to equip students to recognize patterns in data and models, recommend actions and implement organizational changes. Students learn multiple concepts to improve decision making in organizations including the curation of data repositories for cross-functional analytics, recognition of decision biases, recommendation and communication of business process changes, and the establishment of iterative processes for decision analytics. Readings address decision-making dilemmas and challenges facing managers in analytics-rich business environments. Students get an immersive experience in analyzing the individual biases inherent in decision making, the informational requirements and the data analytics governance requirements to ensure effective deployment and continuous renewal of analytics culture at the enterprise level.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 505, 'Introduction to Enterprise Analytics', 3, '{}', 'Ensuring the foundational understanding of contextualized analytics within the business enterprise continuum by covering how data flows and is managed across the landscape of enterprise business processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 506, 'Information Management', 4, '{}', 'Addresses central issues in managing information to achieve competitive advantage and support innovation. Specific topics covered include data quality, data modeling using both entity relationship models and star schemas, data governance, document management, document search, enterprise performance management and visualization techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 508, 'Data Mining I', 3, '{}', 'Charting a roadmap for data-driven decision making and getting a practical understanding of how IT tools and techniques can allow managers to extract predictive analytics and patterns from primarily numeric data.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 509, 'Data Mining II', 3, '{}', 'Uses tools and techniques to analyze unstructured data that are applied to business problems to support informed decision making and the extraction of predictive analytics and patterns from primarily nonnumeric data.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 510, 'Business Process and Workflow Analysis', 3, '{}', 'Builds foundations for process analysis by focusing on information, documents, people, roles and business rules. Provides an introduction to various techniques and tools of process analysis, including an understanding of organizational issues in rolling out change initiatives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 512, 'Business Intelligence', 4, '{}', 'Understanding how organizations make use of Business Intelligence (BI) to gain a sustainable competitive advantage. Builds the foundations for evidence-based managerial decision making. Covers technologies for data warehousing and data mining from a managerial perspective. Covers contemporary topics such as real-time BI, business analytics, and business performance management with the help of case studies, audio-video content, guest speakers, online forums to generate creative thinking and discussion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 515, 'Deep Learning Applications in Business', 3, '{}', 'Deep learning applications have become an integral part of our lives over the last decade. Alexa, Amazon Go, Waymo, Apple Face ID, and Facebook's face recognition applications are all powered by deep learning networks. Applications based on deep learning models cover a wide spectrum of industries including retail, automotive, manufacturing, health care, banking, insurance, agriculture, security and surveillance. Hands-on look at the latest models, trends and challenges of deep learning applications in business.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 525, 'Strategic Value of Information Technology', 3, '{}', 'Instills balanced and disciplined view of IT and business with their interplay. Assigns cases and assignments involving the symbiotic relationship of IT and business to provide real-world exposure for critical thinking and engaging discussion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 540, 'Enterprise Information Security and Controls', 3, '{}', 'Provides a broad survey of information security and controls, utilizing the COBIT framework to illustrate how information security and controls contribute to effective IT governance. Develops an understanding of the issues associated with information security and effective IT governance, with particular emphasis on how management contributes to the effectiveness of an organization's information security program.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 541, 'Business Data Visualization', 3, '{}', 'The design of insightful business data visualizations and dashboards to improve business decision making. Applies advanced data visualization techniques to make sense of large data sets including temporal, geospatial, topical and business data while also making it easier to digest, present and utilize for business needs and users.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 545, 'Managing Enterprise Systems', 3, '{}', 'Investigates major categories of enterprise systems, factors driving software adoption and keys for successful implementation. Pays special attention to evaluating the potential impact of emerging technologies on business environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 560, 'IT Services and Project Management', 3, '{}', 'Provides an overview of the project management process groups and knowledge areas (defined by the PMBOK) and focuses on the application of key concepts, tools and techniques for delivering on-time, on-budget IT project and services that meet end-user needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 565, 'Artificial Intelligence and Business', 3, '{}', 'Explores modern artificial intelligence (AI) technology, applications, techniques and their implications for business. People, process and technology factors related to the innovation and adoption of AI in the commercial enterprise and how it shapes the competition and society in the future. Also covers implications for information systems professionals as it pertains to managing the AI infrastructure (robots, algorithms, platforms).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 575, 'Emerging Technologies', 3, '{}', 'Explores decision models and frameworks applied to assess, evaluate and implement emerging technologies. Explains the principles of sustaining and disruptive technological innovation on the adoption and diffusion of emerging technologies. Discusses the technical, industrial and legal parameters that enable convergence and commoditization of technologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 594, 'Conference and Workshop', 12, '{}', 'Topical instruction, usually in compressed format, leading to academic credit. Often offered off campus to groups of professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 791, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CIS', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 100, 'Principles of Programming with C++', 3, '{'CS'}', 'Principles of problem solving using C++, algorithm design, structured programming, fundamental algorithms and techniques, and computer systems concepts. Social and ethical responsibility.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 110, 'Principles of Programming', 3, '{'CS'}', 'Concepts of problem solving using an object-oriented programming language, algorithm design, structured programming, fundamental algorithms and techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 120, 'Digital Design Fundamentals', 3, '{}', 'Number systems, conversion methods, binary and complement arithmetic, Boolean algebra, circuit minimization, ROMs, PLAs, flipflops, synchronous sequential circuits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 180, 'Computer Literacy', 3, '{'CS'}', 'Students gain fluency in integrating technology to efficiently and effectively solve problems using computational thinking.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 205, 'Object-Oriented Programming and Data Structures', 3, '{'CS'}', 'Problem solving by programming with an object-oriented programming language. Introduces data structures. Overview of computer science topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 220, 'Programming for Computer Engineering', 3, '{}', 'Introduction to C/C++, systems programming, and concurrency.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 230, 'Computer Organization and Assembly Language Programming', 3, '{}', 'Register-level computer organization. Instruction set architecture. Assembly language. Processor organization and design. Memory organization. IO programming. Exception/interrupt handling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 240, 'Introduction to Programming Languages', 3, '{}', 'Introduces the procedural, object-oriented, functional and declarative languages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 259, 'Logic in Computer Science', 3, '{}', 'Logic has been called the calculus of computer science. The argument is that logic plays a fundamental role in computer science, similar to that played by calculus in physical sciences and other engineering disciplines. Indeed, logic plays an important role in computer architecture (Boolean logic, digital gates, hardware verification); software engineering (specification, verification); programming languages (semantics, type theory, logic programming); databases (relational algebra, database query language); artificial intelligence (automated theorem proving, knowledge representation); algorithms and theory of computation (complexity, computability, expressiveness). This course is a mathematically solid introduction to propositional logic, first order logic, logic programming, and their applications in computer science.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 301, 'Computing Ethics', 1, '{}', 'Ethics for computing majors: history of computing, intellectual property, privacy, ethical frameworks, professional ethical responsibilities, and risks of computer-based systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 310, 'Data Structures and Algorithms', 3, '{}', 'Advanced data structures and algorithms, including stacks, queues, trees (B, B+, AVL), and graphs. Searching for graphs, hashing, external sorting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 320, 'Design and Synthesis of Digital Hardware', 3, '{}', 'Design and synthesis of digital hardware with hardware description language, computer-aided design tools, and programmable devices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 325, 'Embedded Microprocessor Systems', 3, '{}', 'System-level programming and analysis of embedded microprocessors systems. Fundamental concepts of digital system design for embedded system applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 330, 'Operating Systems', 3, '{}', 'Operating system structure and services, processor scheduling, concurrent processes, synchronization techniques, memory management, virtual memory, input/output, storage management, and file systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 335, 'Principles of Mobile Application Development', 3, '{}', 'Covers topics pertaining to fundamental concepts of mobile application development principles including design patterns; data persistence; interfacing with services and devices; location and incorporation of existing frameworks; user interface and experience; context awareness; utilization of built-in tools for software profiling, testing, and version control; and security/privacy issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 340, 'Principles of Programming Languages', 3, '{}', 'Formal syntactic and semantic descriptions, compilation and implementation issues, and theoretical foundations for several programming paradigms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 355, 'Introduction to Theoretical Computer Science', 3, '{}', 'Introduces formal language theory and automata, Turing machines, decidability/undecidability, recursive function theory, and complexity theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 360, 'Introduction to Software Engineering', 3, '{}', 'Software life cycle models; project management, team development environments and methodologies; software architectures; quality assurance and standards; legal, ethical issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 365, 'Information Assurance', 3, '{}', 'Concepts of information assurance (IA); basic IA techniques, policies, risk management, administration, legal and ethics issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 408, 'Multimedia Information Systems', 3, '{}', 'Design, use, and applications of multimedia systems. Introduces acquisition, compression, storage, retrieval, and presentation of data from different media such as images, text, voice, and alphanumeric.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 412, 'Database Management', 3, '{}', 'Introduces DBMS concepts. Data models and languages. Relational database theory. Database security/integrity and concurrency.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 414, 'Advanced Database Concepts', 3, '{}', 'Object-oriented data modeling, advanced relational features, JDBC and Web access to databases, XML and databases, object-oriented databases, and object-relational databases.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 420, 'Computer Architecture I', 3, '{}', 'Computer architecture. Performance versus cost tradeoffs. Instruction set design. Basic processor implementation and pipelining.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 423, 'Systems Capstone Project I', 3, '{'L'}', 'Development process: specification, design, implementation, evaluation and testing with economic, social and safety considerations. Technical communication and team skills enrichment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 424, 'Systems Capstone Project II', 3, '{'L'}', 'Continuation of capstone project started in CSE 423.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 434, 'Computer Networks', 3, '{}', 'Network architecture and protocols, principles of network applications, socket programming, flow and congestion control, switching and routing, link-layer technologies, traffic capture and analysis, security.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 438, 'Embedded Systems Programming', 3, '{'L'}', 'Development of embedded system software, I/O handlers, and multiple threaded programs. Scheduling algorithms. Embedded software structures. Real-time operating system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 440, 'Compiler Construction I', 3, '{}', 'Introduces programming language implementation. Implementation strategies such as compilation, interpretation, and translation. Major compilation phases such as lexical analysis, semantic analysis, optimization, and code generation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 445, 'Distributed Software Development', 3, '{}', 'Distributed system architectures and design, service-oriented computing, and frameworks for development of distributed applications and software components.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 446, 'Software Integration and Engineering', 3, '{}', 'Software development using architecture design, composition, workflow, services, data resources, data representations, data management, and development tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 450, 'Design and Analysis of Algorithms', 3, '{}', 'Design and analysis of computer algorithms using analytical and empirical methods; complexity measures, design methodologies, and survey of important algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 460, 'Software Analysis and Design', 3, '{}', 'Object-oriented and structured analysis and design; software architecture and design patterns; component-based development; software safety and reliability.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 463, 'Introduction to Human Computer Interaction', 3, '{}', 'Design, evaluate, and implement interactive software intended for human use.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 464, 'Software Quality Assurance and Testing', 3, '{}', 'Software quality assurance (SQA), software quality metrics, software configuration management, software verification and validation, reviews, inspections, understanding software testing process, functional testing, structural testing, model-based testing, integration, system, and regression testing techniques, software life cycle models and software testing, testing distributed software, bug management, and use of testing tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 466, 'Computer Systems Security', 3, '{}', 'Countermeasures to attacks to computer systems from miscreants (or hackers) and basic topics of cryptography and network security.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 467, 'Data and Information Security', 3, '{}', 'Basic concepts of security and privacy, data and database security, access controls, trust models, watermarking, and private information retrieval.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 468, 'Computer Network Security', 3, '{}', 'Practical network security exposure and hands-on experience about basic security concepts, case studies and useful tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 469, 'Computer and Network Forensics', 3, '{}', 'Identification, extraction, documentation, interpretation, and preservation of computer media for evidentiary purposes, file system forensics, and network forensics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 470, 'Computer Graphics', 3, '{}', 'Introduces basic concepts of interactive computer graphics, realistic rendering, and 3-D viewing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 471, 'Introduction to Artificial Intelligence', 3, '{}', 'State space search, heuristic search, games, knowledge representation techniques, expert systems, and automated reasoning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 472, 'Social Media Mining', 3, '{}', 'Network measures, social network analysis, network models, random graphs, small-world model, power laws, data mining, influence and homophily, information diffusion, epidemics, behavioral analysis, recommendation in social media, community detection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 474, 'Mobile Robotics', 3, '{}', 'Aims to make accessible to undergraduate students a principled and mathematically sound approach to the design of algorithms for robots. Students acquire the mathematical foundations that are required for the implementation and analysis of algorithms for robotic applications through homework problems and a final project. Among course goals is the development of the vocabulary and mathematical background so that the students can follow the current research and development trends in the robotics industry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 475, 'Foundations of Machine Learning', 3, '{}', 'Machine learning techniques: supervised learning, unsupervised learning, and neural networks and deep learning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 476, 'Introduction to Natural Language Processing', 3, '{}', 'Principles of computational linguistics, formal syntax, and semantics, as applied to the design of software with natural (human) language I/O.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 477, 'Introduction to Computer-Aided Geometric Design', 3, '{}', 'Introduces basic concepts of 3-D computer geometry, including curves, surfaces, meshes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 485, 'Computer Science Capstone Project I', 3, '{'L'}', 'First course in capstone sequence for computer science and informatics majors emphasizing development process, technical skills, teamwork, and communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 486, 'Computer Science Capstone Project II', 3, '{'L'}', 'Second course in capstone sequence for computer science and informatics majors continuing the development process, technical skills, teamwork and communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 509, 'Digital Video Processing', 3, '{}', 'Concepts of digital video compression, video analysis, video indexing, browsing and retrieval, video transmission over networks, video processors, and relevant industry standards.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 510, 'Database Management System Implementation', 3, '{}', 'Implementation of database systems. Data storage, indexing, querying, and retrieval. Query optimization and execution, concurrency control, and transaction management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 511, 'Data Processing at Scale', 3, '{}', 'Delves into new frameworks for generating, processing and managing large-scale data-sets. More specifically, covers the following topics: core database concepts, distributed and parallel data systems, deploying and operating data systems in the cloud, NoSQL database systems, and big data tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 512, 'Distributed Database Systems', 3, '{}', 'Distributed database design, query processing, and transaction processing. Distributed database architectures and interoperability. Emerging technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 515, 'Multimedia and Web Databases', 3, '{}', 'Data models for multimedia and Web data; query processing and optimization for inexact retrieval; advanced indexing, clustering, and search techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 520, 'Computer Architecture II', 3, '{}', 'Computer architecture description languages, computer arithmetic, memory-hierarchy design, parallel, vector, multiprocessors, and input/output.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 522, 'Real-Time Embedded Systems', 3, '{}', 'Development of real-time embedded systems, scheduling algorithms, embedded software structures, real-time operating system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 530, 'Embedded Operating System Internals', 3, '{}', 'Investigating Linux source including: memory management, kernel synchronization and driver design topics. Designing, coding, testing, and evaluating embedded operating system software in a Linux environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 531, 'Distributed and Multiprocessor Operating Systems', 3, '{}', 'Distributed systems architecture, remote file access, message-based systems, object-based systems, client/server paradigms, distributed algorithms, replication and consistency, and multiprocessor operating systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 534, 'Advanced Computer Networks', 3, '{}', 'Advanced network protocols and infrastructure, applications of high-performance networks to distributed systems, high-performance computing and multimedia domains, special features of networks.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 535, 'Mobile Computing', 3, '{}', 'Mobile networking, mobile information access, adaptive applications, energy-aware systems, location-aware computing, mobile security and privacy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 536, 'Advanced Operating Systems', 3, '{}', 'Protection and file systems. Communication, processes, synchronization, naming, fault tolerance, security, data replication, and coherence in distributed systems. Real-time systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 539, 'Applied Cryptography', 3, '{}', 'Uses cryptography for secure protocols over networked systems, including signatures, certificates, timestamps, electrons, digital cash, and other multiparty coordination.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 543, 'Information Assurance and Security', 3, '{}', 'Comprehensive understanding of information assurance and security problems with the solutions as well as hands-on experiences about applying these solutions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 545, 'Software Security', 3, '{}', 'Theories and tools for software security, including secure design, threat analysis and modeling, security testing and coding.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 546, 'Cloud Computing', 3, '{}', 'Virtualization, cloud computing, programmable networking, performance evaluation, information assurance, distributed and parallel computing, and cloud computing-based applications. Students must have a solid background in computer architecture, operating system and computer networking to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 548, 'Advanced Computer Network Security', 3, '{}', 'Comprehensive understanding of network security and corresponding solutions, including cryptography, access control, secure Web transactions, e-mail security, and viruses.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 550, 'Combinatorial Algorithms and Intractability', 3, '{}', 'Combinatorial algorithms, nondeterministic algorithms, classes P and NP, NP-hard and NP-complete problems, and intractability. Design techniques for fast combinatorial algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 551, 'Foundations of Algorithms', 3, '{}', 'Advanced topics in formal algorithm design and analysis, including advanced shortest-paths algorithms, amortized analysis, network flows, NP-completeness and selected topics in computational geometry, distributed/parallel, randomized, and approximation algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 552, 'Randomized and Approximation Algorithms', 3, '{}', 'Introduces two important areas of algorithm design for graduate students. A randomized algorithm is allowed to rely on the outcome of a random experiment in deciding on its next step. In may applications, randomized algorithms are simpler than any deterministic algorithms known, but in several cases, they are in fact more powerful or more efficient than any deterministic algorithms.  Covers basic paradigms for randomized algorithm design and analysis, as well as for derandomization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 555, 'Theory of Computation', 3, '{}', 'Rigorous treatment of regular languages, context-free languages, Turing machines and decidability, reducibility, and other advanced topics in computability theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 556, 'Game Theory with Applications to Networks', 3, '{}', 'Strategic conflict as matrix games, notions of equilibrium, definition and existence of Nash equilibrium, zero-sum games, extensive-form games, Bayes Nash equilibrium, potential games, routing games, algorithmic game theory, computation of Nash equilibrium, incentives and pricing in communication networks, application of game theory in wireless networks. Knowledge of calculus, discrete mathematics, probability theory and algorithms is necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 561, 'Modeling and Simulation Theory and Application', 3, '{}', 'Modeling theories, simulation protocols, object-oriented modeling, model design, simulation analysis, network-based systems, discrete-event modeling, continuous modeling, hybrid modeling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 563, 'Software Requirements and Specification', 3, '{}', 'Examines the definitional stage of software development; analysis of specification representations, formal methods, and techniques emphasizing important application issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 564, 'Software Design', 3, '{}', 'Examines software design issues and techniques. Includes a survey of design representations and a comparison of design methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 565, 'Software Verification, Validation, and Testing', 3, '{}', 'Test planning, requirements-based and code-based testing techniques, tools, reliability models, and statistical testing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 566, 'Software Project, Process, and Quality Management', 3, '{}', 'Project management, risk management, configuration management, quality management, and simulated project management experiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 569, 'Fundamentals of Statistical Learning and Pattern Recognition', 3, '{}', 'Concepts of statistical pattern recognition, Bayesian decision theory, parameter estimation, discriminant analysis, basics of artificial neural networks, basics of data clustering. Knowledge of college-level calculus, linear algebra, basic probability theory and proficiency in computer programming is necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 570, 'Advanced Computer Graphics I', 3, '{}', '3D shape analysis techniques: half-edge data structure, polygon mesh processing, fast marching method, iterative closest point algorithms, heat kernel and spectral analysis, isometric embedding.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 571, 'Artificial Intelligence', 3, '{}', 'Definitions of intelligence, computer problem solving, game playing, pattern recognition, theorem proving, and semantic information processing; evolutionary systems; heuristic programming.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 572, 'Data Mining', 3, '{}', 'Advanced data mining techniques: classification, clustering, association, preprocessing; performance evaluation; information assurance, Web mining, security and privacy issues, and other applications. Students must have a solid background in database management systems, search, learning, and statistics to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 573, 'Semantic Web Mining', 3, '{}', 'Data mining techniques for structuring and organizing unstructured sources such as text and Web data into meaningful machine-processable information; computational aspects of information extraction and data linkage; discovery and prediction tasks where text serves as data such as detecting events, measuring public opinion and making recommendations. A background in databases, algorithms and theory of computation is necessary to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 574, 'Planning and Learning Methods in AI', 3, '{}', 'Reasoning about time and action, plan synthesis and execution, improving planning performance, applications to manufacturing intelligent agents.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 575, 'Statistical Machine Learning', 3, '{}', 'Spectral clustering, regression, classification, semi-supervised learning, feature reduction, manifold learning, ranking, kernel learning and multitask learning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 576, 'Topics in Natural Language Processing', 3, '{}', 'Comparative parsing strategies, scoping and reference problems, nonfirst-order logical semantic representations, and discourse structure.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 577, 'Advanced Geometric Modeling I', 3, '{}', 'Advanced concepts of geometric modeling: rectangular and triangular surfaces, triangle meshes, Voronoi diagrams, discrete and continuous shape measures, volumes and volume visualization. Students must have a solid background in linear algebra, calculus, and basic 3D graphics to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 578, 'Data Visualization', 3, '{}', 'Amidst the information flood in which we are currently immersed, visualizations can be a well-placed treetop. The rise of big data has the potential to inform decisions, and visual representations can play a crucial intermediate role in our daily information consumption. Covers techniques and algorithms for creating effective visualizations based on principles from graphic design, visual art, perceptual psychology and cognitive science to enhance the understanding of complex data.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 579, 'Knowledge Representation and Reasoning', 3, '{}', 'Covers knowledge representation and reasoning algorithms in artificial intelligence, shows how they can be used in practice, and provides an overview of current research trends.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 594, 'Conference and Workshop', 12, '{}', 'Topical instruction, usually in compressed format, leading to academic credit. Often offered off campus to groups of professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CSE', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 101, 'Introduction to Informatics', 3, '{'CS'}', 'Concepts, tools, techniques, and applications of informatics. Includes overview of programming, data management, visualization, modeling, and social implications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 111, 'Game Development I', 3, '{'CS'}', 'Introduces video game design, art theory, and concepts as they apply to video game development. Basic art principles used in game development. Covers the fundamental video game art principles of 2-D and 3-D composition, color theory, modeling, and lighting techniques. Demonstrates practical application of these art fundamentals in establishing style guides, concept art, storyboards, and in-game assets. Also includes game design, game production, asset production, and game programming. Hands-on experience by creating 2-D game prototypes belonging to different video game genres and evaluating the techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 200, 'Mathematical Foundations of Informatics', 3, '{'MA'}', 'Practical introduction to the mathematics necessary for studies in informatics. Topics include discrete math, analytic geometry, calculus, and linear algebra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 211, 'Game Development II', 3, '{}', 'Introduces the basics of 3D game development by using a game engine. Focuses on the pipeline of 3D game development including environment and contents creation, scripting to control components, and graphics user interfaces development. Develops several 3D games for multi platforms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 220, 'Applied Data Structures and Algorithms', 3, '{}', 'Thorough grounding in applied knowledge and skills related to algorithms and data structures used in the development of software designed to solve complex problems. Overview of computational and critical thinking skills that can be called upon to analyze and solve complex problems in multiple domains.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 221, 'Advanced Object-Oriented Principles Using Java', 3, '{}', 'Advanced object-oriented programming using the Java language. Design concepts and problem solving.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 310, 'Web-Based Information Management Systems', 3, '{}', 'Relational database design, entity-relationship modeling, relational algebra, SQL, database access through Web, Web data management, introduction to XML, fundamentals of Web application development, Web server architectures, lecture, in-class lab activities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 311, 'Game Engine Development', 3, '{}', 'State-of-the-art techniques for computer game design and programming with an emphasis on 3-D graphics and interaction. Focuses on a practical, example-driven approach to learning the unique art of 3-D game development. Topics include graphics game engines, textures, shader programming, artificial intelligence, physics, modeling, sound effects, and techniques for user interfaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 321, 'Fundamentals of Game Art', 3, '{}', 'Introduces art theory and concepts as they apply to video game development. Covers 2-D and 3-D composition, color theory, modeling, and lighting techniques. Demonstrates practical application of these art fundamentals in establishing style guides, concept art, storyboards, and in-game assets.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 350, 'Evaluation of Informatics Systems', 3, '{}', 'Methods for evaluation of informatics systems, including design of computational and human experiments, ethnography, and analytic techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 360, 'Decision Making and Problem Solving', 3, '{}', 'Practical use of database systems, computer graphics, and modeling to inform decision making.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 411, 'Graphics for Games', 3, '{}', 'Examines real-time rendering of high-quality interactive graphics. Studies advances in graphics hardware and algorithms that make this possible. Examples of techniques and topics include nonphotorealistic rendering, occlusion culling, level of detail, terrain rendering, shadow generation, image-based rendering, and physical simulation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 421, '3-D Modeling and Texturing', 3, '{}', 'Provides working knowledge of 3-D modeling, texture mapping, and paint concepts for the creation of 3-D video games art assets. Students work on concepts such as 3-D lighting, high polygon mesh rendering, and various third-party software and how to integrate them into the asset production pipeline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 441, 'Gaming Capstone', 3, '{}', 'Industry-oriented course emphasizing the video game development process, technical skills, teamwork and communication among the students enrolled in the gaming certificate. Students partner with local industry involved in game development, visual art, and educational gaming and solve real-world problems by working in a team comprised of students from the three tracks available within the certificate.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 460, 'Intelligent Interactive Instructional Systems', 3, '{}', 'Intelligent interactive instructional systems serve as tutors, as learning companions or both. Covers their design, the technology that powers them, the learning theories that motivate them and results from experimental evaluations. Emphasizes intelligent tutoring systems, as they are a more mature technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 462, 'Design for Learning in Virtual Worlds', 3, '{}', 'Focuses on issues of design for learning in virtual worlds by exploring: the history and evolution of virtual worlds, the theories behind the use of virtual worlds for learning, the design of curricula in virtual worlds, design guidelines for elements experienced in virtual worlds to support learning, design guidelines for learning quests and activities in virtual worlds. Also introduces virtual world building using Unity 3D.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 485, 'Informatics Capstone I', 3, '{'L'}', 'Team-based design of an informatics system; working with clients; development of requirements, use cases, class/object diagrams, and plans for quality assurance and other evaluations; technical communication; teamwork.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 486, 'Informatics Capstone II', 4, '{'L'}', 'Implementation of the informatics system designed during CPI 485; work processes; keeping designs consistent with implementations; conducting QA and other evaluations; technical communication; teamwork.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPI', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 100, 'Introduction to Construction', 2, '{}', 'Introduces construction industry sectors and professionals. Additional content includes developing personal competencies required of construction professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 101, 'Construction and Culture: a Built Environment', 3, '{'HU','H'}', 'Analyzes the cultural context of construction, emphasizing its centrality in the evolution and expansion of built environments as expressions of ethical and historical value systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 106, 'Introduction to Concrete Materials for the Built Environment', 4, '{'SQ'}', 'Effects of concrete-making materials (aggregates, cements, admixtures, etc.) on the properties of fresh and hardened concrete. Discusses concrete mixture proportioning along with the calculations and statistical analysis of strength testing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 221, 'Applied Statics', 2, '{}', 'Vectors, forces and moments, force systems, equilibrium, analysis of basic structures and structural components, friction, centroids, and moments of inertia.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 223, 'Strength of Materials', 2, '{}', 'Analyzes strength and rigidity of structural members in resisting applied forces. Stress, strain, shear, moment, deflections, combined stresses, and connections.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 241, 'Surveying', 3, '{}', 'Theory and fieldwork in construction and land surveys.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 243, 'Heavy Construction Equipment, Methods and Materials', 3, '{}', 'Emphasizes Horizontal construction. Fleet operations, maintenance programs, methods, and procedures to construct tunnels, roads, dams, and the excavation of buildings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 244, 'Working Drawing Analysis', 2, '{}', 'Provides the ability to read and understand architectural plans; to understand drawing symbols, dimensions, abbreviations, and terms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 251, 'Microcomputer Applications for Construction', 3, '{}', 'Applies the microcomputer as a problem-solving tool for the constructor. Uses spreadsheets, information management, and multimedia software.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 252, 'Building Construction Methods, Materials, and Equipment', 3, '{}', 'Emphasizes Vertical construction. Methods, materials, codes, and equipment used in building construction corresponding to the division of the CSI Master Format.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 271, 'Construction Safety', 3, '{}', 'Safety procedures and equipment. OSHA requirements for construction. Biological aspects of construction safety.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 273, 'Electrical Construction Fundamentals and Project Management', 3, '{}', 'Electrical construction, design, estimating, scheduling and project management. Electrical safety, blueprints and symbols, theory, sequence of installation. National Electrical Code (NEC).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 296, 'Summer Field Internship', 1, '{}', 'Participation as interns on construction projects to observe and experience the daily activities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 310, 'Testing of Materials for Construction', 4, '{}', 'Structural and behavioral characteristics, engineering properties, measurements, and application of construction materials. Not open to engineering students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 343, 'Advanced Heavy Equipment Operations', 3, '{}', 'Reviews the principles of horizontal construction. Emphasizes fleet operations, maintenance programs, methods and procedures to construct tunnels, roads, dams and the excavation of buildings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 345, 'Mechanical Systems', 3, '{}', 'Design parameters and equipment related to heating and cooling systems for mechanical construction. Biorelated aspects of mechanical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 383, 'Construction Estimating', 4, '{}', 'Analyzes construction drawings and specifications. Utilizes advanced technical and ethical practices in estimating process. Quantity surveying techniques for CSI divisions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 384, 'Environmental Aspects of Heavy Construction', 3, '{}', 'Methods, measures and analysis related to control of erosion, dust generation and storm water discharge during projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 424, 'Structural Design', 3, '{}', 'Economic use of concrete, steel, and wood in building and engineered structures. Design of beams, columns, concrete formwork, and connections.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 441, 'Public Works Capital Construction', 3, '{}', 'Practices and policies employed in delivering capital construction projects by government agencies at various levels. The full lifecycle of the typical public works capital construction project from planning, through design and construction, to operations and maintenance. Additional lecture topics include sustainability, strategic planning, risk management and safety.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 446, 'Marketing for Construction', 3, '{}', 'Provides key marketing practices to include strategic planning, marketing collateral, networking, business development, and interview preparation for project pursuits. Covers all aspects of service-based marketing topics and practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 448, 'Sustainable Construction', 3, '{}', 'Studies the principles associated with sustainable construction: problem definition, definition of sustainability, measures of sustainability, examination of current industry practices, financial and resource impacts of sustainable construction, independent student investigation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 450, 'Geotechnical Applications for Construction', 3, '{}', 'Soil formation, engineering properties and use as building materials. Soil's influence on construction of built environment, including specifications and biological aspects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 451, 'Innovative Project Delivery', 3, '{}', 'Studies design and construction interaction; the RFP (request for proposal) and RFQ (request for quote) process; risk allocation issues; developing successful project proposals, procurement and selection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 453, 'Construction Technology', 3, '{}', 'Building information modeling, including current computer programs and technology planning strategies for advanced project deliveries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 454, 'Trenchless Construction Methods', 3, '{}', 'Theory and practice of trenchless construction methods. Types of equipment, roles of site personnel, engineering design, contractual issues and safety. Lecture, class projects, and site visits. Knowledge of heavy construction equipment, methods and materials required to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 455, 'Construction Project Management II', 3, '{}', 'Advanced topics in administrative procedures, planning processes, and coordination required to successfully complete construction projects on time and budget.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 457, 'Principles of Leadership for Project Managers', 3, '{}', 'Individual, organizational and process/structure leadership in project setting. Four-step leadership transformational model for project leaders. Project leadership tools and applications to motivation, conflict management, team building and delegation of risk.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 465, 'Deductive Logic, Leadership and Industry Structure', 3, '{}', 'Simple, logical explanations of construction and other industries. State-of-the-art supply chain, profit maximizing and efficient practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 467, 'Advanced Procurement Systems', 3, '{}', 'Development of multicriteria decision procurement model for selecting the performing contractor.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 483, 'Advanced Building Estimating', 3, '{}', 'Concepts of pricing and markup, development of historic costs, life cycle costing, change order and conceptual estimating.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 486, 'Heavy Construction Estimating', 3, '{}', 'Methods analysis and cost estimation for construction of highways, bridges, tunnels, dams, and other engineering works.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 487, 'Project Controls', 3, '{}', 'Project staff roles and responsibilities, project documentation process and control, project control processes to manage quality, schedule and budget, quantity tracking and earned value management, and financial management of a construction project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 488, 'Heavy Construction Earthworks', 3, '{}', 'Geotechnical issues with respect to foundation grading, forming and location for various types of infrastructure and transportation projects. Also includes streets, highway and embankment developments, retaining wall system, mechanically stabilized earth walls, and required quality control methods for these activities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 491, 'Construction Project Capstone', 3, '{}', 'Develops understanding and knowledge of topics and concepts required to be a construction manager. A construction project is the basis for applying cost estimating, scheduling, safety, communications and other construction management skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 495, 'Construction Planning and Scheduling', 3, '{'CS'}', 'Various network methods of project scheduling, such as AOA, AON Pert, bar-charting, line-of-balance, and VPM techniques. Microcomputers used for scheduling, resource allocation, and time/cost analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 496, 'Construction Contract Administration', 3, '{'L'}', 'Surveys administrative procedures of general and subcontractors. Studies documentation, claims, arbitration, litigation, bonding, insurance, and indemnification. Discusses ethical practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 501, 'Construction Research Methods', 3, '{}', 'Data collection methodologies, applied parametric and nonparametric statistics, data analysis techniques, technical writing, and data presentation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 502, 'Front-End Planning', 3, '{}', 'Principles and applications for effective early planning of capital facilities, including: finance, economics decision making, risk management, team alignment and front-end planning processes and tools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 507, 'Safety Management for Construction Managers', 3, '{}', 'Executives and managers make better decisions while managing the safety needs for a company in the construction industry using different safety management approaches.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 508, 'Engineering and Construction Failures', 3, '{}', 'Engineering, construction and ethics failures. Focuses on failures in the built environment. Students tailor the assignments and projects to their specific engineering or construction discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 509, 'Advanced Concrete Materials', 3, '{}', 'Properties of Portland cement concrete materials at an advanced level by examining the influence of constituent materials (cements, aggregates and admixtures) on the properties and behavior of fresh and hardened concrete under various types of loading and environmental conditions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 510, 'Sustainable Bio-Based Construction', 3, '{}', 'State of the art on sustainable bio-based construction for buildings and infrastructure. Helps students gain knowledge on the following topics related to construction: various types of emerging bio-materials, novel technologies and processes in bio-material development, emerging technologies for characterization and diagnosis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 511, 'Hazard Mitigation Planning', 3, '{}', 'Examines policies, methodologies and guidance for state and local hazard mitigation planning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 530, 'Facilities Operations and Maintenance', 3, '{}', 'Operations, maintenance and energy management requirements of various types of facilities and built environments. Ethical practices');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 531, 'Facility Management: Building Energy Management', 3, '{}', 'Understand and learn energy management techniques in facilities management. Identifying and performing efficiency evaluations on various types of heating, ventilation and air conditioning systems in typical buildings. Concepts and methods of managing, adjusting and balancing equipment and systems for maximum facility performance. Facilities organizations best practices to maximize energy savings and performance, such as insulation usage, lighting optimization, energy management systems and commissioning procedures. Identification of ancillary benefits to energy-efficient building systems such as indoor air quality, sustainability and energy audits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 532, 'Facilities Project Management', 3, '{}', 'Understand and learn project management techniques in facilities management. Stakeholder analysis, including roles, responsibilities and risk considerations. Asset owner and manager considerations within project components and deliverables. Development of project scope, requirements, schedules, budgets and communication plans from a facility manager's perspective. Best practices of facilities organizations for managing contract types, contract governance and performance management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 534, 'Retrofit Construction', 3, '{}', 'Focuses on energy efficiency retrofits in existing commercial buildings. Defines retrofit and why we retrofit. Examines current industry practices, impacts of retrofit construction on finances and the environment, and independent student investigation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 540, 'Construction Productivity', 3, '{}', 'Productivity concepts. Data collection. Analysis of productivity data and factors affecting productivity. Means for improving production and study of productivity improvement programs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 541, 'Public Works Capital Construction', 3, '{}', 'Practices and policies employed in delivering capital construction projects by government agencies at various levels. The full lifecycle of the typical public works capital construction project from planning, through design and construction, to operations and maintenance. Additional lecture topics include sustainability, strategic planning, risk management and safety.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 545, 'Construction Project Management', 3, '{}', 'Theory and practice of construction project management. Roles of designer, owner, general contractor, and construction manager.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 548, 'Sustainable Construction', 3, '{}', 'Studies the principles associated with sustainable construction: problem definition, definition of sustainability, measures of sustainability, examination of current industry practices, financial and resource impacts of sustainable construction, independent student investigation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 551, 'Alternative Project Delivery Methods', 3, '{}', 'Design/construction interaction; conceptual estimation and scheduling; the RFQ/RFP process; legal, insurance, risk allocation issues; procurement and selection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 554, 'Trenchless Construction Methods', 3, '{}', 'Theory and practice of trenchless construction methods. Types of equipment, roles of site personnel, engineering design, contractual issues, and safety. Lecture, class projects, and site visits. Knowledge of heavy construction equipment, methods, and materials required to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 557, 'Principles of Leadership for Project Managers', 3, '{}', 'Individual, organizational and process/structure leadership in project setting. Four-step leadership transformational model for project leaders. Project leadership tools and applications to motivation, conflict management, team building and delegation of risk.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 565, 'Deductive Logic, Leadership and Industry Structure', 3, '{}', 'Simple, logical explanations of construction and other industries. State-of-the-art supply chain, profit maximizing, and efficient practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 567, 'Advanced Procurement Systems', 3, '{}', 'Development of multicriteria decision procurement model for selecting the performing contractor.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 570, 'Introduction to Advanced Technology Facilities', 3, '{}', 'Introduces advanced technology facilities such as cleanrooms, data centers, research labs, and hospitals. The constructor's viewpoint, including: planning, structures, mechanical, and tool installation. Site visits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 571, 'Construction of Advanced Technology Facilities', 3, '{}', 'Construction issues for advanced technology facilities such as cleanrooms, data centers, research labs, and hospitals. Issues include scheduling, cost estimating, project management, mechanical, safety, commissioning and tool hookup. Site visits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 575, 'Information Technology in Construction', 3, '{}', 'Uses virtual construction methodologies to improve the construction enterprise using communications, facility modeling, and decision making. Site visits.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 589, 'Construction Company Financial Control', 3, '{}', 'Financial accounting and cost control at the company level in construction companies. Accounting systems. Construction project profit calculations. Financial analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 680, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 700, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CON', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
