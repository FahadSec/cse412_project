INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 314, 'Integrative Perspectives on Cultural Dynamics', 3, '{'SB'}', 'Applies integrative and interdisciplinary tools, methods, knowledge and skills to examine the shifting dynamics of cultural diversity in the United States.');
