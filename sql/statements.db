INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GRA', 521, 'Advanced Visual Communication Design Studio I', 5, '{}', 'Students translate data, stories and references that they collect in their research into masterful visual narratives. This studio is an inquisitive environment for exploring ways to frame information and weave stories with graphic design. Students acquire new knowledge as individuals and as a group. Emphasizes studio-based research, conceptual development and articulation of process, voice and a high level of design execution.');
