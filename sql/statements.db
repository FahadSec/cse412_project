INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASU', 10, 'Connect: ASU Orientation', 0, '{}', 'Students enrolled in the Starbucks College Achievement Plan will learn about ASU's mission as the New American University, discover the value of a university degree from a research institution, discuss academic integrity, explore the tools used in online courses, and identify key components of a work/life/school balance plan.');
