INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 100, 'Introduction to Criminal Justice', 3, '{'SB'}', 'History and current practice of the criminal justice system. Roles of law enforcement agencies, courts and correctional agencies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 201, 'Criminal Justice Crime Control Policies and Practices', 3, '{}', 'Dimensions and causes of crime. Criminal justice system responses.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 203, 'Courts and Sentencing', 3, '{}', 'Structure and operation of criminal court system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 204, 'Juvenile Justice', 3, '{}', 'History and development of the juvenile court and the juvenile justice system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 210, 'Introduction to Criminal Investigations', 3, '{}', 'Studies the basic principles of criminal investigation including the analysis of investigative techniques, criminal patterns and modus operandi, interviewing and interrogation strategies, collection and management of evidence, surveillance, and crime scene investigation. Covers theories, philosophies and concepts related to suppression of crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 211, 'Documenting the Crime Scene', 3, '{}', 'Practical approach to evidence identification; documentation of the location of evidence; crime scene sketching; and the collection and handling of evidence from the crime scene, to the crime laboratory, and to presentation in court. Includes lab exercises designed to reinforce important investigative skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 230, 'Introduction to Policing', 3, '{}', 'History of police. Contemporary police work. Problems in policing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 240, 'Introduction to Corrections', 3, '{}', 'Structure and operation of corrections system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 260, 'Criminal Law I: Introduction to Substantive Criminal Law', 3, '{}', 'In-depth study of the historical development, foundational principles and public policy implications of criminal law, focusing on theories of criminal liability and the elements of select crimes against the person.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 270, 'Community Justice', 3, '{'SB'}', 'Collaborative and evidence-based approach to reducing crime, incorporating offenders, victims, the community and criminal justice agencies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 284, 'Professional Growth in Criminal Justice', 3, '{}', 'Targeted field practice with criminal justice agencies. Prerequisite for internship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 300, 'Oral and Written Communication for Criminology and Criminal Justice', 3, '{'L'}', 'Develops critical oral and written communication skills with emphasis on locating, analyzing, properly citing, and persuasively using primary and secondary sources in criminology and criminal justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 302, 'Research Methods', 3, '{'L'}', 'Validity and reliability of research designs and data collection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 303, 'Statistical Analysis', 3, '{'CS'}', 'Fundamentals and application of descriptive and inferential statistics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 304, 'Inequality, Crime and Criminal Justice', 3, '{}', 'Explores variability in the perpetration, apprehension, prosecution and punishment of crime, as well as criminal victimization by race, ethnicity, class and socio-economic status, religion, gender and gender identity, sexual orientation, and disability status.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 305, 'Gender, Crime, and Criminal Justice', 3, '{'C'}', 'Women as offenders, victims and professionals in the criminal justice system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 306, 'Race, Ethnicity, Crime, and Criminal Justice', 3, '{'C'}', 'Minority issues in the criminal justice system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 309, 'Criminology', 3, '{'SB'}', 'Theoretical explanations for criminal behavior.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 315, 'Police Organization and Management', 3, '{}', 'Structure, processes and behavior of police organizations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 317, 'Inside-Out Prison Exchange Program', 3, '{}', 'Pairs university students and correctional students inside a prison to examine the criminal justice system through a combination of theoretical knowledge and practical experience, paying special attention to race, gender, socioeconomic status, age, and ethnicity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 321, 'Evidence: Imperatives of Proof', 3, '{}', 'Problems and means of establishing identity and fact in relation to arrest, detention, adjudication, sentencing, and correctional case management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 350, 'Law and Social Control', 3, '{'SB'}', 'Resolution of social issues through the application of law as an agent of social control. Nature, sanctions, and limits of law. Categories of law and schools of jurisprudence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 403, 'Victims and the Criminal Justice System', 3, '{}', 'Victimization theories. Nature and extent of victimization. Interactions among victims, the criminal justice system and society. Victim rights and services.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 404, 'Juvenile Delinquency', 3, '{}', 'Causes, correlations and strategies to reduce juvenile delinquency.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 405, 'Neighborhoods and Crime', 3, '{}', 'Variation in crime across neighborhoods from ecology of crime and urban sociology perspectives. Influence of social institutions on community crime patterns.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 406, 'Sex Crimes', 3, '{}', 'Prevalence, nature and etiology of sex crimes. Response of criminal justice system to victims and perpetrators of sex crimes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 407, 'Police Use of Force', 3, '{}', 'Explores the use of force options available to police officers. After analyzing the legal foundations of use of force, examines the community context. Also studies future technology and examines current research on the topic. Uses case studies to demonstrate the historical and current issues in police use of force.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 408, 'Drugs and Crime', 3, '{}', 'Patterns of illegal drug use. Problems, practices and policies associated with drugs, crime and drug policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 409, 'Police Accountability', 3, '{}', 'Command and control systems. Managing police culture. Community policing and accountability to the community. Police training.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 410, 'Criminal Procedure I: The Law of Investigations', 3, '{}', 'In-depth exploration of U.S. Constitutional law governing criminal investigative procedures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 411, 'Legal Issues in Corrections', 3, '{}', 'Explores the constitutional and statutory law concerning prisoners in the United States. Places special emphasis on the First, Fourth, Fifth, Sixth, Eighth and Fourteenth Amendment rights of persons under the jurisdiction of correctional agencies, as well as their rights under relevant statutes and case law, including those affecting habeas corpus right.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 412, 'International Terrorism', 3, '{}', 'Structural, organizational, political and legal characteristics of international terrorism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 414, 'Advanced Criminological Theory', 3, '{}', 'Critical assessment of criminological thought using original readings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 416, 'Criminal Procedure II: Adjudication', 3, '{}', 'In-depth study of the constitutional rights attendant to the processing and adjudication of criminal cases in court, from initial court appearances through appeals and collateral attacks.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 417, 'Cyber Terrorism', 3, '{}', 'Terrorism perpetuated online. Terrorism and technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 419, 'Domestic Terrorism', 3, '{}', 'Evolution of domestic terrorism in the United States. History and future of domestic security. Public policy responses. Internal and external political implications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 422, 'Violence in America', 3, '{}', 'Theories, patterns, prevention and policy responses to violence in the United States. Varieties of violence including domestic, gang-related, hate crimes, school violence and sex crimes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 423, 'Sacred Crimes: Religion and Violence', 3, '{'G','H'}', 'Religion is capable of doing great good in the lives of humans, leading them to personal betterment and positive social contributions. Yet, throughout human history religion has also led some individuals to engage in acts of violence and other dangerous behaviors. This course explores the history of several religious traditions and texts and their complicated connections to violence and criminality.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 425, 'Life-Course Criminology', 3, '{}', 'Theories, methods and controversies of the life-course perspective as it applies to crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 427, 'Crime and Media', 3, '{}', 'Examines the relationships among crime and discourse, media representations of crime and criminals, and the structural deficiencies in society that perpetuate relations of domination and oppression based on class, race, gender and sexuality.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 433, 'Death Investigations: Principles of Forensic Medicine', 3, '{}', 'Medico-legal investigation of death via postmortem examinations. Identification, thanatology, thanato-etiology of unexpected natural deaths, accidental deaths, suicides and homicides. Forensic examination of living people following sexual assault, traffic offenses, child abuse and drug crimes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 434, 'Drugs of Abuse', 3, '{}', 'Pharmacological and toxicological characteristics of commonly abused drugs such as alcohol, barbiturates, amphetamines, narcotics, stimulants and hallucinogens. Forensic toxicology. Acute and long-term effects of drug abuse.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 435, 'Crime and Forensic Mental Health', 3, '{'SB'}', 'Clinical assessment of people under court jurisdiction. Topics include psychological paradigms, nomenclature of mental disorders, forensic clinical assessment, mental disorders and crime, legal standards governing mentally disordered criminal offenders.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 443, 'Community Corrections', 3, '{}', 'Probation and parole. Types of community corrections programs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 444, 'Institutional Corrections', 3, '{}', 'Provides an overview of secure correctional facilities in the U.S. with a focus on prisons and jails.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 445, 'Tools for Engaging Correctional Populations', 3, '{}', 'Provides an overview to risk and needs assessment with correctional populations, with a particular focus on introducing tools to engage with and rehabilitate individuals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 450, 'Crime Analysis', 3, '{}', 'Tactical, strategic and administrative aspects of crime analysis and crime mapping as practiced in everyday policing. Environmental criminology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 455, 'Police and International Terrorism', 3, '{}', 'Suicide bomber interdiction, jihadism, and anti-terrorism intelligence gathering. History of prior attacks and policy responses.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 456, 'Etiology of Martyrdom', 3, '{}', 'Development of a jihadist mindset. Social support for beliefs that lionize martyrdom.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 460, 'Criminal Law II: Advanced Substantive Criminal Law', 3, '{}', 'In-depth study of the historical development, foundational principles, and public policy implications of criminal law focusing on parties to crime, inchoate liability, select crimes against the person; crimes against property, public order and the state; and defenses to crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 461, 'Intimate-Partner Violence', 3, '{}', 'Examines the prevalence, causes and consequences surrounding physical, psychological, sexual and financial abuse of family and intimate partners. Pays special attention to underserved populations (e.g., people of color, Indigenous persons, and LGBTQ+ people) who are particularly impacted by systemic racism and hetero-normative worldviews that play important roles in intimate-partner violence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 462, 'Gangs', 3, '{}', 'History and development of gangs. Criminal justice system responses. Nature of gangs and gang members.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 463, 'White Collar Crime', 3, '{}', 'Business, professional and official lawbreaking including consumer fraud, securities violations, unethical behavior and political corruption.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 464, 'Organized Crime', 3, '{}', 'Nature and history of organized crime. Theories of containment. Criminal justice agency responses.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 470, 'Discretionary Justice', 3, '{'L','SB'}', 'Use and abuse of discretion in the criminal justice system. Theoretical and empirical links between discretion and discrimination based on race, ethnicity and gender.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 474, 'Correctional Psychology', 3, '{}', 'Covers the basic principles of correctional psychology, including psychologists' roles in prisons and jails, the unique challenges of working in these settings, establishing successful relationships with correctional staff and inmates, and the various career opportunities in correctional psychology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 496, 'Directed Research', 3, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to contribute to a specific project such as a report or publication. Tasks may include data collection and coding, data analysis, literature reviews and producing research reports. Neither a substitute for a catalog course nor a means of taking a catalog course on an individual basis. Requires application well in advance of regular registration with the student's advisor, the advisor's signature, and approval by both the faculty member with whom the student will work and the director of the school offering the course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 501, 'Seminar in Criminal Justice', 3, '{}', 'Overview of the American criminal justice system, with emphasis on policy issues in police, courts, sentencing, corrections.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 502, 'Seminar in Criminology', 3, '{}', 'Theory and research on the nature, causes, and prediction of criminal careers and events.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 503, 'Research Methods', 3, '{}', 'Introduces data analysis; overview of research methods used in social science research, including experimental design, survey research, field research, and evaluation research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 504, 'Statistical Tools for Criminology and Criminal Justice', 3, '{}', 'Focuses on essential statistical analysis that can be used in criminal justice and related agencies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 505, 'Theory and Practice in Criminal Justice', 3, '{}', 'Capstone course for School of Criminology and Criminal Justice master's students. Students complete a project that requires the application of knowledge from program coursework.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 506, 'Seminar in Crime Analysis', 3, '{}', 'Examines theory and practice of crime analysis in the field of criminal justice. Focuses on data collection, management and dissemination methods; related technology and software; and analytical techniques to examine crime trends, the spatial distribution of crime, and temporal trends in crime for the purpose of understanding and responding to crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 507, 'Social Network Analysis', 3, '{}', 'Examines the theory and practice of social network analysis in the field of criminal justice. Focuses on the major theoretical assumptions of social network analysis, how to collect social network data, how to use social network analysis software, and how to analyze and interpret social network data for the purpose of understanding and responding to crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 508, 'Advanced Seminar in Crime Analysis', 3, '{}', 'Examines advanced topics in the practice of crime analysis in the field of criminal justice. Students learn the terminology, principles and techniques crime analysts use in everyday practice. Provides an exploration of the various approaches to analyzing and reducing crime as well as the theories that inform those approaches. Examines situational analysis and its approach to managing the occurrence of crime; also explores crime prevention through community-based approaches to crime prevention.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 509, 'Data Management', 3, '{}', 'Core concepts and practices used for the organization and management of data.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 510, 'Criminal Justice Planning and Program Evaluation', 3, '{}', 'Examines the application of alternative models of strategic planning to the criminal justice system. Covers methods of evaluating criminal justice policies and practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 511, 'Applied Data Analysis in Criminal Justice', 3, '{}', 'Examines a variety of tools used in the analysis of criminal justice data, including GIS mapping.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 512, 'Seminar in Policing', 3, '{}', 'Graduate-level review of policing and police organizations. Seminar examines research on police strategies and practices designed to address crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 513, 'Seminar in Courts and Sentencing', 3, '{}', 'Overview of the nature, proposed principles, and theoretical doctrine of the courts and sentencing policies in criminal justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 514, 'Seminar in Corrections', 3, '{}', 'Theory, research, and policy issues regarding community-based and institutional correction programs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 515, 'Seminar in Women and Crime', 3, '{}', 'Introduces students to issues regarding women and the criminal justice system, focusing on their roles as offenders, victims and professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 516, 'Seminar in Criminal Justice Organization and Management', 3, '{}', 'How external and internal variables shape criminal justice systems' organizational behaviors. Emphasizes strategic hiring, professional socialization, effective leadership, progressive discipline, interagency collaboration and organizational change.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 517, 'Seminar on Juvenile Delinquency and Juvenile Justice', 3, '{}', 'Examines patterns and correlates of delinquency within the context of theories of delinquency. Reviews the response of the juvenile justice system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 518, 'Seminar on Race/Ethnicity, Crime, and Justice', 3, '{}', 'Theoretical perspectives and research on the overrepresentation of racial minorities as victims, offenders, and defendants in the criminal justice system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 519, 'Seminar on Victimization', 3, '{}', 'Examines theory and research concerning victimization and consequences for victims, society, and the justice system. Also examines effective responses to victims' post-crime needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 520, 'Seminar on Violent Crime', 3, '{}', 'Examines patterns and correlates of violent crime, as well as prevention strategies and policy implications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 521, 'Topics in Psychological Criminology', 3, '{}', 'Topical courses relating to the intersection of various specializations in psychology with criminology and criminal justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 522, 'Seminar on Gangs and Crime', 3, '{}', 'Theoretical perspectives and research on gangs and crime, and on the role of the community and the criminal justice system in causes and control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 523, 'Seminar in Network Criminology', 3, '{}', 'Explores the theoretical foundations for applying the concepts and methods of network science to problems in criminology and criminal justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 524, 'Theories of Punishment', 3, '{}', 'Examines the fundamental philosophical principles of justice and punishment. Includes a comparative and historical perspective.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 525, 'Seminar on Life-Course Criminology', 3, '{}', 'Provides an intensive examination of life-course explanations of crime, the life-course perspective and a theoretical orientation, and research methods used in life-course research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 527, 'Police Accountability', 3, '{}', 'Examines the great authority of police, the various and most common ways that police abuse that authority, the consequences of those abuses, and accountability mechanisms that can prevent those abuses from occurring.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 529, 'Community Corrections', 3, '{}', 'Provides a review of theory, research and policy focusing primarily on correctional strategies and programs that take place within the community setting, as well as the function of community corrections within the larger correctional system. Gives particular attention to the purpose and goals of community-based corrections; the historical development of probation and parole practices; and contemporary community corrections programs and strategies such as evidence-based practices, risk assessment, offender reentry, sex offender management, diversion, restitution, community service programs and other emerging alternatives to traditional incarceration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 530, 'Seminar in Forensics', 3, '{}', 'Explores recent and sometimes controversial developments in forensic science. Includes topics such as quality assurance, the CSI effect and the National Academy of Sciences' report entitled 'Strengthening Forensic Science in the United States: A Path Forward.'');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 531, 'Crime Mapping', 3, '{}', 'Examines the applied research methodology and conceptual framework used to conduct spatial analysis within the crime analysis profession.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 532, 'Sex Crimes', 3, '{}', 'Not only discusses the distinctions among types of offenders, but also applies theory, analyzes public policy, examines the effect of sex offenses on victims, and discusses how we've legislated sexual behavior over human history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 533, 'Seminar in White Collar Crime', 3, '{}', 'Provides a general overview of white collar crimes including forms, contexts and response by the criminal justice system toward offenders and victims. Also addresses theoretical perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 535, 'Seminar in Crime and Forensic Mental Health', 3, '{}', 'This interdisciplinary course, which draws from the fields of criminology, psychology, sociology and law, focuses on the clinical assessment of people under the jurisdiction of a court of law.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 537, 'Criminal Procedure of Investigations for Social and Forensic Scientists', 3, '{}', 'Explores the criminal procedures mandated by the U.S. Constitution as applied to criminal investigations with emphases on pretrial rights under the Fourth, Fifth and Sixth Amendments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 538, 'Criminal Law and Social Control', 3, '{}', 'Examines practical, legal and constitutional limitations of criminal laws and procedures as means of securing social order. Focuses on the formal social control of justice system actors and the limits placed on government when seeking to control crime.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 539, 'Topics in Law, Crime and Justice', 3, '{}', 'Topical courses relating to the intersection of law with criminology, criminal justice, public policy and the social sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 540, 'Qualitative Methods', 3, '{}', 'Provides students with an examination of the design, execution, and write-up of qualitative research. Discusses methods of data collection, including participant-observation, ethnography, and interviewing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 553, 'Terrorism and Weapons of Mass Destruction', 3, '{}', 'Historical evolution of terrorism and weapons of mass destruction. Analyzes current theories and mitigation, preparedness, and response tactics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 554, 'Homeland Security', 3, '{}', 'Presents skills necessary to develop policies, strategies, programs, and organizational structure of an all hazards/all risk homeland security program.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 555, 'Police and International Terrorism', 3, '{}', 'Suicide bomber interdiction, jihadism and anti-terrorism intelligence gathering. History of prior attacks and policy responses.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 556, 'Etiology of Martyrdom', 3, '{}', 'Development of a jihadist mindset. Social support for beliefs that lionize martyrdom.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 557, 'Seminar in Domestic Terrorism', 3, '{}', 'Develops a foundation of theoretical approaches to understanding domestic terrorism in the United States of America.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 558, 'Seminar in International Terrorism', 3, '{}', 'Reviews the various definitions of terrorism and how to apply these to terrorism issues and instances globally.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 559, 'Seminar in Cyber Terrorism', 3, '{}', 'Explores the rapidly changing face of cyber warfare and cyber terrorism. Identifies and characterizes the fundamental aspects of cyber terrorism and the role of computers and the Internet in terrorist acts on information systems and critical infrastructure components. Analyzes cyber warfare techniques such as Denial of Service (DoS) attacks on critical infrastructure, man-in-the-middle attacks, sabotage and espionage. Evaluates the various cybercrimes that finance terrorism and cyber-criminal activities that associate with terrorism. Offers intensive study in specific areas of homeland security and cybersecurity with regard to nation-state cyberwarfare and terrorism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 560, 'Topics in Research Methods', 3, '{}', 'In-depth exploration of either a specific social scientific research method or a narrow range of related methods relevant to criminological and criminal justice research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 583, 'Fieldwork', 12, '{}', 'Structured, supervised field experience in a field science or other discipline requiring experience in field techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 601, 'Seminar on Criminological Theory', 3, '{}', 'Examines historical and contemporary criminological theories.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 602, 'Seminar on Criminal Justice Policies and Practices', 3, '{}', 'Analyzes responses to crime and the operation of criminal justice system, with emphasis on theory and research on effectiveness of policies and practices of the system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 604, 'Regression Models', 3, '{}', 'Use and application of advanced statistical techniques and multivariate analysis of data.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 605, 'Topics in Quantitative Methods', 3, '{}', 'Explores advanced techniques of statistical analysis within the field of criminal justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 606, 'Advanced Topics in Theoretical Criminology', 3, '{}', 'Focuses on criminological theory construction, historical and contemporary theoretical debates within criminology, theory testing, empirical support for theories, and theoretical integration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 614, 'Professional Development Workshop I: Professionalization', 0, '{}', 'A structured series of four workshop sessions to help doctoral students adjust to graduate school and begin to chart a path toward success as a scholar and colleague.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 615, 'Professional Development Workshop II: Preparing to Teach Effectively', 0, '{}', 'A structured series of four workshop sessions to help doctoral students develop teaching skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 616, 'Professional Development Workshop III: Navigating the Academic Job Market in CCJ', 0, '{}', 'A structured series of four workshop sessions to help doctoral students navigate the academic job market.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report or publication. Assignments might include data collection, experimental work, data analysis or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CRJ', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 102, 'Introduction to Human Geography', 3, '{'SB','G'}', 'Systematic study of human use of the Earth. Spatial organization of economic, social, political, and perceptual environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 113, 'United States and Arizona Social Studies', 3, '{'SB','H'}', 'Introduces American (including Arizona) institutions integrating geography, government, history and economic perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 114, 'World Social Studies', 3, '{'SB','G','H'}', 'Introduction to the world from the integrated social science perspectives of history, geography, and government.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 121, 'World Geography', 4, '{'SB','G'}', 'Description and analysis of areal variations in social, economic, and political phenomena in major world regions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 171, 'The Thread of Energy', 3, '{'SB','G'}', 'Follows the thread of energy through every aspect of our lives. Discusses the technical, social, legal, and policy contexts of all energy resources, including present and unconventional fossil fuels, nuclear power, and renewable resources. Addresses energy use throughout history, the influence of energy on quality of life, the role it plays in political strategies and environmental quality, how it shapes our neighborhoods and cities, its contribution to our personal comfort and national security, and how its importance is reflected in the worlds of business and the humanities. Its dual role is as a stand-alone introduction to energy, and as a first step to more advanced studies of energy in the natural, technical, and social sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 240, 'Introduction to Southeast Asia', 3, '{'(HU','SB)','G'}', 'Interdisciplinary introduction to the cultures, religions, political systems, geography, and history of Southeast Asia.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 241, 'The Global Economy in Transition', 3, '{'SB','G'}', 'Production, distribution, and consumption of various types of commodities of the world and relationships to the activities of humans.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 253, 'Introduction to Cultural and Historical Geography', 3, '{'SB','G'}', 'Cultural patterns, including such phenomena as language, religion, and various aspects of material culture. Origins and diffusion and division of the world into cultural areas.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 322, 'Geography of U.S. and Canada', 3, '{'SB','C'}', 'Spatial distribution of relevant physical, economic, and cultural phenomena in the United States and Canada.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 323, 'Geography of Latin America', 3, '{'SB','G'}', 'Spatial distribution of relevant physical, cultural, urban and economic phenomena in Mexico, Central America, South America and the Caribbean.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 325, 'Geography of Europe', 3, '{'SB','G'}', 'Broad and systematic overview of Europe, emphasizing physical, economic, and cultural phenomena.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 326, 'Geography of Asia', 3, '{'SB','G'}', 'Spatial distribution of relevant physical, economic, and cultural phenomena in Asia, excluding the former Soviet Union.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 328, 'Geography of Middle East and North Africa', 3, '{'SB','G'}', 'Spatial distribution of relevant physical, economic, and cultural phenomena in the Middle East and North Africa.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 332, 'Geography of Australia and Oceania', 3, '{'SB','G'}', 'Spatial distribution of relevant physical, economic, and cultural phenomena in Australia, New Zealand, and Pacific Islands.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 344, 'Geography of Hispanic Americans', 3, '{'SB','C'}', 'Examines the homelands, migrations, settlements, landscapes, roles, and selected cultural traditions of Hispanic Americans.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 350, 'The Geography of World Crises', 3, '{'SB','G'}', 'Contemporary world crises viewed from a perspective of geographic concepts and techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 351, 'Population Geography', 3, '{'SB','G'}', 'Demographic patterns; spatial, temporal, and structural investigation of the relationship of demographic variables to cultural, economic, and environmental factors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 356, 'Central American Studies', 3, '{'SB','G'}', 'Part of the summer ASU study abroad Costa Rica Program, which is a language and social sciences immersion program in the capital city, San Jose. A summer offering permits students to enroll in the program without conflicting with full course loads during the fall or spring semester. However, the course is proposed as a stand-alone course that can be offered in a regular semester as well.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 357, 'Social Geography', 3, '{'SB'}', 'Environmental perception of individuals and groups. Stresses the spatial aspect of social and physical environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 359, 'Cities of the World I', 3, '{'SB','G','H'}', 'Historical evolution of urban patterns and structures in the Middle East, India, Southeast Asia, China, Japan, and Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 361, 'Urban Geography', 3, '{'SB'}', 'External spatial relations of cities, internal city structure, and spatial aspects of urban problems in various parts of the world, particularly in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 362, 'Journey Stories: People and Place in Motion', 3, '{'HU'}', 'Explores journeys, storytelling and geographies--real and imagined. Draws on novels, novellas, short stories, film and video in illuminating stories and storytelling as transformative of self, place and culture in human and more-than-human worlds. Enjoins journey stories through the trilogy movement, place and identity; signature themes in cultural geography that resonate across the arts, humanities and social sciences. Students respond to artfully rendered journeys in personal ways via word, image and sound, creating their own journeys en route.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 364, 'Energy in the Global Arena', 3, '{'SB','G'}', 'Production, transportation, and consumption of energy, emphasizing the electric power industry and its environmental problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 373, 'Introduction to Geographic Information Science', 4, '{'SG'}', 'Introduces theoretical and computer-based aspects of Geographic Information Systems, involving data analysis and map generation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 414, 'Teaching Geography Standards', 3, '{}', 'Introduces Arizona Geography Standards for K-12 educators, emphasizing exciting curricula and illustrated with best practices by master teachers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 421, 'Geography of Arizona and Southwestern United States', 3, '{'SB','C'}', 'Geography of the Southwest with an emphasis on Arizona. Divided into physical geography, history, people, and economy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 424, 'Geography of Mexico and Middle America', 3, '{'SB','G'}', 'Central America and Mexico.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 425, 'Geography of the Mexican American Borderland', 3, '{'(L','SB)','G'}', 'Geography of a binational and bicultural region. Examines settlement, boundary issues, ethnic subregions, population change, industrial development, and urban growth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 426, 'Geography of Russia and Surroundings', 3, '{'SB','G'}', 'Examines the geography of Russia and other post-Soviet states.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 432, 'Geography of China', 3, '{'SB','G'}', 'Examines the physical, economic, cultural, social, demographic, agricultural, political, historical, and environmental aspects of the geography of China.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 433, 'Geography of Southeast Asia', 3, '{}', 'Examines the biophysical and social features of Southeast Asian nations and peoples.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 441, 'Economic Geography', 3, '{'SB'}', 'Spatial distribution of primary, secondary, and tertiary economic and production activities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 442, 'Geographical Analysis of Transportation', 3, '{'SB'}', 'Networks, modes, economics, and flows at the urban, national, and international scales.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 485, 'International Field Studies', 12, '{'G'}', 'Organized field study, culture and language in specified international locations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 496, 'Geographic Research Methods', 3, '{'L'}', 'Scientific techniques used in geographic research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 510, 'Solar Energy and Policy', 1, '{}', 'Elucidates and amplifies the importance, role and mechanisms of solar energy policy. Underscores the significance of policy in the successful deployment of solar energy. Complements other courses in the curriculum of the Professional Science Masters (PSM) in Solar Energy, which is largely centered on engineering concepts and techniques. Content is guided by the principle that technical considerations alone are inadequate to stimulate future expansion of solar energy. Additionally, it is stressed that policy considerations are critical to any potential transition to a solar energy economy. Typically taken in conjunction with DC summer workshop 'Science Outside the Lab,' but may be taken by others as stand-alone course. Course consists of lectures and discussions of policy instruments at the national, state and local scales. Visiting experts from the energy industry provide their personal experience and real-world perspective.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 514, 'Human Dimensions of Sustainability', 3, '{}', 'Concepts and definitions of the human dimensions of sustainability; the role of attitudes and values in shaping sustainability goals, practices, and programs; the diversity of values and socio-cultural contexts relating to sustainability; bottom-up and top-down sustainable policy development, social data collection methodologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 515, 'Human Migration', 3, '{}', 'Economic, political, social, and geographic factors underlying population movements. Migration selectivity, streams and counter-streams, labor migration, and migration decision making.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 526, 'Sustainability Science: Interactions Between Human and Environmental Systems', 3, '{}', 'Addresses core ideas in sustainability science--an emerging field of research dealing with the interactions between human and environmental systems. The problem that motivates the course is the challenge of sustainability: improving the well-being of present and future generations in ways that conserve the planet's life support systems over the long term. Students critically discuss a set of presentations and papers covering the field in a systematic way, drawing on and integrating contemporary research from earth systems science, resource economics, institutional analysis, ecology, geography, development studies, health sciences, engineering, and other disciplines. Covers from the theory of sustainability to its multiple applications in the field and explores the impacts of different value systems on both theory and applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 528, 'Geographic Problems and Context', 3, '{}', 'Investigates different forms of geographic problem solving and contemporary trends in geographic research. Emphasizes applied research and questions at the forefront of the field, and the range of techniques being applied by practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 529, 'Contemporary Geographic Thought', 3, '{}', 'Comparative evaluation of current philosophy concerning the nature and trends of geography.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 542, 'Geographical Analysis of Transportation', 3, '{}', 'Examines the geographical aspects of transportation systems. Looks at the geography of networks; transport costs and rates; different modes of transport, trade, economic development, and technology. Studies the movement of freight and passengers at the individual, urban, national, and international scales.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 585, 'Geographic Research Design and Proposal Writing', 3, '{}', 'Prepares students to undertake creative geographic research culminating in the generation of new knowledge. Serves as a foundation for the student in achieving the objective of a productive and successful degree. To accomplish this, instruction stresses a research framework, ethical practice, and the generation of an original research proposal. Specific learning objectives are: to produce an area of specialization statement, literature review, problem statement, description of methods and final proposal; to develop the skills needed to conceive and plan a research project; to communicate ideas effectively in written and oral form; to develop and articulate informed opinions about research designs; to learn about academic publishing format and process; and to understand and engage in ethical research practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 596, 'Geographic Research Methods', 3, '{}', 'Prepares students to conduct geographic research by teaching scientific techniques used to study problems in the field of geography. Provides a strong foundation in geographic research methodologies. Covers critical reading and analysis of peer-reviewed research, types of research methodologies, and elements of research design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 671, 'Introduction to Geographic Teaching', 4, '{}', 'Intensive course in geographic education, scientific method in research on geographic education, research trends, resources for teaching, best practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 672, 'Physical Geography for Teachers', 3, '{}', 'Transfer of matter and energy exhibited in the Earth's climate, hydrology, soils, biogeography, and landforms; case studies; virtual field trips.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 673, 'Human Geography for Teachers', 3, '{}', 'Analyzes cultural, economic, urban, historical, transportation, population, political, and development geography; case studies; virtual field trips.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 674, 'Geographic Techniques for Teachers', 4, '{}', 'Introduces geographic techniques, including GPS, GIS, remote sensing, cartography, qualitative, and field methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 675, 'World Geography for Teachers', 3, '{}', 'Systematic overview of geographic knowledge about different world regions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 676, 'North American Geography for Teachers', 3, '{}', 'Systematic overview of geographic knowledge about different North American regions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 677, 'Geography Across the Curriculum', 4, '{}', 'Intensive course on intergrating reading, writing, and mathematics standards with geography content; selected case studies; best practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 680, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GCU', 683, 'Fieldwork', 12, '{}', 'Structured, supervised field experience in a field science or other discipline requiring experience in field techniques.');
