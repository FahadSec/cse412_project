INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 652, 'Advanced Composition Studies', 3, '{}', 'Selected topics on particular composition theories, practices, pedagogies, and figures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 654, 'Advanced Studies in Rhetoric, Writing, Technology, and Culture', 3, '{}', 'Advanced study of theoretical, methodological, and pedagogical issues concerning the interrelationships among rhetoric, culture, writing, and writing technologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 655, 'Disciplinary Discourses', 3, '{}', 'Investigation of professional and disciplinary issues related to English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 656, 'Studies in Cross-Cultural Discourse', 3, '{}', 'Theoretical and methodological issues in the comparative study of discourses between cultures and communities of practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 662, 'Poetic Genres', 3, '{}', 'Creative writing courses in the long poem, the erotic image, death and transfiguration, reading and influence, and others.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 664, 'Mixed Genres', 3, '{}', 'Creative writing courses in the prose poem, magical realism, the literature of obsession.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 665, 'Creative Methods', 3, '{}', 'Creative writing courses in theory of the novel, poetics, story into film, and others.');
