INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 201, 'Mechanics of Particles and Rigid Bodies I: Statics', 3, '{}', 'Force vectors and force system resultants, equilibrium of particles and rigid bodies, friction, centroid and moments of inertia of an area. Required for aerospace engineering and mechanical engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 202, 'Mechanics of Particles and Rigid Bodies II: Dynamics', 3, '{}', 'Kinematics and kinetics of particles. Work, energy, and momentum principles for particles. Planar kinematics and kinetics of rigid bodies. Conservation of energy and momentum for rigid bodies. Equations of motion for particles and rigid bodies. Vibrations. Required for aerospace engineering and mechanical engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 213, 'Mechanics of Materials', 3, '{}', 'Equilibrium, strain-displacement relations, and stress-strain-temperature relations. Applications to force transmission and deformations in axial, torsional, and bending of bars. Combined loadings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 214, 'Computer-Aided Engineering I', 1, '{}', 'Introduces geometry and construction techniques in CAD, technical drawing conventions, generating drawings from CAD models.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 215, 'Introduction to Programming in MATLAB', 1, '{}', 'Introduces basic topics and concepts of computer programming in MATLAB.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 241, 'Introduction to Thermodynamics', 3, '{}', 'Concepts of equilibrium, properties, states, systems, energy and entropy; 1st and 2nd thermodynamic laws, analysis of open and closed systems, power and refrigeration cycles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 242, 'Introduction to Fluid Mechanics', 3, '{}', 'Introductory survey course covering fundamental concepts relating to engineering fluids. Fluid statics. Integral methods. Differential methods. Dimensional analysis and similarity. Incompressible internal and external flow. Potential flow. Required for Aerospace Engineering and Mechanical Engineering majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 301, 'Applied Experimental Statistics', 3, '{}', 'Introduces key concepts in probability and statistics with engineering applications. Emphasizes modeling variation in observations, characterizing its distribution, and making inferences with regard to quality assurance and control. Develops critical skills such as fitting multivariate models, experimental design, and hypothesis testing, utilizing data from engineering projects, simulations, and laboratory experiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 318, 'System Dynamics and Control I', 4, '{}', 'Dynamics of physical systems, models of mechanical, electrical and electromechanical systems, Laplace transform, transfer functions, time response and frequency response, poles and zeros, block diagrams, stability, introduction to feedback control: closed-loop response, PID compensation, steady-state characteristics, root-locus design and analysis of systems, frequency-domain design concepts, Bode-plots and analysis of systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 341, 'Mechanism Analysis and Design', 3, '{}', 'Positions, velocities, and accelerations of machine parts; cams, gears, flexible connectors, and rolling contact; introduces synthesis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 384, 'Advanced Mathematical Methods for Engineers', 3, '{'CS'}', 'Methods for numerical solutions to engineering problems. Nonlinear equations, quadrature, ordinary differential equations. Analytical and numerical solutions to partial differential equations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 400, 'Engineering Profession', 3, '{'L'}', 'Impact of mechanical and aerospace engineering in a global and societal context; effects of and on globalization, environment, sustainability, economy, politics; engineering ethics and business practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 404, 'Finite Elements in Engineering', 3, '{}', 'Formulation and application of the finite element method through integration of theory, code development and application to problems in heat transport and solid mechanics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 417, 'System Dynamics and Control II', 3, '{}', 'Fundamentals of feedback control using linear transfer function system models. Analysis in time and frequency domains. Design in the s-plane (root locus) and in the frequency domain (loop shaping). State-space representation and state-feedback control, multivariable control with observers, discrete systems and z-transform, nonlinear systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 436, 'Combustion', 3, '{}', 'Combustion of gaseous and condensed-phase fuels. Thermodynamics, mass transfer and chemical kinetics of combustion. Explosion and ignition theories. Structure, propagation and stability of flames. Pollutant formation. Applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 451, 'Applied Machine Learning for Mechanical and Aerospace Engineers', 3, '{}', 'Machine learning is impacting every aspect of our daily lives, from recognizing faces in a crowd to designing drugs to treat epidemic diseases. There is no doubt that, as a result of the impact, the job market for future engineers will be drastically different. Equips engineering students with a working understanding of machine learning and better prepares them for the future job market.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 455, 'Polymers and Composites', 3, '{}', 'Relationship among chemistry, structure, and properties of engineering polymers. Design, properties, and behavior of fiber composite systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 460, 'Applied Computational Fluid Dynamics', 3, '{}', 'Numerical techniques for solving incompressible and compressible flow equations using industrial solvers. Computer-aided analysis of thermofluid systems. Applications to fluid system engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 498, 'Pro-Seminar', 7, '{}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 501, 'Linear Algebra in Engineering', 3, '{}', 'Development and solution of systems of linear algebraic equations. Applications from mechanical, structural, and electrical fields of engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 502, 'Partial Differential Equations in Engineering', 3, '{}', 'Development and solution of partial differential equations in engineering. Applications in solid mechanics, vibrations, and heat transfer.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 503, 'Finite Elements in Engineering', 3, '{}', 'Ideas and methodology of finite element analysis. Applications to solid mechanics, heat transfer, fluid mechanics and vibrations. Knowledge of free-body diagrams and vector calculus operations is recommended.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 504, 'Experimental Methods for Thermal and Fluid Processes', 4, '{}', 'Theory of analog and digital signal analysis as applied to fluid and thermal measurements; optical diagnostics and micro sensors for measuring fluid velocity, pressure and temperature in unsteady flows; particle image velocimetry and thermal anemometry. Applies experimental in a weekly laboratory. Students should have completed an intermediate-level course in fluid dynamics, familiarity with Matlab or some programming language, prior exposure to Fourier series and Fourier transforms, as well as probability and statistics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 505, 'Perturbation Methods', 3, '{}', 'Nonlinear oscillations, strained coordinates, renormalization, multiple scales, boundary layers, matched asymptotic expansions, turning point problems, and WKBJ method.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 506, 'Advanced System Modeling, Dynamics, and Control', 3, '{}', 'Lumped-parameter modeling of physical systems with examples. State variable representations and dynamic response. Introduces modern control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 507, 'Advanced Modern Control and Estimation', 3, '{}', 'System analysis, control, estimation and optimization. Stability, optimality, robustness, adaptation and safety considerations. Continuous-time, discrete-time and hybrid systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 508, 'Digital Control: Design and Implementation', 3, '{}', 'Analysis and design of control systems in which the digital computer plays a major role. Review of continuous control and introduction to the key effects of sampling. Elements of discrete system analysis; z-transform; sampled-data systems; sampling theorem and combined discrete and continuous system and the phenomenon of aliasing. Deterministic design methods for digital control systems (root-locus, frequency response, pole placement and estimators). Multivariable, optimal control and Kalman filtering, followed by nonlinear control and system identification. Emphasizes digital control implementation topics and a case study on robot control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 509, 'Linear Matrix Inequality Methods in Optimal and Robust Control', 3, '{}', 'The recent introduction of Linear Matrix Inequality (LMI) methods in control has dramatically expanded the types and complexity of the systems we can control. For example, consider the problems: gain scheduling for missile attitude control (a switched system); control of robots over noisy communication cChannels (a sampled-data systems); remote control of spacecraft attitude (a delayed system); behavioral therapy (a system with binary inputs); or self-driving vehicles (a case of decentralized control). None of these systems can be studied using classical root-locus or PID control methods. Rather, advances in these fields have been made possible through the increased power and flexibility created by the LMI (optimization-based) approach to control. Background knowledge in controls and linear algebra is recommended to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 510, 'Dynamics and Vibrations', 3, '{}', 'Lagrange's and Hamilton's equations, rigid body dynamics, gyroscopic motion, and small oscillation theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 512, 'Random Vibrations', 3, '{}', 'Reviews probability theory, random processes, stationarity, power spectrum, white noise process, random response of single and multiple DOF systems, and Markov processes simulation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 513, 'Polymers and Composites', 3, '{}', 'Relationship among chemistry, structure, and properties of engineering polymers. Design, properties, and behavior of fiber composite systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 514, 'Vibration Analysis', 3, '{}', 'Free and forced response of single and multiple degree of freedom systems, continuous systems; applications in mechanical and aerospace systems numerical methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 515, 'Structural Dynamics', 3, '{}', 'Free vibration and forced response of discrete multidegree of freedom and continuous systems, cyclosymmetric structures, introduction to rotordynamics, component mode synthesis, vibration of structures with moving mass/pipes with flow.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 518, 'Fundamentals of Microelectronics Packaging', 3, '{}', 'Multidisciplinary foundation course in addressing electrical, thermal, materials, chemical, manufacturing, and reliability problems in packaging.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 519, 'Mechanics of Micro/Nano Systems', 3, '{}', 'Basic processes used for fabricating micro and nanoscale structures used in MEMS. Analyzes the processes and micro/nanoscale structures from a mechanics perspective. Covers the fundamentals of microfabrication, scaling of forces, thermal stresses and strains in thin beams, thermal oxidation and dynamics of MEMS. Optional topics include mechanics of thin films and nanostructured materials.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 520, 'Stress Analysis', 3, '{}', 'Introduces tensors: kinematics, stress analysis, and constitutive assumptions leading to elastic and plastic behavior. Strain energy and energy methods; applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 521, 'Structural Optimization', 3, '{}', 'Linear and nonlinear programming. Problem formulation. Design sensitivity analysis. FEM-based optimal design of structural and mechanical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 522, 'Plasticity', 3, '{}', 'Discusses theory of plastic (and inelastic) deformation in solids, e.g., metals and polymers, with emphasis on the fundamental foundations of the field derived from empirical observations and thermodynamic principles, all within a small strain framework. Assumes some knowledge of undergraduate-level thermodynamics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 523, 'Fracture Mechanics', 3, '{}', 'Linear elastic fracture mechanics. Nonlinear problems. Fatigue fracture. Experimental methods in fracture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 524, 'Theory of Elasticity', 3, '{}', 'Elastic behavior in two and three dimensions.  Airy stress functions and displacement potentials.  Elements of fracture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 526, 'Design of Aerospace Structures', 3, '{}', 'Provides a fundamental understanding of the design of aerospace structures. Topics include flight vehicle loads, metallic and composite materials, manufacturing methods, buckling and crippling of aerospace structures, fatigue, damage tolerance, finite element structural analysis, and design of joint and fittings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 527, 'Finite Elements for Engineers', 3, '{}', 'Direct stiffness, method of weighted residuals, weak formulation, and variational techniques in the solution of engineering problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 528, 'Advanced Computational Mechanics', 3, '{}', 'Teaches methods for treating material and geometric nonlinearities by finite elements for transient and static simulations. Topics to be included are total and updated Lagrangian formulations, linearization and solution methods, stability analysis, nonlinear material models such as hyperelastic and plasticity, and recent research topics such as extended finite element methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 529, 'Continuum Mechanics', 3, '{}', 'Tensors, Eulerian/Lagrangian strains, compatibility equations, Cauchy/nominal stresses, balance laws, Reynolds transport theorem, laws of thermodynamics for a continuum; equations of state, restrictions on constitutive laws, Newtonian fluids, inviscid flows, viscoelasticity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 530, 'Dynamic Behavior of Materials', 3, '{}', 'Focuses on material response under fast mechanical loading, with emphasis on plastic and elastic behavior as a function of pressure, temperature and strain rate, with and without effects of inertia. Starts with a treatment of elastic wave propagation, followed by a study of effects of strain rate and temperature on plastic behavior of metals, including a review of mechanisms and constitutive equations. Then, addresses the theory of shock waves including analysis of shocks using conservation principles as well as thermodynamics, followed by a discussion of dynamic failure. Finally, reviews experimental techniques for dynamic experiments, including drivers and diagnostics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 533, 'Transport Processes I', 3, '{}', 'Unified treatment of momentum, heat, and mass transfer from molecular theory, and continuum points of view. Continuum equations of microscopic and macroscopic systems and multicomponent and multiphase systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 536, 'Combustion', 3, '{}', 'Combustion of gaseous and condensed-phase fuels. Thermodynamics, mass transfer and chemical kinetics of combustion. Explosion and ignition theories. Structure, propagation and stability of flames. Pollutant formation. Applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 540, 'Advanced Product Design Methodology', 3, '{}', 'Surveys research in engineering design process, artifact and design, knowledge, formal and informal logic, heuristic and numerical searches, theory of structure and complexity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 541, 'CADTools for Engineers', 3, '{}', 'Elements of computer techniques required to develop CAD software. Data structures, including lists, trees, and graphs. Computer graphics, including 2- and 3-D algorithms and user interface techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 542, 'Design Geometry and Kinematics', 3, '{}', 'Selected topics in geometry related to current potential applications in CAD/CAM, design theory, kinematics, and robotics. Euclidean isometrics, transformations; 4-D geometry; screw theory and its applications to robotics and assemblies. Differntial geometry; curvature and torsional properties of curves and surfaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 544, 'Mechanical Design and Failure Prevention', 3, '{}', 'Modes of mechanical failure; applies principles of elasticity and plasticity in multiaxial state of stress to design synthesis; failure theories; fatigue; creep; impact.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 545, 'Modern Manufacturing Methods', 3, '{}', 'Investigates the capabilities and equipment of both conventional manufacturing processes and modern manufacturing processes. Emphasizes the fundamental, development and application of casting, forming and sintering processes for conventional manufacturing processes. Emphasizes the emerging topics related to additive manufacturing, field-assisted manufacturing and nanomanufacturing for modern manufacturing processes. Also introduces engineering materials, structure of materials, property of materials and material characterizations in the manufacturing processes. Complements theoretical concepts presented in lectures with the presentation of relevant research topics and/or industrial applications. Develops an understanding of manufacturing process evolution and the capabilities of modern manufacturing processes and equipment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 546, 'Advanced CAE Simulation', 3, '{}', 'Solution of advanced engineering problems with analysis software for structural, dynamic, nonlinear, contact and kinematic problems; selection of modeling parameters; reliability tests on software.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 547, 'Modeling and Control of Robots', 3, '{}', 'Homogeneous transformations, workspace, forward and inverse kinematics, differential kinematics, statics and dynamics, motion planning, motion and force control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 548, 'Probabilistic Methods for Mechanical Analysis', 3, '{}', 'Fundamental probabilistic theory, engineering statistics, data analysis, reliability methods, Monte Carlo simulation with different sampling technique, mechanical and structural analysis under uncertainties, focusing on the application for mechanical, aerospace and civil engineering structures and materials.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 551, 'Applied Machine Learning for Mechanical Engineers', 3, '{}', 'Machine learning impacts every aspect of our daily lives, from recognizing faces in a crowd to designing drugs to treat epidemic diseases. There is no doubt that, as a result of the impact, the job market for future engineers will be drastically different. This course equips engineering students with a working understanding of machine learning and better prepares them for the future job market.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 557, 'Mechanics of Composite Materials', 3, '{}', 'Analysis, design, and applications of laminated and chopped fiber reinforced composites. Micro- and macromechanical analysis of elastic constants, failure, and environmental degradation. Design project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 560, 'Applied Computational Fluid Dynamics', 3, '{}', 'Numerical techniques for solving incompressible and compressible flow equations using industrial solvers. Computer-aided analysis of thermofluid systems. Applications to fluid system engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 561, 'Computational Fluid Dynamics', 3, '{}', 'Finite-difference and finite-volume techniques for solving the subsonic, transonic, and supersonic flow equations.  Method of characteristics. Numerical grid-generation techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 562, 'Advanced Computational Fluid Dynamics: Interfaces', 3, '{}', 'Numerical methods for interfaces in low Mach number flows including level set methods, volume of fluid methods, Lagrangian interface tracking, ghost fluid methods, and immersed boundary methods. Includes coding of the covered numerical techniques and application to different physical interfaces, e.g., liquid/gas phase interfaces, premixed flames and flows in complex geometries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 563, 'Aircraft Propulsion', 3, '{}', 'Fundamentals of gas-turbine engines. Analysis and design of components. Alternative aircraft propulsion systems. Recommend a background in thermodynamics and high-speed aerodynamics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 564, 'Advanced Aerodynamics', 3, '{}', 'Advanced topics in aerodynamics with application to design. Slender body theory. Thin airfoil theory. Transonic and supersonic effects. Sweep theory. Wave drag. Post-stall (vortex) flows. Supersonic and hypersonic flows.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 565, 'Rocket Propulsion', 3, '{}', 'Rocket flight performance, nozzle design, combustion of liquid and solid propellants, component design, advanced propulsion systems, interplanetary missions, testing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 566, 'Rotary-Wing Aerodynamics', 3, '{}', 'Introduces helicopter and propeller analysis techniques. Momentum and blade-element, helicopter trim. Hover and forward flight. Ground effect, autorotation and compressiblilty effects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 570, 'Intermediate Thermodynamics', 3, '{}', 'Applied thermodynamics; gas mixtures, psychrometrics; property relationships, power and refrigeration cycles, and reactive systems. A basic knowledge of thermodynamics is required to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 571, 'Fluid Mechanics', 3, '{}', 'Basic kinematic, dynamic, and thermodynamic equations of the fluid continuum and their application to basic fluid models.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 573, 'Advanced Fluid Mechanics', 3, '{}', 'Mechanics of fluids flows where either the effects of inertia or viscosity are significant. Exact and approximate solutions of the Navier-Stokes equations, laminar flow and flow at low and high Reynolds numbers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 574, 'Multiphase Flow', 3, '{}', 'Models for multiphase flows including dispersed and continuous phase flow models, particle-fluid and particle-particle interactions, droplet collision and coalescence, atomization, and numerical and experimental methods for analyzing multiphase flows.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 575, 'Turbulence', 3, '{}', 'Graduate-level introduction to turbulent flows covering the essential physical basis of turbulence, the statistical description of turbulent flows, dynamics of energy transfer and dissipation in such flows, inner- and outer-variable scaling of turbulent flows, and modern computational modeling and simulation of turbulence and turbulent flows. Requires undergraduate background in fluid mechanics and strong graduate-level mathematics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 576, 'Energy Efficiency', 3, '{}', 'Provides a broad background in energy efficiency analysis and technologies. Applies fundamental concepts from thermodynamics, heat transfer and fluid mechanics to real-world applications including heating, ventilating, air conditioning (HVAC), refrigeration, compressed air systems, steam systems, combined heat and power (CHP). Complements other courses on conventional and renewable power generation by focusing on reducing energy consumption, thus making it easier to meet our energy needs from sustainable sources. To be successful in this course, students should have knowledge of thermodynamics principles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 577, 'Advanced Turbulence', 3, '{}', 'Reynolds equations and their closure. Modeling of simple and complex turbulent flows, calculations of internal and external flows, and application to engineering problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 578, 'Geophysical and Environmental Fluid Dynamics', 3, '{}', 'Dynamics of large-scale flows under the influence of stratification and earth rotation. Thermally and mechanically driven flows in turbulent boundary layer. Waves and instability of geophysical flows. Global-scale circulation. Applications to environmental prediction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 579, 'Wind Energy', 3, '{}', 'Wind energy systems convert kinetic energy in the wind into electrical energy. Focuses on flow machines, including for example, aerodynamics and fluid dynamics. Begins with wind resource assessment utilizing atmospheric boundary layer science and then proceeds to computational fluid dynamics to understand the tools of modern wind resource assessment. Includes applications of the Navier-Stokes equations and some basics from turbulence theory. Resource assessment also includes modern remote sensing techniques such as lidar, radar and in situ instrumentation. Also includes discussions from current events and policy. Requires a background in applied experimental statistics, thermodynamics or aerodynamics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 581, 'Advanced Thermodynamics', 3, '{}', 'Basic concepts and laws of classical equilibrium thermodynamics; applications to engineering systems; introduction and applications of statistical mechanics and kinetic theory; Boltzmann integro-differential equation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 582, 'Renewable Energy: Mechanical Systems', 3, '{}', 'Renewable energy systems are energy conversion systems that rely on naturally replenishing flows of energy. Considers the foundational principles behind many renewable energy systems, and application of these principles to practical systems. Emphasizes covering the following energy systems, some only at an introductory level and some with significant depth: solar, wind, geothermal, biomass, hydropower, energy storage, electric power generation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 585, 'Solar Thermal Engineering', 3, '{}', 'Solar energy availability, flat-plate and concentrating solar thermal collectors, solar heating and cooling, concentrating solar power (CSP).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 586, 'Advanced Heat Transfer', 3, '{}', 'Basic concepts and governing equations. Analysis of laminar and turbulent heat transfer for internal and external flows. Natural and mixed convection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 587, 'Radiation Heat Transfer', 3, '{}', 'Advanced concepts and solution methodologies for radiation heat transfer, including exchange of thermal radiation between surfaces, radiation in absorbing, emitting, and scattering media and radiation combined with conduction and convection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 588, 'Nanoscale Heat Transfer', 3, '{}', 'Explores the fundamental micro and nanoscale origins of thermal transport. Ues energy transport formalism to explain the breakdown of macroscopic transport laws (e.g., Fourier's law) at the micro and nanoscale. Defines and explores the key carriers of heat: phonons, photons, electrons and molecules. Derives the energy states of these carriers and combines this knowledge with statistical thermodynamics to determine the population distribution of these carriers. Combining these distributions with scattering processes enables fundamental laws to be derived within the context of their microscopic/nanoscopic underpinnings. Some of these phenomena include, Fourier's law, Ohm's law, thermopower, Wiedemann-Franz law, Dulong-Petit law, equation of radiative transfer.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 589, 'Heat Transfer', 3, '{}', 'Basic concepts; physical and mathematical models for heat and mass transfer. Applications to conductive, convective, radiative, and combined mode heat transfer.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MAE', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 322, 'Structural Mechanics', 3, '{}', 'Three-dimensional stress and strain analysis, generalized Hooke's Law, failure theories for ductile and brittle materials, combined loading, stress concentrations, energy methods, mechanical vibrations and structural dynamics, fatigue.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 323, 'Computer-Aided Engineering II', 2, '{}', 'Introduces finite-element analysis, pre- and postprocessing, solving problems with FEA.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 324, 'Structural Mechanics Laboratory', 1, '{}', 'Lab companion where students perform supervised experiments to analyze stress and strain for materials loaded in bending, torsion, combined and torsion, tensile tests of materials until failure, stress concentrations, and structural dynamics. Required for mechanical engineering');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 340, 'Heat Transfer', 3, '{}', 'Conduction, convection, radiation and phase change heat transfer processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 342, 'Principles of Mechanical Design', 3, '{}', 'The design process; conceptual and embodiment design of mechanical elements; form synthesis; material selection, failure modes, manufacturability tolerances, common mechanisms and machine elements.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 351, 'Manufacturing Processes', 3, '{}', 'Production technique and equipment. Casting and molding, forming, machining, joining and assembly, computer-integrated manufacturing, rapid prototyping, and electronics manufacturing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 434, 'Internal Combustion Engines', 3, '{}', 'Performance characteristics, combustion, carburetion and fuel-injection, and the cooling and control of internal combustion engines. Computer modeling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 440, 'Renewable Energy: Mechanical Systems', 3, '{}', 'Renewable energy systems are energy conversion systems that rely on naturally replenishing flows of energy. Considers the foundational principles behind many renewable energy systems and application of these principles to practical systems. Emphasizes covering the following energy systems, some only at an introductory level and some with significant depth: solar, wind, geothermal, biomass, hydropower, energy storage, electric power generation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 441, 'Wind Energy', 3, '{}', 'Wind energy systems convert kinetic energy in the wind into electrical energy. Focuses on flow machines, including for example, aerodynamics and fluid dynamics. Begins with wind resource assessment utilizing atmospheric boundary layer science and then proceeds to computational fluid dynamics to understand the tools of modern wind resource assessment. Includes applications of the Navier-Stokes equations and some basics from turbulence theory. Resource assessment also includes modern remote sensing techniques such as lidar, radar and in situ instrumentation. Also includes discussions from current events and policy. Requires a background in applied experimental statistics, thermodynamics or aerodynamics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 445, 'Energy Systems Design I', 1, '{}', 'Applies mechanical engineering principles and techniques to modeling, analysis and synthesis of energy systems and components. Design and optimization. Capstone design project. First part of an energy systems design course focusing on the design of compact heat exchangers and fluid flow systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 446, 'Energy Systems Design II', 2, '{}', 'Applies mechanical engineering principles and techniques to modeling, analysis, and synthesis of energy systems and components. Design optimization. Design project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 472, 'Intermediate Fluid Mechanics', 3, '{}', 'Second course covering fundamental concepts relating to engineering fluids. Flow meters, airfoil and wing aerodynamics; pipe flow; normal and oblique shock waves. Nozzles. Open Channel Flow. Turbomachinery flows including performance curves and maps, scaling and affinity laws. Elective for Aerospace Engineering and Mechanical Engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 482, 'Intermediate Thermodynamics', 3, '{}', 'Applied thermodynamics; gas mixtures, psychrometrics, property relationships, power and refrigeration cycles, and reactive systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 488, 'Mechanical Engineering Design I', 3, '{}', 'Conceptual and embodiment design; modeling; rapid prototyping. Team project. MEE 488 and 489 must be taken in consecutive semesters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 489, 'Mechanical Engineering Design II', 3, '{}', 'Detail design; fabrication and testing. Team project. MAE 488 and 489 must be taken in consecutive semesters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 491, 'Experimental Mechanical Engineering', 3, '{'L'}', 'Experimental and analytical studies of phenomena and performance of fluid flow, heat transfer, thermodynamics, refrigeration, and mechanical power systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MEE', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 410, 'Laboratory Leadership', 1, '{}', 'Clinical lab leadership and management. Applies the principles of personnel management and supervision, allocation of resources, and staff communication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 420, 'Advanced Clinical Urinalysis and Body Fluids', 2, '{}', 'Theory and applications of principles in urinalysis with emphasis on the etiology, pathophysiology, and clinical manifestations of renal and other metabolic defects as reflected by the analysis of urine and other body fluids.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 421, 'Advanced Practicum in Clinical Urinalysis and Body Fluids', 1, '{}', 'Correlation and application of theory in a supervised clinical laboratory. Processing, analysis, and evaluation of urine and body fluids, emphasizing manual and automated techniques of specimen analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 430, 'Advanced Hematology and Hemostasis', 3, '{}', 'Theory and application of principles in hematology, with emphasis on techniques to evaluate blood dyscrasias and analyze body fluids. Etiology, pathophysiology, clinical manifestations, and treatment of blood dyscrasias/hemostatic defects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 431, 'Advanced Practicum in Hematology and Hemostasis', 1, '{}', 'Correlation and application of theory in a supervised clinical laboratory. Processing, analysis, and evaluation of blood samples, emphasizing manual and automated techniques of specimen analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 440, 'Advanced Immunohematology and Immunology', 3, '{}', 'Theoretical and practical application of clinical immunology and immunohematology. Emphasizes serological techniques that aid disease diagnosis and blood donor selection.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 441, 'Advanced Practicum in Immunohematology and Immunology', 2, '{}', 'Correlation and application of theory in a supervised clinical laboratory. Processing, analysis, and evaluation of samples, emphasizing manual and automated techniques of specimen analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 450, 'Advanced Clinical Microbiology', 3, '{}', 'Theoretical and practical application of clinical microbiology. Emphasizes identification of pathogenic organisms, molecular techniques, mycology, virology, and parasitology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 451, 'Advanced Practicum: Clinical Microbiology', 1, '{}', 'Introduces instructional strategies appropriate for the teaching of group exercise classes in secondary school settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 460, 'Advanced Clinical Chemistry', 3, '{}', 'Theoretical and practical applications of clinical chemistry with particular emphasis on correlation of test results to disease status, statistical analysis, pre-analytical testing, critical values, calibration, and trending and tracking of quality control results.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDL', 461, 'Advanced Practicum: Clinical Chemistry', 1, '{}', 'Correlation and application of theory in a supervised clinical laboratory. Processing, analysis, and evaluation of samples, emphasizing manual and automated techniques of specimen analysis routinely used clinical chemistry testing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 100, 'Introduction to Medical Studies', 1, '{}', 'Presents basic concepts related to sciences, social sciences, humanities, statistics and other topics related to medical and health studies. Solidifies concepts through the use of assignments, exams and discussions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 300, 'Historical and Contemporary Issues in Health', 3, '{'L'}', 'History of medicine in the modern world and its relationship to contemporary issues in medicine with emphasis on the broader context of ways in which technology, personalized medicine, genomics, ethical issues and shared decision making between patients and providers have evolved.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 320, 'Applied Medical/Health Care Ethics', 3, '{'HU'}', 'Integrates the theory and practice of ethics within health-related disciplines and the application of bioethics by health science professionals, including health disparities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 325, 'Elements of Hospice and Palliative Care', 3, '{}', 'Introduces hospice and palliative care through direct experience communicating and working with persons on hospice and the nurses, physicians, social workers and chaplains who provide care.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 375, 'Alzheimer's and Other Dementias', 3, '{'SB'}', 'Covers Alzheimer's disease and related dementias through discussions with health care professionals and direct experience working with persons with Alzheimer's disease and their caregivers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 417, 'Impact of Violence on Health', 3, '{}', 'Approaches violence from a public health perspective, using the Centers for Disease Control, National Center for Injury Prevention and Control, as a benchmark. Applies an interprofessional approach to violence, to include medicine, nursing, law, law enforcement, social work, psychology, economics and education, among others. Discusses principles of epidemiology, underlying etiologies and sequelae and evidence-based strategies for prevention throughout the course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 445, 'Prevention and Management of Chronic Disease', 3, '{}', 'The United States spends more on health care than any other developed country in the world, yet our health care outcomes are not superior. We face an epidemic of chronic diseases which are the leading causes of mortality and disability in the U.S., despite being highly preventable. Examines chronic disease in the U.S. and provides an overview of prevention and management with a focus on population health and social determinants of health.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 450, 'Leadership and Professionalism', 3, '{}', 'Leadership, leadership theory and inter-professionalism in practice. Applies didactic content to case studies and activities designed to promote critical thinking and effective teamwork. Solidifies concepts through the use of assignments, exams and discussions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 475, 'Palliative Care: Managing Complex Serious Illness', 3, '{}', 'Includes reading, discussion and study on palliative care including national trends, international models, ethics, mental health issues, business and workforce issues, boundaries between patients and caregivers, the intersection between humanities and palliative care, and the future of the field. Honors contracts are available.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 498, 'Pro-Seminar', 7, '{'L'}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MED', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 205, 'Microbiology', 3, '{'SG'}', 'Introductory microbiology, emphasizing basic principles of microorganisms (bacteria, protozoa, algae, fungi, and viruses) and the role they play in health, ecology, and applied fields.  May not be used for Microbiology credit unless a diagnostic test is passed. Both MIC 205 and MIC 206 required for SG General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 206, 'Microbiology Laboratory', 1, '{'SG'}', 'Principles and lab techniques used in identifying and handling microorganisms. Both MIC 205 and 206 must be taken to secure SG General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 220, 'Biology of Microorganisms', 3, '{}', 'Introductory microbiology for students majoring in the life sciences. Detailed study of microbial cells, their structure, genetics, physiology, and taxonomy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 302, 'Advanced Bacteriology Laboratory', 2, '{'L'}', 'Advanced lab techniques in bacterial growth, physiology, and genetics, with an emphasis on quantitative data and formal lab reports. Both MIC 302 and 401 must be taken to secure L General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 314, 'HIV/AIDS: Science, Behavior, and Society', 3, '{}', 'Overview of the basic biological, behavioral, and psychosocial aspects of HIV disease and AIDS.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 360, 'Bacterial Physiology', 3, '{}', 'Mechanisms and control of cell metabolism, structures, and functions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 379, 'Medical Bacteriology', 3, '{}', 'Principles of medical bacteriology with emphasis on bacterial infections, diagnostic testing, clinical symptoms, pathogenesis, and treatments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 401, 'Research Paper', 1, '{'L'}', 'Paper of 15 or more pages based on library or lab research in collaboration with a faculty member. Required of all Microbiology majors. Both MIC 302 and 401 must be taken to secure L General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 420, 'Immunology: Molecular and Cellular Foundations', 3, '{}', 'Antibody/antigen interactions, cellular response, cytokines, immunogenetics, immunoregulation, autoimmunity, psychoneuroimmunology research/medical perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 421, 'Experimental Immunology', 2, '{}', 'Introduces the basic techniques, methods, and assays used in immunology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 425, 'Advanced Immunology', 3, '{}', 'Surveys recent advances in immunology, including lymphocyte membranes, lymphokines/biochemistry, molecular genetics, theoretical immunology, immunoregulation, neuroimmunology, and immunologic diseases.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 441, 'Bacterial Genetics', 3, '{}', 'Surveys genetic exchange and regulatory processes in bacteria and their viruses. Bacteria and viruses as tools in genetic engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 442, 'Bacterial Genetics Laboratory', 1, '{}', 'Techniques of mutagenesis, mapping, and strain and genetic library construction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 443, 'The Microbial Universe', 3, '{}', 'Diversity and physiology of microorganisms with emphasis on recent advances in the biology of microorganisms of environmental and medical importance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 444, 'The Microbial Universe Laboratory', 1, '{}', 'Techniques used to explore the microbial universe with emphasis on structural, metabolic, genetic, and growth characteristics relevant to the identification and characterization of microorganisms of environmental and medical importance. Not open to students with credit in MIC 206.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 445, 'Techniques in Molecular Biology/Genetics', 3, '{}', 'Molecular genetic principles: plasmid construction, purification, and characterization; PCR; mutageneses; hybridization and sequence analysis; protein quantitation, immunologic detection, and electrophoresis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 446, 'Techniques in Molecular Biology/Genetics Lab', 2, '{}', 'Molecular genetic techniques; plasmid construction, purification, and characterization; PCR; mutageneses; hybridization and sequence analysis; protein quantitation; immunologic detection, and electrophoresis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 461, 'Geomicrobiology', 3, '{}', 'Past and present interactions among microbial life, geological materials, and biogeochemical cycles involving carbon, sulfur, phosphate, nitrogen, and metals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 470, 'Bacterial Diversity and Systematics', 4, '{}', 'Biology, classification, and enrichment culture of the nonpathogenic bacteria.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 481, 'Bacterial Pathogenesis', 3, '{}', 'Human host-bacterial interactions in infectious diseases with emphasis on pathogenesis and molecular mechanisms of bacterial virulence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 485, 'General Virology', 3, '{}', 'Fundamental principles of viruses, their molecular biology, replication, genetics, and pathogenesis.');
