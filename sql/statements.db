INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APH', 300, 'World Architecture I/Western Cultures', 3, '{'HU','H','G'}', 'Historical and contemporary built environments of Western civilizations: Mediterranean, Europe, and the Americas as manifestations of cultural history and responses to environmental determinants.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APH', 313, 'History of Architecture I', 3, '{'(L','HU)','G','H'}', 'Intensive study of history of architecture from earliest traces to 1300. Examines cultures from around the world.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APH', 314, 'History of Architecture II', 3, '{'(L','HU)','G','H'}', 'Intensive introduction to history of architecture from roughly 1300 to present day. Examines cultures from around the world.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APH', 336, '20th-Century Architecture I', 3, '{'HU'}', 'Architecture in Europe and America from the foundations of the modern movement to the culmination of the international style.');
