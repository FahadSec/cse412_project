INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BDE', 502, 'Research Methods in Biological Design', 3, '{}', 'Introduction to research, the scientific method, literature survey, experiments and research documentation, time management, technical writing and peer review, technical presentations, ethics and safety, academic and industrial research, funding and proposals, industrial research and intellectual property.');
