INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 502, 'Theory and Practice in Communication and Persuasion', 3, '{}', 'Theoretical exploration of communicative and persuasive practices as applied in various contexts. Surveys classical, modern, and contemporary theories of persuasion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 505, 'Methods in Applied Communication Research', 3, '{}', 'Examines the intellectual, practical, and ethical dimensions of engaging in applied research. Emphasizes empirical and quantitative methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 506, 'Humanistic Inquiry and Field Research in Communication', 3, '{}', 'Examines interpretive, critical, and rhetorical approaches to communication and advocacy, including hermeneutics, ethnography, and cultural studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 515, 'Ethical Issues in Communication Advocacy', 3, '{}', 'Examines the ethical issues or concerns related to communication advocacy. Variable topics; examines research, theory, and/or practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 516, 'Mediation and Dispute Resolution', 3, '{}', 'Examines advocacy's role in fair negotiation and effective and successful dispute resolution. Examines research, theory, and/or practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 520, 'Communication Campaigns', 3, '{}', 'Explores public communication strategies aimed at advocating for general or specific audiences and/or for particular issues and causes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 522, 'Argumentation and Advocacy', 3, '{}', 'Introduces various models of argumentation and their applications to various spheres of advocacy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 531, 'Communication and Social Change', 3, '{}', 'Examines human communication and technologies of communication as agents of social change within groups, communities, organizations, and/or cultures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 532, 'Advocacy in Interpersonal Settings', 3, '{}', 'Examines particular relational contexts in which advocacy plays a pronounced role (e.g., personal, family, and service provider relationships).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 550, 'Advocacy in Organizational Settings', 3, '{}', 'Examines issues of upward influence, the expression of dissent, and grievance systems within organizational contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 551, 'Democracy and Power in Organizations', 3, '{}', 'Examines structural, historical, relational, and symbolic dimensions of organizational communication and discourses that foster or impede communication advocacy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 557, 'Communication and Technology', 3, '{}', 'Assesses technology's role in the social dynamics of human interaction. Emphasizes the impact of technology with regard to communication advocacy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 565, 'Globalization and Advocacy', 3, '{}', 'Explores the forces leading to increased intergroup contact and their impact on social, economic, and political dynamics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CMN', 570, 'Communication and Advocacy in Social Context', 3, '{}', 'Variable topics course exploring the intersection of communication and advocacy in specific contexts not adressed in other elective courses.');
