INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MIC', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
