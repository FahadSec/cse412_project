INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 101, 'The ASU School of Music Experience', 1, '{}', 'Orientation to university and School of Music resources and procedures as well as career planning and guidance. Through various course discussions and assignments, music students examine the concept of academic integrity and its potential impact on their future and learn about taking an interdisciplinary perspective. Required for all music majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 111, 'Studio Instruction', 2, '{}', 'Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1 hour plus studio class weekly. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 117, 'Applied Jazz Improvisation', 1, '{}', 'Principles, methods, and theory of jazz performance and pedagogy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 121, 'Studio Instruction', 1, '{}', 'Secondary or minor instrument instruction. Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1/2 hour per week. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 127, 'Studio Instruction', 4, '{}', 'Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1 hour plus studio class weekly. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 131, 'Class Piano', 1, '{}', '4-semester sequence (with MUP 132, 231, and 232) designed for those with little or no piano experience. Emphasizes keyboard technique, sight reading, simple accompaniments, and improvisation. 2 hours per week. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 132, 'Class Piano', 1, '{}', '4-semester sequence designed for those with little or no piano experience. Emphasizes keyboard technique, simple accompaniments, and improvisation. 2 hours per week. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 133, 'Class Voice', 1, '{}', '4-semester sequence (with MUP 134, 233, and 234) open to all students.  2 hours per week.  May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 134, 'Class Voice', 1, '{}', 'See MUP 133.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 170, 'Music Theatre I', 2, '{}', 'Develops introductory performance skills for opera and music theater emphasizing the portrayals of simple and complex relationships, character development, solo and ensemble performance, music and dialogue analysis, body awareness, and freedom of the vocal and breath mechanisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 172, 'Music Theatre II', 2, '{}', 'Further develops introductory performance skills for opera and music theater emphasizing the portrayals of simple and complex relationships, character development, solo and ensemble performance, music and dialogue analysis, body awareness, and freedom of the vocal and breath mechanisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 209, 'Beginning Choral Conducting', 1, '{}', 'Essentials of choral conducting techniques. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 210, 'Beginning Instrumental Conducting', 1, '{}', 'Essentials of instrumental conducting techniques. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 217, 'Applied Jazz Improvisation', 2, '{}', 'Emphasizes basic jazz literature, chord symbol reading, melodic patterns and concepts, ear training, analysis of improvised solos, and pedagogical issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 231, 'Class Piano', 1, '{}', 'See MUP 131.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 232, 'Class Piano', 1, '{}', 'See MUP 131.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 235, 'Jazz Piano', 1, '{}', '2-semester sequence (with MUP 236) designed for jazz keyboard experience. Emphasizes chord symbol reading, simple improvisation, and voicing. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 236, 'Jazz Piano', 1, '{}', 'See MUP 235.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 250, 'Diction for Singers', 1, '{}', 'Use of phonetics in the study of song and opera literature. Language emphasis differs each semester. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 270, 'Music Theatre III', 2, '{}', 'Develops intermediate performance skills for opera and music theater emphasizing the portrayals of simple and complex relationships, character development, solo and ensemble performance, music and dialogue analysis, body awareness, and freedom of the vocal and breath mechanisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 272, 'Music Theatre IV', 2, '{}', 'Further develops intermediate performance skills for opera and music theater emphasizing the portrayals of simple and complex relationships, character development, solo and ensemble performance, music and dialogue analysis, body awareness, and freedom of the vocal and breath mechanisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 311, 'Studio Instruction', 2, '{}', 'Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1 hour plus studio class weekly. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 319, 'Advanced Audio Engineering in the Arts', 2, '{}', 'Synthesis and application of analog and digital recording methods (voices and instruments), mixing and the advanced skills needed to create professional recordings (CDs, DVDs, internet sound files, etc.). Further emphasis on electronic music production, computer based applications, MIDI, and contemporary studio hardware.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 321, 'Studio Instruction', 1, '{}', 'Secondary or minor instrument instruction. Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1/2 hour per week. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 327, 'Studio Instruction', 4, '{}', 'Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1 hour plus studio class weekly. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 337, 'Studio Instruction: Collaborative Piano', 2, '{}', 'Repertoire to be selected from vocal and instrumental literature. 1 hour lesson per week. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 340, 'Intermediate Conducting', 2, '{}', 'Further study in conducting of instrumental and choral music, including score reading, gesture and interpretation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 360, 'Musicianship for Singers', 1, '{}', 'Covers reading in the treble and bass clefs (other clefs may be covered time permitting), conducting, intervallic patterns, harmonic outlining, rhythm, dictation, sight reading melodies, musical preparation of repertoire, score scanning and score marking. Additional topics may be addressed as specific student skills and goals are assessed. Assignments are sung and drilled with movable do solfeggio and the takadimi rhythm solmization system. All of the repertoire studied and performed are vocal literature, including: art song, opera, oratorio, musical theatre, jazz, popular.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 370, 'Music Theatre V', 2, '{}', 'Interpretational study of advanced acting and musical theatre performance techniques. Includes exploration of several historical and contemporary music styles in musical theatre, applying best practices for research, solo and ensemble performance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 373, 'Music Theatre Performance and Production', 1, '{}', 'Participation in music theatre and opera productions, including auditioning for productions, adhering to departmental policies and completing assigned technical hours.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 374, 'Music Theatre Technical Production', 1, '{}', 'In-depth study of approaches to technical music theatre. Classroom study of costumes, sets and lighting (Section 1 Topic: Foundations) is followed by practical experience on a technical running crew for a music theatre and opera production (Section 2 Topic: Practicum). The two sections should be taken in consecutive semesters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 388, 'Collaborative Piano', 1, '{}', 'Performance class for collaborative piano majors. Repertoire includes vocal and instrumental selections. Advanced analysis of ensemble techniques, style and performance practice in preparation for graduate study or professional application.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 417, 'Applied Jazz Improvisation', 2, '{}', 'Emphasizes listening, analysis, and performance of advanced jazz literature and composition in contemporary styles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 438, 'The Enterprising Musician', 3, '{}', 'Familiarizes students with concepts of entrepreneurship and explores good business practices. The broad objectives of the course are to understand the importance of entrepreneurial thinking and opportunity recognition in the music industry; and to provide to the students the concepts, capabilities and information to achieve a desired career in music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 439, 'Music Product Creation and Development', 3, '{}', 'Processes and proven methodologies of new music product creation and development. Assesses and evaluates the quality of different models and techniques through case studies and by talking to potential customers. Develops a business model canvas for a personal new music product idea or project. Taught in conjunction with the online Udacity course How to Build a Start Up, combined with experiential learning in and out of the classroom. Students should have a basic idea of a business model for a startup or project they will pursue throughout the duration of the course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 440, 'Keyboard Harmony', 1, '{}', 'Performance-oriented class emphasizing chord progressions, harmonization, figured bass realization, stylistic improvisation, transposition, open score reading, and sight reading.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 441, 'Popular Music Ensemble', 1, '{}', 'Focuses on one or several popular music styles involving in-depth study and application of specialized, professional skills in performing with two or more players of like or unlike instruments and/or voice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 442, 'Instrumental Ensembles', 2, '{}', 'Students enhances musical diversity and artistic depth by participating in numerous ensembles as well as other artistic and educational projects. Each project focuses on specific pedagogical and/or musical learning outcomes. Together they comprise a rich assortment of musical experiences and promote the primary course objective. The curriculum accommodates the distinctive needs of each student and is tailor-made to each individual's career aspirations. Consequently, learning outcomes are highly individualized and student driven. This ensemble is available to both music majors and non-majors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 443, 'Choral Ensembles', 2, '{}', 'Active, thoughtful participation in music ensembles can provide for development beyond the possibilities of individual study alone. The ensemble setting is, in part, a forum for the synthesis of component parts of a complete music education. Includes the study and performance of diverse musical expressions that represent a variety of historical and cultural perspectives.These ensembles provide the opportunity to prepare and present varied music composed for the mixed choir from the major stylistic periods with the intent of serving composers, performers and listeners by performing choral music of the highest possible quality. This ensemble is available to both music majors and non-majors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 444, 'Maroon and Gold Concert Bands', 2, '{}', 'Open to all ASU students. Composed of students who are music majors, as well as those pursuing other fields of study, the group rehearses and performs mainly traditional band literature, giving one or two major performances per semester. Students must audition with the band staff for seating placement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 448, 'Classical Style: Performance and Analysis', 2, '{}', 'Integrates historical and theoretical analysis with performance, examining classical period repertoire.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 449, 'The Unaccompanied Music of JS Bach: Performance and Analysis', 2, '{}', 'Integrates historical and theoretical analysis with performance, examining the unaccompanied works of J.S. Bach.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 450, 'Choral Union', 2, '{}', 'Large mixed ensemble focuses on the study, synthesis and performance of major choral masterworks while gaining in-depth understanding of significant choral repertoire and historical ramifications. This ensemble is available to both music majors and non-majors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 451, 'Repertoire', 2, '{}', 'In-depth study of music for performing, involving advanced interpretive and technical concepts from historical, stylistic, and performance-related perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 453, 'Art Song Repertoire', 2, '{}', 'A survey of repertoire for one voice and piano beginning with Haydn and continuing through the present day. Focuses on the core Art Song Repertoire of Germany and France, but also explores a variety of composers, eras and regions, including Italy, Spain, England and America.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 454, 'Song Literature', 2, '{}', 'American, Russian, Spanish, Scandinavian, and contemporary song.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 455, 'Arizona Statesmen', 2, '{}', 'Membership in this ensemble involves the in-depth study and application of specialized musical skills in the performance of a variety of historically and musically standard male repertoire, within historical context. This ensemble is available to both music majors and non-majors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 457, 'Sol Singers', 2, '{}', 'Membership in this ensemble involves the in-depth study and application of specialized musical skills in the performance of historically and musically important repertoire for treble voices. This ensemble is available to both music majors and nonmajors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 458, 'ASU Gospel Choir', 2, '{}', 'The ASU Gospel Choir is a vocal ensemble open to students and community members regardless of their faith. The ensemble performs a variety of song forms drawn from the African American religious tradition, including traditional and contemporary gospel, anthems, hymns and spirituals. The Gospel Choir maintains an active performance schedule both on and off campus.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 459, 'Barrett Choir', 2, '{}', 'The Barrett Choir is an opportunity for students from all over the university, but primarily from Barrett, The Honors College, to participate in a high-quality choral ensemble. The group seeks to harness the creativity and intellect of honors students to create a collaborative ensemble that operates on a unique model of student-centered learning. The repertory, mostly chosen by the members, is highly varied, ranging from world music to pieces in the great choral tradition to newer music and popular music. There are normally two major concerts per semester, with some occasional smaller performances. This ensemble is available to both music majors and non-majors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 460, 'The Visiting Quartet Residency Program', 2, '{}', 'Develops skills as a chamber musician and provides opportunities to work with members of the visiting string quartet in residence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 461, 'Marching and Athletic Bands', 1, '{}', 'Development of specific professional performing skills through staging of formations and drills for football games and other events. Explores a wide variety of musical repertoire from current top 40 hits to classical. These ensembles are available to both music majors and nonmajors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 465, 'Music Theatre VI', 2, '{}', 'Further interpretational study of advanced acting and musical theatre performance techniques. Includes exploration of historical and contemporary styles in musical theatre, applying best practices for research, solo and ensemble performance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 466, 'Broadway Dance', 2, '{}', 'Designed for students in the collaborative performance areas of musical theatre (music, theater and dance). Examines the development of landmark styles and theories specifically in musical theater and Broadway dance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 467, 'Music Theatre Dance Technique', 1, '{}', 'Students gain a basic understanding of body awareness and alignment, strength and technique with a focus on musical theatre vocabulary as well as basic turns and leaps. The first semester students focus on the individual dancer, spatial awareness, development of character and intention. The second semester students work on basic partnering skills and partner dances. Intended to be a full-year course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 470, 'Music Theatre VII', 2, '{}', 'Development and practice of professional music theatre skills in solo and ensemble performance with an emphasis on applied skills and industry immersion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 471, 'Music Theatre: Workshops', 1, '{}', 'The development of specific skills for musical-dramatic interpretation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 472, 'Music Theatre VIII', 2, '{}', 'Further development and practice of professional music theatre skills in solo and ensemble performance with an emphasis on applied skills and industry immersion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 476, 'New Music Ensemble', 1, '{}', 'In-depth study, synthesis, and performance of classical works written within the last twenty years for small ensemble and for instruments with electronics. Involves discussion and realization of advanced interpretive and technical concepts related to contemporary music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 479, 'Chamber Music Ensembles', 1, '{}', 'Faculty-coached ensembles involving in-depth study and application of specialized, professional skills in performing with two or more players of like or unlike instruments and/or voice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 481, 'Performance Pedagogy and Materials', 2, '{}', 'Principles and methods of performance techniques for each performance field. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 482, 'Piano Pedagogy', 2, '{}', 'Continuation of MUP 481 (Piano). Problems and techniques of teaching intermediate to advanced piano students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 485, 'Percussion Ensemble', 1, '{}', 'Rehearsal and performance of standard and original repertoire for the percussion ensemble and related instruments. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 486, 'Jazz Band', 1, '{}', 'Analysis, rehearsal and performance of new, traditional, and Latin literature for jazz ensembles. Development of advanced sight-reading and instrumental doubling skills. 4 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 487, 'Collaborative Piano', 1, '{}', 'Piano accompaniments found in vocal and instrumental literature; discussion of styles and performance practices; experience in public performance. 2 hours per week. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 489, 'Ethnomusicology Ensembles', 1, '{}', 'Students apply indigenous music theories to actual practice, contextualized in background reading and listening, as they refine their ability to perform orally generated music and sometimes dance traditions. This ensemble is available to both music majors and nonmajors, and may require an audition.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 495, 'Performance', 0, '{}', 'For candidates of a BM degree in which 1/2 recital is a requirement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 496, 'Performance', 0, '{}', 'For candidates of a BM degree in which a full recital is a requirement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 503, 'Literature for Teaching Voice', 1, '{}', 'Equips students with knowledge of repertoire for teaching applied voice, and to cultivate skills in mindfully assigning said repertoire. Topics and activities include learning about the types of repertoire across many styles, pedagogical implications of repertoire, benefits and pitfalls in assigning repertoire to the developing singer, and assignment of repertoire to true and hypothetical students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 504, 'Voice Teaching Practicum', 1, '{}', 'Provides opportunities to develop practical teaching skills through experiential learning for voice and/or music education graduate students who have a strong interest in teaching applied voice lessons. Topics and activities include teaching, teaching observations, guided teaching activities, and the creation of a teaching portfolio.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 507, 'Group Piano Practicum', 2, '{}', 'Curricula, materials, and teaching techniques for group teaching at the university and community college levels. Observation/supervised teaching in group piano.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 508, 'Studio Observation', 1, '{}', 'Weekly observation of studio teaching by various piano faculty. Paper as final requirement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 509, 'Jazz Keyboard Harmony', 1, '{}', 'Emphasizes jazz chords and chord progressions, harmonization, voicing, and analysis of transcriptions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 510, 'Jazz Keyboard Harmony', 1, '{}', 'Continuation of emphasis on jazz chords and chord progressions, harmonization, voicing, and analysis of transcriptions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 511, 'Studio Instruction', 2, '{}', 'Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1 hour plus studio class weekly. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 517, 'Advanced Improvisation', 1, '{}', 'Improvisation techniques within the context of advanced jazz literature. Must be taken in sequence with MUP 518.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 521, 'Studio Instruction', 1, '{}', 'Secondary or minor instrument instruction. Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1/2 hour per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 527, 'Studio Instruction', 4, '{}', 'Bassoon, cello, clarinet, contrabass, cornet, euphonium, flute, guitar, harp, harpsichord, horn, oboe, organ, percussion, piano, saxophone, trombone, trumpet, tuba, viola, violin, voice. Minimum contact of 1 hour per week. May be repeated for credit. May not be taken for audit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 538, 'The Enterprising Musician', 3, '{}', 'Familiarizes students with concepts of entrepreneurship and explores good business practices. The broad objectives of the course are to understand the importance of entrepreneurial thinking and opportunity recognition in the music industry; and to provide to the students the concepts, capabilities and information to achieve a desired career in music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 539, 'Music Product Creation and Development', 3, '{}', 'Processes and proven methodologies of new music product creation and development. Assesses and evaluates the quality of different models and techniques through case studies and by talking to potential customers. Develops a business model canvas for a personal new music product idea or project. Taught in conjunction with the online Udacity course How to Build a Start Up, combined with experiential learning in and out of the classroom. Students should have a basic idea of a business model for a startup or project they will pursue throughout the duration of the course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 540, 'Principles of Integrated Conducting', 2, '{}', 'Score preparation and conducting techniques for instrumental music. Concentration on study of historical styles. Required of DMA students in Instrumental Music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 541, 'Song', 2, '{}', 'Seminar on solo song from its beginning to the present day.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 542, 'Instrumental Ensembles', 2, '{}', 'Students participate in numerous ensembles and artistic and educational projects that together, comprise a rich assortment of musical experiences. Graduate students especially need opportunities to hone their skills in a great variety of performance opportunities. During the academic year, each student participates in a variety of projects that present a wide range of musical experiences. The curriculum is designed to accommodate the distinctive needs of each student in that artistic-educational experiences are, to a large extent, tailor made to each individual's career aspirations. Consequently, learning outcomes are highly individualized and student driven, rather than institutionally convenient. Each artistic-educational project focuses on specific pedagogical and/or musical learning outcomes that, in various ways, promote the primary course objective: the enhancement of musical diversity and artistic depth in every student. Audition required during the first week of classes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 543, 'Choral Ensembles', 2, '{}', 'Active, thoughtful participation in music ensembles can provide for development beyond the possibilities of individual study alone. The ensemble setting is, in part, a forum for the synthesis of component parts of a complete music education. Includes the study and performance of diverse musical expressions that represent a variety of historical and cultural perspectives. These ensembles provide the opportunity to prepare and present varied music composed for the mixed choir from the major stylistic periods with the intent of serving composers, performers and listeners by performing choral music of the highest possible quality.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 548, 'Classical Style: Performance and Analysis', 2, '{}', 'Integrates historical and theoretical analysis with performance, examining classical period repertoire.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 549, 'The Unaccompanied Music of JS Bach: Performance and Analysis', 2, '{}', 'Integrates historical and theoretical analysis with performance, examining the unaccompanied works of J.S. Bach.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 550, 'Choral Union', 2, '{}', 'Open to all students in the university and to interested singers in the community by audition. Preparation and performance of the larger choral works. 2 hours per week. An audition is required to participate in the ensemble.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 551, 'Repertoire', 2, '{}', 'Literature available for performance in all performing media.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 554, 'Major Repertoire, Score Study, and Performance Practice', 3, '{}', 'Conducting course comprising a survey of significant repertoire (encompassing, over multiple semesters, literature from choral, musical theater/opera, orchestral, and wind band genres); an introduction to historical performance practice; and a range of score study techniques. Conducting majors are required to enroll each semester (or as advised) and, subsequently: display a comprehensive historical knowledge of the repertoire and development of ensembles over the past 500 years, demonstrate command of standard performance practices with respect to each historical era, and exhibit a clear understanding of varied methods of score study.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 555, 'Arizona Statesmen', 2, '{}', 'Rehearsal and performance of music for mixed voices. 3 hours per week.  An audition is required to participate in the ensemble.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 557, 'Sol Singers', 2, '{}', 'Rehearsal and performance of music for mixed voices. 2 hours per week. An audition is required to participate in the ensemble.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 558, 'ASU Gospel Choir', 2, '{}', 'The ASU Gospel Choir is a vocal ensemble open to students and community members regardless of their faith. The ensemble performs a variety of song forms drawn from the African American religious tradition, including traditional and contemporary gospel, anthems, hymns and spirituals. The Gospel Choir maintains an active performance schedule both on and off campus.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 560, 'The Visiting Quartet Residency Program', 2, '{}', 'Develops skills as a chamber musician and provides opportunities to work with members of the visiting string quartet in residence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 561, 'Marching and Concert Bands', 1, '{}', 'Staging of formations and drills for football games and other events (fall); masterpieces of symphonic band literature (spring). Meets MWF.  An audition may be required to participate in the ensemble.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 565, 'Acting and Movement', 2, '{}', 'Process class with exercises specifically developed to assist the student actor to learn the fundamental techniques of acting with an emphasis on the actor's approach to characterization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 566, 'Broadway Dance', 2, '{}', 'Designed for students in the collaborative performance areas of musical theatre (music, theater and dance). Examines the development of landmark styles and theories specifically in musical theater and Broadway dance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 567, 'Music Theatre Dance Technique', 1, '{}', 'Students gain a basic understanding of body awareness and alignment, strength and technique with a focus on musical theatre vocabulary as well as basic turns and leaps. The first semester focuses on the individual dancer, spatial awareness, development of character and intention. The second semester works on basic partnering skills and partner dances. Intended to be a full year.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 570, 'Music Theatre: Techniques', 1, '{}', 'Exercises and improvisations for the singing actor emphasizing body awareness, isolations, and freedom of the vocal and breath mechanisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 571, 'Music Theatre: Workshops', 1, '{}', 'Develops specific skills for the musical-dramatic interpretation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 573, 'Music Theatre: Performance', 1, '{}', 'Participation in Lyric Opera Theatre productions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 574, 'Music Theatre Technical Production', 1, '{}', 'Introduces technical music theatre. Classroom study of costumes, sets and lighting (Section 1, Topic: Tech) followed by practical experience on a technical running crew for a Lyric Opera Theatre production (Section 2 Topic: Problems). The two sections should be taken in consecutive semesters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 576, 'New Music Ensemble', 1, '{}', 'Rehearsal and performance of music written in the last 20 years.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 579, 'Chamber Music Ensembles', 1, '{}', 'String, brass, woodwind, percussion, keyboard, vocal, and mixed ensembles. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 581, 'Performance Pedagogy and Materials', 2, '{}', 'Principles and methods of performance techniques for each performance field.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 582, 'Anatomy and Physiology of Singing', 3, '{}', 'Detailed study of the anatomy, physiology, neurology and acoustics of the singing voice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 585, 'Percussion Ensemble', 1, '{}', 'Rehearsal and performance of standard and original repertoire for the percussion ensemble and related instruments. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 586, 'Jazz Band', 1, '{}', 'Rehearsal and performance of new, traditional, and Latin literature for jazz bands. 4 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 587, 'Ethnomusicology Ensembles', 1, '{}', 'Performance learning experience for the music of various cultures of the world.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 588, 'Collaborative Piano', 1, '{}', 'Piano accompaniments found in vocal and instrumental literature; discussion of styles and performance practices; experience in public performance. 2 hours per week.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 596, 'Solo Performance', 1, '{}', 'May be full recital, major operatic role, solo performance with orchestra, ensemble, or lecture recital.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 597, 'Solo Performance', 1, '{}', 'See MUP 596.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 684, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 727, 'Studio Instruction', 4, '{}', 'Minimum contact of 1 hour per week. May be repeated for credit. Conducting students register for 2 semester hours; all other students register for 4 semester hours.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 751, 'Seminar in Piano Literature', 2, '{}', 'In-depth study of selected topics related to the standard piano literature. Requires research paper, bibliography, class presentation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 784, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 791, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 796, 'Solo Performance', 15, '{}', 'May be full recital, major operatic role, solo performance with orchestra, ensemble, or lecture recital.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUP', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSI', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSI', 601, 'Contemporary Topics in Music Research', 2, '{}', 'Required for all PhD in Music students. Content includes readings and discussions of topics drawn from the arts and other disciplines that impact and interact with music, music practices, and music research. Requires literature review or research paper.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSI', 602, 'Current Issues in Music Research', 2, '{}', 'Required for all PhD in Music students. Content includes readings and discussions of topics drawn from current public policies, events, and popular culture that impact and interact with music, music practices, and music research. Requires literature review or research paper.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MSI', 755, 'Historical Research in Music', 3, '{}', 'Required for all PhD in Music students. Knowledge and insights related to conducting historical research in music. Includes development of a mini-proposal for a dissertation on the history of music education, or draft of a historical research paper suitable for presentation or publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 123, 'Beginning Composition', 1, '{}', 'Intended for freshmen and sophomores in the composition concentration. Introduces composing. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 125, 'Basic Music Theory', 3, '{}', 'Notation, scales, keys, modes, intervals, chords, basic part writing and composition. Development of related aural skills through sightsinging and dictation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 221, 'Music Theory: 18th Century', 3, '{}', 'Styles, techniques, and idioms of 18th-century music; emphasis on analysis, composition (part writing), and related aural skills, with applications for performance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 222, 'Music Theory: 19th Century', 3, '{}', 'Styles, techniques, and idioms of 19th-century music; emphasis on analysis, composition (part writing), and related aural skills, with applications for performance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 223, 'Music Theory: 20th Century', 3, '{}', 'Styles, techniques, and idioms of 20th-century music; emphasizes innovative treatments of musical elements, analysis, and composition; related aural skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 224, 'Arranging for Popular Musicians', 3, '{}', 'Explores the contemporary practice of composition, arrangement and notation for popular music ensembles and productions. Through the study of scores, recordings and transcriptions, students gain an understanding of contemporary techniques. From this study, they develop their own ideas and apply understanding through original work.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 225, 'Popular Music Theory', 3, '{}', 'Builds upon the broad foundation of music theory built in previous courses, shifting focus toward engaging with music theory through the lens of contemporary popular music and specific styles/genres. Explores concepts of harmony, melody, rhythm, form and arrangement as it pertains to a popular music repertoire and deepens understanding of the tools in context of students' own musicianship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 320, 'Modal Counterpoint', 2, '{}', 'Counterpoint based on 16th-century vocal polyphonic style.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 321, 'Tonal Counterpoint', 2, '{}', 'Counterpoint based on 18th-century polyphonic style.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 323, 'Composition', 3, '{}', 'Writing music compositions, with emphasis on basic techniques and smaller structures. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 327, 'Intermediate Form and Analysis', 3, '{}', 'Organizing elements in the most important contrapuntal and homophonic musical forms from the Renaissance through the 19th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 415, 'Scoring for Instrumental Jazz Ensembles I', 2, '{}', 'Analyzes scores and recordings, application of orchestration and four and five-part harmonization techniques frequently used in arranging for the contemporary jazz ensembles and studio orchestra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 416, 'Scoring for Instrumental Jazz Ensembles II', 2, '{}', 'Analyzes scores and recordings, application and creation of new arrangements for contemporary jazz ensembles and/or studio orchestra.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 422, 'Musical Acoustics', 3, '{}', 'Properties of sound and tone. Harmonic series, instruments, the ear, auditorium acoustics, and the reproduction of sound. Assumes a thorough knowledge of musical notation, intervals, scales, and harmony, or 2 years of music theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 425, 'Studies in 20th- and 21st-Century Theory', 3, '{}', 'Continued development of analytical techniques and aural skill, with an examination of theoretical systems applicable to 20th- and 21st-century music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 428, 'Advanced Form and Analysis', 3, '{}', 'Organizing principles of the large forms of musical composition in the 19th and 20th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 429, 'Canon and Fugue', 3, '{}', 'Writing of canons and fugues in tonal style.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 430, '20th- and 21st-Century Counterpoint', 3, '{}', 'Counterpoint studies utilizing 20th- and 21st-century idioms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 433, 'Orchestration', 3, '{}', 'Studies scoring music for full and chamber orchestras; examines individual orchestral instruments (characteristics and performance techniques). Requires a working knowledge of a computer music notation program.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 436, 'Electronic Studio Techniques I', 2, '{}', 'The study of computer music systems emphasizing advanced audio synthesis and resynthesis as well as their application to professional compositional environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 437, 'Electronic Studio Techniques II', 2, '{}', 'Principles of digital electronic music systems and their applications in the composition of electronic music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 440, 'Jazz Theory and Ear Training', 2, '{}', 'Advanced study of jazz harmonic systems. Daily oral drills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 441, 'Jazz Composition', 3, '{}', 'Creative writing in the smaller forms and in the idiom of jazz.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 495, 'Final Project', 0, '{}', 'Half recital of compositions or approval of a large-scale composition or a research paper.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 496, 'Theory Project', 3, '{}', 'Supervised individual writing project dealing with music theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 500, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 516, 'Baroque Music', 3, '{}', 'Detailed analysis of selected examples of music from the Baroque period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 517, 'Classic Music', 3, '{}', 'Detailed analysis of selected examples of music from the Classic period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 518, 'Romantic Music', 3, '{}', 'Detailed analysis of selected examples of music from the Romantic period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 519, 'Late 19th-Early 20th Century Music', 3, '{}', 'Detailed analysis of selected examples of music from the late 19th and early 20th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 520, 'Analytical Techniques', 3, '{}', 'Analytical techniques systematically applied to music. Concentration on structural and compositional procedures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 523, 'Advanced Composition', 3, '{}', 'Advanced music composition, including complex techniques and larger structure. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 525, 'Pedagogy of Theory', 3, '{}', 'Practices and principles of teaching music theory. Emphasizes most desirable and practical offerings possible. Comparative studies of existing practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 535, 'Survey of Rhythm and Meter Studies', 3, '{}', 'Exposes students to seminal research in rhythm and meter studies with the aim of introducing a range of methodologies and common issues found in this area of research. Reading assignments include music theory and music cognition literature relating to questions of rhythm and meter in diverse repertoires, including classical, post-tonal, world, and popular music genres. Students reflect on the strengths and weaknesses of various approaches, analytical techniques, and the implications that theories of rhythm and meter have for performance practice and write and present a final research paper on a topic relating to rhythm and meter studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 540, 'Jazz Theory and Ear Training', 3, '{}', 'Surveys some of the most influential trends in the jazz tradition with in-depth analysis of harmonic systems, linear elements, and compositional strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 723, 'Advanced Composition', 3, '{}', 'Special problems in writing in complex forms and textures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MTC', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 211, 'Introduction to Digital Sound', 3, '{}', 'Examines how technology is used as listeners, creators, critics, remixers, and a wide variety of other roles to participate in and gain a deeper understanding of music and its role in our lives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 311, 'Composing and Performing for Hybrid Ensembles', 3, '{}', 'Provides opportunities to perform music in a variety of digital and hybrid contexts using both physical and digital instruments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 411, 'Advanced Interactive Sound', 3, '{}', 'Explores the development of interactive music systems. Covers a variety of sound synthesis methods with particular emphasis as to how they can be used in real-time sound creation. Covers techniques for sensing such as computer vision, sound analysis, tangible interfaces and 3D sensing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 511, 'Advanced Interactive Sound', 3, '{}', 'Explores the development of interactive music systems. Covers a variety of sound synthesis methods with particular emphasis as to how they can be used in real-time sound creation. Covers techniques for sensing such as computer vision, sound analysis, tangible interfaces and 3D sensing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MDC', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('NAN', 505, 'Nanoscience and Society', 2, '{}', 'Integrates nanoscience and nanotechnology with potential societal issues involved in the students' NAN 593 Applied Projects. Coordinated by the faculty and research staff at the Center for Nanotechnology in Society.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('NAN', 506, 'Innovation and IP Management', 2, '{}', 'Intellectual Property (IP) Management in the context of innovation (research, regulations, manufacture, access to markets, and trade) with emphasis on strategies, public-private interface, licensing, and technology transfer.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('NAN', 511, 'Materials Physics I', 3, '{}', 'Fundamentals of materials physics: crystal structure, diffraction, elasticity, point defects, dislocations, lattice vibrations, thermal properties, periodic potential, band structure.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('NAN', 512, 'Materials Physics II', 3, '{}', 'Electronic behavior of materials: energy bands, electronic properties, metals, semiconductors, insulators, optical properties, magnetic properties, superconductivity, biophysics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('NAN', 542, 'Topics in Biophysics', 3, '{}', 'Overview of modern biology, length scales: emphasizes molecular and cellular biology. Nonequilibrium systems: compare and contrast stochastic processes in biological and physical systems.');
