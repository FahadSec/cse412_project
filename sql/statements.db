INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 502, 'Managerial Economics', 4, '{}', 'Applies microeconomic analysis to managerial decision making in areas of demand, production, cost, and pricing. Evaluates competitive strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 503, 'Global Business Environment', 4, '{}', 'Macroeconomic analysis of issues related to economic growth, inflation, interest rates behavior, unemployment, exchange rate determination, and global competitiveness.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 504, 'Fundamentals of Managerial Economics', 3, '{}', 'Applies microeconomic analysis to managerial decision making in areas of demand, production, cost and pricing. Evaluates competitive strategies. Designed for students with limited business experience and without an undergraduate business degree.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 510, 'Microeconomic Theory and Applications', 4, '{}', 'Applies economic theory to production, consumer demand, exchange, and pricing in a market economy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 525, 'Applied Regression Models', 3, '{}', 'Simple linear regression, multiple regression, indicator variables, and logistic regression. Emphasizes business and economic applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 527, 'Categorical Data Analysis', 3, '{}', 'Discrete data analysis in business research. Multidimensional contingency tables and other discrete models.');
