INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 101, 'Introductory  Chemistry', 4, '{'SQ'}', 'Elements of general chemistry. Adapted to the needs of students in nursing and kinesiology and those preparing for general chemistry. Recommended for General Studies credit. Cannot be used for major credit in chemical or biochemical sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 107, 'Chemistry and Society', 3, '{'SQ'}', 'General chemical principles and concepts presented in context of social and technological issues, e.g., energy, pollution, global warming, and others. This lecture course may be combined with the laboratory CHM 108 for those interested in lab course credit. Cannot be used for major credit in chemical or biochemical sciences. Both CHM 107 and CHM 108 must be taken to secure SQ credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 108, 'Chemistry and Society Laboratory', 1, '{'SQ'}', 'Experiments in chemical principles presented in chemistry and society lectures (CHM 107). Students must complete both CHM 107 and CHM 108 to receive an SQ (General Studies).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 111, 'General Chemistry Laboratory for Majors I', 1, '{'SQ'}', 'Laboratory course in general chemistry for chemistry and biochemistry majors intended to complement CHM 117 lecture course. Covers a diverse range of laboratory experiments appropriate to chemistry and biochemistry majors designed to reinforce chemical principles and to develop a broad range of chemical synthesis skills. Uses modern analytic and characterization techniques to study the physical and chemical properties of substances. Suitable for students who have completed introductory chemistry at the high school level. Both CHM 117 and CHM 111 must be taken to secure SQ General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 112, 'General Chemistry Laboratory for Majors II', 1, '{'SQ'}', 'Continuation of CHM 111 laboratory course in general chemistry for chemistry and biochemistry majors intended to complement CHM 118 lecture course. Covers a diverse range of laboratory experiments appropriate to chemistry and biochemistry majors designed to reinforce chemical principles and to develop a broad range of chemical synthesis skills. Uses modern analytic and characterization techniques to study the physical and chemical properties of substances. Both CHM 118 and CHM 112 must be taken to secure SQ General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 113, 'General Chemistry I', 4, '{'SQ'}', 'Principles of chemistry. Adapted to the needs of students in the physical, biological, and earth sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 114, 'General Chemistry for Engineers', 4, '{'SQ'}', 'Chemical principles with emphasis toward engineering. Students without high school chemistry or are chemical engineering majors must enroll in the CHM 113, 116 sequence instead of CHM 114.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 116, 'General Chemistry II', 4, '{'SQ'}', 'Continuation of CHM 113. Equilibrium theory, thermodynamics, kinetics, electrochemistry, nuclear chemistry, descriptive chemistry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 117, 'General Chemistry for Majors I', 3, '{'SQ'}', 'Lecture course in general chemistry for chemistry and biochemistry majors and is complemented by the CHM 111 laboratory course. Covers electronic structure of atoms and molecules, intermolecular forces, bonding models, molecular geometry, physical states of matter, gas laws, and chemistry of condensed matter and modern materials. Highlights chemical reactivity, periodic trends and descriptive chemistry. Both CHM 111 and CHM 117 must be taken to secure SQ General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CHM', 118, 'General Chemistry for Majors II', 3, '{'SQ'}', 'Continuation of CHM 117 lecture course in general chemistry for chemistry and biochemistry majors and is complemented by the major's CHM 112 laboratory course. Topics include thermodynamics, properties of solutions, chemical equilibrium, aqueous chemistry, acids and bases, electrochemistry, chemical kinetics, nuclear chemistry, coordination chemistry. Both CHM 118 and CHM 112 must be taken to secure SQ General Studies credit.');
