INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 601, 'Job Market Workshop: Designing Your Future', 3, '{}', 'Focuses on the professional training of graduate students who pursue an academic or alt-ac (alternative academic) job. The material enables students to learn more about their professional opportunities and improve their chances on the job market. In a workshop format, some of the professionalization skills may include conference presentations, increasing professional digital presence, using job search tools, applying for internships and postdocs, writing job letters and grant proposals, managing stress, preparing for interviews, interviewing, presenting job talks and teaching demonstrations, and creating work/life balance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 602, 'Advanced Studies in Theory and/or Criticism', 3, '{}', 'Seminar courses on the principles, strategies, and applications of critical, cultural, and/or literary theory and/or criticism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 603, 'Advanced Studies in Comparative Literature', 3, '{}', 'Seminar courses on the problems, methods, and principles of comparative analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 604, 'Interdisciplinary Cultural Studies', 3, '{}', 'Seminar courses on work from literature, anthropology, and/or other disciplines, with an emphasis on cultural influences and functions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 606, 'Advanced Studies in English Education', 3, '{}', 'Current research, issues, and trends in English education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 625, 'Advanced Studies in Second Language Writing', 3, '{}', 'Provides opportunities to explore advanced research topics as well as current issues and controversies in the field of second language writing--an interdisciplinary field of inquiry simultaneously situated in applied linguistics, rhetoric and composition, and teachers of English to Speakers of Other Languages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 632, 'Advanced Studies in Medieval and Renaissance Literature and Culture', 3, '{}', 'Seminar in works of the Medieval or Renaissance periods, studied in their cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 635, 'Advanced Studies in British Literature', 3, '{}', 'Seminar courses on works produced in or about England, Scotland, and Wales, studied in their cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 636, 'Advanced Studies in American Literature', 3, '{}', 'Seminar courses on works produced in or about the United States, studied in their cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 639, 'Advanced Studies in Modernism and Postmodernism', 3, '{}', 'Seminar courses on topics in Modernist and Postmodernist studies. May include literary and theoretical texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ENG', 642, 'Advanced Studies in Ethnic, Anglophone, or Post-Colonial Literatures', 3, '{}', 'Seminar courses that consider the influence of cultural or geopolitical developments on the production and circulation of texts.');
