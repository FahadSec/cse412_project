INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 131, 'Design and Human Behavior', 3, '{'SB'}', 'Applies conceptual design to issues of programming and space planning, user needs, and behavior.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 223, 'Drafting for Interior Design', 3, '{}', 'Orthographic, paraline, axonometric, and perspective projection; shades and shadows; and basic descriptive geometry for interior designers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 253, 'Interior Materials, Finishes, and Specifications', 3, '{}', 'Survey, evaluation, and specification of materials, finishes, and performance criteria for interiors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 261, 'Interior Design Studio I', 3, '{}', 'Studio problems in interior design related to behavioral response in personal and small-group spaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 262, 'Interior Design Studio II', 3, '{}', 'Focuses on environments that entertain or host the public, including but not limited to nightclubs, restaurants, theaters, hotels, city and country clubs, golf facilities, cruise ships and conference facilities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 310, 'History of Interior Design I', 3, '{'HU','H'}', 'Evolution of interior design as an expression of cultural influences from the ancient periods to 1835. Analysis of interior architecture, planning and lighting of interior spaces, and furniture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 311, 'History of Interior Design II', 3, '{'HU','H'}', 'Evolution of interior design as an expression of cultural influences 1835 to present. Analysis of interior architecture, planning and lighting of interior spaces, and furniture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 341, 'Interior Codes: Public Welfare and Safety', 2, '{}', 'Codes and regulations as performance criteria for interior design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('INT', 351, 'Ambient Environment', 3, '{}', 'Surveys environmental control systems, acoustics, and lighting issues.');
