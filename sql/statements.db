INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APH', 414, 'Great Cities', 3, '{'(L','HU)','H'}', 'Historical survey of great cities from antiquity to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APH', 421, 'First Concepts: What is... The Writing, Philosophy, and Culture of Architecture', 3, '{'L','HU'}', 'Examines the major writings and concepts that surround the history of architecture. Focuses on (but not exclusively) work in the 20th century.');
