INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASB', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASB', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASB', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASB', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASB', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 100, 'Discovering the Professions', 1, '{}', 'Focuses on learning and engaging with information and tools that provide the basis for successful life/career planning decisions. Also designed to assist students in balancing career preparation with academic success.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 130, 'Introduction to Environmental Science', 4, '{'SQ'}', 'Introduces the interconnected nature of Earth's hydrosphere, lithosphere (soils and rocks), atmosphere, and biosphere.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 200, 'Building Your Professional Self', 0.5, '{}', 'Focuses on developing a professional narrative, prepares students for the expectations of upper-division coursework and covers how to effectively use online resources resulting in developing the professional self.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 207, 'Applied Plant Taxonomy', 3, '{}', 'Introduces identification of vascular plants emphasizing seed plants. Surveys seed plant families.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 225, 'Soils', 3, '{'SQ'}', 'Fundamental physical, chemical and biological properties of soils as related to sustainable natural resource management. Both ABS 225 and ABS 226 must be taken to secure General Studies SQ credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 226, 'Soils Laboratory', 1, '{'SQ'}', 'Selected exercises to broaden the background and understanding of basic soil principles. Both ABS 225 and ABS 226 must be taken to secure General Studies SQ credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 260, 'Fundamentals of Sustainable Horticulture', 4, '{'SG'}', 'Principles and practices of horticulture, emphasizing development, growth, and propagation of horticultural plants and environmental factors that affect these processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 270, 'Sustainable Biological Systems', 3, '{}', 'Ecological foundations of sustainable biological systems, anthropogenic impacts on ecosystem structure and function, and ecological risk assessment. Examines climate change, water scarcity, deforestation, soil degradation, and human displacement on global ecosystem functions and utilization of natural resources.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 271, 'Veterinary Medicine Today', 3, '{}', 'Introduces the role of the veterinarian as related to the fields of food supply and veterinary medicine.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 274, 'Introduction to Wildlife Management', 4, '{}', 'Fundamentals of wildlife conservation and management through lectures, guest speakers and lab activities. Divided into three parts: (1) introduction to conservation, management and the scientific method; (2) themes of earth, water, fire and air; and (3) wildlife techniques and field methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 300, 'Career Competencies and Skills', 0.5, '{}', 'Builds on skills developed in previous coursework. Focuses on ensuring students can articulate and give examples of their knowledge and experience in applied biological science core competencies as well as career readiness skills such as leadership, teamwork, communication, professionalism and technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 302, 'Ethical and Policy Issues in Biology', 3, '{}', 'Policy environment and ethics in the practice of biology. Covers ethical reasoning, policy formulation, and regulatory agencies with examples from biotechnology and the environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 311, 'Molecular and Cellular Biology', 3, '{}', 'Overview of the biology of the cell; emphasizes structure and function of biomolecules within the cell and their applications associated with recombinant DNA technology, genetic engineering, plant and agriculture biotechnology, and human pharmaceutical development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 312, 'Structure and Function', 3, '{}', 'Surveys structural and functional attributes of plant and animals of particular importance in the applied biological sciences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 314, 'Applied Plant Physiology', 3, '{}', 'Understanding control of plant growth and developmental processes through innovated technologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 318, 'Unseen Life on Earth', 3, '{}', 'Develops an understanding of how microbes exist in the context of both their environment and the organisms they come in contact with. Focuses on how the microbial interactions with the environment play a vital role in the multitude of biological processes that support the Earth system. Studies a myriad of extreme environments that host microbial systems, such as cloud, hydrothermal vent, acid-mine drainage and gut microbiomes. Utilizes a synthesis-driven approach where students apply their knowledge on microbial systems to discern whether life is possible on other planets. Also pays attention to the tools used to evaluate microbial diversity and function, applications of microbial ecology, and how microbes can be harnessed to improve life on Earth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 350, 'Applied Statistics', 3, '{'CS'}', 'Statistical methods with applications in the biological sciences and natural resource management. Uses computers and the Internet.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 355, 'Ecology and Adaptations of Vertebrates', 3, '{}', 'Evolution, systematics, diversity, zoology, conservation and anatomical and physiological adaptations and specializations of the vertebrates.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 360, 'Southwest Home Gardening', 2, '{}', 'Multimedia course for nonmajors surveying contemporary topics in Southwest home horticulture, including landscaping, flower and vegetable gardening, citriculture, interiorscaping, and others.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 362, 'Landscape Plants', 4, '{}', 'Identification, design, and use of plants in urban landscapes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 363, 'Sustainable Landscape Practice', 4, '{}', 'Landscape technologies and practices including landscape design theory, job costing and bidding, sprinkler and drip irrigation design, landscape installation, landscape valuation techniques, landscape ordinances, and maintenance practices of commercial and residential landscapes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 364, 'Urban Forestry', 3, '{}', 'Care, maintenance, and valuation of the urban forest, including public and private landscape codes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 365, 'Future of Agriculture: Vertical Farming', 3, '{}', 'Examines vertical farming in today's agricultural market. Students study the applied innovative technologies, suitable plants, operation management, and the future of vertical farming in modern agriculture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 368, 'Plant Propagation', 3, '{}', 'Theory and application of sexual and asexual propagation techniques. Considers plant materials used both for urban horticulture and ecological restoration applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 369, 'Hydroponic Food Crop Production', 3, '{}', 'Covers the principles and practices of hydroponic systems and their applications for indoor vertical farming and greenhouse production. Covers fundamental topics of crop production in hydroponic systems, including plant nutrition, nutrient solution, growing media, soilless cultures, system applications and planting methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 370, 'Ecology', 3, '{}', 'Interactions between organisms and their environments; structure and dynamics of populations, communities, ecosystems, and landscapes, with emphasis on vegetation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 372, 'Captive Animal Behavior Management', 3, '{}', 'Natural animal behaviors during captive animal management including wild and domestic animal training and enrichment, exhibit design, husbandry, and livestock management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 376, 'Wildlife Ecology', 3, '{}', 'Explores population biology, conservation genetics, habitat ecology, ecological communities, competition, invasive species, habitat fragmentation and connectivity, and other related topics. Students work in small groups to design, implement, and complete a field-based research project on the ASU Polytechnic campus.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 377, 'International Wildlife Conservation', 3, '{}', 'Studies wildlife around the world including biogeography, biodiversity, conservation topics and the complexity of conserving wildlife in both developed and developing nations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 378, 'Animal Nutrition', 3, '{}', 'Principles of nutrient metabolism in animal species; emphasizes understanding the interaction of animals with their environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 380, 'Restoration and Wildlife Plants', 3, '{}', 'Important wildland plants, including invasive and endangered species, wildlife food species, and species used for ecosystem restoration such as rangeland revegetation and seeding.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 381, 'Natural Resources Policy', 3, '{}', 'Policies and regulations affecting management of natural resources, with emphases on wildlife, rangeland and ecological restoration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 384, 'Natural Resources Measurements', 3, '{}', 'Inventory, sampling and monitoring techniques of natural resources. Data evaluation and interpretation as related to management, restoration and conservation needs of natural resources.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 414, 'Plant Pathology', 3, '{'L'}', 'Identification and control of biotic and abiotic factors that cause common disease problems to plants.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 417, 'Comparative Immunology', 3, '{}', 'Examines unique but also shared immunological characteristics of animals with the goal of understanding how different animals respond to microorganisms such as viruses, bacteria, fungi and parasites. Presentation of material is guided by hierarchy of structural organization, using molecules, organelles, cells, tissues, organs, organ systems, whole animals and even ecosystems and the environment to convey important concepts related to our relationships with pathogens.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 430, 'Watershed Management', 3, '{}', 'Hydrologic, physical, biological, and ecological principles applied to watershed management. Impact of ecosystem manipulations on water yield and quality.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 434, 'Soil Ecology', 3, '{}', 'Soils viewed in an ecosystem context, soil-plant relationships, nutrient budgets, and abiotic factors that influence soil processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 435, 'Applied Systems Ecology', 3, '{}', 'Simulation modeling as a tool to study ecological processes and human impact on ecosystems and organisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 440, 'Ecological Restoration Techniques', 3, '{}', 'Techniques for ecological restoration, riparian and wetland restoration, and monitoring restoration success.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 441, 'Ecological Restoration Practicum', 1, '{}', 'Field experience in the evaluation and monitoring of implemented ecological restoration projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 452, 'Commercial Applications of Fermentation', 3, '{}', 'Biochemical applications and technologies of fermentation including use of fungi and bacteria in fermentation to improve human health and immunity, and produce pharmaceutical drug supplements and medicines, foods, beverages, and alternative fuels.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 460, 'Organic Gardening', 2, '{}', 'Applies principles and practices of organic gardening in the low desert, including environmental impacts of modern food production.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 462, 'Greenhouse/Nursery Management', 4, '{}', 'Greenhouse structures, environment, and nursery operations. Includes irrigation, nutrition, and other principles relative to production of nursery crops.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 464, 'Desert Horticulture', 3, '{}', 'Students learn about and practice the principles of sustainable horticulture in an arid climate. Includes capstone projects on sustainability issues related to horticultural practice in arid environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 470, 'Life History of Mammals', 3, '{}', 'Evolution, classification, environmental, anatomical and physiological adaptations, ecology, and conservation of mammals worldwide.  Emphasis on the identification, habitat relationships, conservation, and management techniques of Arizona and select North American mammals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 472, 'Applied Herpetology', 3, '{}', 'Systematics, evolution, ecology and life history of amphibians and reptiles emphasizing wildlife management methodologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 473, 'Applied Ornithology', 3, '{}', 'Evolution, ecology, classification and life history of birds, emphasizing wildlife management methodologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 474, 'Riparian Ecosystem Management', 3, '{}', 'Management principles of riparian ecosystems based on fluvial geomorphological and ecological concepts. Discusses human impacts to riparian ecosystems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 475, 'Habitat Management for Small Wildlife', 3, '{}', 'Habitat management considerations and practices for small game and nongame wildlife species in North America.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 476, 'Big Game Habitat Management', 3, '{}', 'Conservation and management issues focused on large mammals in North America. Topics include habitat ecology, nutrition, migration, predator-prey dynamics, trophic cascades, habitat management and restoration of populations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 479, 'Ecosystem Management and Planning', 3, '{'L'}', 'Principles of ecosystem management, with emphasis on economic and policy constraints on the planning process. Risk assessment and management. Includes the process of complying with NEPA while developing rangeland management plans.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 481, 'Riparian and Wetland Restoration', 3, '{}', 'Principles and problems in the restoration of degraded riparian and wetland ecosystems. Construction of wetlands.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 485, 'GIS in Natural Resources', 3, '{}', 'Principles of Geographic Information Systems (GIS) utilized in natural resource management. Use of computers for spatial inventory and analysis of natural resources.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 486, 'Introduction to Remote Sensing', 4, '{}', 'Remote sensing is an important source of environmental information that can support a deeper understanding of trends and clarify management strategies in a wide range of ecological application. Focuses on the application of satellite and aerial/drone remote sensing in environmental science and management. With this basic background, course participants use remote sensing data to make spatial decision. Presents the concepts of biodiversity and wildlife management parallel to the remote sensing information. Image processing instruction and exercises begin with an analysis of digital imagery and proceed through the three broad classes of processing techniques: preprocessing, enhancement and information extraction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 489, 'Undergraduate Research', 3, '{}', 'Undergraduate research under the supervision of an applied biological sciences faculty member.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 490, 'Applied Biological Sciences Seminar', 1, '{}', 'Current literature and significant developments related to applications of the biological sciences. May be repeated for credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 500, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 541, 'Advanced Rangeland Ecosystem Management', 4, '{}', 'Combines the principles of ecology, planning and restoration in the application of rangeland ecosystem management. Studies fundamental topics such as range plant physiology, range plant ecology, rangeland stocking-rates and grazing systems in considering management practices for rangeland ecosystems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 542, 'Management of Fire Adapted Ecosystems', 3, '{}', 'Fire adapted ecosystems which evolved with high-frequency and low-intensity fires now support a range of regimes from no fire to uncharacteristically large and intense fires, causing a concern about the current and future role of wildland fire as well as forest and rangeland health. Explores management considerations and techniques for reintroducing fire to these ecosystems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 550, 'Vegetation Dynamics', 3, '{}', 'Dynamics of vegetation linking physiological, population, and community ecology. Collection and analysis of vegetation data.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 555, 'Wildlife Dynamics', 3, '{}', 'Explores major concepts in population and community ecology with an emphasis on their application to the study and management of wildlife species. Explores the theory and sometimes controversial nature of these concepts in the form of lectures and discussions. Instructor and student workshops and oral presentations of student research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 560, 'Ecological Modeling', 3, '{}', 'Mathematical modeling of animal populations, food-web structural and ecological processes in space and time. May use modeling tools on computer clusters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 570, 'Advanced Animal Nutrition', 4, '{}', 'Metabolic and physiological interactions of nutrients in wild and domesticated animals consuming natural feeds.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 586, 'Remote Sensing in Environmental Resources', 4, '{}', 'Principles and application of remote sensing technologies in natural resource management using computerized data from aerial photography and satellite imagery.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ABS', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 100, 'All About Data: Design, Query, and Visualization', 3, '{'CS'}', 'Data modeling and design, spreadsheets, relational databases and querying, visualization and XML.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 101, 'Introduction to Computer Science', 3, '{'CS'}', 'Concepts of problem solving, structured and object-oriented programming in Java, fundamental algorithms, computer system concepts. Social and ethical responsibilities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 102, 'Object-Oriented Programming', 3, '{'CS'}', 'Object-oriented programming using introductory algorithms and data structures. Overview of concepts in computer science.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 131, 'Global Cybersecurity', 3, '{'G'}', 'Introduces the basic concepts and tenets of cybersecurity from a global and cultural perspective, instead of a technical perspective. Explores how various nation-states view and employ cybersecurity in order to achieve their national strategic imperatives. Students articulate and understand fundamental cybersecurity concepts and principles rooted in U.S. and other national policy, compare and contrast various nation-state approaches to national security, the monitoring of other nations and foreign citizens, and how these relate to cybersecurity policies and practices. Students identify and describe cultural and political philosophies regarding personal data and personal privacy and their influence on national security policy and objectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 201, 'Data Structures and Algorithms', 3, '{'CS'}', 'Static and dynamic data structures, recursive and iterative sorting and searching, object-oriented design and programming, software design models and implementation/testing strategies, professional responsibilities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 240, 'Introduction to Programming Languages', 3, '{}', 'Introduces the procedural, object-oriented, functional and declarative languages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 320, 'Database Systems', 3, '{}', 'Entity-relationship and relational data models. Database design theory. Relational algebra, calculus and SQL query languages. Query optimization. XML fundamentals. Transactions, recovery and concurrency control.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 321, 'Database Development & Applications', 3, '{}', 'Stored procedures and triggers. Security and authorization. Call-level interface. Database Web applications. Advanced topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 330, 'Computer Networks', 3, '{}', 'Network architecture and protocols, principles of network applications, socket programming, flow and congestion control, switching and routing, link-layer technologies, traffic capture and analysis, security.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 331, 'Network Forensics Analysis', 3, '{}', 'Fundamental principles of network forensics and traffic analysis, evidence acquisition, network intrusion detection and analysis, anomaly detection and analysis, malicious activity detection, network tunneling, and malware forensics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 350, 'Systems Programming', 3, '{'CS'}', 'Introduces operating systems as resource managers, processes, threads, memory allocation, file systems, protection, system calls, application program interfaces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 351, 'Governance, Risk and Compliance', 3, '{}', 'Broad requirements for effective cybersecurity governance, the elements required to develop an appropriate governance strategy, and the actions required to implement it.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 361, 'Secure Coding Concepts', 3, '{}', 'Secure software development process including designing secure applications, writing secure code that can withstand attacks, and security testing and auditing. Focuses on the security issues a developer faces, common security vulnerabilities and flaws, and security threats. Explains security principles, strategies, coding techniques and tools that can help make code more resistant to attacks. Students write and analyze code that demonstrates specific security development techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 386, 'STEAM Service Learning', 3, '{}', 'Guided process in which students deepen their understanding of their STEAM content area through knowledge-based problem solving that provides a service to others in the community.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 399, 'Supervised Research', 3, '{}', 'Experience within the context of current faculty research projects. Student is assigned responsibility depending on qualifications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 401, 'Managing Cyber Risks in Enterprise Business Processes', 3, '{}', 'Covers the activities, decisions, risks and controls associated with enterprise business processes. Specific focus on the activities in the sales-to-cash (revenue cycle) and purchase-to-pay (expenditure cycle) processes. Students understand how IT enables performing those processes more efficiently and effectively. In addition to identifying internal controls necessary for accurate, complete performance of processes, also covers the fundamentals of cybersecurity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 402, 'Privacy, Ethics and Compliance Issues', 3, '{}', 'Covers the activities, mechanisms and challenges associated with implementing the principles of privacy into corporate structures. Also provides an overview of the privacy regulatory landscapes as well as emerging challenges with privacy. Specific focus areas include the Generally Accepted Privacy Principles framework and the implementation of privacy controls via the use of process and technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 420, 'Big Data Systems', 3, '{}', 'Modern technologies to manage, process and analyze big data. Data processing of large data sets with Map/Reduce. NoSQL data stores. NewSQL databases. Data visualization. Applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 421, 'Data Mining and Warehousing', 3, '{}', 'Introduces data mining from a database perspective, including classification, clustering, and association rules. Data warehousing methodologies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 422, 'XML and Databases', 3, '{}', 'XML data model, schema specification and validation, querying, storage, indexing, and current research topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 423, 'Data Science', 3, '{}', 'Computational and machine learning methods to extract insights from data: data collection, data cleaning and transformation, exploratory data analysis, data visualization, and machine learning (supervised and unsupervised) algorithms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 430, 'Wireless Networks and Security', 3, '{}', 'Comprehensive course on wireless networks and security covering current industry standards, protocols, research and future trends. Topics include fundamentals of wireless communications, wireless LAN configuration and analysis, wireless security and cellular networks. Sets the groundwork for students to acquire professional certifications such as CWNA, CWAP and CWSP.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 431, 'Network Security', 3, '{}', 'Digital threats and attacks, cryptography, confidentiality, authentication protocols, message integrity, security in network applications, intrusion detection, malicious software countermeasures, firewalls.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 432, 'Distributed Systems', 3, '{}', 'Distributed system models, distributed programming technique, clock synchronization, distributed snapshot, distributed mutual exclusion, concurrency control, fault tolerance, security, recent advances.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 461, 'Security Operations', 3, '{}', 'How organizations monitor and manage the security of their infrastructures in the real world. Explains architecture principles,the use of monitoring tools and technologies, how to interpret different security data sources, as well as how to identify and respond to various security threats within a complex infrastructure. Requires students to utilize security toolsets within a laboratory setting to demonstrate their understanding of the technologies as well as course concepts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 486, 'Peer Mentoring in STEM', 1, '{}', 'Advanced students learn and apply mentoring skills in science classes or teaching labs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
