INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 501, 'Introduction to Professional Music Therapy', 3, '{}', 'Introduces the profession of music therapy through lecture, active discussion, and observations of clinical sessions. Experiential learning is achieved through music making in peer groups.');
