INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EDP', 652, 'Multivariate Procedures for Data Analysis', 3, '{}', 'Educational applications of multivariate methods, including MANOVA, discriminant analysis, and exploratory factor analysis. Emphasizes analyzing data and reporting results.');
