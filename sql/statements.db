INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 100, 'Introduction to Biomedical Engineering', 3, '{}', 'Introduces engineering design methods, experimental practices, data gathering, presentation and statistical analysis. Lecture/labs focus on device design and application, including hands-on experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 111, 'Engineering Perspectives on Biological Systems', 3, '{}', 'Biological concepts for the emerging engineer. Introduces biological and earth systems engineering, materials, structures, fluid mechanics, bioelectricity, and the dynamic, nonlinear nature of nature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 121, 'Programming in Biomedical Engineering', 1, '{}', 'Introduces computer programming in the context of scientific and engineering problem solving. Topics include loops, functions, pointers, data structures, and algorithms in the context of a biomedical problem. Culminates in an introduction to object-oriented programming, with a look at modern wireless (iPad and iPhone) devices for biomedical applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 122, 'Statistics for Biomedical Engineers', 2, '{}', 'Introduces statistics tests in the context of common biomedical experiments. Students perform basic linear regression and correlation analyses, experiment using power analysis, generate descriptive statistics, and perform statistical tests using statistical software.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 182, 'Biomedical Engineering Product Design and Development I', 1, '{}', 'Continuation of hands-on training and implementation of medical device product design and development skills; FDA-regulated U.S. and global medical device healthcare markets and opportunities, business development in regulated environments, medical product and project management planning, exempt and Class I medical devices, medical product component and device levels, identification and selection of customer needs, translation of customer needs, product specification development, virtual and physical prototypes, prototype product specification verification, product design and development process documentation, FDA regulatory best-industry practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 200, 'Conservation Principles in Biomedical Engineering', 3, '{}', 'Applies bioengineering analysis and problem solving of mass, energy, and charge balances to medical and biological systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 210, 'Programming for Biomedical Engineers: Introduction to Computers, Programming and Data', 3, '{'CS'}', 'Computers are ubiquitous tools in all engineering disciplines. Biomedical engineering pushes the limits of computational power in all applications--from small computers embedded in medical devices to large computer clusters used in research projects. Understanding the fundamental operation of computers, and how to efficiently and effectively program them, are critical knowledge and skills for biomedical engineers. Covers the fundamentals of computer architecture and operation, and how to program them. Utilizes the MATLAB and Python programming languages and prepares students for applying these languages for problem solving in upper-level biomedical engineering classes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 211, 'MATLAB & Mathcad in Biomedical Engineering', 1, '{}', 'Introduces the basics of MATLAB and Mathcad programming in the context of biomedical engineering conservation of mass problem solving.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 213, 'Biomedical and Bioengineering Ethics', 1, '{}', 'Explores ethical issues in biomedical and medical sciences and engineering practices and includes professional ethics, medical ethics, engineering ethics and the Biomedical Engineering Society (BMES) Code of Ethics, the ethics of human and animal tissue and subjects use in biomedical science and engineering research, responsible conduct for research, ethical issues in the design and manufacture of medical devices for developing and developed countries, and the impact of biomedical engineering solutions on society and the environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 214, 'FDA Regulatory Processes and Technical Communications', 1, '{}', 'Develops writing skills to factually and effectively communicate all facets of biomedical product design and development activities that are required to successfully conceive, develop, and launch a medical device into the market place using the FDA quality systems best practices that are utilized by the health care technology industry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 235, 'Physiology for Engineers', 4, '{}', 'Physiology of the nervous, muscular, cardiovascular, endocrine, renal, and respiratory systems. Emphasizes use of quantitative methods in understanding physiological systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 282, 'Biomedical Engineering Product Design and Development II', 1, '{}', 'Hands-on training and implementation of product design skills that combine virtual analytical model prototype development with component and sub-assembly integration to develop both virtual and physical prototypes of a medical device product at the product architecture level and verify selected product architecture specifications from design input and earlier design process verification steps.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 300, 'Bioengineering Product Design', 3, '{}', 'The fundamentals of financial and organizational structure of business and how it influences bioengineering analysis, design, and decision making; biomedical ethics and device and drug regulation; and fundamentals of business and technical management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 301, 'Numerical Methods in Biomedical Engineering', 2, '{}', 'Applications of numerical methods to biomedical engineering problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 316, 'Biomechanics for Biomedical Engineers', 3, '{}', 'Biomechanics provides conceptual and mathematical tools that are necessary for understanding how living things move. Focuses on human biomechanics with an emphasis on the musculoskeletal system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 318, 'Biomaterials', 4, '{}', 'Material properties of natural and artificial biomaterials. Tissue and blood biocompatibility. Uses of materials to replace body parts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 322, 'Statistics for Biomedical Engineering', 1, '{}', 'Introduces parametric and nonparametric statistics tests in the context of common biomedical experiments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 331, 'Transport Phenomena for Biomedical Engineering', 3, '{}', 'Transport phenomena with emphasis on momentum, energy, and mass transport in living systems, medical devices, and other therapeutic/diagnostic applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 340, 'Thermodynamics for Biomedical Engineers', 3, '{}', 'Thermodynamics is a branch of science dealing with processes (chemical or mechanical) and the subsequent energy and mass transfers. Studies whether a process is favorable (possible) or unfavorable (impossible) while assessing energy and mass transfer within each specific stage of the process. The operation of living systems is heavily based on the use of chemical reactions and or mechanical processes such as beating of the heart, biochemical reactions of a drug in liver, phase equilibria. Covers the fundamental principles and laws of thermodynamics. Helps students think thoroughly about the applications of thermodynamics in biological sciences and bioengineering with the main emphasis on basic concepts and problem-solving skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 350, 'Signals and Systems for Bioengineers', 3, '{}', 'Applies principles of calculus and ordinary differential equations to analysis and computer processing of biosignals and linear modeling of biosystems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 360, 'Control in Biological Systems', 3, '{}', 'Illustrates the basic principles of control through examples from evolved and synthetic biological systems. Surveying topics including feedback and feedforward control mechanisms, insulation of circuits via modularity, and discovering optimal intervention strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 362, 'Methods in Molecular and Cellular Biology', 1, '{}', 'Gives junior-level undergraduate students hands-on experience on the methods used in molecular and cellular biology lab, especially methods to engineer or analyze DNA and protein.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 370, 'Microcomputer Applications in Biomedical Engineering', 3, '{}', 'Uses microcomputers for real-time data collection, analysis, and control of experiments involving actual and simulated physiological systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 382, 'Biomedical Engineering Product Design and Development III', 1, '{}', 'Continuation of hands-on training and implementation of medical device product design and development skills; experimental practices, data gathering, presentation, and statistical analysis in regards to demonstration of validation and verification tools and testing, identification and selection of customer needs, translation of customer needs, product specification development, virtual and physical prototypes, prototype product specification verification, product design and development process documentation, FDA regulatory best industry practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 413, 'Biomedical Instrumentation', 3, '{'L'}', 'Principles of medical instrumentation. Studies of medical diagnostic instruments and techniques for the measurement of physiologic variables in living systems. Both BME 413 and BME 423 need to be taken together to secure L General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 416, 'Advanced Biomechanics', 3, '{}', 'Mechanical properties of bone, muscle, and soft tissue. Static and dynamic analysis of human movement tasks such as locomotion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 417, 'Biomedical Engineering Capstone Design I', 4, '{'L'}', 'Technical, regulatory, economic, legal, social, and ethical aspects of medical device systems engineering design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 423, 'Biomedical Instrumentation Laboratory', 1, '{'L'}', 'Lab experience with problems, concepts, and techniques of biomedical instrumentation in static and dynamic environments. Both BME 413 and BME 423 need to be taken together to secure L General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 434, 'Applications of Bioengineering Transport Phenomena', 3, '{}', 'Develops mathematical models of transport phenomena in physiological systems, medical devices, and pharmacokinetic analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 465, 'Magnetic Resonance Imaging', 3, '{}', 'Provides a foundation in the fundamental concepts of magnetic resonance imaging and some applications to measure physiological processes and changes with respect to disease. Incorporates real-time demos using a bench-top MRI system. Several local MR experts present guest lectures on specialized topics. Requires a working knowledge of the fundamentals of signal analysis and Fourier transform theory and linear algebra to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 467, 'Tissue Engineering and Regenerative Medicine', 3, '{}', 'Covers the fundamentals of engineering and developmental biology concepts relevant to tissue engineering and regenerative medicine (TERM). Topics covered range from basic cell biology principles to nutrient exchange parameters to cell material interactions that inform engineering design of regenerative strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 490, 'Biomedical Engineering Capstone Design II', 4, '{'L'}', 'Individual projects in medical systems or medical device design and development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 507, 'Introduction to Digital Image Processing and Analysis', 3, '{}', 'Core course with four sections for students seeking to concentrate in biomedical imaging informatics. The first section analyzes the concepts and terms of image processing and analysis. The second section evaluates key algorithms in image processing (e.g., image filtering, Fourier transform, image restoration). The third section examines advanced image analysis methods (e.g., image segmentation, mathematical morphology, image topology, shapes and boundaries, color processing, image coding and compression, wavelet, and special effects). The fourth section develops the practical applications of image processing and analysis in medicine and biology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 521, 'Advanced Motor Neuroscience for Engineers', 3, '{}', 'Overview of sensorimotor brain structures. Application of nonlinear, adaptive, optimal, and supervisory control theory to eye-head-hand coordination and locomotion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 525, 'Surgical Techniques', 2, '{}', 'Principles of surgical techniques, standard operative procedures, federal regulations, guidelines, and state-of-the-art methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 526, 'Introduction to Neural Engineering', 3, '{}', 'Introduces the field of neural engineering. Neural engineering, as with all engineering fields, can be thought of as applied scientific understanding, in this case, applied neuroscience. Covers aspects of neurophysiology, neuroanatomy, neurosurgery, biophysics, electronics and signal processing; integrates these knowledge areas into neural engineering applications. Covers an entire process for conducting research in neural engineering: surgical implantation of electrodes, data acquisition, processing neural signals, decoding neural data, and controlling prosthetic devices. Provides foundation for working with neural engineering applications and conducting research in neural engineering.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 527, 'Biomedical Device Design', 3, '{}', 'Basic phenomena behind various types of biomedical devices which measure indicators using optical, thermal and other modalities. Provides an introductory coverage of the theory and history of these devices and includes some hands-on activities to use, reverse engineer and analyze data gathered from in-class activities with currently existing devices. Uses Matlab to perform most of the analysis. Preliminarily exposes students to principles of device design and testing with consideration to FDA regulations and requirements.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 556, 'Human Systems Neuroscience', 4, '{}', 'Systems-level neuroscience course focusing on human diseases. Classes involve lectures and/or other activities such as observations of clinical procedures and tours of clinical facilities (e.g., the neurosurgical suite, imaging facilities).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 561, 'Clinical Neuroscience', 3, '{}', 'Fundamental mechanisms that underlie diseases and disorders of the central and peripheral nervous systems. Introduces assessment and pharmacological, surgical and rehabilitative management of nervous systems disorders. Prior knowledge of neural systems is helpful. Identifies the mechanisms by which key disease and trauma states impact neural systems and how clinicians identify the locus and type of damage based on clinical and imaging criteria.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 564, 'Cyber Biomedical Systems', 3, '{}', 'Engineering principles involved in the modeling, design and analysis of hybrid systems that involve computers controlling and interacting with biomedical systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 565, 'Magnetic Resonance Imaging', 3, '{}', 'Provides a foundation in the fundamental concepts of magnetic resonance imaging and some applications to measure physiological processes and changes with respect to disease. Incorporates real-time demos using a bench-top MRI system. Several local MR experts present guest lectures on specialized topics. Requires a working knowledge of the fundamentals of signal analysis and Fourier transform theory and linear algebra to be successful in this course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 566, 'Medical Imaging Instrumentation', 3, '{}', 'Design and analysis of imaging systems and nuclear devices for medical diagnosis, therapy and research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 567, 'Tissue Engineering and Regenerative Medicine', 3, '{}', 'Covers the fundamentals of engineering and developmental biology concepts relevant to tissue engineering and regenerative medicine (TERM). Topics covered range from basic cell biology principles to nutrient exchange parameters to cell material interactions that inform engineering design of regenerative strategies. Prior knowledge of high-level engineering mathematics (up to differential equations), engineering transport concepts (particularly mass transfer), and biology is expected.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 568, 'Human Factors and System Design in Assistive Technologies', 3, '{}', 'The field of human factors engineering as it relates to the design, development and use of assistive technology for individuals with sensory, motor and cognitive disabilities. Includes lectures, guest lectures, hands-on demonstrations and class discussions of relevant and current topics related to human-centered design of assistive technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BME', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
