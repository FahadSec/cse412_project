INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EVO', 539, 'Computing for Research', 3, '{}', 'Works mainly with command-line programs, focusing on sequence-based analysis. An interactive flipped course, with required hands-on aspects both in class debugging and troubleshooting and for out-of-class assignments where students work on problem solving and implementation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EVO', 557, 'Practical Applications in Computational Life Sciences', 3, '{}', 'Working group to troubleshoot challenges in computational life sciences.');
