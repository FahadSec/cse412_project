INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 711, 'Macroeconomic Analysis I', 3, '{}', 'Current theories of output, employment, inflation, and asset prices as well as major aggregates. Introduces dynamic optimization techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 712, 'Microeconomic Analysis I', 3, '{}', 'Theory of production, consumer demand, resource use, and pricing in a market economy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 713, 'Macroeconomic Analysis II', 3, '{}', 'Focuses on growth theory, dynamic general equilibrium models, monetary theory, open-economy issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 714, 'Microeconomic Analysis II', 3, '{}', 'General equilibrium, welfare economics, production, and capital theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 715, 'Advanced Macroeconomic Analysis', 3, '{}', 'Focuses on current research areas in macroeconomics and monetary theory with emphasis on methods in economic dynamics and numerical techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 716, 'Advanced Economic Theory I', 3, '{}', 'Economic behavior under uncertainty; markets and contracts under asymmetric information; the theory of games with incomplete information and applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 717, 'International Macroeconomics', 3, '{}', 'Intertemproal approach to the current account, international business cycles, international risksharing and home bias, models of exchange rate determination, balance of payment crises, sovereign debt and default.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 721, 'Labor Economics I', 3, '{}', 'Develops basic theoretical models for analyzing labor market issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 722, 'Labor Economics II', 3, '{}', 'Extensions/criticisms of labor market theories. Applications to a variety of policy issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 725, 'Econometrics I', 3, '{}', 'Problems in the formulation of econometric models. Emphasizes  estimation, hypothesis testing, and forecast of general linear models.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 726, 'Econometrics II', 3, '{}', 'Estimation and inference of qualitative and limited dependent variable models as well as general multiple equation models.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 727, 'Quantitative Methods in Economics', 3, '{}', 'Generalized method of moment estimation, estimation with censored and truncated samples, nonlinear models, panel-data models, econometrics of nonstationarities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 736, 'International Trade Theory', 3, '{}', 'Theories of comparative advantage and their empirical verification. Theory and political economy of commercial policy. Resource transfers and the role of the multinational corporation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 741, 'Public Economics', 3, '{}', 'Economics of collective action, public spending, taxation, and politics. Impact of central governmental activity on resource allocation and income distribution.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 753, 'Industrial Organization', 3, '{}', 'Analyzes structure, conduct, and performance in industrial markets; the economics of organizations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 760, 'Economics of Growth and Development', 3, '{}', 'Economic problems, issues, and policy decisions facing the developing nations of the world.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 770, 'Mathematics for Economists', 3, '{}', 'Surveys mathematical ideas encountered in economics and econometrics. Topics include measure theory, probability theory, and mathematical statistics.');
