INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 502, 'Counseling Research Data Analysis', 3, '{}', 'Descriptive statistics, visual approaches, estimation, and inferential methods for univariate and bivariate counseling research problems using statistical software.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 522, 'Theories of Counseling and Psychotherapy', 3, '{}', 'Presents major theories of psychological intervention as well as underlying personality theory upon which they are based.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 523, 'Psychological Tests', 3, '{}', 'Standardized tests in the study of the individual, with emphasis on test score interpretation in counseling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 527, 'Prevention and Consultation', 3, '{}', 'Community focus with emphasis on outreach, prevention, psychoeducation, consulting, and advocacy from a systematic multicultural perspective.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 529, 'Lifespan Human Development', 3, '{}', 'Issues and models of human development as it occurs across the lifespan.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 534, 'Occupations and Careers', 3, '{}', 'The world of work, career development, education, and training for occupational entry and mobility.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 545, 'Assessment and Diagnosis', 3, '{}', 'Theory and methods commonly used to assess mental health diagnoses utilizing the most current version of the Diagnostic and Statistical Manual of Mental Disorders.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 567, 'Group Dynamics and Counseling', 3, '{}', 'Group process factors, theory, and diversity issues determining effective interaction in small groups. Emphasis placed on lecturettes, self awareness, and experiential components.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 577, 'Counseling Prepracticum', 3, '{}', 'Focus on racial, social, and cultural factors in the development of helping relationships through integration of cognitive and affective self-awareness with counseling skills.');
