INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 521, 'Stars and Interstellar Medium I', 3, '{}', 'Radiative transfer, atomic and molecular properties, stellar atmospheres, line profiles, nonlocal thermodynamic equilibrium, interstellar gas and dust, star formation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 522, 'Stars and Interstellar Medium II', 3, '{}', 'Stellar structure, radiative transport, boundary conditions, equations of state, nuclear reactions, opacity, nucleosynthesis, chemical evolution of the galaxy, stellar evolution.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 523, 'Stars and Interstellar Medium III', 3, '{}', 'Structure of the interstellar medium, gaseous nebulae, recombination theory, ionization fronts and shock waves, galactic magnetic fields, magnetohydrodynamics, molecular clouds.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 531, 'Galaxies and Cosmology I', 3, '{}', 'Structure and evolution of the Milky Way, stellar properties, populations and associations/clusters, interstellar medium, dark matter.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 532, 'Galaxies and Cosmology II', 3, '{}', 'Structure of galaxies and the nearby universe, Hubble sequence, kappa-space, stellar populations, active galaxies, galaxy environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 533, 'Galaxies and Cosmology III', 3, '{}', 'Issues in modern cosmology, the distance scale, cosmological parameters, cosmological tests, cosmic background radiation, early universe, galaxy formation and evolution.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 540, 'Astronomical Instrumentation and Data Analysis', 3, '{}', 'Studies a range of optical astronomical hardware including telescopes, digital detectors and instrument design and theory. Focuses on the use of CCDs in terms of operation, calibration, use, measurement, limitations, noise and signal, statistics, and preliminary understanding prior to data reduction. Discusses practical optical astronomical observing: the choice of filters and their design, planning your observing run, bringing your data home and the types of data you will need. Final class project requires the reduction and measurement of data using computers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report or publication. Assignments might include data collection, experimental work, data analysis or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AST', 692, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report or publication. Assignments might include data collection, experimental work, data analysis or preparation of a manuscript.');
