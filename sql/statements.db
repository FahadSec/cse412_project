INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report or publication. Assignments might include data collection, experimental work, data analysis or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 140, 'The Nature of Knowledge Systems', 3, '{'HU'}', 'Introduces students to the structure and divisions of knowledge systems in the university setting. After surveying the different types of disciplines, students learn processes for using knowledge and insights from different sources in an integrative manner.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 150, 'Introduction to Military Studies', 3, '{}', 'Focuses on the way interdisciplinary scholars in national security at the interface of history and political science ask questions and propose answers. Introduces the basics of research, writing and criticism with an eye toward how solutions are constructed from diverse sources. Evaluates primary and secondary sources including identifying purpose, intended audience and biases.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 201, 'Intellectual Fusion', 3, '{'SB'}', 'Introduces students to the history of interdisciplinarity. Examines major theories and methods of interdisciplinarity, and students use these methods to begin to examine complex interdisciplinary problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 298, 'Honors Directed Study', 6, '{}', 'Independent study or practicum under the supervision of a faculty member.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 300, 'Theories and Applications of Organizational Studies', 3, '{'L','SB'}', 'Critical analysis and application of organizational theory and conceptual perspectives to identify and solve problems, emphasizing personal and organizational application.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 301, 'Becoming Interdisciplinary', 3, '{}', 'The history of interdisciplinarity and methods of integrating knowledge and insights from multiple perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IDS', 302, 'Integration Skills', 3, '{}', 'Develops integrative skills to create new understandings, models, products and ideas.');
