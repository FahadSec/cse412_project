INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 232, 'Human Development', 3, '{'SB'}', 'Lifespan development from conception through adulthood, with emphasis on family influences. Recognition of individuality within the universal pattern of development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 244, 'Children Facing Health Care Challenges', 3, '{}', 'Provides a comprehensive introduction to family-centered care, psychosocial assessment of the hospitalized and chronically ill child, and techniques to support their coping. Utilizes the principles/theories of child development, and introduces the role of a child-life specialist in the promotion of effective coping through play, preparation and education among children, youth and families facing challenges related to health care and hospitalization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 312, 'Adolescence', 3, '{'SB'}', 'In-depth examination of the developmental and social processes that help explain modern adolescence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 337, 'Early Childhood Intervention', 3, '{}', 'Explores how child development theory affects practice with children and families, emphasizing development of young children and early intervention.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 338, 'Child Development Practicum', 4, '{}', 'Supervised practicum in the Child Development Lab preparing students for work in child care centers and agencies serving young children and families.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 350, 'Inequality and Diversity in Education', 3, '{'L','C'}', 'Advanced exploration of various dimensions of diversity in relation to educational systems and outcomes. Through sociological and psychological research, case studies and films, students examine how educational inequality is created and reproduced through individual actions and institutional constraints. Also considers the connection between schooling and human rights and ways in which educational inequality may be addressed.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 356, 'Race and Child Development', 3, '{'SB','C'}', 'Reviews the current research and literature about the particular role of race in child development. Even though race is not a valid biological construct, it has significant social implications as it shapes how children interact and are treated by others. Through primarily class discussions/activities, presentations and papers, addresses: (1) What are the research and theoretical issues in studying race? (2) What does race and racism mean? (3) How does race influence developmental processes?');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 414, 'The School-Aged Child:  Development in the Elementary Years', 3, '{}', 'Examines children's physical, social-emotional and neurological development across the elementary school years, as well as the socialization influences of primary people and contexts engaged with children during this developmental period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 418, 'Aging and the Life Course', 3, '{'SB','H'}', 'Examines aging as a process of change and adaptation over the lifespan, patterned by social, cultural, and historical forces.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 430, 'Infant and Toddler Development in the Family', 3, '{'SB'}', 'Discover the world of babies and toddlers. Learn about important changes in biological/physical, cognitive, emotional and social domains of development from conception to age three. Consider the influences of parents, family and environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 450, 'Childhood Disorders and Family Functioning', 3, '{'L'}', 'Examines the presentation, development and treatment of a wide range of neurodevelopmental, psychological, emotional and behavioral disorders of childhood. Applies family theories to understand the implications of these disorders for children and families.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 469, 'Multivariate Statistics for Social Sciences', 3, '{}', 'Provides instruction and hands-on experience with statistical methods for examining the effects of multiple variables. Focuses on multivariate techniques and how statistical techniques can lead to a better understanding of substantive issues. As this is an applied course, lectures and coursework focus more on the application and interpretation of multivariate techniques than on the mathematics behind the statistical techniques. Topics typically include data visualization, correlation, simple regression, multiple regression, logistic regression, multinomial regression, ordinal regression, and principal component analysis. Also teaches how to conduct these statistical techniques and undertake basic programming in the R computer program.');
