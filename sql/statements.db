INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 566, 'Instrumental Literature and Pedagogy for Schools', 3, '{}', 'Provides an in-depth examination of ensemble music and teaching techniques to instrumental music teachers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 567, 'Professional Practicum', 2, '{}', 'Students provide music therapy for small groups at a community agency providing services to people who are intellectually disabled, physically disabled, and the elderly.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 568, 'Choral Music, Advanced Rehearsal Techniques', 3, '{}', 'Musical and vocal techniques necessary for presentation of choral literature. Analysis and experimentation with psychological, acoustical, and other problems of rehearsal and performance.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 569, 'Professional Practicum II', 2, '{}', 'Students continue to provide music therapy for small groups at a community agency providing services to people who are intellectually disabled, physically disabled, and the elderly.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 570, 'Choral Literature for Schools', 3, '{}', 'Comprehensive study and analysis of choral music for the high school with special emphasis on octavo literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 571, 'Music Therapy Seminar I: Supervision and Administration', 2, '{}', 'Covers health care administrative skills pertinent to music therapy practice, including budgeting, resource allocation, and staff supervision.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 572, 'Music Therapy Seminar II with Lab: Advanced Practice in Music', 1, '{}', 'Deepens and broadens knowledge and skills in music therapy clinical practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 573, 'Music Therapy Seminar III with Lab: Music Therapy Theory and Philosophy', 3, '{}', 'Explores current theory in music therapy and develops the theoretical basis for the profession.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 574, 'Special Topics in Music Therapy', 3, '{}', 'Introduces graduate students to advanced music therapy topics related to clinical practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 575, 'Music Therapy Music Skills: Accompaniment', 1, '{}', 'Teaches new forms of musical accompaniment pertinent to music therapy practice, broadening and deepening existing skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 579, 'Psychology of Music', 3, '{}', 'Nature of musicality and its evaluation. Review of recent research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
