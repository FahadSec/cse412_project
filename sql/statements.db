INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BUS', 436, 'Frontiers in Risk Management', 3, '{}', 'Highlights practices and challenges faced by firms in terms of identifying, measuring and communicating a variety of business and financial risks, as well as designing and implementing risk management strategies as a critical component of the overall strategic management process. Emphasizes a variety of financial and nonfinancial risks in a holistic framework. Develops an understanding of the risk management practices and challenges faced by firms in a variety of business settings and industries.');
