INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 610, 'Advanced Studies in Linguistics', 3, '{}', 'Different topics such as morphology, semantics, typology, history of linguistics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 614, 'Advanced Studies in Syntax', 3, '{}', 'Focuses on morphosyntactic feature checking, movement, phrase structure, and their crosslinguistic differences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 615, 'Advanced Studies in Sociolinguistics', 3, '{}', 'Language in its social setting, e.g., social variation among native speakers, sociolinguistics aspects of second-language acquisition, and language policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 616, 'Advanced Studies in Discourse Analysis', 3, '{}', 'Advanced analysis of spoken and written discourse emphasizing different methodological and theoretical issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 617, 'Advanced Studies in Historical Linguistics', 3, '{}', 'Overview of theories on language change, the history of languages, and of methods in historical linguistics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 620, 'Advanced Studies in Second-Language Acquisition', 3, '{}', 'Selected topics in second-language acquisition, such as phonetics and phonology, morphology, syntax, and discourse.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 625, 'Advanced Studies in Second Language Writing', 3, '{}', 'Provides opportunities to explore advanced research topics as well as current issues and controversies in the field of second language writing--an interdisciplinary field of inquiry simultaneously situated in applied linguistics, rhetoric and composition, and teachers of English to Speakers of Other Languages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 655, 'Advanced Disciplinary Discourses in Linguistics and Applied Linguistics', 3, '{}', 'Advanced investigation of professional and disciplinary issues related to linguistics and applied linguistics.');
