INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 313, 'Probability, Evidence, and Decision', 3, '{}', 'Philosophical topics in epistemology and practical reasoning; probability and its use in representing and reasoning about belief, evidence, and rational choice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 314, 'Philosophy of Science', 3, '{'HU'}', 'Structure and justification of scientific theories, explanation, and theory change. Roles of observation and laws, theoretical concepts and entities, reduction, probability, confirmation, space and time, and causation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 322, 'History of Science I', 3, '{'HU','H'}', 'Development and application of scientific thinking from ancient times through the 17th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 323, 'History of Science II', 3, '{'HU','H'}', 'Development and application of scientific thinking from the 18th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 330, 'History of Biology: Conflicts and Controversies', 3, '{'H'}', 'Focuses on 19th and 20th centuries, considering biology as a discipline. Evolution, problems of heredity, development, and cell theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 331, 'History of Medicine', 3, '{'HU','H'}', 'Scientific study of the human body, changing theories of disease, evolution of practical opinions on treatment, and the emerging institutionalization of medical practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 332, 'The Darwinian Revolution', 3, '{}', 'Origin, nature, and subsequent history of the ideas of Charles Darwin.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 336, 'Exploration and Science', 3, '{'SB','H'}', '500-year survey of exploration as a historical process and cultural activity of Western civilization, with emphasis on its links with modern science.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 340, 'Biology and Society', 3, '{}', 'Explores interactions between biological sciences and society, e.g., biomedical, environmental, ethical, historical, legal, philosophical, political, and social issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 349, 'Nature, Sustainability and Religion', 3, '{}', 'Explores how world religions have conceptualized the natural world, what they have contributed to the current environmental crisis and how they might also serve as inspiration to address the crisis. By focusing on Judaism, Christianity and Islam, the course introduces students to the field of religion and ecology and explores its relationship to other academic disciplines such as environmental philosophy and ethics, justice studies and gender studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 410, 'Biomedical Research Ethics', 3, '{'L'}', 'Considers issues related to values in science such as collaboration, finances, legal issues, media, mentoring, ownership of ideas, scientific integrity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 452, 'Philosophy of Biology and Medicine', 3, '{}', 'Examines fundamental topics in the philosophy of biology and their implications for medicine. Examples include biological mechanisms and evidence-based medicine, species concepts and race, reductionism and the nature of individuality, and theories of function and health. Uses these topics throughout the semester to address two overarching questions about whether biology is fundamentally different from physics and whether medicine can be reduced to applications of biology.');
