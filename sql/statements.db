INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HPS', 516, 'History of Biology', 3, '{}', 'Covers themes such as the voyages of discovery, systematics, and classification; morphology and embryology; Darwin and evolutionary biology; the rise of genetics; molecular and organismal approaches of post-WWII biology; recent history of biology.');
