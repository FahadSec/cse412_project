INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GLG', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GLG', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 101, 'Elementary German I', 4, '{'G'}', 'Reading, writing, speaking, and understanding of basic German, with emphasis on pronunciation and grammar.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 102, 'Elementary German II', 4, '{'G'}', 'Reading, writing, speaking, and understanding of basic German, with emphasis on pronunciation and grammar.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 110, 'Intensive German I', 8, '{'G'}', 'Students learn to read, listen, write and speak German. Develops ability to communicate in German using a variety of tools. Uses German to ask and answer questions; name and describe persons, things, places and events; deals with certain situations; narrates orally and in writing; writes letters and postcards; fills out forms; and comprehends some texts. Expands knowledge of the cultures of the German-speaking countries. Students contribute to the class discussions what they might already know.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 201, 'Intermediate German I', 4, '{'G'}', 'Intensive review of grammar, with emphasis on the development of the skills of speaking, listening comprehension, reading, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 202, 'Intermediate German II', 4, '{'G'}', 'Intensive review of grammar, with emphasis on the development of the skills of speaking, listening comprehension, reading, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 210, 'Intensive German II', 8, '{'G'}', 'Helps develop reading, writing, listening and speaking skills and strategies with activities both inside and outside of class. Guides student further into learning about the cultures of the German-speaking countries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 310, 'Making Sense of German Grammar', 3, '{'G'}', 'Contextualized German grammar in written and spoken German.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 311, 'Express Yourself in German', 3, '{'G'}', 'Builds and refines conversational skills in German through a focus on important contemporary issues. Strengthens active vocabulary knowledge, trains listening and speaking skills, and develops strategies to understand and respond in everyday German situations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 312, 'Explore German Culture', 3, '{'G'}', 'Discovers how advancing reading proficiency in German can grant access to an array of ideas from the realms of literature, film, science, politics, history and sustainability. Each semester, students contribute to building a unique syllabus based on individual class interests.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 313, 'Create in German: Writing, Speaking, Performing', 3, '{'G'}', 'Invites all students on a journey through a variety of writing and speaking adventures in German. From academic to creative writing, and from theater to presentational speaking, offers opportunities to bring all language skills to life.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 315, 'Germanic Mythology', 3, '{'HU','G'}', 'Religious and spiritual beliefs and practices of the ancient Germanic peoples as reflected in their culture, including the pantheon of their gods, as well as the reception of these beliefs through and including contemporary times.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 319, 'Work with German', 3, '{'G'}', 'Navigates the German business world in real-life situations. Apply for a job, be a trainee and develop a business project in German. Communicate effectively and with cultural awareness in a professional context. Organize and present clear, effective business communications and vocabulary applicable to modern business usage.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 416, 'German Civilization', 3, '{'HU','H','G'}', 'From 1600 through 1945.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 420, 'Discover - Discuss - Design: German Language and Culture', 3, '{'L','HU'}', 'Covers current research approaches to the German-speaking world such as environmental, migration and memory studies to help students articulate a unique question about the German-speaking world based on their individual interests. Over the course of the semester, students explore their focal area by designing a project that can take a variety of forms, from the classic research paper to digital exhibitions, audio-visual media, or artworks. Develops research skills, discovers interdisciplinary approaches, and discusses outcomes with peers and the community. The majority of course materials and language of instruction are in German.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 421, 'German Literature Survey, 800-1800', 3, '{'HU'}', 'From the beginning to Classicism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 422, 'German Literature Survey, 1800-Today', 3, '{'L','HU'}', 'From Romanticism to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 431, 'German Humor', 3, '{}', 'Popular and literary texts (collections of jokes, pranks and comedies) in the print and visual media provide a basis of comparison for students to reflect critically on the function of humor in German culture from the sixteenth century until the present. Exposes the different types and functions of German humor and understands both the historical and the contemporary theoretical debate on the study of (German) humor. Taught almost exclusively in German, with reading and writing assignments done in the target language.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 441, 'Fairy Tales', 3, '{'HU','G'}', 'Foundational fairy tale texts collected by the Brothers Grimm and written by Hans Christian Andersen and their modern permutations reflect basic human desires, fears and aspirations as well as the values of the period in which they are written. These stories and their variants in popular culture across the globe permit students to reflect critically on the functions of fairy tales in both past and present civilizations. Students perceive the ways in which ancient tales can be utilized for various purposes and understand the contemporary theoretical debate on the study of fairy tales and their role in popular culture. Course taught in English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 445, 'The Holocaust in German-Mediated Memory', 3, '{'HU'}', 'Examines representations of the Holocaust from across different media (literature, film, architecture, memoir, photography) and from across multiple generations of German engagement with this period in history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 446, 'Weimar-Era German Film', 3, '{}', 'Examines early film production in Germany during the period of the Weimar Republic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 447, 'German Modernism', 3, '{}', 'Examines examples of modernism in literature, art and film from German-speaking Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 455, 'Berlin: Migration, Arts and Activism', 3, '{'G'}', 'Deals with the culture and politics, arts and activism of migrants in Germany based on the example of Berlin, using historical and contemporary examples. Critically engages with such concepts as migration and identity, transnationalism, assimilation, multilingualism, artivism, activism, xenophobia and racism. Examines examples from various art forms, including film and the creative arts, literature, media and social media to help understand the impact of migration and immigrants on Germany and German society and compares it to experiences in other countries, especially the U.S. A focus on Berlin allows an in-depth and focused micro-observation of the differences and commonalities of various immigrant groups and responses of the local communities in a defined urban space. In order for students to reflect on the impact of migration in German society and the migrants themselves, also looks at migration through the lens of movements for social change and justice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 465, 'Intercultural Communicative Competence', 3, '{}', 'Focuses on the importance of culture(s) and the ways in which culture(s) effects and interrelates with communication. Communicating across cultures is very important in order for us to be able to function in a globalized, diverse world, e.g., workplace, city. Using our own experiences and discussions as well as the readings, course increases our sensitivity, understanding and awareness of our own culture(s) as well as other culture(s). Approaches the study of intercultural communicative competence in a theoretical and practical way.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 550, 'German for Reading Knowledge', 3, '{}', 'Teaches non-speakers of foreign language, or those with only basic notions of the language, how to read in a foreign language at an advanced level. Consists of a concise overview of grammar as well as reading comprehension practice for students who plan to take the Graduate Foreign Language Exam or others who would merely like to learn to read in a foreign language. Focuses only on reading and not on listening, speaking, or writing as in a traditional language course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('GER', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
