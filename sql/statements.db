INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EDP', 597, 'Applied Research and Reflection', 3, '{}', 'The dual purpose of the project is to synthesize learning from multiple areas of scholarship on teaching and learning, and to reflect on the content and process of the master's degree program. Culminating product includes a reflective paper with a corresponding project, such as a literature review, curricular model, technology product, or other project as agreed upon by the student and the student's advisor.');
