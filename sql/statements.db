INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 510, 'Linguistics', 3, '{}', 'Overview of linguistics, its subfields, and some of its applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 511, 'Phonetics and Phonology', 3, '{}', 'Current trends in phonological theory and its basis in acoustic and articulatory phonetics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 513, 'Semantics', 3, '{}', 'Current approaches to linguistic meaning with particular attention to English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 514, 'Syntax', 3, '{}', 'Analyzes syntactic structures using a generative theoretical model with a focus on English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 515, 'American English', 3, '{}', 'Development of the English language in America, including regional and social varieties, and its relationship to other immigrant and native languages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 516, 'Pragmatics and Discourse Analysis', 3, '{}', 'Studies language use in context and language structures in spoken and written texts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 517, 'History of the English Language', 3, '{}', 'Surveys the development of the English language, with an emphasis on major linguistic transformations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 520, 'Second-Language Acquisition Theories', 3, '{}', 'Theories of second-language acquisition, including the linguistic, cognitive, and sociocultural aspects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 521, 'Methods of Teaching English as a Second Language', 3, '{}', 'Methods of teaching English as a second language, language teaching trends, practical applications, and the teaching of different skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 522, 'Grammar for TESL', 3, '{}', 'Surveys major grammatical structures in English and how they can be taught to ESL speakers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 523, 'Language Testing and Assessment', 3, '{}', 'Introduces the theory and practice of language test construction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 524, 'Curriculum Design and Materials Development', 3, '{}', 'Practical guide to curriculum and materials development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LIN', 525, 'Teaching Second Language Writing', 3, '{}', 'Provides a critical introduction to the field of second language writing as well as issues and strategies in the teaching of second language writing in a wide variety of contexts. After exploring various instructional contexts as well as the characteristics of different types of students and their texts, considers various instructional practices and strategies, focusing on course and assignment designs, reading-writing connection, teacher and peer feedback, grammar instruction, classroom assessment, plagiarism and text borrowing strategies, and negotiating language differences.');
