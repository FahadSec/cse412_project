INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EVO', 601, 'Principles of Evolution', 3, '{}', 'Introductory course for graduate students; covers the principles of evolutionary biology from theoretical, empirical, and historical perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EVO', 610, 'Research Areas of Evolution', 1, '{}', 'Broadly conceived description of the current research areas of evolutionary biology. Expected of all first-year evolution students.');
