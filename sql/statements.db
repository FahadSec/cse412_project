INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AMS', 201, 'Introduction to American Studies', 3, '{'L','C'}', 'Provides an intellectual foundation for the interdisciplinary study of American culture, history, and society. Introduces students to ideas, methods, tools, and theories in the vibrant field of American studies.');
