INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 594, 'Conference and Workshop', 12, '{}', 'Topical instruction, usually in compressed format, leading to academic credit. Often offered off campus to groups of professionals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 754, 'Teacher Education Research and Policy', 3, '{}', 'Reading and discussion of research and policy documents in teacher education with a focus on arts and music teacher preparation. Includes development of a research project or essay suitable for publication or presentation, an abbreviated prospectus for a dissertation, or literature review.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 756, 'Qualitative Research in Music Teaching and Learning', 3, '{}', 'Studies qualitative research and methods in music education, including readings and discussion, data collection and data analysis. Includes development of an abbreviated prospectus for a qualitative dissertation, or a draft of a qualitative research paper suitable for presentation or publication. Prerequisite: doctoral student in music education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 757, 'Quantitative Research in Music Teaching and Learning', 3, '{}', 'Studies quantitative research and methods in music education, including readings and discussion, data collection, and analysis. Includes development of an abbreviated prospectus for a quantitative dissertation or a draft of a quantitative research paper suitable for presentation or publication.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 780, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 784, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 791, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 795, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 140, 'Sounds and Cultures', 3, '{'HU','G'}', 'Introduces a range of practical and intellectual challenges presented by encounters with various kinds of music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 201, 'Media Literacy for Musicians', 3, '{'CS'}', 'Instruction in basic Macintosh computer literacy, including generic applications and music-specific programs with hands-on experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 339, 'Undergraduate Bibliography I', 1, '{}', 'Research tools and methods in musicology, concentrating on resources pertinent to the study of music from Antiquity to the Baroque.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 340, 'Undergraduate Bibliography II', 1, '{}', 'Research tools and methods in musicology, concentrating on resources pertinent to the period 1750-present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 341, 'Music History I', 3, '{'HU'}', 'Western European art music from Antiquity through Baroque. Identification/analysis through listening; application of historical and analytical concepts; research project comprising fresh thesis, detailed musical research, personal reflection, fluent English writing, correct citation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 342, 'Music History II', 3, '{'HU'}', 'Western European, American art music from Classical period to present. Identification/analysis through listening; transfer/application of music/analytical concepts; research project comprising fresh thesis, insightful musical research/reflection, fluent English writing, correct citation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 344, 'Music in World Cultures', 3, '{'HU','G'}', 'Examines the relations among music, dance, theatre, religion, and social status in Asia, Africa, Oceania, Europe, and the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 345, 'Music in Renaissance Cities', 3, '{'HU'}', 'Examines the civic contexts for music from 15th-16th century Europe through analysis and discussion of style, composition, civic histories and broader sociocultural concepts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 363, 'Survey of Russian Music', 3, '{'HU'}', 'Examines music and musical life in Russia, the Soviet Union, and the post-Soviet C.I.S. from the Middle Ages to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 365, 'Popular Music and Race', 3, '{'(L','HU)','C'}', 'Examines popular music in the United States with emphasis on historical, sociocultural, economic and aesthetic concerns as they intersect with issues of race and race relations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 437, 'Music in the Baroque Era', 3, '{}', 'Examines music, culture and aesthetics in the 17th and early 18th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 438, 'Topics in 18th-Century Music', 3, '{'L','H'}', 'Selected topics exploring the musical styles of the 18th century and their cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 439, 'Topics in 19th-Century Music', 3, '{}', 'Selected topics exploring the musical styles of the 19th century and their cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 440, 'Topics in 20th-Century Music', 3, '{'L','HU'}', 'Selected topics exploring the musical styles of the 20th century and their cultural contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 441, 'Concerts in the 20th Century', 3, '{'HU'}', 'Presents a concert-centered approach to 20th-century music history, investigating important concerts that emphasized artistic collaboration, interaction between 'high' and 'low' genres, and other major issues affecting the changing means of creating, disseminating, and receiving music in the twentieth century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 445, 'Music of the Silk Road', 3, '{'HU','G'}', 'Provides an overview of music across the territories historically traversed by the silk roads, emphasizing the social contexts of music-making, and of a variety of musical practices found in this region. Studies the dynamics of musical exchange and change, the relationship of music to ecology within this varied geography, and how political interventions into the realm of culture have informed musical meaning, often by obscuring or highlighting long-standing social interconnections that have structured musical sound. Further studies the relationship of belief systems, economic patterns and gender ideologies to music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 447, 'Music and Healing', 3, '{'HU','H','G'}', 'Combines elements of musicology, philosophy, anthropology, music therapy, neuroscience, psychology; investigates ways music has been proven to positively affect the 'human condition'; focuses on what music means to humans.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 456, 'History of Opera', 3, '{}', 'Development of opera from its creation ca. 1600 to present. Emphasis placed on major stylistic developments and representative works.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 490, 'Capstone Project I: Undergraduate Thesis Research', 3, '{}', 'Selection of topic and research toward the completion of an undergraduate thesis of no fewer than 50 pages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 491, 'Capstone Project II: Undergraduate Thesis', 3, '{}', 'Completion of an undergraduate thesis in musicology or ethnomusicology of no fewer than 50 pages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
