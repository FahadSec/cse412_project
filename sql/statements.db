INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 671, 'Multicultural Counseling', 3, '{}', 'Provides awareness of the influence of sociocultural variables on human development and explores implications for counseling minority populations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CED', 672, 'Couple and Family Counseling', 3, '{}', 'Couple and family counseling theories. Emphasizes a systems-communication model utilizing cocounseling.');
