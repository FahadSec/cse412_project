INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 597, 'Capstone', 3, '{}', 'Reflective, experience-based integration of theory and practice; professional development for individuals working with English language learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 341, 'Physical Chemistry with a Biological Focus', 3, '{}', 'Thermodynamics, equilibrium, states of matter, solutions and chemical kinetics with a focus on biological systems. For students in biochemical, premedical, biomedical and educational curricula.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 361, 'Advanced Principles of Biochemistry', 3, '{}', 'Advanced consideration of the structures, properties, and functions of proteins, enzymes, nucleic acids, carbohydrates, and lipids; principles and theory of the utilization and synthesis of these materials by living systems, and the relationship of these processes to energy production and utilization. The course is recommended for pre-health students. No credit for Biochemistry majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 367, 'Elementary Biochemistry Laboratory', 1, '{}', 'Qualitative/quantitative analyses of constituents of biological systems, enzyme activity measurements and metabolic studies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 371, 'Modern Concepts in Biochemistry', 3, '{}', 'Detailed examination of the chemical structure, conformation and function of proteins, carbohydrates, lipids and nucleotides; analysis of the energetics, enzymes and control of the metabolic pathways by which these molecules are degraded or synthesized.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 372, 'Modern Concepts in Biochemistry Laboratory', 1, '{}', 'Analyzes protein structure, purification and function using spectroscopic, chromatographic, electrophoretic and other methods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 392, 'Introduction to Research Techniques', 3, '{}', 'Instrumental methods and philosophy of research by actual participation in chemical research projects. May be repeated for total of 6 semester hours.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 461, 'General Biochemistry', 3, '{}', 'Structure, chemistry, and metabolism of biomolecules and their role in the biochemical processes of living organisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 462, 'General Biochemistry', 3, '{}', 'Continuation of BCH 461.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 463, 'Biophysical Chemistry', 3, '{}', 'Principles of physical chemistry as applied to biological systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 467, 'Analytical Biochemistry Laboratory', 3, '{'L'}', 'Quantitative analysis, separation and purification of biological molecules. Applies chemical and physical methods to the characterization of biological macromolecules.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 501, 'Current Topics in Biochemistry', 2, '{}', 'Varies by topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 520, 'Chemical Biology', 3, '{}', 'Chemical biology, broadly defined, is the application of chemical tools toward the study biological problems. The purpose of this class is to provide the foundation for translational research involving cross-disciplinary approaches to tackling significant problems in the biosciences. This one-semester class will cover topics ranging from traditional molecular biology and biochemistry to state-of-the-art techniques in genomics and proteomics. Particular emphasis will be placed on modern concepts in enzymology. The class will be taught using a combination of current literature and text-based materials.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 561, 'Advanced Topics in Biochemistry', 3, '{}', 'Topics selected from emerging areas of biochemistry based primarily on current literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 564, 'Bionanotechnology', 3, '{}', 'The use of biomolecules, bioelectronics, and biomedicine for applications in nanotechnology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 568, 'Molecular Mechanisms of Photosynthesis', 3, '{}', 'Structure and function of photosynthetic complexes; mechanism of energy conversion in plants, bacteria, and model systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BCH', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
