INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 325, 'Physiological Psychology', 3, '{}', 'Relationships of physiological processes to behavior. Emphasizes nervous system functioning.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 347, 'Fundamentals of Genetics', 3, '{}', 'Science of genetics, gene flow, and genetic engineering in humans and other organisms.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 348, 'Fundamentals of Genetics Laboratory', 1, '{}', 'Investigational activities using modern molecular techniques to analyze various genetic phenomena.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 359, 'Animal Physiology Laboratory', 1, '{}', 'Investigative lab experience using animals (including humans) and models.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 362, 'The Human Environment', 3, '{}', 'Evolution of humans' physiological, ecological, and behavioral interaction with their environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 363, 'Genes, Race, Gender, and Society', 3, '{'L'}', 'Examines biological events and theories on race and gender and their interface with societal views.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 365, 'The Human Organism', 4, '{'SG'}', 'Analyzes human anatomy, physiology, and behavior in an ecological and evolutionary context with the methods of comparative biology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 369, 'Biology of Human Sexuality', 3, '{}', 'Multilevel perspective on human sexuality and reproduction. Integrates psychological with evolutionary, physiological and neuroscientific approaches.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 380, 'Analysis of Nonhuman Forensic DNA Evidence', 3, '{}', 'Nonhuman biological materials are so pervasive in the environment that they often contaminate victims, perpetrators and crime scenes by means of direct deposit or secondary transfer. As the reliability, sensitivity and specificity of animal, plant and microbial DNA assays improve with the advent of new technologies, the potential probative value of DNA information from nonhuman biomaterials also increases, especially for establishing or refuting alibis, determining the chronology of a criminal event and for crime scene reconstruction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 386, 'STEAM Service Learning', 3, '{}', 'Guided process in which students deepen their understanding of their STEAM content area through knowledge-based problem solving that provides a service to others in the community.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 388, 'STEM Research Fundamentals', 3, '{'L'}', 'Teaches research skills in the natural sciences. Topics covered include the basic research techniques of the study organism and system, experimental design, literature searches, statistical analysis and preparation of scientific papers and posters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 425, 'Analytical Chemistry for Life Sciences', 3, '{}', 'Presents an analysis of chemicals, including the theoretical and practical considerations regarding the separation, detection and quantitative determination of chemicals. Illustrates the application of different analytical procedures using some of the common chemicals of concern in the forensics and environmental fields.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 426, 'Analytical Chemistry for Life Sciences Lab', 2, '{}', 'Students learn basic principles of analytical chemistry and explore a wide range of common techniques and methods with applications to forensic science and life sciences. Places specific focus on techniques common in modern analytical laboratories, including gas chromatography, mass spectrometry, FTIR and NMR spectroscopic techniques. Other topics include interpretation of analytical data and evaluation of error and uncertainty in measurements.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 430, 'Environmental and Human Toxicology', 4, '{}', 'Lecture/fieldwork course investigating toxic substances in the environment and their effect on the health of organisms, including humans.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 432, 'Fundamentals of Pharmacology', 3, '{}', 'Aspects of pharmacology using an integrated approach to familiarize students with natural and synthetic therapeutic agents used in medicine.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 434, 'Marine Ecology', 3, '{}', 'Examines ecological processes in oceans, seas, and estuaries that emphasizes species- and community-level phenomena.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 447, 'Molecular Genetics Laboratory', 1, '{}', 'Investigative lab involving DNA isolation, PCR, gel electrophoresis, enzyme digestion and ligation, transformation, and hybridization analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 475, 'Principles of Toxicology', 3, '{}', 'Presents the fundamental principles of toxicology and its application to both human and environmental situations. Starts with the means of defining and quantifying toxic effects through dose response curves (e.g. LD50, EC50, LOEL, NOEL). Then focuses on absorption pathways for toxicants as well as the degradation metabolism pathways for toxicants. Finally focuses on the mechanism of toxic action for common poisons and environmental pollutants. Emphasizes toxicants that students are likely to encounter in forensics and environmental fields.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 480, 'Advanced Topics in Human Forensic DNA Typing', 3, '{}', 'Examines the science of current forensic DNA typing methods by focusing on the technology behind STR, Y-STR and mtDNA typing, real-time PCR, serology tests, capillary electrophoresis, expert systems and statistics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('LSC', 486, 'Peer Mentoring in STEM', 1, '{}', 'Advanced students learn and apply mentoring skills in science classes or teaching labs.');
