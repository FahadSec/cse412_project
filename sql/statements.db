INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ASL', 102, 'American Sign Language II', 4, '{}', 'Further development of receptive/expressive conversation skills in ASL; finger spelling. Continued exploration of deaf culture.');
