INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 210, 'Engineering Mechanics I: Statics', 3, '{}', 'Force systems, resultants, equilibrium, distributed forces, area moments, fluid statics, internal forces, friction, and stability.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 212, 'Engineering Mechanics II: Dynamics', 3, '{}', 'Kinematics and kinetics of particles, translating and rotating coordinate systems, rigid body kinematics, dynamics of systems of particles and rigid bodies, and energy and momentum principles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 213, 'Introduction to Deformable Solids', 3, '{}', 'Strain-displacement and stress-strain-temperature relations. Stresses and deformations due to axial, shear, torsional and bending moments. Buckling, stability.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 241, 'Surveying', 3, '{}', 'Theory and fieldwork in construction and land surveys.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 243, 'Heavy Construction Equipment, Methods and Materials', 3, '{}', 'Emphasizes Horizontal construction. Fleet operations, maintenance programs, methods, and procedures to construct tunnels, roads, dams, and the excavation of buildings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 271, 'Construction Safety', 3, '{}', 'Safety procedures and equipment. OSHA requirements for construction. Biological aspects of construction safety.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 296, 'Summer Field Internship', 1, '{}', 'Participation as interns on construction projects to observe and experience the daily activities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 321, 'Structural Analysis and Design', 4, '{}', 'Statically determinate and indeterminate structures (trusses, beams, and frames) by classical and matrix methods. Introduces structural design.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 351, 'Geotechnical Engineering', 4, '{}', 'Index properties and engineering characteristics of soils. Compaction, permeability and seepage, compressibility and settlement, and shear strength.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 353, 'Civil Engineering Materials', 4, '{}', 'Structure and behavior of civil engineering materials, including steel, aggregate, concrete, masonry, asphalt, wood, composites. Atomic structure and engineering applications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 383, 'Construction Estimating', 4, '{}', 'Analyzes construction drawings and specifications. Utilizes advanced technical and ethical practices in estimating process. Quantity surveying techniques for CSI divisions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 400, 'Earth Systems Engineering and Management', 3, '{'(L','HU)','H'}', 'Introduces earth systems engineering and management, and the technological, economic and cultural systems underlying the terraformed Earth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 421, 'Concrete Structures', 3, '{}', 'Behavior of concrete structures and the design of reinforced and prestressed concrete members, including footings. Partial design of concrete building system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 450, 'Geotechnical Applications for Construction', 3, '{}', 'Soil formation, engineering properties and use as building materials. Soil's influence on construction of built environment, including specifications and biological aspects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 452, 'Foundations', 3, '{}', 'Applies soil mechanics to foundation and retaining wall systems, including bearing capacity, settlement, and lateral earth pressure.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 453, 'Construction Technology', 3, '{}', 'Building information modeling, including current computer programs and technology planning strategies for advanced project deliveries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 455, 'Construction Project Management II', 3, '{}', 'Advanced topics in administrative procedures, planning processes, and coordination required to successfully complete construction projects on time and budget.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 486, 'Integrated Civil Engineering Design', 4, '{'L'}', 'Requires completion of a civil engineering design in a simulated practicing engineering environment. Limited to undergraduates in their final semester.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 495, 'Construction Planning and Scheduling', 3, '{'CS'}', 'Various network methods of project scheduling, such as AOA, AON Pert, bar-charting, line-of-balance, and VPM techniques. Microcomputers used for scheduling, resource allocation, and time/cost analysis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CNE', 496, 'Construction Contract Administration', 3, '{'L'}', 'Surveys administrative procedures of general and subcontractors. Studies documentation, claims, arbitration, litigation, bonding, insurance, and indemnification. Discusses ethical practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 120, 'Introduction to Counseling', 3, '{'SB'}', 'Provides an introduction to and broad overview of the field of counseling. Covers several counseling topics related to ethics, prevention, intervention and treatment, as well as multicultural counseling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 220, 'Self in a Multicultural Society', 3, '{'C'}', 'Covers current theories and research on culture, race and ethnicity, gender and other specific aspects of cultural diversity in a multicultural society. Explores themes, theories, topics and research related to power, privilege and oppression in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 240, 'Counseling Skills', 3, '{}', 'Provides an introduction to counseling skills and intervention techniques used in treatment. Focuses on active listening, self-awareness and other related skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 260, 'Survey of Careers in Counseling and Applied Psychology', 1, '{}', 'Provides a broad overview of the various career opportunities in counseling and applied psychology fields.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 310, 'Addictions I: Foundations and Basic Principles', 3, '{}', 'Covers the history, overview and principles of addiction as well as symptoms of co-occurring mental health issues prevalent in individuals with addictions and substance abuse disorders. Students also learn basic screening and assessment tools used to detect, evaluate and manage risks associated with individuals who have co-occurring disorders');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 315, 'Trauma and Crisis Intervention', 3, '{}', 'The role of counselors in traumatic events, crises and situations. Also introduces students to counseling skills and techniques particularly specialized for crises such as child abuse, partner violence, war, natural disasters and many others.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 320, 'Theories of Counseling and Psychotherapy', 3, '{}', 'Provides the theoretical foundations and knowledge related to the principles of counseling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 325, 'Professional Practice and Ethics in Counseling', 3, '{}', 'Discusses ethical issues (confidentiality, informed consent, integrity, respect) and professional codes of conduct in the counseling field.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 330, 'Group Counseling', 3, '{}', 'Provides an overview of theories, techniques and approaches to group counseling and therapy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 335, 'Addiction Group Counseling', 3, '{}', 'Offers an overview of the role and efficacy of group therapy in substance abuse treatment. Students learn about the different types of substance abuse treatment groups; screening and criteria for groups; group development and stages of treatment; and group leadership skills and techniques (e.g., motivational interviewing). Through weekly discussions, journal reflections and experiential activities, students learn the various techniques used in group substance abuse treatments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 340, 'Multicultural Counseling Theory and Practice', 3, '{}', 'Provides an overview of multicultural theory, research and practice. Topics include acculturation, identities, prejudice and other related topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 342, 'Community Outreach, Prevention and Intervention', 3, '{}', 'Provides an understanding of the methodologies in community outreach and prevention treatment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 351, 'Issues in Assessment, Diagnosis and Treatment', 3, '{}', 'Builds upon previous coursework. Provides students with an understanding of the major screening, assessment and diagnostic approaches for substance abuse and addictions. Students obtain knowledge of (1) models, (2) instruments and (3) diagnostic criteria, and the ability to link this knowledge to treatment planning and interventions for addictions. The goal of this course is not to diagnose or treat individuals with disorders. Rather, it is designed to give students a basic understanding of diagnostics and evidence-based treatments to inform work in the mental health field.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 400, 'Advanced Counseling Research Methods', 3, '{}', 'Builds upon introductory research methods and provides students with a strong foundation of research knowledge in counseling. Covers critical reading and analysis of peer-reviewed journal articles, types of research methodologies and proposal development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 420, 'School Counseling', 3, '{}', 'Provides an overview of counseling in school settings. Covers the role of school counselors in supporting youth and adolescents in many areas such as developmental disabilities/mental illness, as well as ethical and legal issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 440, 'Addictions II: Treatment, Recovery and Relapse Prevention', 3, '{}', 'Builds upon previous course work. Covers philosophies and models of treatment, recovery, relapse prevention and continuing care in substance abuse counseling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 493, 'Honors Thesis', 6, '{}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 498, 'Pro-Seminar', 7, '{}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CAP', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 568, 'Group Intervention', 3, '{}', 'Provides advanced MC or PhD students with supervisory experience serving as Group Leaders or Co-Leaders to students currently enrolled in the prerequisite course. Time dedicated to group leadership or supervision accumulates as 'Direct Contact' or 'Group Supervision' hours and fulfills the 'maladaptive/intervention' requirements or may be applied as an elective for MC students and PhD students. Students are selected for enrollment in this course with required instructor or departmental approval.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 622, 'Child Counseling and Play Therapy', 3, '{}', 'The use of play and expressive arts in counseling. Major areas of focus include: theoretical orientations to play therapy and child counseling, foundations of counseling, practice of play therapy and child counseling skills, and assessment/treatment planning. Integrates play media such as guided imagery, puppets and dolls, art, creative dramatics, music and movement, games, and sand play. Demonstrations and experiential activities are a primary method of teaching and learning the theory and practice of play counseling. Includes both directive and nondirective play counseling experiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 623, 'Counseling At-Risk Youth', 3, '{}', 'Examines the behaviors that place adolescents at risk for not achieving career goals and interventions to address these behaviors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 635, 'Professional Development', 1, '{}', 'A series of presentations designed to expose beginning doctoral students to the multidimensional facets and competencies for being a counseling psychologist.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 644, 'Psychology of Careers', 3, '{}', 'Advanced career counseling, including theory, research, and practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 645, 'Professional Issues and Ethics', 3, '{}', 'Ethical, legal, and professional issues of concern to practitioners and researchers functioning in a variety of settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 650, 'Counseling Latinos', 3, '{}', 'Survey course addressing mental health concerns and perspectives for individuals and families of Latino, Spanish-speaking origin.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 651, 'Personality Assessment', 3, '{}', 'Training in the use of major objective personality instruments to assess psychological symptoms; emphasizes MMPI-2 and MCMI-III.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 652, 'Individual Intellect Assessment', 3, '{}', 'Examines the nature of human intellectual/cognitive abilities, their standardized measurement, and related issues in interpretation and practice. Provides preliminary training and experience in the administration and interpretation of two of the most widely used tests in this area: the Wechsler Adult Intelligence Scale, 4th ed. & the Wechsler Intelligence Scale for Children, 5th ed. Introduces other instruments at a topical level. Emphasizes theoretical foundations, practice skills, cultural considerations and ethical issues in intellectual/cognitive assessment. Operates under the broad principles of a cooperative learning approach. By researching and presenting on key course concepts, students practice incorporating a scholarly and empirical basis into their clinical work to develop specialized expertise in clinical assessment. Class format includes instructor- and student-led didactics, discussion and practice labs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 653, 'Neuropsychology', 3, '{}', 'Assumes students have knowledge of basic topics in physiological psychology and/or in brain and behavior-type courses. Concentrates on human neuropsychology; the main topics are related to functional brain organization and hemispheric specialization, with a particular emphasis on functional assessment. Includes several other topics related to late-breaking empirical developments in the field. Read and discuss articles and studies, employing a large variety of methodologies, ranging from the 1950s to the latest publications, with both normal and pathological subject populations. Much of the course is spent discussing brain assessment techniques to study cognitive function and dysfunction. Emphasizes commonly used approaches in the assessment and measurement of human behavior and how the human brain in responsible for cognition, language, memory, spatial processing, emotion and personality.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 654, 'Biobases of Behavior', 3, '{}', 'Fulfills the APA requirement for a three-semester-hour course devoted to the broad and general biological bases that underlie behavior, and surveys a variety of fundamental physiological principles. Designed for students enrolled in an APA-accredited professional psychology training program; it does not focus on application of physiological principles to psychological practice. Capitalizes on most students' prior familiarity with psychopathology to help enable acquisition of general information about important brain structures and functions. Includes information about basic nervous system-related biochemistry, cellular structure and physiology, and anatomy and physiology. Students participate in classroom problem solving activities during which an array of principles is covered based on reading assignments. Challenges students to consider the role of biological, evolutionary and genetic factors in the expression of an array of typical and atypical behavior.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 660, 'Prevention and Consultation', 3, '{}', 'Community focus with emphasis on outreach, prevention, psychoeducation, consulting, and advocacy from a systematic multicultural perspective.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 668, 'Grant Writing and Grant Makers: The Washington Context', 3, '{}', 'Develops successful proposals for educational research projects. Students gain experience writing for federal agencies, reviewing, and being evaluated on grant proposals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 670, 'Individual Differences', 3, '{}', 'Focuses on definitions and assessment of ways humans vary on psychological constructs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 671, 'Multicultural Counseling', 3, '{}', 'Provides awareness of the influence of sociocultural variables on human development and explores implications for counseling minority populations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 673, 'Trauma in Counseling', 3, '{}', 'Addresses the impact of crisis, disasters, sexual assault and other trauma-causing events on individuals and communities, as well as the treatment of trauma-related disorders. Examines trauma through the impact on the brain regions, impact on memory, variations of emotional regulation and somatic responses. Also addresses operations of emergency management systems within clinical mental health agencies and in the community. Also gives attention to vicarious trauma and caring for the caregiver. Demonstrations and experiential activities are a primary method of teaching and learning the theory and practice of working with trauma. Includes both directive and nondirective experiences.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 674, 'Counseling Women', 3, '{}', 'Explores women's development and its implications for counseling. Sexism in mental health, sex differences in diagnosis and psychopathology, and women's particular treatment needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 675, 'Health and Wellness Counseling', 3, '{}', 'Theory, research, and practice in health and wellness counseling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 676, 'Social Bases of Behavior', 3, '{}', 'In-depth examination of contemporary research and theory in selected areas of social psychology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 677, 'Advanced Counseling', 3, '{}', 'Advanced topics in counseling theory, research, and practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 678, 'Supervision Theory', 3, '{}', 'Models of therapist development and clinical supervision.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 679, 'History and Systems of Psychology', 3, '{}', 'Examines the development and differentiation of the discipline of psychology from its origins in philosophy to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 680, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 690, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 692, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 694, 'Substance Use, Disorders, Addiction and Therapy', 3, '{}', 'Develops knowledge and skills for conducting psychotherapeutic and educational interventions to address substance use disorders and associated concerns.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 701, 'Science and Practice of Counseling Psychology', 3, '{}', 'Directed experiences involving the integration of theory, research, and practice in counseling psychology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CPY', 702, 'Research Methods in Counseling Psychology', 3, '{}', 'Applies experimental and/or quasi-experimental methods to theory construction and treatment evaluation in counseling psychology.');
