INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EVE', 591, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('EVE', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report or publication. Assignments might include data collection, experimental work, data analysis or preparation of a manuscript.');
