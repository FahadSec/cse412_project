INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 500, 'Research Methods', 12, '{}', 'Course on research methods in a specific discipline.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 511, 'Advanced Agribusiness Management', 3, '{}', 'Analysis of organization behavior, change, and resource requirements within agribusiness systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 514, 'Advanced Agribusiness Analysis', 3, '{}', 'Vertical integration and differentiation in food and agricultural industries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 528, 'Advanced Agribusiness Marketing', 3, '{}', 'Theory and analysis of marketing strategy related to food and fiber products. Covers concepts of strategic market planning (e.g., segmenting, targeting and positioning), with application of these concepts at an advanced level. Also examines the broad social, cultural, political and economic issues that impact marketing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 529, 'Advanced Agribusiness Marketing Channels', 3, '{}', 'Analyzes agribusiness market channel systems. Formulation of marketing strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 532, 'Advanced Agribusiness Finance', 3, '{}', 'Financial management of agribusiness firms; agribusiness financial analysis, investment analysis, agricultural risk management, and introduction to agricultural financial intermediaries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 535, 'Commodity Analysis', 3, '{}', 'Analysis of commodity markets.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 552, 'International Agricultural Policy', 3, '{}', 'Uses international trade theory to analyze the effects of government policies, trade agreements, and exchange rates on agribusiness.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 554, 'Advanced International Trade', 3, '{}', 'Advanced international practices in trading of agribusiness, technology, and resource products and services.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 560, 'Advanced Agribusiness Management Systems', 3, '{}', 'Development and use of decision support systems for agribusiness management decision making.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 561, 'Applied Econometrics', 3, '{}', 'Uses model building, hypothesis testing, and empirical analysis in solving agribusiness problems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 570, 'Managerial Economics for Agribusiness', 3, '{}', 'Concepts in micro- and macroeconomics applied to agribusiness management environments: price formation, market structure, information economics, fiscal and monetary policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 583, 'Fieldwork', 12, '{}', 'Structured, supervised field experience in a field science or other discipline requiring experience in field techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 589, 'Agribusiness Capstone', 3, '{}', 'Strategic management of organizations focusing on developing value-creating strategies in dynamic environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('AGB', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
