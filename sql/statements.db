INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MBB', 490, 'Capstone: Issues in Biotechnology', 2, '{'L'}', 'Integrates science and biotechnology industry issues dealing with intellectual property, regulatory issues, business practices, and commercialization. Both MBB 490 and MBB 491 for L General Studies credit.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MBB', 491, 'Capstone: Issues in Molecular Biosciences', 2, '{'L'}', 'Integrates molecular science and societal issues dealing with ethics, regulatory issues, and societal needs that pertain to molecular biosciences. Both MBB 490 and MBB 491 for L General Studies credit.');
