INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSC', 498, 'Pro-Seminar', 7, '{'L'}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSC', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSC', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation (e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 502, 'The Health Service System and the Business of Health Care', 3, '{}', 'Acquaints students with the various organizations and actors across the value chain of health care. Assesses provider, supplier, payor and IT multiple perspectives. Exposes the major concepts needed to understand the health sector globally (key business and clinical functions). Students emerge being able to be dropped down into a conversation about a health care company or problem and understand a large proportion of what is being discussed.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 506, 'Managing Health Care Organizations', 3, '{}', 'Examines the dynamics within health care organizations. Provides a deep-dive into the best practices and key considerations for health care managers as they seek to operate within and modify their organizations. Employing concepts from organizational behavior and systems theory, provides students with a framework to define and resolve operational challenges associated with the management of a health care organization. Asks students to think as senior manager or consultants and propose solutions that are evidence based, feasible and demonstrate knowledge of the key contingencies associated with the context.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 512, 'Healthcare Economics', 3, '{}', 'Economics of production and distribution of healthcare services, with special emphasis on the impact of regulation, competition, and economic incentives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 550, 'Health Care Marketing', 3, '{}', 'Marketing is a set of processes, relationships and co-creations meant to create, communicate and deliver value to the customer. Takes a customer-centric approach to health care marketing. Examines the changing roles in health care, organizations' approaches to patient-centricity, patient empowerment, responsibilities and experience. In addition, considers patient metrics such as patient engagement, customer effort, net promotion and patient satisfaction measures used by the U.S. government's Centers for Medicare and Medicaid Services. Considers marketing from the patient/provider, organizational and network/system points of view, organizational buying and selling processes, and the development and navigation of health care networks/systems from a marketing organization, service provider's and customer experience perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 552, 'Health Care Supply Chain Management', 3, '{}', 'Examines the variety of organizations that constitute the health sector value chain with an emphasis on suppliers, distributors, IT intermediaries, group purchasing organizations, regulatory bodies, and payers. Key considerations include the design of stakeholder relationships along the value chain and the extent to which business and clinical considerations drive decision making and influence outcomes. Topics include strategic sourcing and contracting, stakeholder incentives and risk across the value chain, physician/professional perspectives and the management of the value chain relationships necessary to achieve high quality, ethical behavior and safe patient care.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 575, 'Entrepreneurship, Innovation and Strategy in the Health Care Sector', 3, '{}', 'Problems with health care quality, access and costs bedevil all countries. Focuses on the creation of global business innovations in health care that can better meet these needs. At its end, students should understand the opportunities for different kinds of health care innovations and how to create viable strategies and business models to meet them. Enables students to craft business models that attain alignment between an entrepreneurial health care venture and the Six Factors that shape this sector: financing, structure, public policy, consumers, technology and accountability. Covers these issues in every part of the sector, including insurance, services, IT, medical devices, biotechnology, diagnostics and pharmaceuticals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HSM', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HHE', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HHE', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 150, 'Introduction to Health Innovation', 3, '{}', 'Introduces health care innovation including diffusion of innovation, technology and current health care innovative ideas and practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 250, 'Health Entrepreneurship and Value Creation', 3, '{}', 'Change your mindset. Focuses on recognizing and questioning your assumptions, learning how to gather and accept feedback, and designing pivots to improve innovative and entrepreneurial outcomes. Seeks value creation opportunities in health and health care settings through opportunity recognition, value creation and business modeling. Students create an innovative change idea, demonstrate proficiency in lean startup methodologies, and 'pitch or ditch' a ready-to-launch idea.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 298, 'Honors Directed Study', 6, '{}', 'Independent study or practicum under the supervision of a faculty member.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 311, 'Health Innovation: Foundational Concepts', 3, '{}', 'Provides foundational knowledge for leading health innovation. Organized around the Seven Pillars of Innovation: innovation process, leadership structure, outcomes, evidence-based practice, finance, policy, and technology and communication. Emphasizes principles of health innovation, in addition to characteristics, skills and knowledge required for the innovator.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 313, 'Philosophy of Science in Health Care: Understanding Paradigms of Health and Healing', 3, '{}', 'Philosophy of science and its application to health and healing paradigms. Employs selected schools of thought and individual philosophers within philosophy of science to closely examine health and healing paradigms. Discusses concepts such as 'life' and 'death'. Explores questions: What is science? What is health? Examines the way in which philosophy of science informs perceptions and definitions of health and healing in individuals and populations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 344, 'Management and Leadership in Health Organizations', 3, '{}', 'Focuses on management, administration and leadership strategies in health institutions with an emphasis on organizational innovation and entrepreneurial thinking. Managing in health organizations includes unique challenges related to labor relations, practice codes of conduct, federal privacy considerations, and accreditation regulations. Includes a review of leadership methods, labor laws and practices for effective management in health- and health care-related organizations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 412, 'Transforming Health Care: A Systems Perspective for Innovation', 3, '{}', 'Investigates the essential elements of systems thinking as a framework for innovation in addressing current and future issues in health care. Demonstrates systems thinking to present and future problems in health care and utilizes the knowledge and understanding of behavioral and learning styles to facilitate innovation in diverse systems. Stresses the importance of collaboration to the systems perspective, in addition to the value of individual worldviews.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 421, 'Health Care Policy: The Innovator's View', 3, '{}', 'Innovation perspective on policy decision making in the health care environment. This perspective views policy-making as a fluctuating, dynamic and evolving process as seen through a lens of innovation principles. Explores the relationship between politics and policy and discusses factors influencing policy development. Identifies methods to formulate and implement policy and presents models to evaluate, analyze and modify health care policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 425, 'Facilitating Innovation to Achieve Outcomes', 3, '{}', 'Principles of facilitation that promote and enable the implementation of innovation to achieve outcomes. This approach identifies facilitation as requiring collaboration, continuous evaluation, and a focus on identifying and measuring health outcomes. Discusses characteristics of effective teams, innovation leadership behaviors that impact change, and involvement of end users. Compares and contrasts traditional project management processes with innovative facilitation techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 428, 'Technology: A Tool to Advance Innovation in Health Care', 3, '{}', 'Explores the role of technology in achieving the Triple Aim: improved quality, improved patient experience and decreased cost. Compares and contrasts tools of technology that hold the potential to advance innovation in health care. Examines the relationship among the users, their behaviors and technology and how this interaction advances communication, automation, organization and standardization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 497, 'Honors Colloquium', 6, '{}', 'A class emphasizing discussion, written and/or oral presentation by students, and active participation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 538, 'Innovation and the Individual', 3, '{}', 'Role of the innovator, including the essential insight, leadership styles, vision, ethical wisdom, resiliency and the development of an attitude supportive of risk taking. Examines and correlates different situational challenges with types of intelligence, behaviors, learning styles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 540, 'Understanding and Applying Principles of Evidence-Based Practice', 3, '{}', 'Understanding how application of evidence-based practice principles assists health care professionals to provide the best care.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 542, 'Systems Thinking in a Complex Environment', 3, '{}', 'Explores systems thinking theories and methodologies in the context of the complex and often chaotic environment of health care. Examines opportunities and challenges for innovation and innovative leadership in the health care system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 543, 'Approaches to Health Care Innovation', 3, '{}', 'Theory-based survey course introduces approaches to lead innovation in an organization. Many change initiatives are designed by those not completing the work, leaving workers isolated, restricted and powerless. Teaches health care leaders to value collaboration with and seeking input from those who will carry out the work. Analyzes approaches to planned change: positive deviance, appreciative inquiry and creation of a learning organization. Students construct a change plan using an appropriate approach to address a health care problem.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 544, 'Information Technology (IT) for Health Care Innovation', 3, '{}', 'Transforming health care through the development of sound information technology strategies is critical to the success of most health care organizations today. Examines information technology trends and their strategic application in health care. Learners create a plan for engaging IT resources and capabilities, and for executing an IT-based strategy for solving a problem in health care. Topics covered include: strategic processes, digital transformation, IT assets, design thinking, value creation, IT governance and transition change management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 545, 'Public Speaking for Innovation: Building Your Presence and Making the Case for Change', 3, '{}', 'Builds presence as a speaker and a leader. Public speaking has evolved dramatically over the last few years. Now it is about creating a public presence; e.g., on YouTube, Facebook live, TED Talks. Public speaking for innovation is a combination of finding and building one's presence as a speaker--and translating that to an ability to explain and persuade, making the case for change. Students build their presence and gain the foundational knowledge necessary to successfully explain and persuade in various modalities of media, from speaking in person to video creations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 548, 'Advanced Principles and Concepts of Innovation', 3, '{}', 'Explores and analyzes the innovation in complex systems, focusing on developing specific skills for successful innovations. Specifically addresses strategies to overcome barriers and resistance to innovation in health care.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 550, 'Health Care Policy and Innovation', 3, '{}', 'Introduces the political, organizational, and economic dimensions of health care policy, emphasizing strategies for innovative policy and practice.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 552, 'Financing for Innovation', 3, '{}', 'Provides an overview of healthcare economics; develops budgeting, planning, and management skills; and culminates in financial analysis skills necessary to facilitate management decision making or writing business plans or grant proposals.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 554, 'Outcomes Evaluation', 3, '{}', 'Outcomes measurement, management, and evaluation for health care practitioners and/or leaders.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 561, 'Innovation Methods', 2, '{}', 'Explores methods to generate innovation for use in a health care setting. Provides tools and practice in generating ideas, exploring utilizing appreciative inquiry, and demonstrating competencies related to the theories and processes of innovation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 563, 'Capstone: Final Innovation Project', 5, '{}', 'Applies the concepts learned in MHI core courses and presents evidence of knowledge in innovation principles, applications and strategies for implementation and evaluation. Develops an innovation project in the form of a presentation. Provides a comprehensive methodology for completing all aspects of an innovation opportunity. Student-directed demonstrations of the application of learned leadership skills and techniques for facilitating team and individual implementation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 570, 'Innovation Proposal: Culminating Experience for Health Care Innovation Certificate', 4, '{}', 'Applies concepts learned and operationalizes skills acquired throughout the Health Care Innovation certificate. Students envision emerging paths toward an outcome by first presenting a problem they have identified within a health care organization, and creating collaborative relationships and developing strategies to initiate change in their chosen setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 571, 'Health Care Innovation Leadership Portfolio: Culminating Experience', 3, '{}', 'Examines and applies personal leadership potential. Focuses on the effective development and facilitation of human networks to advance innovation in the health care space. Challenges students to appraise and analyze personal leadership strengths, challenges and opportunities. Explores strategies to evaluate students' current networks; develops the students' emergent professional networks. Gives focus to building relationships necessary to advance innovation in the health care space. Students develop and implement an action plan to advance their personal leadership growth and network utilization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HCI', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 101, 'Elementary Modern Hebrew', 4, '{'G'}', 'Reading, writing, speaking, and understanding of basic modern Hebrew, with emphasis on pronunciation and grammar.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 102, 'Elementary Modern Hebrew', 4, '{'G'}', 'Reading, writing, speaking, and understanding of basic modern Hebrew, with emphasis on pronunciation and grammar.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 131, 'Biblical Hebrew I', 3, '{'G'}', 'Offers an introduction to Biblical Hebrew, the language in which most of the books of the Hebrew Bible/Old Testament have been written. Provides the basic tools to understand and translate Biblical Hebrew texts. Covers the Hebrew alphabet, basic vocabulary, morphology and syntax. Students learn idiomatic expressions, basic literary techniques in narratives and poetry, as well as specificities of Semitic languages. Emphasizes a critical approach to the ancient text. Also introduces the evolution of the biblical text, from ancient manuscripts to contemporary editions and translations. Includes an introduction to the development of writing and to the evolution of Hebrew, from earliest inscriptions to its revival in modern Israel. The texts translated in class are situated in the history of Israel, but also in their Jewish and Christian cultural and religious contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 132, 'Biblical Hebrew II', 3, '{'G'}', 'Completes the study of basic Biblical Hebrew grammar and vocabulary (Futato's Beginning Biblical Hebrew, chapters 21-40). Prepares students to translate narrative and legal texts from Hebrew Bible. Students learn to use dictionaries, grammars, paradigms and critical editions of the biblical text.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 201, 'Intermediate Modern Hebrew', 4, '{'G'}', 'Intensive review of grammar, with emphasis on the development of the skills of speaking, listening comprehension, reading, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 202, 'Intermediate Modern Hebrew', 4, '{'G'}', 'Intensive review of grammar, with emphasis on the development of the skills of speaking, listening comprehension, reading, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 231, 'Biblical Hebrew III', 3, '{'G'}', 'Advanced study of Biblical Hebrew. Deepens understanding of the grammar and composition of the Hebrew Bible. Translates texts of different literary genres (narrative, legal, poetic, prophetic) and different periods. Introduces the basic principles of textual criticism and different approaches to interpret the texts of the Hebrew Bible.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 232, 'Biblical Hebrew IV', 3, '{'G'}', 'Addresses advanced topics in Biblical Hebrew grammar and interpretation. The first half of the semester is devoted to texts from the Hebrew Bible; the second half on extra-biblical texts written in ancient Hebrew (for example: Dead Sea Scrolls, Rabbinic texts, ancient Hebrew poetry). Topics and texts change annually.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 313, 'Advanced Modern Hebrew', 3, '{'G'}', 'Continues developing speaking, reading, and writing skills in modern Hebrew, with a focus on different aspects of the Israeli culture studied in literature, cinema, and music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 314, 'Advanced Modern Hebrew', 3, '{'G'}', 'Continues developing speaking, reading, and writing skills in Modern Hebrew, with a focus on different aspects of the Israeli culture studied in literature, cinema, and music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 316, 'Literary Readings of the Hebrew Bible', 3, '{'L','HU'}', 'Literary analysis of two of the main genres of the Hebrew Bible: narrative art and poetry. Students learn the main literary conventions of biblical prose and poetry, and how these conventions are applied or subverted. Each class comprises the study of a literary aspect, close readings, and thematic discussions. Biblical texts are read only in translation. They are situated in the religious, literary and artistic traditions that have developed on their basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 347, 'Modern Israeli Literature in Translation', 3, '{'HU','G'}', 'Modern Hebrew, or Israeli Hebrew, started at the end of the 19th century in Europe and today it is the language of the State of Israel. During this short period modern Hebrew developed literature with its unique local characteristic, as well as universal ideas. Reads and examines four books against their social and historical background.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 348, 'Israeli Society and Culture', 3, '{'HU','G'}', 'Different partitions of the Israeli society, based on ethnic origin, by religious affiliation or by political association. Depicts how Israel is an absorbing immigration state. Israel is a welfare state and the course covers aspects of a welfare state, such as health, retirement, public transportation, art, higher education and more.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 349, 'Israeli Wars in Israeli Movies', 3, '{'HU','G'}', 'From its first day, the State of Israel has been in a state of war with the Arab countries. Every few years, the fighting erupted into an open war. At the beginning of the country, there was a consensus that the wars were forced on the Israelis, and therefore, the heavy toll on human life was unavoidable. Since the Six Day War, 1967, and the political change in 1977, Israeli society is split in attitude toward war. The absolute belief in the political and military leaders was shaken, and new political agendas entered the political arena. To this divided society, the Intifadas occurred with the waves of terrorism. These events added fire to the heated debates in Israel. One means of expression is through the cinema. In this course, students watch, analyze, and discuss selected movies on the Israeli wars and terrorism in the streets of Israel. Through these movies, produced by Israelis, we'll try to understand the mood and attitude of the Israelis toward war, enemies, and political and military leaders.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 375, 'Contemporary Culture of Israel', 3, '{'HU','G'}', 'Intense study of aspects of historical, social, political, and cultural modern life in Israel. Beginning of Zionism to present day.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 411, 'Judaism and the Beginnings of Christianity', 3, '{'HU','H'}', 'Studies the beginnings of Christianity in relation to Judaism: their common context, the process of their distinction and their mutual influence. Through a historical and thematic approach, explores the parallel construction of Jewish and Christian identities. Combines readings from primary sources (Hellenistic Judaism, Qumran, New Testament, Early Christianity, Rabbinic Judaism) and up-to-date scholarly literature.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HEB', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 101, 'ASU and Herberger Institute Experience', 1, '{}', 'Introduces all new first-time ASU students to the unique elements, culture, challenges and opportunities of their university and the Herberger Institute for Design and the Arts. Explains and defines success with integrity, ASU resources and involvement, the Importance of arts to society and the community, personality and learning style, and success in college and beyond.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 111, 'Art and Society', 3, '{}', 'Introduces the interplay between art and society, particularly how one influences the other.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 125, 'Survey of the Fashion Industry', 3, '{'HU'}', 'Prepares the student to know how the fashion industry operates and what to expect when working in it. From manufacturing to retail, it is important for all members of the fashion industry to understand how the processes interrelate.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 210, 'Creativity and Innovation in Design and the Arts', 3, '{}', 'Designed to get students thinking in a focused manner about creative practice and process. Many young designers and artists arrive at higher education in their discipline having taught themselves or having received a piecemeal education in the directed application of ideas. Aids students in knowing why and how they are creative. Covers types of creativity, how inspiration happens, how to take a project from idea to reality, how to work creatively in teams and how to fuse creative ideas with an understanding of environment to help realize work in socially useful ways.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 225, 'Fashion Design', 3, '{}', 'Expands upon fashion design fundamentals learned earlier to encompass additional aspects of the fashion industry, which includes fashion as business, and as a sociological and cultural phenomenon.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 252, 'Foundations Arts and Design Entrepreneurship', 3, '{}', 'Develops entrepreneurial knowledge and skills supporting the self-actualization, self-efficacy and market acumen of artists and designers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 296, 'Creative Futures: Studio', 3, '{}', 'Develops skills in creating fully realized design ideas and creative ventures, while consciously developing skills in self-analysis, cultural competencies and communication. Through the coursework, readings and projects, students develop new methods and systems of thought for dealing with challenging, complex, collaborative projects. In sequence with the sequel course, these courses are designed as a critical 'making' component of the BS in Technological Leadership.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 310, 'Socially Engaged Practice', 3, '{}', 'Students gain a wide literacy in the community arts field through reading, discussion and technical practice. Examines theories and techniques of community-based project development. Pairs artists with communities to create art rooted in the needs, values, desires, triumphs and challenges of that community. Helps prepare the artist of the 21st century with skills to apply artistic practices in diverse contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 311, 'Work and Careers in the Cultural and Creative Industries', 3, '{}', 'Introduces fundamental questions in the study of work and careers in the cultural and creative industries, including an overview of historical changes in the relationship between the individual and his or her work environment; comparisons between different types of work arrangements (part-time, freelance, internships); and practical implications for contemporary workers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 397, 'Capstone Development', 1, '{}', 'Prepares students for a successful capstone project. Students develop a formal proposal and identify a mentor for the project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 410, 'Design and the Arts Corps Seminar', 3, '{}', 'Explores specific strategies and approaches to socially engaged art practices through readings, visiting artist presentations and direct engagement with community-based projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 420, 'Design and the Arts Business Administration', 3, '{}', 'Design and the arts business seminar course that covers core business principles for design or arts businesses such as human resources, legal issues, accounting and finance. Asks with each of these areas how these disciplines uniquely work within design or arts settings. Investigates global business topics and discusses why ethics is integral to every design or arts business enterprise. Provides a broad understanding of design or arts business activities and practices, allowing more effective participation in business discussions in the future.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 493, 'Honors Thesis', 6, '{}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 496, 'Creative Futures: Advanced Studio', 6, '{}', 'Develops advanced skills in creating fully realized design ideas and leading creative ventures, while consciously developing skills in self-analysis, cultural competencies and communication. Through the coursework, readings and projects, students develop new methods and systems of thought for dealing with challenging, complex, collaborative projects. In sequence with a prerequisite course, these courses are designed as a critical 'making' component of the BS in Technological Leadership.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 498, 'Pro-Seminar', 7, '{'L'}', 'Small-group study and research for advanced students within their majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 510, 'Design and the Arts Corps Seminar', 3, '{}', 'Explores specific strategies and approaches to socially engaged art practices through readings, visiting artist presentations and direct engagement with community-based projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 511, 'Purpose, Collaboration and Accountability', 3, '{}', 'Help students identify their personal purpose and how that connects to arts, culture, design and cross-sectoral work. Also builds capacity for arts and design-based modalities of collaboration that unite people across sectors, art forms, and institutional systems. Addresses hybrid business models, creative facilitation skills, deep listening, co-designing processes and projects, while analyzing the ethics of work aimed at public good outcomes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 512, 'Proposal Development', 2, '{}', 'Students create a draft proposal for their culminating applied projects, research their chosen topic, make a case for how the project intersects with a gap in the field, and learn how to pitch ideas.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 513, 'Applied Creative Research and Evaluation Methods', 3, '{}', 'Working on a real-world project related to art and design in collaboration with a field partner, students learn to construct robust research questions and identify appropriate methods to pursue an area of inquiry. Introduces principles of equitable evaluation and research to prepare students to understand how their work in the broad art and design fields engages with and impacts communities. Explores various methods including qualitative interviews, focus groups, observation and design thinking approaches. Emphasizes understanding how art and design intersect with the converging contexts of place and community, field and discipline, economy and policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 514, 'Creativity and Impact, Opportunities for Change', 3, '{}', 'Art and design work, organizational structures, and collaboration opportunities are expanding. Disrupts frameworks of art and design work to introduce pressing questions and new ways of working aimed at more effective and sustainable engagement with other sectors. Organized around five core themes: context/equity, cross-sector work, changemaking and leadership, structures of work, evaluation and accountability. Discussion is tied to pressing current issues through regular engagements with local and national field leaders, ASU faculty and National Accelerator for Cultural Innovation fellows.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 521, 'Counting Arts and Culture: Reasoning with Empirical Evidence', 3, '{}', 'Builds understanding of economic concepts as applied to the arts and culture sector, with specific attention to how art and culture enterprises work as businesses, how such enterprises function in our wider economic system, which are growing and which disappearing (and why), all toward the goal of building the student's capacity to lead arts and culture enterprise in a complex economic environment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 522, 'Media Literacy and Distribution', 3, '{}', 'Addresses the impact that digital distribution has had on traditional media business models and the way media and our culture are changing because of it. Also looks at the way in which industries are adapting to this change in the media landscape. Three distinct sections: content literacy, production and distribution metrics, economic and cultural implications.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 581, 'Emerging Media Colloquium', 1, '{}', 'Focuses on professional and community development outcomes for the graduate programs in emerging media. Students have significant contact with partners and guests from industry and community, are able to discuss and expand their interests and views with their faculty and peers, share and discuss progress on research and professional projects, create a personal professional development plan, and refine and present their portfolio of work for public review. In addition, this course serves as a point of connection between student cohorts and faculty for the purposes of social/community development and wellness.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 583, 'Fieldwork', 12, '{}', 'Structured, supervised field experience in a field science or other discipline requiring experience in field techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 584, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 592, 'Research', 12, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a thesis or dissertation, report or publication. Assignments might include data collection, experimental work, data analysis or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 595, 'Continuing Registration', 1, '{}', 'Used in situations where registration is necessary but where credit is not needed. Replaces arbitrary enrollment in reading and conference, research, thesis, dissertation, etc. Used by students when taking comprehensive examinations, defending theses or dissertations, or fulfilling the continuous enrollment requirement in doctoral programs. Credit is not awarded, and no grade is assigned.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HDA', 599, 'Thesis', 12, '{}', 'Supervised research focused on preparation of thesis, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 510, 'Introduction to Higher Education', 4, '{}', 'Overview of American higher education, including philosophical, political, and social aspects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 527, 'Seminar: Student Affairs Administration', 3, '{}', 'Organizational models, administrative competencies and skills, and emerging challenges of student affairs administration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 573, 'Applied Inquiry in Higher Education', 3, '{}', 'Provides an overview of research, evaluation and assessment practices in higher education and strategies for utilizing these practices as professionals in higher education. Focuses on learning about and demystifying the process of scientific inquiry, with the intent of translating that knowledge to regular practice in a higher education professional career. It is not a highly technical methods course--no advanced knowledge of statistics or qualitative techniques is assumed--but instead gives practitioners a foundation in educational research design as well as the tools to be literate, thoughtful, critical and frequent consumers of research.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 593, 'Applied Project', 12, '{}', 'Preparation of a supervised applied project that is a graduation requirement in some professional majors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 603, 'Foundation of Data Analytics in Higher Education', 1, '{}', 'How decisions are made in higher education and how data and analytics can support university leaders.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 604, 'Analytics Process and Technology in Higher Education', 2, '{}', 'Process and technology used to facilitate data-informed decision making in higher education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 605, 'Data Management and Preparation for Higher Ed Analytics', 3, '{}', 'Data preparation and management for data-informed decision making in higher education institutions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 606, 'Advanced Analytic Methods for Higher Education', 3, '{}', 'Advanced analytic methods, including data mining, in support of data-informed decision making in higher education institutions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 607, 'Visualization and Presentation for Higher Education', 3, '{}', 'Principles of effective presentation and visualization of data for exploratory, predictive and persuasive purposes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 608, 'Trends in Advanced Analytic Methods for Higher Education', 3, '{}', 'Emerging advanced analytic method in use within higher education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 620, 'Diversity in Higher Education', 3, '{}', 'Overview of the demographic profile of college students, faculty, and staff. Addresses issues of access, retention, and development.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 634, 'The American Community College', 3, '{}', 'Philosophy, history, curriculum, faculty, students, organization, financing, research, policy and current issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 644, 'Higher Education Finance and Budgeting', 3, '{}', 'Financial planning and budgeting in higher education institutions. Issues related to financing public and private colleges and universities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 649, 'Law and Policy in Higher Education', 3, '{}', 'Analyzes legal issues related to higher education; examines key court decisions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 679, 'The American College Student', 3, '{}', 'Overview of American college student from demographic, background characteristics, and values/attitudes/perspectives. Includes access, persistence, and degree completion.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 688, 'Organizational Theory', 3, '{}', 'Major views of organizations and their influence on role definition and participant behaviors in educational organization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 689, 'Leadership in Higher Education', 3, '{}', 'Theory and practice of leadership and administration in higher education institutions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 691, 'Seminar', 12, '{}', 'A small class emphasizing discussion, presentations by students, and written research papers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 790, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 792, 'Research', 15, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research that is expected to lead to a specific project such as a dissertation, report, or publication. Assignments might include data collection, experimental work, data analysis, or preparation of a manuscript.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HED', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of dissertation, including literature review, research, data collection and analysis, and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HIN', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HIN', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 100, 'Global History to 1500', 3, '{'HU','H','G'}', 'Surveys human origins; early civilizations in Africa and Eurasia; the ancient Mediterranean; the pre-Columbian Americas; the rise of Islam and Muslim empires; medieval Europe; the Indian sub-continent and imperial China, to 1500.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 101, 'Global History Since 1500', 3, '{'HU','H','G'}', 'Surveys Africa, the Americas, and Eurasia; changes in communication, communities, demography, economics, environment, politics, religion, technology, warfare, and women.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 102, 'Europe and the Mediterranean: Ancient and Medieval', 3, '{'SB','H'}', 'Origins and development of European societies and institutions from the ancient world through the Middle Ages.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 103, 'Early Modern Europe: Renaissance to Enlightenment', 3, '{'(HU','SB)','H'}', 'The development of European societies and institutions from the Renaissance and Reformation through the Age of Enlightenment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 104, 'Modern Europe: French Revolution to European Union', 3, '{'(HU','SB)','G','H'}', 'The development of European societies and institutions from the French Revolution to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 106, 'Asian Civilizations', 3, '{'(HU','SB)','G','H'}', 'Civilizations of China, Japan, and India from antiquity to the 17th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 108, 'Introduction to Japan', 3, '{'SB','G','H'}', 'Historical survey of the people, culture, politics, and economy of Japan, supplemented by audiovisual presentations. Intended for nonmajors.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 109, 'United States to 1865', 3, '{'(HU','SB)','H'}', 'Growth of the Republic from colonial times through the Civil War period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 110, 'United States Since 1865', 3, '{'SB','H'}', 'Growth of the Republic from the Civil War to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 111, 'Introduction to Asia', 3, '{'HU','H','G'}', 'Offers a basic introduction to the geography, history, cultures, states, and societies of South and Southeast Asia as well as China, Japan, and Korea.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 112, 'Foundations of Democracy', 3, '{'HU','H'}', 'Introduces an interdisciplinary study of democracy with special attention to its historical development as a form of government.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 113, 'United States and Arizona Social Studies', 3, '{'SB','H'}', 'Introduces American (including Arizona) institutions integrating geography, government, history and economic perspectives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 114, 'World Social Studies', 3, '{'SB','G','H'}', 'Introduction to the world from the integrated social science perspectives of history, geography, and government.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 130, 'The Historian's Craft', 3, '{}', 'Introduces the ways historians ask questions and propose answers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 141, 'Russian Civilization: From Tsars to Putin', 3, '{'(L','HU)','G','H'}', 'For centuries, Russians have argued about who they are as a people and a nation by turning to their history, religion, literature, arts and language. With a foundation in Russian history and geography and using primary sources, we survey debates over Russian history and civilization to understand how Russians have imagined themselves and their nation and empires.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 191, 'First-Year Seminar', 3, '{}', 'Small course emphasizing student-faculty discussion and interaction.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 194, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 203, 'Historical Themes in Latin America', 3, '{'SB','H'}', 'General introduction to selected themes in Latin American history. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 204, 'Historical Themes in the US', 3, '{}', 'General introduction to selected themes in United States history. May be repeated for credit when topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 210, 'American Social History', 3, '{'(L','SB)','H'}', 'American society from the colonial period to the present. Ethnicity, race, age, and sex as factors in historical experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 211, 'Jews and Judaism in America', 3, '{'SB','H','C'}', 'Chronological analysis of Jews and Judaism in American history and letters.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 214, 'The World's Game: History and Culture of Soccer', 3, '{'G'}', 'Provides a survey of the cultural significance and global reach of soccer, both historically and in contemporary culture. Students study materials from around the world in several mediums; they view films that explore the cultural impact of the game; read literary works that focus on soccer; and engage with scholarly articles and contemporary news media dedicated to the game. Provides a comprehensive sense of the history, personalities, rivalries, fandom, and even the finances and scandals related to the world's game.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 230, 'Introduction to Jewish Civilization', 3, '{'HU','H','G'}', 'Overview of Jewish civilization from antiquity to the present. Answers the following questions: Who are the Jews? What do Jews believe? How did Jews interact with non-Jews? What was the lasting contribution of Jews to the world? In the attempt to answer these questions, provides the skills of being a good college student. Presents Judaism as an evolving civilization with an overarching distinctive culture but with a variety of subcultures and ethnic diversity. Presents the Jewish civilization through literature, films, music, and internet resources, with an overview of its complexity and diversity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 240, 'Introduction to Southeast Asia', 3, '{'(HU','SB)','G'}', 'Interdisciplinary introduction to the cultures, religions, political systems, geography, and history of Southeast Asia.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 245, 'Introduction to India and South Asia', 3, '{'HU','H','G'}', 'Provides an introduction to the history and major contemporary political, economic, social, religious and cultural features of the South Asia region at large and of selected South Asian nations in particular.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 260, 'Introduction to Islam', 3, '{'HU','G'}', 'Examines Islamic beliefs, ceremonies, festivals, and institutions.  Assumes no prior knowledge about Islam.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 280, 'History of Science, Technology and Innovation', 3, '{'SB','G','H'}', 'Fast-paced introduction to the intertwined histories of science, technology and innovation as they shape the globe from the ancients to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 300, 'Experience of America's Modern Wars', 3, '{'H'}', 'Surveys the history of America's wars, military and veterans from the American Revolution through today. Also exposes how American society has understood the role of the military and those who served, both during and after their service in American wars.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 301, 'Global History of Health', 3, '{'SB','G','H'}', 'Examines disease and health transitions in broadest context of human history, from primate ancestors to hunter-gatherers, to transcontinental contact, colonialism, industrialization, urbanization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 302, 'Studies in History', 3, '{}', 'Specialized topics in history. Explores countries, cultures, and issues in history, and their interpretation in historical scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 303, 'Studies in Asian History', 3, '{}', 'Specialized topics in Asian history. Explores countries, cultures, and issues in history, and their interpretation in historical scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 304, 'Studies in European History', 3, '{}', 'Specialized topics in European history. Explores countries, cultures, and issues in history, and their interpretation in historical scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 305, 'Studies in Latin American History', 3, '{}', 'Specialized topics in Latin American history. Explores countries, cultures, and issues in history, and their interpretation in historical scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 306, 'Studies in United States History', 3, '{}', 'Specialized topics in United States history. Explores regions, cultures, and issues in history, and their interpretation in historical scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 307, 'Studies in African History', 3, '{'H'}', 'Specialized topics in African history. Explores countries, cultures, and issues in history, and their interpretation in historical scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 308, 'History and Philosophy of Sustainability', 3, '{}', 'Examines sustainability issues in a broadly interdisciplinary way, but with a special emphasis on humanities perspectives, focusing on human ideas, values and institutions as they have evolved over time.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 309, 'Exploration and Empire', 3, '{'(L','HU)','H'}', 'Surveys European discovery, exploration, and imperialism in the early modern and modern periods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 310, 'Film as History', 3, '{'HU'}', 'In-depth study of moving image media as recorder, object, and writer of history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 312, 'History of Women in Science and Medicine', 3, '{'HU','SB'}', 'In-depth inquiry into the history of gender issues in Western science and medicine from Greco-Roman antiquity through the beginning of the 21st century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 314, 'American Cultural History Since 1865', 3, '{'(HU','SB)','H'}', 'Culture, including ideas, ideals, the arts, and social and economic standards, from the age of industrialism to modern U.S.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 315, 'Political History of the U. S.', 3, '{'SB','H'}', 'American political history since independence, focusing post-1865. Evaluates major trends in issues, presidential leadership, elections, and state politics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 316, '20th-Century U.S. Foreign Relations', 3, '{'SB','H'}', 'U.S. relations with foreign powers from the late 19th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 317, 'History of Postwar U.S. Conservatism', 3, '{}', 'Traces the origins of U.S. conservatism as it developed in the work of U.S. and European thinkers, intellectuals and authors after WWII.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 318, 'History of Engineering', 3, '{'(L','SB)','G'}', 'The history of engineering from the earliest record to modern times, examining the social, cultural, and economic effects on society.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 319, 'History of Aviation', 3, '{'SB','H'}', 'The history of aviation, focusing on technical, political, economic, social and cultural aspects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 320, 'U.S. Urban History since 1850', 3, '{'SB','H'}', 'History of the city in American life from the mid-19th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 321, 'Constitutional History/US to 1865', 3, '{'SB','H'}', 'Origin and development of the American constitutional system from colonial period through the Civil War.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 322, 'Constitutional History of the United States Since 1865', 3, '{'H'}', 'Development of the U.S. constitutional system from Reconstruction to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 323, 'Historical Studies in Race, Crime, and the Law', 3, '{}', 'Studies the historical relationship between the criminal justice system in the United States and a segment of society that has traditionally faced official and unofficial discrimination: American racial minorities. Focuses on the experiences of Mexican Americans and Africans Americans with the criminal justice system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 324, 'Food in Latin America', 3, '{'SB','G','H'}', 'Uses food as a critical lens for studying Latin American culture and history and looks at how the evolution of human development, and especially the food that we eat, is influenced by social, political and economic context. Studies the production and commodification of food in Latin America, and its globalization. Also covers how Latin American cuisine sheds light on the formation of cultural identity and perceptions of nationalism.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 325, 'Immigration and Ethnicity in the United States', 3, '{'SB','H','C'}', 'Origins, historical development, and future of a multiethnic society, 1492 to 2050.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 326, 'The Global History of the Trans-Atlantic Slave Trade', 3, '{'SB','H'}', 'Reading- and writing-intensive course that employs critical thought to look at the origins, development, abolition, and impact of Atlantic slave trade as a global economic enterprise and great human tragedy; slavery experience.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 327, 'Women in U.S. History, 1600 to 1880', 3, '{'(HU','SB)','C','H'}', 'Historical discussion of American women of diverse racial, religious, and ethnic groups and classes; focuses on changing definitions of women's roles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 328, 'Women in U.S. History, 1880-1980', 3, '{'(HU','SB)','C','H'}', 'Historical discussion of American women of diverse racial, religious, and ethnic groups and classes; focuses on changing definitions of women's roles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 329, 'Women in 20th-Century U.S. West', 3, '{'H','C'}', 'Nuanced exploration of how women of various cultures have contended for and shaped the U.S. West, including the West ofimagination.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 330, 'Historical Thinking', 3, '{}', 'Advanced examination of the ways historians ask questions and propose answers.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 331, 'Mexican American History to 1900', 3, '{'SB','H','C'}', 'Mexican American history from pre-Hispanic origins to frontier journeys north through 19th-century life in the U.S. Southwest.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 332, 'Mexican American History Since 1900', 3, '{'SB','H','C'}', 'Traces the formation of Mexican American communities across the rural and urban U.S. and examines 20th-century immigration from Mexico.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 333, 'African American History to 1865', 3, '{'SB','H','C'}', 'In-depth examination of African Americans' roles in American history, thought, and culture from slavery to 1865.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 334, 'African American History Since 1865', 3, '{'(HU','SB)','C','H'}', 'In-depth examination of African Americans' role in American history, thought, and culture from 1865 to present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 335, 'History of Black Women in America', 3, '{'SB','H'}', 'Advanced examination of the challenges that black women have faced historically in America and how they have responded to those challenges socially, politically, economically, and culturally.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 336, 'Islamic Civilization', 3, '{'HU','H'}', 'Global historical survey of Islamic cultures and societies up to the modern period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 337, 'American Indian History to 1900', 3, '{'(HU','SB)','C','H'}', 'Cultural, economic, political, and social continuity and change of American Indian communities to 1900.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 338, 'American Indian History since 1900', 3, '{'(HU','SB)','C','H'}', 'Cultural, economic, political, and social continuity and change of American Indian communities from 1900 to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 339, 'Islam in the Modern World', 3, '{'HU','G'}', 'Examines the worldwide transformations of Islamic religion, cultures, and societies in the modern period.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 340, 'American Military History', 3, '{'SB','H'}', 'Studies the role of the military in American life during war and peace from colonial times to the present day.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 341, 'U.S. West 19th Century', 3, '{'SB','H'}', 'Social, political, and economic development of the trans-Mississippi West, beginning with the Louisiana Purchase and ending in 1900.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 342, 'U.S. West 20th Century', 3, '{'SB','H'}', 'Role of the western states in U.S. history since 1890 emphasizing politics, the environment, industry and labor, and ethnic minorities.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 343, 'American Southwest', 3, '{'(L','SB)','H'}', 'Development of the region from 1848 to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 344, 'Arizona', 3, '{'SB','H'}', 'Emergence of the state from early times to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 345, 'Environmental History', 3, '{'L'}', 'Examines the interaction between humans and the natural world in the United States from the late 19th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 346, 'Ancient Greece II: Late Classical and Hellenistic Period', 3, '{'SB','H'}', 'Traces the transformation of the Greek world between the end of the Peloponnesian War (404 B.C.) and the absorption of the last major Greek state by growing power of Rome (30 B.C.), with special attention to how Greeks in this period lived and thought and how the period's political and economic changes helped to create new forms of culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 347, 'Ancient Greece I: Bronze Age through the Peloponnesian War', 3, '{'SB','H'}', 'Traces Greek History from its prehistoric beginnings through the Peloponnesian War (431-404 B.C.), with a special focus on the Archaic (ca. 800-480 B.C.) and Early Classical periods.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 348, 'Rome', 3, '{'SB','H'}', 'History and civilization of Rome from the beginning of the Republic to the end of the Empire.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 349, 'Early Middle Ages', 3, '{'(HU','SB)','H'}', 'Political, socioeconomic, and cultural developments of Western Europe from the 5th through 10th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 350, 'Later Middle Ages', 3, '{'(HU','SB)','H'}', 'Political, socioeconomic, and cultural developments of Western Europe from the 11th through 15th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 351, 'Renaissance Europe', 3, '{'(L','HU','SB)','H'}', 'Culture of the Renaissance in Italy and Northern Europe from the 14th to the early 16th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 352, 'Europe's Reformations', 3, '{'(L','HU','SB)','H'}', 'Causes and implications of the major Protestant, Catholic, and Radical religious reformations in 16th- and 17th-century Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 353, 'Old Regime in Europe', 3, '{'SB','H'}', 'Society and culture of Europe during the 17th and 18th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 354, 'Revolutionary Europe', 3, '{'SB','H'}', 'Political, social, economic, and intellectual currents in Europe from the French through the Russian Revolutions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 355, 'Total War and the Crisis of Modernity', 3, '{'SB','G','H'}', 'Forces of change and instability in early 20th-century Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 356, 'Europe since 1945', 3, '{'SB','G','H'}', 'Europe in its world setting since World War II, emphasizing major political and social issues from 1945 to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 357, 'Historical Perspectives on Philosophy and Religion', 3, '{}', 'Comprehensive, advanced study of central concepts in the philosophy of religion in the history of Western philosophy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 358, 'Jewish History from Antiquity to 1492', 3, '{'SB','H'}', 'Continuity and change in political, legal, economic and sociocultural history of the Jews from ancient through medieval times.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 359, 'Jewish History from 1492 to 1948', 3, '{'SB','G','H'}', 'Jewish history from early modern through modern times, highlighting emancipation, enlightenment, and Jewish responses to modernity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 360, 'The Crusades: Religion & Conflict in the Middle Ages', 3, '{'SB','H'}', 'Examines the origins, the development, and the outcomes of the movement that we call 'crusades' by connecting the military aspect of the crusades to broader developments. Focuses primarily, but not exclusively, on the period 1095-1453. Geographically covers chiefly the Mediterranean, and, to a lesser extent, northeastern Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 361, 'Witchcraft and Heresy in Europe', 3, '{'(L','HU)','H'}', 'Background, origins, and development of the Inquisition; persecution of women and marginal groups.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 362, 'Sex and Society in Classical and Medieval Europe', 3, '{'SB','H'}', 'Family life, sex roles, and marriage, and their relationship to political, economic, and religious change in classical and medieval Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 363, 'Sex and Society in Early Modern Europe', 3, '{'(HU','SB)','H'}', 'Family life, sex roles, and marriage and their relationship to political, economic, and religious change in early modern Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 364, 'Sex and Society in Modern Europe', 3, '{'(L','SB)','H'}', 'Family life, sex roles, and marriage, and their relationship to political, economic, and social changes in modern Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 365, 'World Wars I and II: Europe's Eastern Front', 3, '{}', 'In-depth study of the history of the Eastern Front in Europe in the two world wars.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 366, 'England to 1689', 3, '{'SB','H'}', 'In-depth examination of the political, economic, and social development of the English people to the late 17th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 367, 'Modern Britain', 3, '{'SB','H'}', 'In-depth examination of the political, economic, and social development in Britain from 17th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 368, 'Culture and Imagination in European History', 3, '{'HU','H'}', 'Topics in European cultural and intellectual history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 369, 'History and Memory of the Holocaust', 3, '{}', 'In-depth study of the history of the genocide of Jews, the rise of modern antisemitism within the context of Nazi Germany, and the memory and memorialization of the Holocaust post 1945.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 370, 'Eastern Europe in Transition', 3, '{'SB','G','H'}', 'Democratization, privatization, and identity transformations since the fall of communism in contemporary Eastern Europe and the former Soviet Union.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 371, 'Language Hegemony and Culture', 3, '{}', 'Historical and cultural analysis of language and the ideological constructs imposed upon resident populations in the Southwest North American region since 1540. Analyzes the suppositions, ideologies, premises and mechanisms by which Spanish and English became imposed among Native peoples and later national populations of Mexicans during the 19th century and continuing with their descendants and migrating populations crossing the bifurcated border.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 372, 'Modern Middle East', 3, '{'SB','G','H'}', 'In-depth examination of the effect of the West and modernization on Middle Eastern governments, religion, and society in the 19th and 20th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 373, 'Roman History I: Foundations and Republic', 3, '{}', 'Traces the growth of Roman power from Rome's humble beginnings in the eighth century B.C. to its domination of the Mediterranean and the transformation of the Roman Republic into the Roman Empire after the Battle of Actium (31 B.C.).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 374, 'Roman History II: The Roman Empire', 3, '{}', 'The Roman Empire from the reign of the first emperor, Augustus (27 B.C.-A.D. 14), to that of Justinian (A.D. 527-565), focusing on the lives of the Roman emperors, the society and culture of the empire, and the profound changes that eventually contributed to the empire's collapse.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 375, 'Colonial Latin America', 3, '{'SB','H'}', 'Ancient civilization, exploration and conquerors, and colonial institutions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 376, 'Modern Latin America', 3, '{'SB','G','H'}', 'In-depth examination of the development of independent Latin American republics since 1821.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 377, 'Sports in United States History', 3, '{'(HU','SB)','H'}', 'Focuses on major themes in American sports history, organized topically around fall sports. Themes encourage students to think critically about relationships among sport and society, identity, community, media, capitalism and education. Lectures, discussions, readings and films explore race, gender, class and sexuality as categories of analysis, and the role of sport in constructing ideas about the body along gender and racial lines. Also highlights social justice issues in sport as well as athlete activism throughout the twentieth century. Explores the ways in which the participation of Black, Latino/a and Native American athletes has both mirrored and diverged from Black, Latino/a and Native American citizens' participation in society throughout United States history. Also studies the process through which the equal opportunity to participate in school sports became defined as a civil right and, more recently, a human right.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 379, 'History of College Sports', 3, '{}', 'Student-controlled intercollegiate contests quickly transformed into big-time college sports by the early twentieth century, revolutionizing the sports industry in the United States as well as the landscape of higher education. The American college sports enterprise is unique; nowhere else in the world does a large, amateur, revenue-generating system of school sports exist. After an exploration of the origins of intercollegiate athletics and the creation of the modern National Collegiate Athletic Association, topics include integration, women's intercollegiate athletics, the big business of amateurism, and scandal. Video lectures, readings and films introduce students to these often controversial topics, grounding them in the historical record. Many of the current debates concerning college sports have been taking place for more than a century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 380, 'Studies in the History of Science', 3, '{}', 'Major themes in the history of science with varying topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 381, 'Studies in the History of Ideas', 3, '{}', 'Major themes in the history of ideas with varying topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 382, 'Studies in the History of Innovation and Technology', 3, '{}', 'Studies the development and impact of innovation processes and technology on society, culture, and environment throughout history. Specific themes and topics vary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 383, 'China', 3, '{'SB','H'}', 'Political, economic, social, and cultural history of the Chinese people from early times to the 17th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 384, 'Modern China: 1700 to the Present', 3, '{'SB','G','H'}', 'Political, economic, social, and cultural history of the Chinese people from the 17th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 385, 'History of Chinese Medicine', 3, '{'HU','H','G'}', 'Explores developments of Chinese traditions dealing with the natural world, science, and medicine.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 386, 'Interpreting China's Classics', 3, '{'(L','HU)','H'}', 'Study of selected Confucian and/or Taoist classics and ways they have been read in both Asian and Western scholarship.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 387, 'Japan', 3, '{'(L','SB)','H'}', 'Political, economic, social, and cultural history of the Japanese people from early times to the 17th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 388, 'Japan', 3, '{'SB','G','H'}', 'Political, economic, social, and cultural history of the Japanese people from the 17th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 389, 'Japan Society and Values/Premod', 3, '{}', 'Effects of economic and social transitions on personal and social values as reflected in the dramatizations of contemporary events.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 390, 'Jewish Philosophy', 3, '{}', 'The history of Jewish philosophy from its origins in the Hebrew Scriptures to postmodern expressions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 391, 'Modern Southeast Asia', 3, '{'SB','G','H'}', 'Vietnam, Laos, Cambodia, Thailand, Burma, Malaysia, Singapore, Brunei, Indonesia, and Philippines since 1750: imperialism, revolution, and independence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 392, 'Modern India', 3, '{}', 'In-depth study of modern India and its neighboring countries from the 18th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 395, 'Combat in Film', 3, '{'SB','H'}', 'Interdisciplinary approach to and qualitative method for analyzing war movies and military dramas as both historical and cinematic narratives that represent the experiences of infantry marines and soldiers in 20th- and 21st-century U.S. wars.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 397, 'Greece and Rome at War', 3, '{}', 'Familiarizes students with the major battles, leaders and tactics of Classical Greece and Rome. Supplements lectures with student participation including study of original sources, group presentations, and gaming battle simulations. This multifaceted approach gives a greater sense of the evolution of the leaders, armies and tactical systems of the Greeks and Romans.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 404, 'Conquest and Encounters in Colonial Latin America', 3, '{'L','G'}', 'How did Hernan Corts and Francisco Pizarro defeat the Aztec and Inca Empires? Despite numeric inferiority, limited provisions, and little knowledge about the Americas, Corts and Pizarro defeated advanced civilizations that had experienced warriors and millions of inhabitants. Possible explanations include the conquistador's valor, advanced weaponry, superior military strategy, Indian allies, and biological attacks. In their encounters with native peoples of the Americas, Europeans were not always conquerors. lvar Nez Cabeza de Vaca and Lope de Aguirre failed both in their attempt to conquer a large Indian civilization and in their effort to find mineral wealth. A few Europeans such as Bartolom de Las Casas spoke out against the abuses committed against the Indians during the conquest. Explores these issues from the perspective of both Europeans and native peoples and looks at the long-term impact for Latin America and its peoples.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 405, 'Colonial American History to 1763', 3, '{'SB','H'}', 'Political, economic, social, and cultural history of the colonial era. Concentrates on English colonies, with some consideration of Spanish, French, and other colonial regions in North America.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 406, 'American Revolution/1763-1789', 3, '{'SB','H'}', 'Causes, course, and consequences of the American Revolution culminating in the ratification of the Constitution.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 407, 'Early U.S. Republic/1789-1850', 3, '{'(L','SB)','H'}', 'Political, social, economic, and cultural development of the United States from the Revolution to 1850.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 408, 'Civil War and Reconstruction', 3, '{'(L','SB)','H'}', 'Explores the causes, conduct, and consequences of the American Civil War, concentrating on the years 1848 to 1877.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 409, 'Emergence Modern U.S. 1877-1918', 3, '{'SB','H'}', 'Triumph of modern political, social, and economic structures and values, 1877-1918; role of region, religion, race, and ethnicity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 412, 'Contemporary U.S., 1973- Present', 3, '{'SB','H'}', 'End of the Cold War, political crises, and cultural transformations in the U.S.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 413, 'Contemporary America', 3, '{'(L','SB)','H'}', 'The United States from 1945 to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 414, 'Political History and Leadership', 3, '{}', 'Research seminar in political values and leadership. Combines lecture, readings and guest lectures by leaders in business, public affairs and the media. Culminates in a substantial written research effort.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 419, '20th Century Chicano/a History', 3, '{'C'}', 'Historical development of the Chicano community in the 20th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 423, 'The Tudor Monarchy', 3, '{'SB','H'}', 'Political, cultural, and social foundations of 16th-century England.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 424, 'The Stuart Monarchy', 3, '{'SB','H'}', 'Political, social, economic, and cultural developments in 17th-century England.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 426, 'The British Empire', 3, '{'H'}', 'British imperialism and colonialism in Africa, the Americas, Asia, and the South Pacific.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 427, 'French Revolution/Napoleonic Era', 3, '{'SB','H'}', 'Conditions in Pre-Revolutionary and Revolutionary France; organization of France under Napoleon and impact of French changes upon Europe.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 429, 'Modern Germany', 3, '{'SB','G','H'}', 'Germany since 1871.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 430, 'The Ottoman Empire in the Classical Period', 3, '{}', 'Focuses on the largest and most powerful Islamic polity from the late Middle Ages until the early twentieth century: its formation, institutions, social and cultural life, and its multifaceted impact on Europe and the Near East.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 432, 'Eastern Europe the Balkans, 20th Century', 3, '{'G','H'}', 'Politics and culture in Eastern Europe and the Balkans from World War I to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 435, 'The Russian Empire', 3, '{'SB','H'}', 'Development of Russian imperial institutions and civil society from the 17th to the early 20th centuries.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 436, 'Rise & Fall of Soviet Communism', 3, '{'SB','G','H'}', 'In-depth study of Communist revolutionaries' rule of Russia, focusing on utopian culture, Stalinist terror, heroism in war, and the breakup of the former Soviet Union.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 438, 'Modern Spain', 3, '{'(HU','SB)','G','H'}', 'Cultural, economic, political, and social development of modern Spain.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 439, 'Athenian Democracy', 3, '{'H'}', 'Discussion-based seminar familiarizes participants with the origins, characteristic institutions and political ideology of the Athenian democracy of the 5th and 4th centuries BCE. Engages selected topics in the study of Athenian democracy and gives participants an idea of the substance of contemporary debates within scholarship. Topics will include: The origins of the Athenian democracy; women, slaves, and other subordinate social groups within the democracy; elite critique of democratic ideology and episodes of outright civil war; leaders and masses; the role of the court system within the democracy; democracy and the Athenian empire; and violence in Athenian society. The goal of the course, in addition to a greater familiarity with the Athenian democracy, will be a research paper with a well-chosen topic and a distinctive thesis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 441, 'Latin America and the World Economy', 3, '{'L','G','H'}', 'Globalization did not begin in the late twentieth century, but rather global trade has linked Latin America with the rest of the world for over five hundred years. The region has been an active and important participant in the world economy since Columbus's first voyage in 1492. This course explores the social, cultural, economic, and environmental impact of such participation on both Latin America and the rest of the world. It is organized both chronologically and thematically. We will study both colonial and post-colonial trade. The commodities studied include sugar, rubber, bananas, and cocaine.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 443, 'The United States and Latin America', 3, '{'SB','G','H'}', 'Latin American struggle for diplomatic recognition, attempts at political union, participation in international organizations since 1810, and relations between the United States and Latin America.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 444, 'Latin American Independence', 3, '{'(L','HU)','G'}', 'History of independence movements throughout the Western hemisphere from the late 18th century until the mid-19th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 445, '20th-Century Cuba', 3, '{'SB','G','H'}', 'History of Cuba from colonial era to formation of the early republic; political, economic, social development in late 20th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 447, 'Modern Mexico', 3, '{'SB','H'}', 'In-depth examination of political, economic, social, and cultural developments in Mexico from 1810 to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 448, 'Mexican/U.S. Border', 3, '{}', 'Analyzes the evolution of the Mexican/U.S. border region from the mid-19th century to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 449, 'Mexican Immigration to the U.S.', 3, '{'(L','SB)','H','C'}', 'Assesses the evolution of immigration and emigration public policy since the mid-19th century. Seeks to contextualize the relationship between immigration and other elemental nation-state issues as the economy, education, identify, and civil rights.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 451, 'Chinese Cultural History I', 3, '{'(HU','SB)','H'}', 'China's classics in translation studied both for their intrinsic ideas and for the origins of Chinese thought.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 452, 'Chinese Cultural History II', 3, '{'SB','G','H'}', 'Evolution of Confucian thought, its synthesis with Taoism and Buddhism, and modern reactions against, and uses of, Confucian traditions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 454, 'History of Genocide', 3, '{'G','H'}', 'Examines the individuals and institutions responsible for the most infamous episodes of state-sanctioned violence in the 20th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 456, 'The Vietnam War', 3, '{'SB','G','H'}', 'Intersection of American and Asian histories in Vietnam, viewed from as many sides as possible.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 459, 'Gandhi and the Politics of Nonviolence', 3, '{}', 'In-depth study of Gandhi and the Ghandhian political philosophy and practice of nonviolence.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 460, 'World War II', 3, '{'HU','H','G'}', 'Explores the causes, conduct and consequences of World War II. When did World War II start? How was it fought, and what did that mean for soldiers, the homefront and millions of civilians in Japanese or German occupied regions? What did the world that emerged in its wake look like? The course explores these fundamental questions with an emphasis on multiple explanations of the causes of the war in Asia and Europe and the contingency of Allied victory. Explores the war as both a total and a global war in which perceptions of race, class and gender shaped the experience of combatants and noncombatants, many of whom died in indiscriminate bombing campaigns. Racial ideologies also led to the brutalization of soldiers, which in turn fueled the violence against civilians, as exemplified by the Holocaust and Japanese war crimes. Finally, the course turns to the global order emerging after the war and the different narratives that emerged from it.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 465, 'Women in Europe, 1750 to Present', 3, '{'(HU','SB)','H'}', 'European women's diverse religious, ethnic, national, and economic roles in society, culture, and politics, 1750 to the present.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 480, 'Methods of Teaching History: Classroom Resources', 3, '{}', 'Methods in instruction, organization, and presentation of the subject matter of history and closely allied fields.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 481, 'Methods of Teaching History: Community Resources', 3, '{}', 'Identify community-based resources for teaching history, work with resources, and learn how to integrate them into the secondary classroom.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 482, 'Writing and the History of Science, Ideas and Technology', 3, '{'L'}', 'Writing-intensive course examines intersections between texts and major themes in the history of science and technology.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 484, 'Internship', 12, '{}', 'Structured practical experience following a contract or plan, supervised by faculty and practitioners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 485, 'History in the Wild', 3, '{}', 'History outside of conventional academic texts and purposes. Students craft historical narratives in ways that engage an audience beyond the professor, present information in forms other than (but not excluding) writing, and identify and use sources found outside of archives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 493, 'Honors Thesis', 6, '{'L'}', 'Supervised research or creative activity focused on preparation and completion of an undergraduate honors thesis or creative project.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 494, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 495, 'Methods of Historical Inquiry', 3, '{'L'}', 'Capstone course in history as an academic discipline, including formulation of research questions, use of primary and secondary sources to evaluate, form and support historical arguments. Students draft and revise a scholarly research paper.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 496, 'South, Southeast, and East Asia in the Global Matrix', 3, '{}', 'In this capstone seminar, majors in Asia Studies take up broad comparative themes, integrating knowledge from the major's three tracks (East, South, and Southeast Asia) and executing a research project culminating in a major paper.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 499, 'Individualized Instruction', 3, '{}', 'Provides an opportunity for original study or investigation in the major or field of specialization, on an individual and more autonomous basis.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 502, 'Public History Methodology', 3, '{}', 'Introduces historical research methodologies, techniques, and strategies used by public historians. Readings, short papers, and guest speakers. Required for students in the public history concentration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 504, 'Digital Humanities: Critical Theory and Methods', 3, '{}', 'Digital technologies are now intricately woven into our lives--whether it's in terms of authoring a paper in a text editor, sending texts and emails, or doing data analysis. Popular narratives of the 'big' and the 'new' celebrate and sometimes bemoan the ways that these tools shape our communication, access to information, habits and even our physical bodies. Focuses on the histories of these technologies, their operations, popular conceptions about such tools, and their affordances and limits for humanities scholarship. Students learn how to leverage digital technologies, but also how to critically read them. Broad ranging; in addition to surveying the 'big tent,' students are encouraged to utilize their final projects to engage with specific disciplinary digital tools and methods relevant to their scholarly work.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 515, 'Studies in Historiography', 3, '{}', 'Methods and theories of writers of history.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 520, 'Masters of Nonfiction', 3, '{}', 'Students read and critically discuss works by well-known nonfiction writers to improve their own writing skills. Consists of weekly writing exercises, reviewed with workshop-style discussions centered on individual authors, themes or genres such as history, journalism, personal essays, biography, character profiles, travelogues, book reviews and op/ed commentary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 525, 'Historical Resource Management', 3, '{}', 'Identification, documentation, and interpretation of historic period buildings, sites, and districts. Emphasizes interdisciplinary efforts among historians, architects, and anthropologists.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 527, 'Historical Administration', 3, '{}', 'Preparation of historians in administration of archives and historical sites, museums, societies, and offices in government agencies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 532, 'Community History', 3, '{}', 'Techniques and methods of community history emphasizing local resources. Required for community history option.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 573, 'Museum Administration', 3, '{}', 'Formal organization and management of museums, governance, personnel matters, fund raising and grantsmanship, legal and ethical issues.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('HST', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
