INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 798, 'Research Colloquium', 2, '{}', 'Interactive research forum featuring research presentations and discussion among musicology students, faculty and guest speakers from across campus and other institutions. Encourages students to engage in constructive dialogue with their peers as well as with faculty and guest speakers in a collegial environment conducive to exchange and collaboration. Supervised by a faculty panel from the musicology division.');
