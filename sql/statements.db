INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IAP', 492, 'Honors Directed Study', 6, '{}', 'Independent study in which a student, under the supervision of a faculty member, conducts research or creative work that is expected to lead to an undergraduate honors thesis or creative project.');
