INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('DCI', 707, 'Scholarly Practices Seminar: Proposal Writing', 3, '{}', 'Covers proposal writing for students seeking funding to support scholarly initiatives across research designs and methods.');
