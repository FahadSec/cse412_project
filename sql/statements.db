INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 549, 'Foundations of Music Education', 3, '{}', 'A treatment of historical perspectives, philosophy-aesthetics identified with music education, and learning theories applied to music teaching/learning. Basic research and writing skills appropriate to graduate studies in music education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 550, 'Studies in Music Curricula', 3, '{}', 'Scope and sequence of musical experiences. Develops criteria for the evaluation of music curricula.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 551, 'Listening and Creating', 3, '{}', 'Studies the nature of music listening and music creating and their relationship. Reviews research and pedagogical practices.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 555, 'String Pedagogy and Rehearsal Techniques', 3, '{}', 'String pedagogy and orchestral rehearsal skills for teachers who work with K-12 string classes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 559, 'Social and Cultural Studies in Music Education', 3, '{}', 'Considers how music education is shaped by social and cultural concepts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 560, 'Jazz Pedagogy', 3, '{}', 'Study of pedagogy, repertoire, and technique of instruction in jazz styles, ensemble techniques, and performance practice for school ensembles.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 563, 'Theory and Practice in Music Therapy II', 3, '{}', 'Organization, administration, and use of music in rehabilitation with children and adolescents with special needs.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 564, 'Instrumental Music, Advanced Rehearsal Techniques', 3, '{}', 'In-depth analysis of instrumental techniques in preparation for a thorough discussion of band tuning problems and solutions. Discussion of productive conducting and rehearsal techniques for school music teachers.');
