INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 211, 'Macroeconomic Principles', 3, '{'SB'}', 'Basic macroeconomic analysis. Economic institutions and factors determining income levels, price levels, and employment levels.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 212, 'Microeconomic Principles', 3, '{'SB'}', 'Basic microeconomic analysis. Theory of exchange and production, including the theory of the firm.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 213, 'Honors Macroeconomics', 3, '{'SB'}', 'Introduces modern macroeconomic analysis. Theory of national income, unemployment, inflation, and economic growth and its application to economic policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 214, 'Honors Microeconomics', 3, '{'SB'}', 'Introduces modern microeconomic analysis. Theories of consumer behavior, production and cost. Output and price determination in a variety of market settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 221, 'Business Statistics', 3, '{'CS'}', 'Introduces methods of statistical description, inference, probability, sampling, hypothesis testing and regression analysis with focus on business application.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 231, 'Honors Business Statistics', 3, '{'CS'}', 'Applies statistical methods used in business and economic modeling and research. Emphasizes regression analysis and modeling.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 304, 'Classical to Modern Economic Thought', 3, '{}', 'Provides an introduction to the schools of thought concerned with explaining and influencing economic activity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 306, 'Survey of International Economics', 3, '{'SB','G'}', 'Surveys international trade issues, commercial policy, trade theory, customs unions, and international monetary topics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 312, 'Intermediate Microeconomic Theory', 3, '{'SB'}', 'Expands on models covered in principles. Models consumer and producer choice, market structure and welfare.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 313, 'Intermediate Macroeconomic Theory', 3, '{'SB'}', 'Expands on topics covered in principles. Emphasizes models of growth, employment, economic fluctuations, monetary and fiscal policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 315, 'Money & Banking', 3, '{}', 'Functions of money. Monetary systems, credit functions, banking practices, and central banking policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 331, 'Economics of the European Economic Union', 3, '{'SB','G'}', 'Examines broad economic policies seeking to influence individuals' decisions, the history of European economic development and policies, as well as institutions and social systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 335, 'South Asia and the World Economy', 3, '{'SB','G'}', 'Develops and discusses alternative theories of trade, development, and growth in the context of south Asia and the world economy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 342, 'Sports Economics', 3, '{}', 'Applies microeconomic concepts of competitive and imperfectly competitive markets, collective bargaining, antitrust issues and public finance to the sports industry.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 345, 'Survey of Environmental and Resource Economics', 3, '{}', 'Economic principles applied to private and public policy decisions involving environmental resources, natural resource management, and policy analyses. Focuses on how economic methods can contribute to making better environmental decisions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 355, 'Economics of Healthcare', 3, '{}', 'Uses economic tools and models to discuss and evaluate the American healthcare system. Applies standard concepts in microeconomics, macroeconomics, and public choice economics to the study of the evolution and the current state of the healthcare system and to the study of proposals for change. Focuses on the promise and problems of using economic tools in a historical, ethical, and cultural context to understand the current healthcare system.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 360, 'Economic Development', 3, '{'SB','G'}', 'Theories of economic growth and development. Role of capital formation, technological innovation, population, and resource development in economic growth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 365, 'Economics of Russia and Eastern Europe', 3, '{'SB','G'}', 'Origins and analysis of contemporary institutions. Comparative development and differentiation in the 20th century.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 382, 'Managerial Economics', 3, '{}', 'Applies economic analysis to managerial decision making. Market analysis in the context of the socio-legal environment. Does not satisfy Economics major requirements.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 404, 'History of Economic Thought', 3, '{'SB'}', 'Development of economic doctrines, theories of mercantilism, physiocracy, classicism, neoclassicism, Marxism, and contemporary economics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 410, 'Applied Regression Analysis and Forecasting', 3, '{}', 'Applies regression techniques to forecasting in business and institutional environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 411, 'Current Analysis of the U.S. Economy', 3, '{}', 'Studies the major data releases used by economists to track the U.S. economy. Emphasizes how these data are used and their connection to macroeconomic theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 413, 'Advanced Honors Macroeconomics', 3, '{}', 'Develops and uses various macroeconomic models to evaluate important economic policies. Emphasizes honing students' policy evaluation skills.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 414, 'Advanced Honors Microeconomics', 3, '{}', 'Develops and uses various microeconomic models to evaluate important economic issues. Emphasizes the economics of uncertainty and information.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 416, 'Game Theory and Economic Behavior', 3, '{}', 'Introduces game theory and its application to various economic situations under conditions of complete and incomplete information.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 421, 'Labor Economics', 3, '{'SB'}', 'Labor supply and demand, unemployment, determinants of wages, earnings inequality, labor unions and the structure of labor contracts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 423, 'Economics of Education', 3, '{}', 'Focuses on educational decisions, including individual choices, institutional strategies and government policies. Topics include measuring the costs and returns of education, the relationship between education and inequality, and the role of K-12 and postsecondary institutions in economic growth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 425, 'Introduction to Econometrics', 3, '{}', 'Elements of regression analysis: estimation, hypothesis tests, prediction. Emphasizes use of econometric results in assessment of economic theories.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 436, 'International Trade Theory', 3, '{'SB','G'}', 'Comparative-advantage doctrine, including practices under varying commercial policy approaches. Economic impact of international disequilibrium.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 438, 'International Monetary Economics', 3, '{'SB','G'}', 'History, theory, and policy of international monetary economics. Balance of payments and exchange rates. International financial markets, including Eurocurrency markets.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 441, 'Public Economics', 3, '{'SB'}', 'Public goods, externalities, voting models, public expenditures, taxation, and budget formation with emphasis on the federal government.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 445, 'Environmental Economics', 3, '{}', 'Advanced topics in environmental economics with quantitative methods for developing information that is used for environmental policy making.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 453, 'Industrial Organization and Competition Policy', 3, '{}', 'Development of public policies toward business. Antitrust activity. Economic effects of government policies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 455, 'Health Economics', 3, '{}', 'Focuses on health care decisions, including individual and government policies. Topics include demand for and supply of health care, payment for health care, the impact of health care reform and the techniques economists use to evaluate health care interventions.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 470, 'Mathematical Economics', 3, '{}', 'Integrates economic analysis and mathematical methods into a comprehensive body of knowledge within contemporary economic theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 475, 'Capstone in Economics', 3, '{'L'}', 'Capstone course integrating several areas of economics.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ECN', 479, 'Experiments in Economics', 3, '{}', 'Experiments on risk and time preferences, trust, collective action, social preferences and norms, gender, information, discrimination and market behavior.');
