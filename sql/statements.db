INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('ACO', 501, 'Database Systems and Problem Solving in Python', 3, '{}', 'Design and implementation of databases for scientific applications. Defining and querying database systems using the SQL industry standard language. Data exchange using XML. Programming in Python to solve realistic problems using scientific data.');
