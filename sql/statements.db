INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 260, 'Repertoire for Music Therapy', 2, '{}', 'Music skills repertoire for music therapy, including units on brass, strings, woodwinds, electronic instruments, computer music, and improvisation techniques.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 261, 'Music Therapy Treatment Process', 3, '{}', 'Orientation to preclinical experience with emphasis on observation skills, assessment, goal setting, and professional`ethics. Requires off-campus observations.');
