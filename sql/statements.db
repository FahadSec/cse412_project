INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IGD', 799, 'Dissertation', 15, '{}', 'Supervised research focused on preparation of a dissertation, including literature review, research, data collection, analysis and writing.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 110, 'Introduction to Integrated Behavioral Health', 3, '{}', 'Introduces major clinical concepts and practice models related to team-based medical and behavioral health care integration.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 120, 'Introduction to Integrated Health Care Systems', 3, '{}', 'Provides an introduction and overview of the American health care system and its driving forces, with a focus on the move to integrated health care systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 415, 'Behavioral Health Over the Lifespan', 3, '{}', 'Common issues and concerns faced throughout the different life stages and the associated behavioral health needs. Presents evidence-based strategies for primary prevention and early intervention. Provides case examples to effectively illustrate the application of the key constructs addressed in the course.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 420, 'Population Health Management', 3, '{}', 'Presents an overview of population health and its management. Emphasizes a solutions-based perspective, consistent with the Quadruple Aim.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 425, 'Cognitive and Behavioral Counseling', 3, '{}', 'Frameworks for conceptualizing health behavior change. Reviews evidence-based approaches that address patient's behavioral health needs in primary care and other medical settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 430, 'Medical Pathophysiology, Terminology and Behavioral Health', 3, '{}', 'Provides an overview of pathophysiology and medical treatment of common medical conditions in primary care (e.g., diabetes, coronary heart disease, hypertension, asthma, cancer). Emphasizes medical terminology, pathophysiology, common medical treatments and behavioral health issues associated with those conditions that students can expect to see in integrated medical settings, especially integrated primary care. Students learn appropriate medical terminology for symptoms, diseases, treatments and medical tests so they can communicate effectively with inter-professional medical teams. Discusses the role of the behavioral health provider as it relates to these conditions as well.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 520, 'Integrated Health: The Patient-Centered Medical Home (PCMH)', 3, '{}', 'Covers the Patient-Centered Medical Home (PCMH), with an emphasis on alignment in primary care and other integrated health care systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 530, 'Evidence-Based Integrated Health Care', 3, '{}', 'Provides a health care manager's introduction to evidence-based guidelines for integrated health care design and delivery systems.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 540, 'Integrated Health Care Process Improvement', 3, '{}', 'Team-based quality improvement evaluation, measurement and health care system transformation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 550, 'Integrated Health Care Operations', 3, '{}', 'Integrated health care operations, including an in-depth exploration of health care and integrated care systems, planning, program assessment, implementation and change management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 560, 'Integrated Health Care Data Analytics', 3, '{}', 'Data analytic tools and techniques used to plan, implement and improve integrated health care transformation.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 570, 'Integrated Health Care Financial Management', 3, '{}', 'Integrated health care financial management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 585, 'Team-Based Consultation and Leadership', 3, '{}', 'Team-based leadership and consultation in integrated health care.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 586, 'Behavioral Health Professional Writing Style', 3, '{}', 'Focuses on improving overall professional writing skills, with an emphasis on how to write a literature review, use APA format, cite sources ethically and access available writing resources as necessary.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('IBC', 590, 'Reading and Conference', 12, '{}', 'Independent study in which a student meets regularly with a faculty member to discuss assignments. Course may include such assignments as intensive reading in a specialized area, writing a synthesis of literature on a specified topic, or writing a literature review of a topic.');
