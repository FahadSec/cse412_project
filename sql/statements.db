INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MHL', 598, 'Special Topics', 4, '{}', 'Topical courses not offered in regular course rotation--e.g., new courses not in the catalog, courses by visiting faculty, courses on timely topics, highly specialized courses responding to unique student demand.');
