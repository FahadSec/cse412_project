INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BIS', 343, 'Social Processes in Organizations', 3, '{'SB'}', 'Examines social processes and human interactions within different organizational contexts and engages in experiential learning through team-based projects.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BIS', 345, 'Organizational Ethics', 3, '{'HU'}', 'Explores and considers normative and applied ethics within a variety of organizations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BIS', 350, 'Diversity and Organizations', 3, '{'L','C'}', 'Students engage in a critical analysis of individual, cultural and organizational diversity.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BIS', 353, 'Interdisciplinary Leadership', 3, '{'SB'}', 'Offers interdisciplinary perspectives on the theory and practice of leadership in a range of organizational environments');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BIS', 357, 'Assessment in Organizations', 3, '{}', 'Considers the role and function of assessment in effective organizations. Provides the knowledge and tools required to identify, collect and analyze information to assess organizations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 220, 'Foundations of Structured English Immersion', 3, '{'L'}', 'Examines current educational practices and historical legal issues. Prepares teacher candidates with a provisional Structured English Immersion endorsement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 221, 'Theories for Acquiring and Learning Languages', 3, '{}', 'Languages are tools people use and learn to use for a variety of purposes. While nearly all people naturally acquire at least one language in their lifetime, the construct of bilingualism or multilingualism can occur as either an elective or a forced process. The context or the decision that leads to second or multiple language learning can have profound effects on the way and the extent to which those languages are learned. Examines the variables affecting both language acquisition and language learning as well as some methods to better facilitate these processes.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 322, 'Reading Methods in BLE/ESL Settings', 3, '{}', 'Instructional/assessment strategies, including systematic, research-based phonics, vocabulary, comprehension, spelling, writing, phonemic writing in Spanish/English.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 324, 'Social Studies for Diverse Language Classrooms', 3, '{}', 'Social studies education standards, curriculum, and instructional strategies for English language learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 335, 'Language Diversity in Classrooms', 3, '{'SB'}', 'Issues in sociolinguistics and language variation in schools with a focus on classroom interaction, instruction, curriculum, assessment, and language policy.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 336, 'Culturally Sustaining and Transformative Pedagogies', 3, '{}', 'Prospective educators examine culturally sustaining and transformative pedagogies (CSTP). Use of CSTP frames their reflection on educator positionality, attunement to cultural and community knowledge resources in field experiences, and development of their own teacher praxis plans oriented toward CSTP.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 338, 'Foundations of Second Language Acquisition and Development', 3, '{'L'}', 'Introduces educators to theoretical and critical perspectives related to the nature of language, language variation and language acquisition among second language learners and bi/multilinguals, with considerations for bilingualism and biliteracy development, instruction and assessment in various K-12 education settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 396, 'Clinical Experience I', 2, '{}', 'Applies course content in BLE/ESL elementary education classrooms grades 1-8: emphasizes observation, pupil management, planning and delivery of instruction, and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 397, 'Clinical Experience II', 2, '{}', 'Applies course content in BLE/ESL elementary education classrooms grades 1-8: emphasizes observation, pupil management, planning and delivery of instruction, and assessment.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 400, 'Principles of Language Minority Education', 3, '{}', 'Overview of philosophical and theoretical foundations of bilingual education and ESL models of instruction. Other topics include significant legislative and judicial measures.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 404, 'Instructional Design for Culturally and Linguistically Diverse Students', 3, '{}', 'Provides a foundation in instructional design to meet the academic and linguistic needs of culturally and linguistically diverse students, with a focus on English language learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 407, 'SEI for Secondary Students', 3, '{'L'}', 'Examines foundations, assessment, English language learner proficiency standards, and strategies (including SEI) for the 7-12 classroom.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 408, 'Structured English Immersion (SEI) Methods', 3, '{'L'}', 'Examines current educational practices and historical legal issues. Prepares teacher candidates with a provisional Structured English Immersion endorsement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 411, 'Science Curriculum, Instruction and Assessment in BLE/ESL Settings', 3, '{}', 'Methods, management strategies, and assessment procedures for teaching science to BLE/ESL students in elementary schools.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 412, 'Mathematics Curriculum, Instruction and Assessment in BLE/ESL Settings', 3, '{}', 'Teaching, management, and assessment of mathematics in K-8 BLE/ESL settings.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 413, 'Language Arts in BLE/ESL Settings', 3, '{}', 'Literacy and language arts instruction; assessment of reading and writing; transition literacy skills, particularly critical reading of content.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 436, 'Equitable Practices in Diverse Classrooms', 3, '{}', 'Provides theoretical lenses to analyze the culture of schools and classrooms and equips future educators to recognize the hidden curriculum in classroom organization, student regulation and disciplinary actions, discourse, relationships, pedagogical choices, and other environmental and curricular factors that perpetuate inequities. Explores tools and approaches to transform the educational context and to advocate for just and equitable learning experiences for historically marginalized youth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 478, 'Student Teaching: BLE/ESL Elementary Schools 1-8', 15, '{}', 'Student teaching in BLE/ESL elementary education classrooms grades 1-8: synthesized experience in curriculum, instruction, assessment and classroom management.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 481, 'Reading Practicum', 3, '{}', 'Applies concepts from BLE 414. Supervised school-based experiences in teaching reading to BLE/ESL students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 511, 'Introduction to Language Minority Education', 3, '{}', 'Historical, philosophical, theoretical, and pedagogical foundations of language minority education in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 520, 'ESL for Children', 3, '{}', 'Examines approaches to second-language development for children congruent with recent research in second-language acquisition in children.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 522, 'Bi/Multilingual Acquisition and Development', 3, '{}', 'Introduces educators to theory and research related to the acquisition and development of languages among second language learners, and bi/multilinguals, with considerations to language and bi/literacy development and instruction in formal schooling contexts and other learning environments.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 529, 'Language Assessment and Evaluation of ESL/BLE Students', 3, '{}', 'Assessment and evaluation of linguistic minorities; test theory, score interpretation; culturally responsive assessment; assessment of language and school achievement.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 531, 'Linguistically and Culturally Sustaining Pedagogy', 3, '{}', 'Examines asset-based approaches to educating multilingual learners that acknowledge and sustain home culture and identity (including approaches to assessment, instruction, theories of learning and development and advocacy).');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 534, 'Language, Policy and Power: Past and Present', 3, '{}', 'Examines theories, concepts and findings in language policy and planning for educating multilingual learners locally, nationally and globally.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 535, 'Sociolinguistic Issues in Bilingual Education', 3, '{}', 'Surveys major theoretical issues (e.g., language situations, communicative competence, language attitudes) interrelating language, social processes, and bilingual education.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 536, 'Education for Social Transformation', 3, '{}', 'Theoretical and practical issues concerning education for social transformation. Recognizes features, reflections, affordances and challenges to transforming education through interactions and exchanges. Also considers assessment frameworks to assist educating multilingual learners.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 537, 'Linguistics for Educators', 3, '{}', 'Main theories and approaches within linguistics and their application to language learners in educational contexts. Areas include phonetics, phonology, morphology, syntax and translanguaging.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 538, 'Methods for Educating Multilingual Learners', 3, '{}', 'Applied methods and pedagogies for educating multilingual learners. Includes pedagogies supported by second language acquisition (including multilingual perspectives); multiliteracies; and bilingualism including heteroglossic bilingual instructional practices, translanguaging practices, critical multilingual awareness and complex language/literacy use.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 561, 'Parent Involvement in Language Minority Education Programs', 3, '{}', 'Examines issues, approaches, and strategies for improving parental and community involvement in the schooling of language minority children and youth.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('BLE', 580, 'Practicum', 12, '{}', 'Structured practical experience in a professional program, supervised by a practitioner and/or faculty member with whom the student works closely.');
