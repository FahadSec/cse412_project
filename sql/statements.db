INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 101, 'The ASU School of Music Experience', 1, '{}', 'Introduces all new first-time ASU students to the unique elements, culture, challenges and opportunities of their university, the School of Music and the Herberger Institute for Design and the Arts. Covers the music education and music therapy programs in the School of Music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 110, 'Introduction to Music Education', 2, '{}', 'Overview of music education and career options. Orientation to student characteristics, teacher roles, teaching qualities, and philosophical and historical foundations. Requires school observations.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 154, 'Gesture and Sound', 1, '{}', 'Covers fundamentals of musical leadership through gestures associated with sounds in the performance of music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('MUE', 161, 'Introduction to Music Therapy', 2, '{}', 'Overview of the profession of music therapy and its applications in mental health, rehabilitation, and special education.');
