INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 533, 'Research Issues in Child Development', 3, '{}', 'In-depth exploration and critique of research focusing on child development in a family setting.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 534, 'Risk and Resilience in Children and Adolescents', 3, '{}', 'Integrates child development, family theory, and research to understand risk and resilience in children and adolescents.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 570, 'Child Development for Infant Family Practice', 3, '{}', 'Examines development during infancy and early childhood with an emphasis on social and emotional development, mental health, and early parent-child relationships.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 571, 'Infant Mental Health for Infant Family Practice', 3, '{}', 'Surveys the biological and environmental risk factors that jeopardize the mental health of infants and young childre; overview of prevention and intervention strategies.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 572, 'Assessment of Infants, Toddlers, and Families for Infant Family Practice', 3, '{}', 'Developmentally appropriate assessment of mental disorders and developmental disabilities in infants, toddlers, and preschool children and parent-child relationships.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 573, 'Cultural Competence for Infant Family Practice', 3, '{}', 'Examines cultural issues impacting parent-child relationships and child rearing and the effectiveness of infant mental health and early intervention services on families of various ethnic and cultural backgrounds.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 574, 'Observation and Reflection for Infant-Family Practice', 3, '{}', 'Strategies for observing infant behavior and parent-child relationships and skills for reflective, relationship-based intervention.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 575, 'Advanced Observation and Reflection for Infant-Family Practice', 3, '{}', 'Advanced strategies for observing infant behavior and parent-child relationships and skills for reflective relationship-based intervention.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('CDE', 576, 'Applied Research Methods for Infant Family Practice', 3, '{}', 'Survey and critical examination of child development, parent-child relationships, family relationships, and early intervention research and the application to IFP.');
