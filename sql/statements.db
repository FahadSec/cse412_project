INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 200, 'Introduction to Asian Pacific American Studies', 3, '{'(HU','SB)','C'}', 'Examines historical and contemporary issues facing Asian Americans and Pacific Islanders in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 210, 'Introduction to Ethnic Studies in the U.S.', 3, '{'C'}', 'Covers diversity of experiences and relations among racial and ethnic groups in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 220, 'Introduction to Social Transformation', 3, '{}', 'Introduces key concepts in the study of social change, social innovation, and social transformation. Studies examples of social movements, social activism, community organizing, and coalition building and how such efforts have transformed society and culture.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 294, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 310, 'Asian Pacific American Arts and Cultures', 3, '{'HU','C'}', 'In-depth exploration of Asian Pacific American cultural expression in art, literature, film, theatre, dance, and music.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 315, 'Asian Pacific American Literature', 3, '{'HU','C'}', 'In-depth exploration of the literary history, critical reception, and major theories in Asian Pacific American poetry, fiction, and prose.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 330, 'Asian Pacific American Genders and Sexualities', 3, '{'(L','SB)','C'}', 'In-depth exploration of gender and sexuality issues as they relate to Asian Pacific American experiences, including interracial relationships, stereotypes, feminism, queer theory.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 340, 'Asian Pacific Americans and Media', 3, '{'HU','C'}', 'Advanced study of the social construction of Asian Pacific American media images and resistance to those images in various historical contexts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 345, 'Asian Pacific Americans and Film', 3, '{'HU','C','H'}', 'In-depth examination of the representations of Asian Pacific Americans in narrative, popular, experimental, and documentary film.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 347, 'Immigration, Education and the Family', 3, '{'L'}', 'Cultural processes related to immigration and their impact on the social construction of the immigrant family and the education of other children. Immigration theories, immigration debates and discourses, the immigration landscape of the 21st century, conceptualizations of the family, and the methodological challenges family researchers face. Topics include issues pertaining to education and the family and globalization.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 350, 'Inequality and Diversity in Education', 3, '{'L','C'}', 'Advanced exploration of various dimensions of diversity in relation to educational systems and outcomes. Through sociological and psychological research, case studies and films, students examine how educational inequality is created and reproduced through individual actions and institutional constraints. Also considers the connection between schooling and human rights and ways in which educational inequality may be addressed.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 352, 'Borders, Prisons and Suburbs', 3, '{'SB','C'}', 'Explores how the relationship between race and space has been important to the development of broad patterns of social inequality in the United States. Through a variety of readings, films and other material, examines how ideas of race and space have been important to the making and maintenance of national boundaries, citizenship, regional histories and spatial typologies such as border, ghetto, suburb and prison.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 355, 'Asian American Psychology', 3, '{'SB','C'}', 'Reviews the current research and literature about the psychological experiences of Asian Americans, with the appreciation for the historical, sociopolitical and cultural contexts that invariably shape the behavior and well-being of individuals in this diverse population group. Using psychological theory and research as a guiding framework, explores family relationships, identity development, sexuality, model minority stereotypes, racism and discrimination, media images, dating/marriage, race relations, mental health, and other issues affecting Asian American lives.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 356, 'Race and Child Development', 3, '{'SB','C'}', 'Reviews the current research and literature about the particular role of race in child development. Even though race is not a valid biological construct, it has significant social implications as it shapes how children interact and are treated by others. Through primarily class discussions/activities, presentations and papers, addresses: (1) What are the research and theoretical issues in studying race? (2) What does race and racism mean? (3) How does race influence developmental processes?');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 360, 'Asian Pacific American Experience', 3, '{'(HU','SB)','C'}', 'Advanced study of the historical and contemporary experiences of Asian Pacific American racial/ethnic groups in the United States.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 370, 'Indigeneity, Race and Ethnic Relations in Hawaii', 3, '{'H','C'}', 'Examines Native Hawaiian history, culture and worldviews prior to contact with Europe and the United States. Using colonialism, race and racism as the analytical lens, provides a deeper understanding to how racial ideologies and colonialism impacted the cultural, economic, political, environmental and social well-being of the island's indigenous population; the predominately nonwhite immigrant groups that were recruited to work in Hawaii's agricultural industries; the various institutions (e.g., tourism and the military) that continue to maintain structures of inequality and oppression of Kanaka Maoli (Native Hawaiians) and other racial and ethnic groups on the islands; and the ways Native Hawaiians and locals in Hawaii have resisted these efforts.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 394, 'Special Topics', 4, '{}', 'Covers topics of immediate or special interest to a faculty member and students.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 428, 'Critical Race Theory', 3, '{}', 'In-depth analysis of the ways in which race has been historically utilized, constructed, and contested in American civil society.');
INSERT INTO Course (subject, course_number, title, credits, General_Studies, description) VALUES ('APA', 450, 'Asian Pacific American Contemporary Issues', 3, '{'SB','C'}', 'Advanced study of the issues shaping Asian Pacific American communities, including immigration, politics, education, health, family, gender, youth, interracial relations, and other contemporary topics.');
